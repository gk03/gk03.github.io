var twweb="http://www.ghtinc.com/tw/index.php"
var enweb="http://www.ghtinc.com/index.php"
var jpweb="http://www.ghtinc.com/jp/index.php"
var cnweb="http://www.ghtinc.com/sc/index.php"

//document.write(navigator.appName+'<br />');
if( navigator.appName == 'Netscape' )
{
  web=( navigator.language );
}  
else
  web=( navigator.browserLanguage );
// document.write(navigator.appName+'<br />'+web);  

if( web=='zh-TW' ) 
{

   location.replace(twweb);     

}
else if( web=='zh-tw' ) 
{

   location.replace(twweb);     

}
else if( web=='ja' )
{

    location.replace(jpweb);
}    
else if( web=='zh-cn' )
{
    location.replace(cnweb);
}
else if( web=='zh-CN' )
{
    location.replace(cnweb);
}        
else
  location.replace(enweb);
  
