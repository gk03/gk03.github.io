<script>
		$(".gapp").click(function() {
		$('html,body').animate({
		scrollTop: $(".workapp").offset().top-80},
		1000);
		});
		$(".gdivice").click(function() {
		$('html,body').animate({
		scrollTop: $(".blk6").offset().top-80},
		2000);
		});
		$(".sgpanel").click(function() {
		$('html,body').animate({
		scrollTop: $(".totlsec").offset().top-80},
		3000);
		});
		$(".ufeed").click(function() {
		$('html,body').animate({
		scrollTop: $(".slidercon-tp").offset().top-80},
		3000);
		});
		function myHeight(){
			var ww = $(window).width();
			var wh = $(window).height()-80;
			var blkh = wh - $(".blkhead").innerHeight();
			var inrh = $(".inerblk").height();
			var blkp = (blkh - inrh)/2;
				
				if(ww > 991){
	 				$(".blksection").css("min-height", wh );
					$(".blkseccon .mycontainer").css("min-height", blkh );			
					if(blkp > 50){
						$(".blkseccon .mycontainer").css({
							"padding-top": blkp,
							"padding-bottom": blkp
						});		
					}else{
						$(".blkseccon .mycontainer").css({
							"padding-top": 50,
							"padding-bottom": 50
						});
					};

					$('.subMenu, .secsub').smint({
						'scrollSpeed' : 1000
					});
				}else{
					$('.subMenu').smint({
						'scrollSpeed' : 1000
					});
				};
		}
	
		$(document).ready(myHeight);
		$(window).resize(myHeight);
		
		</script>

