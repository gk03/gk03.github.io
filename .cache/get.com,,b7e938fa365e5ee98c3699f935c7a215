            evt.preventDefault();
            var $showed = jQuery(jQuery(this).parent()).find('ul:first');
            var $list = jQuery(jQuery(this).parent()).find('.show-more-list');
            if (jQuery(this).html() == "- Show less") {
                $list.removeAttr('style');
                $showed.removeAttr('style');
                jQuery(this).html(jQuery(this).attr('old-text'));
            } else {
                $list.css('display', 'block');
                $showed.css('margin-bottom', '0');
                jQuery(this).attr('old-text', jQuery(this).html());
                jQuery(this).html("- Show less");
            }
        });

        if (jQuery('.jqDatepicker').datepicker) {
            gns.utils.update_datepicker();
        }

        // Make all checkboxes with attr readonly really readonly
        jQuery('body').on('click', 'input:checkbox[readonly=readonly]', function () {
            return false;
        });

        change_country();

        var show_label = 'True';
        var apply_button_label = 'APPLY NOW';
        if (show_label=='True'){
            jQuery('span.apply-now-button').each(function (){
                if (jQuery(this).parents('.apply-now-wrapper').html()==undefined){
                    var issuer = jQuery('div[cc-issuer]').attr('cc-issuer') || 'issuer';
                    issuer += issuer.endsWith('s') ? "'" : "'s";
                    jQuery(this).html(apply_button_label).wrap("<div class='apply-now-wrapper'></div>");
                    jQuery(this).after('<span class="issuer text-center">At ' + issuer + ' secure website</span>');
                }
            });
        }

        // Close mobile menu whe clicking outside
        if (isMobileVersion()) {
            $("html,body").mouseup(function (e) {
                var subject = $("#main-header");
                if (e.target.id != subject.attr('id') && !subject.has(e.target).length) {
                    jQuery('.menu-section').slideUp('50');
                }
            });
        }

        set_lazyload();
        // Web Font Loading
        loadWebFonts();

        jQuery(document).on('opened', '[data-reveal], [data-dropdown-content]', function () {
            set_lazyload();
        });
        jQuery(document).on('mouseenter click', '.dropdown-toggle, a.toggle-topbar, .has-submenu a, .has-dropdown a', function () {
            set_lazyload(function () {
                resizeRating();
            });
        });
        $(window).on('resize', function(){
            resizeRating();
        });
    });

