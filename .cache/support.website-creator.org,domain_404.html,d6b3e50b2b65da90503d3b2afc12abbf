// Update the url_params from command line params
{
var query = document.location.href;
var url_params = {};
if ( query.indexOf( "?" ) != -1 ) {
  query = query.substring( query.indexOf( "?" ) + 1 );
  var params = query.split( "&" );
  for ( var i = 0; i < params.length; i++ ) {
     var pair = params[ i ].split( "=" );
     if ( pair.length == 2 ) {
        url_params[ unescape( pair[ 0 ] ) ] = unescape( pair[ 1 ] );
     }
  }
}

if ( url_params.message_id && url_params.user_id ) {
  var at = new ActionTrace();
  query = document.location.href;
  if ( query.indexOf( "?" ) != -1 ) {
     query = query.substring( 0, query.indexOf( "?" ) );
  }
  url_params.page = query;
  var user_id = url_params.user_id;
  delete url_params.user_id;
  if ( url_params.action ) delete url_params.action;
  at.AddAction( user_id, 'load page from bugger', url_params );
}
}

