              'title=' + escape( document.title ) + '&' +
              'url=' + escape( document.location.href ) + '&' +
              'node_id=4046626', 400,540, {resizable : 0,scrollbars:0});
   }

   function PrintPreview( sPrintTemplate, iPopupWidth, iPopupHeight, sCopyrightText, sPageEncoding ) {
      var bFirstParam   =  true;
      window.name =  'PrintPageSource';
      if ( typeof(sPrintTemplate) == 'undefined' || sPrintTemplate == '' ) {
         sPrintTemplate = "/Shared/Html/print_page_common.html" ;
      }

      if ( !iPopupWidth  ) { iPopupWidth  =  770; }
      if ( !iPopupHeight ) { iPopupHeight =  620; }
      if ( SK.Singletons.env.get( 'site_title' ) != '' ) {
         sPrintTemplate += "?siteTitle=" + escape( SK.Singletons.env.get( 'site_title' ) );
         bFirstParam =  false;
      }
      var params = new Array( );
      params[params.length] = "pageTitle=" + escape( SK.Singletons.env.get( 'page_title' ) );
      if ( sCopyrightText == '' ) {
         
         var sCustomCopyright    = "";
         var sResellerCopyright  = "Powered%20by%20OurProduct.";
         sCopyrightText          = ( sCustomCopyright == "" ) ? sResellerCopyright : sCustomCopyright;
      }

      params[params.length] = "copyrightText=" + sCopyrightText;
      params[params.length] = "page_encoding=" + sPageEncoding;
      // Make it non-cacheable:
      params[params.length] = "_x=" + ( new Date() ).getTime();

      SKPopup( sPrintTemplate + ( bFirstParam ? "?" : "&" ) + params.join("&"), iPopupWidth, iPopupHeight );
   }

   function GetCopyrightText ( oElmt, str_copyright_text ) {
      if ( typeof( str_copyright_text ) == 'undefined' ) {
         str_copyright_text = "";
      }

      var copyright_elmt_wrapper = ( typeof( oElmt ) != 'undefined' ) ? oElmt : document.getElementById("AREA__FOOTER_SHARED_AREA");

      if ( copyright_elmt_wrapper ) {
         for ( var i = 0; i < copyright_elmt_wrapper.childNodes.length; i++ ) {
            if ( copyright_elmt_wrapper.childNodes[i].innerHTML != 'undefined' ) {
               str_copyright_text = GetCopyrightText( copyright_elmt_wrapper.childNodes[i], str_copyright_text );
            }
         }
   
         if ( str_copyright_text != null && typeof( oElmt ) != 'undefined' ) {
            if ( oElmt.nodeType == 3 && oElmt.nodeValue != null ) {
               str_copyright_text += oElmt.nodeValue + ' ';
            }
         }
      }

      return str_copyright_text;
   }
