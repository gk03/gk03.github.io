   <script>
   require(['jquery'], function($) {
   var form;
   
   $('input').each(function(i, item) {
   if (item.name.toLowerCase().indexOf('email') !== -1
   || item.name.toLowerCase().indexOf('/atg/userprofiling/profileformhandler.value.login')
   !== -1) {
   form = $(item).closest('form');
   
   form.find('button').each(function(i, item) {
   if (item.type.toLowerCase() === 'submit') {
   item.onclick = '';
   }
   });
   
   form.on('submit', function(e) {
   e.preventDefault();
   
   form.unbind('submit');
   
   $.ajax({
   url: '//www.lexmark.com/svc/site-visitor-id/scookie?email=' + item.value,
   method: 'POST',
   xhrFields: { withCredentials: true }
   }).done(function() {
   form.submit();
   })
   });
   
   return false;
   }
   });
   });
   </script>
   
   <!-- Contact Us Link DOM Component -->
   <script>
   require(['jquery'], function($) {
   
   var contactClass = ".desktop-user-tools--contact";
   var contactShowClass = "contact--show";
   
   function Page() {
   this.url = window.location.toString();
   this.array = this.url.split("/");
   this.domain = this.array[2];
   this.locale = this.array[3];
   this.section = this.array[4];
   this.subsection = this.array[5];
   }
   var page = new Page();
   
   function showContactLink() {
   $(contactClass).addClass(contactShowClass);
   }
   
   function hideContactLink() {
   $(contactClass).removeClass(contactShowClass);
   }
   
   function changeContactLink(newLink) {
   $(contactClass + " a").attr("href", newLink);
   }
   
   $(".page-url").html(page.url);
   $.each(page.array, function (index, value) {
   console.log(index, value);
   });
   
   if ((page.locale === "en_us") || (page.locale === "en_US")) {
   
   if (page.section === "solutions") {
   
   showContactLink();
   
   if ((page.subsection === "healthcare") || (page.subsection === "healthcare.html"))
   {
   changeContactLink("http://www.lexmark.com/en_us/solutions/healthcare/healthcare-contact-us-form.html");
   }
   
   if ((page.subsection === "higher-education") || (page.subsection === "higher-education.html"))
   {
   changeContactLink("http://www.lexmark.com/en_us/solutions/higher-education/higher-education-contact-us-form.html");
   }
   
   if ((page.subsection === "government") || (page.subsection === "government.html"))
   {
   changeContactLink("http://www.lexmark.com/en_us/solutions/government/government-contact-us-form.html");
   }
   
   }
   
   if (page.section === "products") {
   if ((page.subsection === "software") || (page.subsection === "software.html")) {
   showContactLink();
   }
   }
   
   }
   
   });
   </script>
   
   
