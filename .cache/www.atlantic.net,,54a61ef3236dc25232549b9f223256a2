					//alert('successful' + $("displaymessage",xmldata).text());
					HandleSectionDisplay('showstep2');
					jQuery('html, body').animate({scrollTop: 0}, 'slow');
				} else if (jQuery("status", xmldata).text() == "99") {
					window.location = "https://clouddev.atlantic.net/?page=processsignup";
				} else {
					//alert('got back ' + xmldata);
					var respmsg = jQuery("displaymessage", xmldata).text();
					//alert('failed' + $("displaymessage",xmldata).text());
					//alert(respmsg.length);
					if (respmsg.length > 0) {
						alert(respmsg);
						
					} else {
						// browsers like IE are not able to interpret ajax response mostly with new lines
						//respmsg =$("displaymessage", xmldata).text();
						//alert('in IE ' + respmsg);
						regularex = new RegExp("<displaymessage>(.*)</displaymessage>", "m");
						mymatch = regularex.exec(xmldata);
						alert(mymatch[1]);
						//window.location.replace("https://clouddev.atlantic.net/signup/");
					}

					// we can mark fields as invalid, set focus to them
				}

			}
		});
		*/

	},
	// set this class to error-labels to indicate valid fields
	success: function (label) {
		// set &nbsp; as text for IE
		label.html("&nbsp;").addClass("checked");
	}
});
</script>
