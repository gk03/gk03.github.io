<script>
    
    $(document).ready(function () {
        // Hack: Sort the items of root level ONLY in reverse mode
        $("#topmenu ul li.root").each(function (index) {
            if ($(this).hasClass("root") && $(this).attr("data-parentid") != "") {
                $(this).remove();
            }
        });
        var path = window.location.href;
        path = path.replace(/\/$/, "");
        path = decodeURIComponent(path);

        $("#topmenu ul li a").each(function(){
            var href = $(this).attr("href");
            if (path.indexOf(href, 0) > 0) {
                $(this).closest('li.root').children("a").addClass('active');
            }
        });
        
        $('ul.dropnav>li').tsort({ attr: "data-order", order: 'desc' });
        $('#topmenu').show();
    });
</script>
