<script language="JavaScript1.2" type="text/javascript">
	if (document.layers) {
		document.write('<script language="JavaScript1.2" src="/Liquid_Content/Modules/Javascript/AQ_MenuJSNS4.js" type="text/javascript"><\/script>');
		var AQ_Right_Limit = window.innerWidth;
		window.captureEvents(Event.RESIZE); 
		window.onresize = function AQ_WindowResizeNN4(){
				hideAll();
				AQ_Right_Limit = window.innerWidth;
			}
		function hideAll(){
			for(i=0;i < document.layers.length; i++){
				if(document.layers[i].visibility != "hide" && document.layers[i].visibility != "inherit")
					AQ_hideLayer(document.layers[i]);
				}
			}
	}
	function AQ_getRightLimit(){
		var AQ_Right_Limit;
		if (document.all) {
			AQ_Right_Limit = document.body.clientWidth;
		}
		else if (document.getElementById) {
			AQ_Right_Limit = window.innerWidth;
		}
		return AQ_Right_Limit;
	}
	document.onload += AQ_WindowResize();

</script>

