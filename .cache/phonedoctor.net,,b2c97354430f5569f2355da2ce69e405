    <script type="text/javascript">
        var $ = jQuery.noConflict();
        jQuery("document").ready(function ($) {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 35) {
                    $('.header').addClass("f-nav");
					$('.logo').addClass("trans")
                } else {
                    $('.header').removeClass("f-nav");
					$('.logo').removeClass("trans");
                }
            });
        });
    </script>







