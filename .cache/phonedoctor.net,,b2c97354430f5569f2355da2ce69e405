<script type="text/javascript">
jQuery(window).load(function (){
	jQuery('.call_back_btn, .red-btn').click(function(){
		jQuery("#bontact_main_widg #bontact_main_ifrm").css({'width':'390px' , 'height':'480px'});
		if(!jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('.dock-container').hasClass('collapsed'))
			{
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('.dock-container').addClass('collapsed')
			}
		if(jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').hasClass('notTrans'))
			{
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').removeClass('notTrans')
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').addClass('trans')
			}
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#sms_wrapper').removeClass('animated slideInUp');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#callback_wrapper').removeClass('animated slideOutDown');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#callback_wrapper').addClass('animated slideInUp');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#sms_wrapper').addClass('animated slideOutDown');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#bon-wrapper2 > div').addClass('hidden');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#callback_wrapper').removeClass('hidden');
	});
	jQuery('.text_sms_btn, .green-btn').click(function(){
		jQuery("#bontact_main_widg #bontact_main_ifrm").css({'width':'390px' , 'height':'480px'});
		if(!jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('.dock-container').hasClass('collapsed'))
			{
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('.dock-container').addClass('collapsed')
			}
		if(jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').hasClass('notTrans'))
			{
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').removeClass('notTrans')
				jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#widget-body').addClass('trans')
			}
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#sms_wrapper').removeClass('animated slideOutDown');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#callback_wrapper').removeClass('animated slideInUp');
	  jQuery("#bontact_main_widg #bontact_main_ifrm").contents().find('#callback_wrapper').addClass('animated slideOutDown');
