    <script type="text/javascript" language="JavaScript">

        showResponse = function (responseText, statusText, xhr, $form) {

            //Check to see if the validation was successful

            if (responseText.success) {

                window.location.href = 'http://www.ishir.com/thankyou.htm';

            } else {

                //Create a message and a HTML list to display in the response alert

                var list = "";

                $.each(responseText.errors, function () {

                    var caption = this.field;

                    var parts = caption.split('_');

                    if (caption == 'secure') {

                        $('#' + this.field).val('What is Result?*');

                    } else {

                        $('#' + this.field).val(parts[1]);

                    }

                    $('#' + this.field).addClass('error');

                    list += this.msg + "\n";

                });

                //Add the HTML to the response alert and fade it in

                alert(list);

                jQuery('#submit_btn').show();

                jQuery('#cWait').hide();

            }

        }



        $(document).ready(function () {

            var options = {
                success: showResponse, // post-submit callback 

                type: 'post',
                dataType: 'json'

            };

            $('#contact').submit(function () {

                jQuery('#submit_btn').hide();

                jQuery('#cWait').show();

                jQuery(this).ajaxSubmit(options);

                return false;

            });



        });

    </script>





