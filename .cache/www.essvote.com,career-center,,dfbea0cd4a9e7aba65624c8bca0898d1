$('#emailSignupLink').click(function(evt){
	evt.preventDefault();
	$('html,body').animate( { 'scrollTop':$('#topHalf').offset().top});
	// if you're on a crappy browser or running Internet Explorer 9 in Compat mode
	// then you'll get a very boring result and a shake of my head in your general direction
	if (badIE == true){
		$('#signupWrapper').css({
		   border: '2px solid #f5b225'
		});
	} else {
		$('#signupWrapper').css({
		   background : 'rgba(236,236,236,.4)',
		   borderRadius : '5px',
		   boxShadow : '3px 3px 10px #DDD'
		});
	}
});
</script>




