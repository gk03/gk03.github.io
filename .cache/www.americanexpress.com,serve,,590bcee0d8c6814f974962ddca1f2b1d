		<script type="text/javascript">
	if (window.location.href.toString().toLowerCase().indexOf('personalization=delete') != -1) {
		var date = new Date();
		date.setTime(date.getTime() + (-1 * 24 * 60 * 60 * 1000));
		var expires = "; expires=" + date.toGMTString();
		document.cookie = 'personalization' + "=" + 'delete' + expires + ";domain=.serve.com;path=/";
	}
	function getCookie(name) {
		var value = "; " + document.cookie;
		var parts = value.split("; " + name + "=");
		if (parts.length == 2) return parts.pop().split(";").shift();
	}
	if (getCookie('personalization') && document.getElementById('pg_home') !== null) {
		var url = window.location.href.split('?');
		var params = url[1] ? '?' + url[1] : '';
		switch (getCookie('personalization')) {
			case "Serve|True":
			window.location.href = '/customer/' + params;
			break;
			case "ServeClassic|True":
			window.location.href = '/customer1/' + params;
			break;
			case "ServeCashReload|True":
			window.location.href = '/customer2/' + params;
			break;
			case "ServeCashBack|True":
			window.location.href = '/customer3/' + params;
			break;
		}
	}
</script>
