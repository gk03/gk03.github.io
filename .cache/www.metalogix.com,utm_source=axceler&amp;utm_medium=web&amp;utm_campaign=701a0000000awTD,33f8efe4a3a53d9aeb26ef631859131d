<script type="text/javascript">jq10(document).ready(function(){
	var parameters = window.location.href.slice(window.location.href.indexOf('?') + 1);
	if (parameters != null)
	{
		var parametersLower = parameters.toLowerCase();
		if ((parametersLower.indexOf("http") == -1)
		&& (parametersLower.indexOf("indexCatalogue") == -1)
		&& (parametersLower.indexOf("year") == -1)
		&& (parametersLower.indexOf("vk") == -1)
		)
		{
			var frame = jq10("iframe");
			if (frame != null) {
			var frameSrc = frame.attr("src");
			frame.attr("src",frameSrc + "?" + parameters);
		}
	jq10("a").each(function()
	{
		var aHref = jq10(this).attr('href');
		var aClass = jq10(this).attr('class');
		if (aClass == null
			|| aClass.indexOf('banner') < 0) {
			// not a banner ad so do normal forwarding of
			// URL parameters for tracking
			if (aHref != null
				&& aHref.indexOf('mailto') < 0
				&& aHref.indexOf('javascript') < 0) {
				if (aHref.indexOf('?') > 0) {
					jq10(this).attr('href', aHref + '&' + parameters);
				} else {
					jq10(this).attr('href', aHref + '?' + parameters);
				}
			}
		} else {
			// banner ad - pick up the product parameter from the URL
			// swap spaces for underlines and then add it along with
			// the other parameters as utm_content=product_name
			var ParametersList = parameters.toLowerCase().split("&");
			var prodindex = -1;
			if (ParametersList != null
				&& ParametersList.length > -1) {
				var i;
				for (i = 0; i < ParametersList.length; i++) {
					if (ParametersList[i].indexOf("product=") > -1) {
						prodindex = i;
					}
				}
			}
			if (prodindex > -1) {
				// we found product=, get the name and change spaces to underlines
				var name = ParametersList[prodindex].slice(8, ParametersList[prodindex].length).replace(/%20+/g, "_");
				if (aHref.indexOf('?') > 0) {
					jq10(this).attr('href', aHref + '&utm_content=' + name);
				} else {
					jq10(this).attr('href', aHref + '?utm_content=' + name);
				}
			}
		}
	});
	}
}
});</script>
