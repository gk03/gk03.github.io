        <script>
            $(function () {
                var settings = {
                    mouseWheelSpeed: 30
                };
                $('#left-nav .facet .scroll-wrapper').jScrollPane(settings);
                $('#left-nav .facet h2').click(function () {
                    $(this).parent().toggleClass('active');
                    var $scrollwrapper = $(this).parent().find('.scroll-wrapper');
                    var $height = 0;
                    $scrollwrapper.find('li').slice(0, 5).each(function () {
                        $height += $(this).outerHeight();
                    });
                    $scrollwrapper.height($height);
                    $scrollwrapper.data('jsp').reinitialise();

                    $('#scrollableleft').trigger('resize');
                });
            });
            //$('#left-nav .facet .scroll-wrapper ul li a').on("click", function () {
            //    ModalPanel.enableOverlay(true);
