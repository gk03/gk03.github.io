  <script>
// execute your scripts when the DOM is ready. this is mostly a good habit
$(function() {

	// initialize scrollable
	$(".scrollable").scrollable({circular: true}).autoscroll({ autoplay: false });

});

function initMenu() {
	  $('#accordion ul').hide();
	  $('#accordion ul:first').show();
	  $('#accordion ul.ul-show').show();
	 
	  $('#accordion li a.expand').click(
		function() {
		  var checkElement = jQuery(this).next();
		  if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
			return false;
			}
		  if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
			jQuery('#accordion ul:visible').slideUp('normal');
			checkElement.slideDown('normal');
			return false;
			}
		  }
		);

    $('.accordion-sub-list ul').hide();
    //$('.accordion-sub-list ul:first').show();
    //$('.accordion-sub-list ul.ul-show').show();

    $('.accordion-sub-list li a.expand-sub-list').click(
        function() {
          var checkElement = jQuery(this).next();
          checkElement.slideToggle('normal');
          $('.expand-sub-list').next('ul').not(checkElement).slideUp();
        }
        );
}
	
$(document).ready(function() {initMenu();});

