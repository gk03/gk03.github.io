	'"<strong>Web EDI</strong> is the wave of the future - simple, easily accessible and service oriented."<br><strong>D. Frazier - Data Link Services Inc</strong>',

	

	'"<strong>Web EDI</strong> has made receiving orders and billing customer as easy as turning on your computer."<br><strong>S. Blanchard - Home of the Hebert Candies</strong>',

	

	'"Why use any other edi-system, when <strong>Web EDI</strong> is the best, easiest to use of the services out there."<br><strong>P. Wingard - Piknik Products Company</strong>');

	

    var current = 0;

    var element = document.getElementById('testimonials');

    /* cross browser method to set opacity */

    function setOpacity(level) {

      element.style.opacity = level;

      element.style.MozOpacity = level;

      element.style.KhtmlOpacity = level;

      element.style.filter = "alpha(opacity=" + (level * 100) + ");";

    }



    /* rotate and create timers to fade the testimonial in */

    function fadeIn(){

      element.innerHTML = comments[current];

      current = (current + 1) % comments.length;

      for (i = 0; i <= 1; i = i + 0.05) {

        setTimeout("setOpacity(" + i + ")", i * 1000);

      }

      setTimeout("fadeOut()", 4000);

    }



    /* create timers to fade the testimonial away */

    function fadeOut() {

      for (i = 0; i <= 1; i = i + 0.05) {

        setTimeout("setOpacity(" + (1 - i) + ")", i * 1000);

      }

      setTimeout("fadeIn()", 1000);

    }

    fadeIn(); 

      </script>

</div>

<script type="text/javascript">

swfobject.registerObject("FlashID");

</script>

