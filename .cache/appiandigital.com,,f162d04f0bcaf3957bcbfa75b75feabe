    <script>
        $(function () {
            $('#Carousel').carousel({
                interval: 5000
            });
        });
        // Activate Carousel
        $("#Carousel").carousel();

        // Enable Carousel Indicators
        $(".item").click(function () {
            $("#Carousel").carousel(1);
        });

        $('.carousel-indicators li').click(function (e) {
            e.stopPropagation();
            $('#Carousel').carousel($(this).data('slide-to'));
        });
        $(document).ready(function () {
            $('a[href*=#]:not([href="#Development"]):not([href*="#Branding"]):not([href*="#Photography"]):not([href*="#Applications"]):not([href*="#Marketing"])').on('click', function (e) {
                e.preventDefault();
                var target = this.hash;
                var $target = $(target);
                $('html, body').stop().animate({
                    'scrollTop': $target.offset().top
                }, 900, 'swing', function () {
                    window.location.hash = target;
                });
            });
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#tabs a").click(function (e) {
                e.preventDefault();
                $(this).tab('show');
            });
        });
    </script>
    <script type="text/javascript">

        $(window).scroll(function () {
            if ($(document).scrollTop() > 50) {
                $('nav').addClass('shrink');
            } else {
                $('nav').removeClass('shrink');
            }
        });
    </script>
