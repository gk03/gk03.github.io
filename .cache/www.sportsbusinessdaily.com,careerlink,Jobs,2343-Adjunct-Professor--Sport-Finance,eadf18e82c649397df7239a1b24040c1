			$(window).load(function () {
				$.getJSON('https://api.sportsbusinessdaily.com/api_careerlink/1/inthenews/2343?APIKey=88777F91-A175-4EBD-9FC4-719BB2B97834', function (data) {
					var NewsItems = [];
                    if (data.length > 0) {
					    $.each(data, function (key, val) {
					        var thumb = (val.thumb != null && val.thumb != "") ? "<img src='" + val.thumb + "' />" : ""
                            
						    NewsItems.push('<li>' + thumb + val.title + '</li>');
                            
					    });
					    $(".news-list").html(NewsItems.join(""));
                    } else {
                        $(".news-list").html("No recent articles found. Please check back later.");
                        $(".in-the-news .scroller .viewport").css("height", "32px");
                    }
				});
			})
        </script>
