<TITLE>Internet Access Point Corp. Houston</TITLE></HEAD>
<SCRIPT LANGUAGE="JavaScript">


<!-- Baseline text scroller for Navigator

/* Hide Script -->
<!--
*/

function scroll_text(seed)
{
   var m1  = "Welcome to Internet Access Point";
   var m2  = " . . . Houston's Quality Internet Connection.";
   var msg=m1+m2;
   var out = " ";
   var c   = 1;

   if (seed > 100) {
      seed--;
      var cmd="scroll_text(" + seed + ")";
      timerTwo=window.setTimeout(cmd,100);

/* Hide Script -->
<!--
*/

   }
   else if (seed <= 100 && seed > 0) {
      for (c=0 ; c < seed ; c++) {
         out+=" ";
      }
      out+=msg;
      seed--;
      var cmd="scroll_text(" + seed + ")";
      window.status=out;
      timerTwo=window.setTimeout(cmd,100);

/* Hide Script -->
<!--
*/

   }
   else if (seed <= 0) {
      if (-seed < msg.length) {
         out+=msg.substring(-seed,msg.length);
         seed--;
         var cmd="scroll_text(" + seed + ")";
         window.status=out;
         timerTwo=window.setTimeout(cmd,100);
      }
      else {
         window.status=" ";
         timerTwo=window.setTimeout("scroll_text(100)",75);
      }
   }
}
// End of Script -->
</SCRIPT>

<!--#exec cgi="/cgi-bin/counter"-->

<body bgcolor="#ffffff" vlink="#006633" onLoad="timerONE=window.setTimeout('scroll_text(100)',500);">
