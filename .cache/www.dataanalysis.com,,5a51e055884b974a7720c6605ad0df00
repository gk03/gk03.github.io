						<div class="sendy subscribe" id="sendy-166">
    <div class="intro">
        Subscribe to our newsletter:    </div>
    <form id="sendy-subscribe-166">
        <input type="text" name="email" placeholder="Email Address" />
        <button type="submit" name="Sign up" class="btn span3">Sign Up</button>
        <input type="hidden" name="mid" value="166" />
    </form>
</div>

<script>
    jQuery('form[id="sendy-subscribe-166"]').submit(function (form)
    {
        var formId = '#' + form.target.id;
        var email = jQuery(formId + ' input[name=email]').val();
        var url = 'email=' + email + '&13ca9fd9dca0b2bce226f66e3d89a878=1&mid=166';
        jQuery("#sendy-166").fadeOut('500');
        jQuery.ajax({
            url: 'http://www.dataanalysis.com/modules/mod_sendy/ajax/subscribe.php',
            data: url,
            type: "POST",
            dataType: 'text json',
            success: function (ret)
            {
                if (ret.success == true)
                {
                    jQuery("#sendy-166").html(ret.html).fadeIn('1000');
                }
                else
                {
                    jQuery("#sendy-166").html("There was an error processing your subscription.").fadeIn('1000');
                }
            }
        });
        return false; // Don't actually submit the form
    });
</script>		</div></div>
            
