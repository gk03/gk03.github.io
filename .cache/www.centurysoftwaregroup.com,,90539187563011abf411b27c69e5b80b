window.onload=function(){
setTimeout('nextTestimonial();', 0);
setInterval('nextTestimonial();', 20000);
}
function nextTestimonial()
{
    var newID;
    opacity=0;
        if(W3CDOM && !IEDOM)
            getObject('testimonialBox').style.MozOpacity = opacity;
        else if(IEDOM)
            getObject('testimonialBox').style.filter = 'alpha(opacity=' + opacity + ')';
    setText();
    fadeIn();
    do
        newID = Math.floor(Math.random()*(3+0.9));
    while (newID == testimonialID)
    testimonialID = newID;
}
function setText()
{
    getObject('testimonial').innerHTML = testimonials[testimonialID];
    getObject('TVNclient').innerHTML = client[testimonialID];
}
function fadeIn()
{
    if(opacity < 100)
    {
        if(W3CDOM && !IEDOM)
        {
            getObject('testimonialBox').style.MozOpacity = opacity/100;
        }
        else if(IEDOM)
        {
            getObject('testimonialBox').style.filter = 'alpha(opacity=' + opacity + ')';
        }
        opacity+=6;
    setTimeout('fadeIn()', 80);
    }
}
