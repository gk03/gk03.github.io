	function resetSubmit( pf ) {
		$('adminForm_'+pf).addEvent('submit', function(e) {
			new Event(e).stop();
				e.stop();
				var xSubmit = new Request.HTML(
					{url:'http://www.goldensoftware.com/index.php?option=com_aicontactsafe',
					evalScripts:false,
					update:$('displayAiContactSafeForm_'+pf),
