  <script>
    var detectSite = "www.services.avnet.com";
    var s_account = "";
    var businessUnit = "";

    detectSite = detectSite.toLowerCase();
    switch (true) {
      case /^(auth-test|test|auth).avnet\.com$/.test(detectSite):
        s_account = "avndev";
        break;
      case /^(www\.)?services((?!-test|-dev|auth(-test)?))?.avnet\.com$/.test(detectSite):
        s_account = "avnservices";
        businessUnit = "Services";
        break;
      case /^(www\.)?gov((?!-test|-dev|auth(-test)?))?.avnet\.com$/.test(detectSite):
        s_account = "avngov";
        businessUnit = "TS";
        break;
      case /^(www\.)?((?!test|dev|auth(-test|-dev)?))?avnet\.com$/.test(detectSite):
        s_account = "avnInc";
        break;
      default:
        s_account = "avnavndemocentral";
    }

  </script>
  <script src="/Style Library/Avt/scripts/AppMeasurement.js"></script>
  <script>
    function oscStrip(a) {
      a = a.split("|").join("");
      a = a.split("&").join("");
      a = a.split("'").join("");
      a = a.split("#").join("");
      a = a.split("$").join("");
      a = a.split("%").join("");
      a = a.split("^").join("");
      a = a.split("*").join("");
      a = a.split(":").join("");
      a = a.split("!").join("");
      a = a.split("<").join("");
      a = a.split(">").join("");
      a = a.split("~").join("");
      a = a.split(";").join("");
      return a;
    }

    function getPageName() {
      var pageName = document.location.href;
      pageName = pageName.replace(/^http:\/\/www\.|^http:\/\//, '');
      var ayame = pageName.split('/');
      pageName = pageName.replace(/$\//, '');
      if (ayame.length <= 2 && (ayame[1] == 'index.asp' || ayame[1] == '')) {
        return pageName;
      } else {
        return pageName;
      }
    }

    function getSection(currentUrl) {
      var siteCollectionUrl = '/';

      switch (true) {
        case /\/en-us\/who-we-are\//.test(currentUrl):
          return "who-we-are";
          break;
        case /\en-us\/what-we-do\//.test(currentUrl):
          return "what-we-do";
          break;
        case /\en-us\/who-we-help\//.test(currentUrl):
          return "who-we-help";
          break;
        case /\en-us\/contact-us\//.test(currentUrl):
          return "contact-us";
          break;
        case /\en-us\/legal\//.test(currentUrl):
          return "contact-us";
          break;
        default:
          return "welcome";
          break;
      }

      return "";
    }


    if (!(typeof s === "undefined")) {
      //get region for site
      var strCurrentUrl = location.pathname;

      s.pageName = getPageName();
      s.pageType = "";
      // s.channel = getSection(strCurrentUrl);
      s.channel = "en-us";
      s.prop1 = "Avnet";
      s.prop2 = "US";
      s.prop3 = businessUnit;
      s.prop4 = "";
      s.prop5 = "";
      s.prop6 = ""!=='' ? "" : "Unauthenticated";
      s.prop7 = "";
      s.prop9 = "Services";

      s.prop12 = "";
      s.prop13 = "US";

      /* Conversion Variables */
      s.campaign = "";
      s.state = "";
      s.zip = "";
      s.events = "";
      s.products = "";
      s.purchaseID = "";
      s.eVar1 = "";
      s.eVar2 = "";
      s.eVar3 = "";
      s.eVar4 = "";
      s.eVar5 = "";

      /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
