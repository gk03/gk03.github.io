        <script tpye="text/javascript">
			$( document ).ready(function() {
				$("#menu-toggle").on({
					click: function(){toggleNav();}
				});

				$(window).resize(function(){
    				if($( window ).width() > 890){if($("#menu-toggle").hasClass("menu-out")){toggleNav();}
    				}
    			});
			});

			function toggleNav(){
				if( $("#menu-toggle").hasClass("menu-out") ){
					$("#menu-toggle").removeClass("menu-out");
					$('#body, #footer, #home-body').css({borderRight: '0px solid #f3f4f4'}).animate({
						right: "0"
					}), 200;
				}else{
					$("#menu-toggle").addClass("menu-out");
					$('#body, #footer, #home-body').css({borderRight: '1px solid #B4B1B6'}).animate({
						right: "230",
					}), 200;
				}
			}			

        </script>

