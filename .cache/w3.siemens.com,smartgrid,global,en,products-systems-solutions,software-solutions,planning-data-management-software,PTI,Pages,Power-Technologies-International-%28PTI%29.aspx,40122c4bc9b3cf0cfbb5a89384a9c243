<script type="text/javascript">

jQuery(document).ready(function(){

	var metatagDLC = $('meta[name="SG DownloadCenter"]').attr("content");
	metatagDLC = metatagDLC.replace(/-/g, " ");
	metatagDLC = metatagDLC.replace(/\//g, " ");
	metatagDLC = metatagDLC.substr(0,50);
	metatagDLC = metatagDLC.replace(/ /g, "%20");
	metatagDLC = metatagDLC.replace(/%20%20/g, "%20");
	var xmlrequest = '/download-center/default.aspx?fct=search&pos=search_resultsonly&datapool=mixed&languagefilter=&xml=true&mandator=ic_sg&search_str=' + metatagDLC + '';

	var headline = true;
	var content = false;

	$.ajax({
        type: "GET",
		url: xmlrequest,
		dataType: "xml",
		success: function (xmlData) {
			//var xmlData = text2XML( xmlData );
			saveArray = {};
			$(xmlData).find("DisplayFilter[name='DataSourceObjectType']").children("Option").children("value").each(function(){
				saveArray[$(this).text()] = $(this).text();
				content = true;
			});
			var sortedArray = [];
			var $productGroups = {};

			var constructForRCth = "";
			var constructForRCtd = ""
			var widthTDMoreInfo1 = "83px";
			var widthTDMoreInfo2 = "169px";
			var colspanForRCtd = "";
			var size;
			if($(".s-right #dlc-application").length == 0) {
				constructForRCth = "<th width='60px' class='tabDate'>Status</th>";
				widthTDMoreInfo1 = "87px";
				widthTDMoreInfo2 = "474px";
				headline = false;
			}

			for (var key in saveArray) {
			    if (saveArray.hasOwnProperty(key)) {
			    	sortedArray.push(saveArray[key]);
			    }
			}
			sortedArray = sortedArray.sort();
			if(content){
				section = $('<div class="section"/>').appendTo($("#dlc-application"));
				(headline == true) ? $("<h3>Downloads</h3>").appendTo($(section)) : "";
				collapsiblebody = $('<div class="collapsible-body-xslt clearfix"/>').appendTo($(section));
				var dlcurl = "https://www.downloads.siemens.com/download-center/Download.aspx?pos=download&fct=getasset&mandator=ic_sg&id1=";

				$.each(sortedArray, function(i, actualtab){
					if(actualtab !== "All"){
						cteaser = $('<div class="c-teaser clearfix"/>').appendTo(collapsiblebody);
						collapsible = $('<div class="collapsible-xslt clearfix" style="background: url(/_layouts/Querplex/Siemens/WCMS3/images/icon-plus-gray.gif) no-repeat 0 3px;"/>').appendTo(cteaser);
						$('<div class="collapsible-header-xslt" style="overflow: hidden;padding-left: 18px;margin-bottom: 5px;cursor: pointer"/>').html('<h2>' + actualtab + '</h2>').appendTo(collapsible);
						contentblock = $('<div class="collapsible-body-xslt clearfix productcategory" style="display: none"/>').appendTo(collapsible);

						var $assets = $(xmlData).find('Asset');
						$assets.each(function(){
							if(actualtab == $(this).children("DataSourceObjectType").text()){
								($(this).children("ProductGroup").children("value").text() !== "") ? checkCategory = $(this).children("ProductGroup").children("value").text() : checkCategory = "CategorylessDocuments";
								categoryTitle = checkCategory;

								if(checkCategory == "CategorylessDocuments"){
									checkCategory = '_tab'+(actualtab + checkCategory).replace(/\W*/gi, "");
								} else {
									checkCategory = 'tab'+(actualtab + checkCategory).replace(/\W*/gi, "");
								}

								if(!$productGroups[checkCategory]) {
									$productGroups[checkCategory] = true;
									if(categoryTitle !== "CategorylessDocuments"){
										buildTabAndCat = $('<div class="category" id="' + checkCategory +'"/>').appendTo(contentblock);
										$('<div class="categoryHead"/>').html('<h2>\u25ba' + categoryTitle + '</h2>').appendTo(buildTabAndCat);
										categoryBody = $('<div class="categoryBody"/>').appendTo(buildTabAndCat);
									} else {
										buildTabAndCat = $('<div class="category" id="' + checkCategory +'"/>').appendTo(contentblock);
										categoryBody = $('<div class="categoryLessBody" style="display:block"/>').appendTo(buildTabAndCat);
									}
									contentblocktable = $('<table cellspacing="0" cellpadding="0"/>').appendTo(categoryBody);
									contentblocktable.append('<thead><tr><th width="13%" class="tabSize">Type & Size</th><th width="12%" class="tabLanguage">Language</th><th width="*" class="tabTitle">Title</th>' + constructForRCth + '</tr></thead>');
									contentblocktablebody = $('<tbody/>').appendTo(contentblocktable);

									//var $tabStatus=contentblocktable.find('th.tabTitle');
									//$tabStatus.data('title',$tabStatus.html()).html($tabStatus.html()+'&nbsp;'+'\u25bc');
								}

								var dateIssued = $(this).children("DateIssued").text();
								if($(".s-right #dlc-application").length == 0) {
									constructForRCtd = "<td width='60px'>" + dateIssued + "</td>";
								}
								var assetsize = parseFloat($(this).children("AssetSize").text() / 1048576);
								var assetsizeStr = "" + parseFloat($(this).children("AssetSize").text() / 1048576);
								var komma = assetsizeStr.indexOf(".");
								if(assetsize < 0.1){
									size = "<100 KB";
								} else if(assetsize < 1) {
									size = assetsizeStr.substr(komma+1,2) + "0 KB";
								} else if(assetsize < 10) {
									size = assetsizeStr.substr(0,komma) + "," + assetsizeStr.substr(komma+1,1) + " MB";
								} else {
									size = assetsizeStr.substr(0,komma) + " MB";
								}

								if($(this).children("AssetLanguage").children().length > 1) {
									var actuallanguages = "";
									$(this).children("AssetLanguage").children("value").each(function(){
										actuallanguages = actuallanguages + " & " + $(this).text().substr(0,2);
									});
									actuallanguages = actuallanguages.substr(3);
								} else {
									actuallanguages = $(this).children("AssetLanguage").children("value").text().substr(0,2);
								}

								var $itemInfoTR = $(
									'<tr class="iteminfo">' +
										'<td width="13%">' +
											'<ul class="linklist moreinfo">' +
												'<li>' +
													'<a class="icon icon-' + $(this).children("AssetExtension").text() + '" href="' + dlcurl + $(this).children("DataSourceObjectId").text() + '" target="_blank">' +
														'<span></span>' +
													'</a><nobr>' +
													size +
												'</nobr></li>' +
											'</ul>' +
										'</td>' +
										'<td width="12%">' +
											actuallanguages +
										'</td>' +
										'<td width="*">' +
											'<a href="' + dlcurl + $(this).children("DataSourceObjectId").text() + '" target="_blank">' +
												$(this).children("AssetTitle").children("value").text() +
											'</a>' +
										'</td>' +
										constructForRCtd +
									'</tr>'
								);
								$($("div#" + checkCategory).find("tbody")).append($itemInfoTR);

								var $itemMoreInfoTR = $(
									'<tr class="moreinfo" style="display:none">' +
										'<td width="' + widthTDMoreInfo1 + '">' +
											'<p>Business Fields:</p>' +
											'<p>Productbrand:</p>' +
											'<p>Description:</p>' +
										'</td>' +
										'<td width="' + widthTDMoreInfo2 + '">' +
											'<p>' + $(this).children("AssetBusinessField").children("value").text() + '</p>' +
											'<p>' + $(this).children("ProductBrand").children("value").text() + '</p>' +
											'<p>' + $(this).children("AssetDescription").text() + '</p>' +
										'</td>' +
									'</tr>'
								);
								$($("div#" + checkCategory).find("tbody")).append($itemMoreInfoTR);

								var dateParts;
								if($(this).children("DateIssued").text().indexOf(".") > 0){
									dateParts = $(this).children("DateIssued").text().split('.');
									(dateParts[0].length < 2) ? dateParts[0] = "0" + dateParts[0] : dateParts[0];
									(dateParts[1].length < 2) ? dateParts[1] = "0" + dateParts[1] : dateParts[1];
									var dateStr = dateParts[2]+dateParts[1]+dateParts[0];
								} else {
									dateParts = $(this).children("DateIssued").text().split('/');
									(dateParts[0].length < 2) ? dateParts[0] = "0" + dateParts[0] : dateParts[0];
									(dateParts[1].length < 2) ? dateParts[1] = "0" + dateParts[1] : dateParts[1];
									var dateStr = dateParts[2]+dateParts[0]+dateParts[1];
								}

								$itemInfoTR.data('rowinfo',{
									size: 		assetsize,
									language: 	actuallanguages,
									date: 		dateStr,
									title: 		$(this).children("AssetTitle").children("value").text(),
									$moreInfo: 	$itemMoreInfoTR
								});
							}

						});


					}
				});

				collapsiblexslt = function (a) {
				    var b = function (b) {

						if($(this).next().children().find(".categoryLessBody").find('th.tabTitle').length == 1){
							sortIt($(this).next().children().find(".categoryLessBody").find('th.tabTitle'),"sortitplease");
						}

				        b.preventDefault();
				        var d = a(b.currentTarget).attr("data-switch-title"),
				            c = a(b.currentTarget).parent().parent().find(".collapsible-body-xslt");
				        a(c).slideToggle(300, function () {
				            a(b.currentTarget).parent().toggleClass("active")
				        });
				        a(b.currentTarget).attr("data-switch-title", a(b.currentTarget).attr("title")).attr("title", d);
				        if (c.find(".slider").length > 0) {
				            siemens.slider.init();
				            c.find(".realSlider").length > 0 && c.find(".img-container:first").trigger("click");
				            window.setTimeout(function () {
				                RepositionFooter("W")
				            }, 200)
				        }
				    }, c = function () {
				            a(".collapsible-xslt .collapsible-header-xslt").bind("click", b)
				        };
				    return {
				        init: c
				    }
				}(jQuery);
				collapsiblexslt.init();


				$('.productcategory').each(function() {
					var $divs = $(this).find('> div');
					$divs.sort(function(a,b) {
						if (a.id < b.id) return -1;
						else if (a.id>b.id) return 1;
						return 0;
					});
					$divs.appendTo($(this));
				});
			}
		}
	});

	function sortIt(element,sorting){
		var $this 		= $(element),
			which 		= $this.attr('class'),
			$table 		= $this.parent().parent().parent(),
			$itemRows 	= $table.find('tr.iteminfo'),
			direction 	= $this.data('direction'),
			titleData 	= $this.data('title'),
			field;

		if(sorting !== "dontsort" || direction == undefined){
			if (direction==undefined) direction = true;
			if (!titleData) {
				titleData = $this.html();
				$this.data('title',titleData);
			}
			$this.siblings().each(function() {
				var $sibling = $(this);
				var titleData = $sibling.data('title');
				if (titleData) $sibling.html(titleData);
			});
			$this.html(titleData+'&nbsp;'+(direction ? '\u25b2' : '\u25bc'));
			//$table.find('tr.iteminfo, tr.moreinfo').empty();
			if (which == 'tabSize') field='size';
				else if (which == 'tabLanguage') field='language';
				else if (which == 'tabTitle') field = 'title';
				else if (which == 'tabDate') field = 'date';
			$itemRows.sort(function(a,b) {
				var dataA = $(direction ? a : b).data('rowinfo');
				var dataB = $(direction ? b : a).data('rowinfo');
				var valA = dataA[field];
				var valB = dataB[field];
				return valA==valB ? 0 : (valA < valB ? -1 : 1);
			});
			$itemRows.each(function() {
				var $this = $(this);
				var data = $this.data('rowinfo');
				$table.append($this);
				$table.append(data.$moreInfo);
			});

			direction = !direction;
			$this.data('direction',direction);
		}
	}


	$("#dlc-application ul.moreinfo li").live("mouseover", function() {
		$(this).parents().parents().next("tr").show();
		$(this).parent().parent().parent().addClass("grey_tr");
	});
	$("#dlc-application ul.moreinfo").live("mouseout", function() {
		$(this).parents().parents().next("tr").hide();
		$(this).parent().parent().removeClass("grey_tr");
	});
	$("#dlc-application table.searcheditems").each( function(index) {
		$($("span.searcheditems")[index]).html(" (" + $(this).children().children("tr.iteminfo").length + ")");
	});
	$(".categoryHead").live("click", function() {
		if($(this).hasClass("opened")){
			$(this).next().slideUp();
			$(this).removeClass("opened");
			$(this).html("<h2>\u25ba" + $(this).text().substr(1,$(this).text().length) + "</h2>");
		} else {

			if($(this).next().children().find('th.tabTitle').length == 1){
				sortIt($(this).next().children().find('th.tabTitle'), "dontsort");
			}

			$(".categoryHead").removeClass("opened");
			$($(this).parent().parent()).find(".categoryBody").hide();
			$(this).addClass("opened");
			$(this).parent().show();
			$(this).next().slideDown();
			$($(this).parent().parent()).find(".categoryHead h2").each(function(){
				$(this).text("\u25ba" + $(this).text().substr(1,$(this).text().length));
			})
			$(this).html("<h2>\u25bc" + $(this).text().substr(1,$(this).text().length) + "</h2>");
		}
	});

	$(".categoryHead").find('th.tabPlatzhalter, th.tabTitle, th.tabDate, th.tabSize, th.tabLanguage').live("click", function(){
		sortIt($(this), "sortitplease");
	});


	function text2XML(txt) {
        var xmlDoc;
        if (window.DOMParser) {
                xmlDoc=(new DOMParser()).parseFromString(txt,"text/xml");
        } else {
                xmlDoc=new ActiveXObject("Microsoft.XMLDOM");
                xmlDoc.async="false";
                xmlDoc.loadXML(txt);
        }
        return xmlDoc;
	};
});
