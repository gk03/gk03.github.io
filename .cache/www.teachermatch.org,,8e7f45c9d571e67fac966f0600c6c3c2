			$AutoPlay: false,
			$AutoPlayInterval: 3000,
			$Loop: 0,
			$DragOrientation: 0,
			$PauseOnHover: 0,
			$SlideshowOptions: {
				$Class: $JssorSlideshowRunner$,
				$Transitions: _SlideshowTransitions
