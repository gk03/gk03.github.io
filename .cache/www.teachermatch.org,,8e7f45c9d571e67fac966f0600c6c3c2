					<div class="control-group">
						<div class="">
							Please make sure that pop ups are allowed on your browser.
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<button class="btn" data-dismiss="modal" aria-hidden="true" onclick="hideDiv();">Close</button> 		
				</div>
			</div>
		</div>
	</div>
	
	<script language='javascript'>
		jQuery(document).ready(function(){
			if (!window.console) {
				console = {log: function() {}};
			}
			function getVisitedURL()
			{
				var ddd=document.URL;
				document.getElementById("visitedPageURL").value=""+ddd+"";
				document.getElementById("visitedPageTitle").value=""+document.title+"";
			}
			getVisitedURL();
			function showDisplay(dis)
			{
				jQuery('#errordivFeedBack').empty();
				jQuery('#lodingImg').hide();
				if(dis.value==1)
				{
					jQuery('#feedback').hide();
					jQuery('#support').show();
					jQuery('#msgSubject').focus();
		
				}else
				{
					jQuery('#feedback').show();
					jQuery('#support').hide();
					jQuery('#feedbackmsg').focus();
				}
	
				jQuery('#ratingDiv').html("<input  value='0' id='rating_simple1' name='starRating' type='hidden'>");
				setRating();
				removeFile();
	
				jQuery('#msgFeed').find(".jqte_editor").css("background-color", "");
				jQuery('#msgSpt').find(".jqte_editor").css("background-color", "");
				jQuery('#msgSubject').css("background-color", "");
				jQuery('#msgFeed').find(".jqte_editor").html("");
				jQuery('#msgSpt').find(".jqte_editor").html("");
				document.getElementById("msgSubject").value="";
			}
		
			function validateEmail(sEmail) {
				var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
				if (filter.test(sEmail)) {
					return true;
				} else {
					return false;
				}
			}
		
			function validateFeedBack()
			{

				jQuery('#errordivFeedBack').empty();
				jQuery('#msgSubject').css("background-color", "");
				jQuery('#f1').css("background-color", "");
				jQuery('#msgFeed').find(".jqte_editor").css("background-color", "");
				jQuery('#msgSpt').find(".jqte_editor").css("background-color", "");

				jQuery('#inputFirstName').css("background-color", "");
				jQuery('#inputLastName').css("background-color", "");
				jQuery('#inputEmail').css("background-color", "");
				jQuery('#inputDistrict').css("background-color", "");

				var cnt=0;
				var focs=0;

				var reqType=jQuery('input[name=reqType]:radio:checked').val();
				
				
				document.getElementById("rqtype").value=reqType;
				document.getElementById("reqType1").value=reqType;
				if(reqType=="3c")
				{
					if (jQuery('#msgFeed').find(".jqte_editor").text().trim()=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter FeedBack.<br>");
						if(focs==0)
							jQuery('#msgFeed').find(".jqte_editor").focus();
							jQuery('#msgFeed').find(".jqte_editor").css("background-color", "#F5E7E1");
							cnt++;focs++;
					} else {
						var charCount=jQuery('#msgFeed').find(".jqte_editor").text().trim();
						var count = charCount.length;
						if(count>1000)
						{
							jQuery('#errordivFeedBack').append("&#149; Message length cannot exceed 1000 characters.<br>");
							if(focs==0)
								jQuery('#msgFeed').find(".jqte_editor").focus();
							jQuery('#msgFeed').find(".jqte_editor").css("background-color", "#F5E7E1");
							cnt++;focs++;
						}
					}

				} else {

					if(jQuery.trim(jQuery('#inputFirstName').val())=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter First Name.<br>");
						if(focs==0)
							jQuery('#inputFirstName').focus();
							jQuery('#inputFirstName').css("background-color", "#F5E7E1");
						cnt++;focs++;
					}
					if(jQuery.trim(jQuery('#inputLastName').val())=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter Last Name.<br>");
						if(focs==0)
							jQuery('#inputLastName').focus();
							jQuery('#inputLastName').css("background-color", "#F5E7E1");
						cnt++;focs++;
					}
					if(jQuery.trim(jQuery('#inputEmail').val())=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter Email.<br>");
						if(focs==0)
							jQuery('#inputEmail').focus();
						jQuery('#inputEmail').css("background-color", "#F5E7E1");
						cnt++;focs++;
					} else {
							if (validateEmail(jQuery('#inputEmail').val())) {
									//return true;
							} else {
									jQuery('#errordivFeedBack').append("&#149; Invalid Email Address.<br>");
									if(focs==0)
									jQuery('#inputEmail').focus();
									jQuery('#inputEmail').css("background-color", "#F5E7E1");
									cnt++;focs++;
							}		
					}

					if(jQuery.trim(jQuery('#inputDistrict').val())=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter District.<br>");
						if(focs==0)
							jQuery('#inputDistrict').focus();
						jQuery('#inputDistrict').css("background-color", "#F5E7E1");
						cnt++;focs++;
					}
				
					if (jQuery('#msgSpt').find(".jqte_editor").text().trim()=="")
					{
						jQuery('#errordivFeedBack').append("&#149; Please enter Feedback.<br>");
						if(focs==0)
							jQuery('#msgSpt').find(".jqte_editor").focus();
						jQuery('#msgSpt').find(".jqte_editor").css("background-color", "#F5E7E1");
						cnt++;focs++;
					}else
					{
						var charCount=jQuery('#msgSpt').find(".jqte_editor").text().trim();
						var count = charCount.length;
						if(count>1000)
						{
							jQuery('#errordivFeedBack').append("&#149; Feedback length cannot exceed 1000 characters.<br>");
							if(focs==0)
								jQuery('#msgSpt').find(".jqte_editor").focus();
							jQuery('#msgSpt').find(".jqte_editor").css("background-color", "#F5E7E1");
							cnt++;focs++;
						}
					}
				}
	
				if(cnt==0)
					return true;
				else
				{
					jQuery('#errordivFeedBack').show();
					return false;
				}
			}

			function saveFeedBack()
			{
				if(!validateFeedBack())
					return;

				jQuery('#lodingImg').show();
				document.getElementById('frmFeedbackUpload').action="https://platform.teachermatch.org/feedBackUploadBeforeLoginServlet.do";
				document.getElementById('frmFeedbackUpload').submit();
			
			}
			function hideFeedBack(flg)
			{

				jQuery('#lodingImg').hide();
				jQuery('#myModal1').modal('hide');
				if(flg==1)
				{
					jQuery('#message2show').html("Your message is successfully sent to TeacherMatch Support Team.");
					jQuery('#myModal2').modal('show');
				}
				else if(flg==2)
				{
					jQuery('#message2show').html("Your feedback is successfully sent to TeacherMatch Team.");
					jQuery('#myModal2').modal('show');
				}
				else if(flg==3)
				{
					alert("Oops: Your Session has expired!");  document.location = 'https://platform.teachermatch.org/signin.do';
				}

				document.getElementById('msgSubject').value="";
				//document.getElementById('msg').value="";
				document.getElementById('feedbackmsg').value="";
			}
			function addFileType()
			{
				//jQuery('#file1').empty();
				//jQuery('#file1').html("<a href='javascript:void(0);'onclick='removeFile();'><img src='images/can-icon.png' title='remove'/></a> <input id='f1' class='left20 top-35' name='f1' size='20' title='Choose a File' type='file'>&nbsp;Maximum file size 10MB.");
				jQuery('#file1 .btn-removefile').removeClass("hidden");
				jQuery('#file1 .btn-attachfile').addClass("hidden");
				jQuery('#file1-input').removeClass("hidden");
			}
			function removeFile()
			{
				//jQuery('#file1').empty();
				//jQuery('#file1').html("<a href='#' class='btn-attachfile'><img src='https://www.teachermatch.org/wp-content/themes/teacher-match/images/attach.png'/></a> <a href='#' class='btn-attachfile'>Attach a File</a>");
				jQuery('#file1 .btn-removefile').addClass("hidden");
				jQuery('#file1 .btn-attachfile').removeClass("hidden");
				jQuery('#f1').val('');
				jQuery('#file1-input').addClass("hidden");
			}
			function feedBack()
			{
				jQuery('#myModal1').modal('show');
				jQuery('#file1 .btn-removefile').addClass("hidden");
				jQuery('#file1 .btn-attachfile').removeClass("hidden");
				jQuery('#msgSubject').focus();
				jQuery('#errordivFeedBack').empty();
				jQuery('#msgSubject').css("background-color", "");
				document.getElementById('msgSubject').value="";
	
			}
			jQuery('.btn-contact').click(function(e) {
				e.preventDefault();
				feedBack();
			});
			
			function resetFbModal() {
				jQuery('.btn-reset').addClass('hidden');
				jQuery('.tel-applicant').addClass('hidden');
				jQuery('.tel-partner').addClass('hidden');
				jQuery('.reqTypes').removeClass('hidden');
				subjectLine = "";
				jQuery('.fb-info').addClass('hidden');
				jQuery('.fb-feedback').addClass('hidden');
				jQuery('.fb-support').addClass('hidden');
				jQuery('.fb-contactinfo').addClass('hidden');
				jQuery('#buttonSubmit-frmFeedbackUpload').addClass('hidden');
				jQuery('.hdrSubject').html('').addClass('hidden');
				jQuery('input:radio[name="reqType"]').prop('checked', false);
				jQuery('#ratingDiv').html("<input  value='0' id='rating_simple1' name='starRating' type='hidden'>");
				setRating();
				removeFile();
				jQuery('#msgFeed').find(".jqte_editor").css("background-color", "");
				jQuery('#msgSpt').find(".jqte_editor").css("background-color", "");
				jQuery('#msgSubject').css("background-color", "");
				jQuery('#msgFeed').find(".jqte_editor").html("");
				jQuery('#msgSpt').find(".jqte_editor").html("");
				jQuery('.divErrorMsg').html('');
			}
		
			jQuery('#myModal1').on('show', function () {
				resetFbModal();
			});
		
			jQuery('.btn-reset').click(function(e) {
				e.preventDefault();
				resetFbModal();
			});
			
			jQuery('.btn-attachfile').click(function(e) {
				e.preventDefault();
				addFileType();
			});
			
			jQuery('.btn-removefile').click(function(e) {
				e.preventDefault();
				removeFile();
			});
		
			jQuery('#buttonSubmit-frmFeedbackUpload').click(function(e) {
				e.preventDefault();
				//validateFeedBack();
				saveFeedBack();
			});
		
			jQuery('input:radio[name="reqType"]').change(function(){
				var subjectLine = "";
				if (jQuery(this).is(':checked') && jQuery(this).val() == '1a') {
					subjectLine = "Support Request: Product Information";
					jQuery('.tel-applicant').addClass('hidden');
					jQuery('.tel-partner').addClass('hidden');
					jQuery('.fb-info').removeClass('hidden');
					jQuery('.fb-feedback').addClass('hidden');
					jQuery('.fb-support').addClass('hidden');
					jQuery('.fb-contactinfo').addClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').addClass('hidden');
				} else if (jQuery(this).is(':checked') && jQuery(this).val() == '2a') {
					subjectLine = "Support Request by Applicant: Login Help";
					jQuery('.tel-applicant').removeClass('hidden');
					jQuery('.tel-partner').addClass('hidden');
					jQuery('.fb-info').addClass('hidden');
					jQuery('.fb-feedback').addClass('hidden');
					jQuery('.fb-support').removeClass('hidden');
					jQuery('.fb-contactinfo').removeClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').removeClass('hidden');
				} else if (jQuery(this).is(':checked') && jQuery(this).val() == '2b') {
					subjectLine = "Support Request by Applicant: Question";
					jQuery('.tel-applicant').removeClass('hidden');
					jQuery('.tel-partner').addClass('hidden');
					jQuery('.fb-info').addClass('hidden');
					jQuery('.fb-feedback').addClass('hidden');
					jQuery('.fb-support').removeClass('hidden');
					jQuery('.fb-contactinfo').removeClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').removeClass('hidden');
				} else if (jQuery(this).is(':checked') && jQuery(this).val() == '3a') {
					subjectLine = "Support Request by Partner: Technical Support";
					jQuery('.tel-applicant').addClass('hidden');
					jQuery('.tel-partner').removeClass('hidden');
					jQuery('.fb-info').addClass('hidden');
					jQuery('.fb-feedback').addClass('hidden');
					jQuery('.fb-support').removeClass('hidden');
					jQuery('.fb-contactinfo').removeClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').removeClass('hidden');
				} else if (jQuery(this).is(':checked') && jQuery(this).val() == '3c') {
					subjectLine = "Support Request by Partner: Feedback";
					jQuery('.tel-applicant').addClass('hidden');
					jQuery('.tel-partner').addClass('hidden');
					jQuery('.fb-info').addClass('hidden');
					jQuery('.fb-feedback').removeClass('hidden');
					jQuery('.fb-support').addClass('hidden');
					jQuery('.fb-contactinfo').addClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').removeClass('hidden');
				} else {
					subjectLine = "";
					jQuery('.tel-applicant').addClass('hidden');
					jQuery('.tel-partner').addClass('hidden');
					jQuery('.fb-info').addClass('hidden');
					jQuery('.fb-feedback').addClass('hidden');
					jQuery('.fb-support').addClass('hidden');
					jQuery('.fb-contactinfo').addClass('hidden');
					jQuery('#buttonSubmit-frmFeedbackUpload').addClass('hidden');
				}
				jQuery('#msgSubject').val(subjectLine);
				jQuery('.hdrSubject').html('<br />'+subjectLine).removeClass('hidden');
				jQuery('.btn-reset').removeClass('hidden');
				jQuery('.reqTypes').addClass('hidden');
			});
		});
	</script>
	<!-- End feedback and support -->

	<!-- abbreviated script to initialize editor, may be removed -->
	<script type="text/javascript">
		jQuery(document).ready(function(){
			jQuery('textarea').jqte();
		});
	</script>
	<script type="text/javascript">
		window.top.hideFeedBack = function (flg)
			{
				jQuery('#lodingImg').hide();
				jQuery('#myModal1').modal('hide');
				if(flg==1)
				{
					jQuery('#message2show').html("Your message is successfully sent to TeacherMatch Support Team.");
					jQuery('#myModal2').modal('show');
				}
				else if(flg==2)
				{
					jQuery('#message2show').html("Your feedback is successfully sent to TeacherMatch Team.");
					jQuery('#myModal2').modal('show');
				}
				else if(flg==3)
				{
					alert("Oops: Your Session has expired!");  document.location = 'https://platform.teachermatch.org/signin.do';
				}

				document.getElementById('msgSubject').value="";
				document.getElementById('msg').value="";
				document.getElementById('feedbackmsg').value="";
			}
	</script>
	
