<script type="text/javascript">
var timeout    = 400;
var closetimer = 0;
var ddmenuitem = 0;
function jsCountry_open()
{  jsCountry_canceltimer();
   jsCountry_close();
   ddmenuitem = $(this).find('ul').css('visibility', 'visible');}
function jsCountry_close()
{  if(ddmenuitem) ddmenuitem.css('visibility', 'hidden');}
function jsCountry_timer()
{  closetimer = window.setTimeout(jsCountry_close, timeout);}
function jsCountry_canceltimer()
{  if(closetimer)
   {  window.clearTimeout(closetimer);
      closetimer = null;}}
$(document).ready(function()
{  $('#jsCountry > li').bind('mouseover', jsCountry_open)
   $('#jsCountry > li').bind('mouseout',  jsCountry_timer)});
document.onclick = jsCountry_close;
</script>

