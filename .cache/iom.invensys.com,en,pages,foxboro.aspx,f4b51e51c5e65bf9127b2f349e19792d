	function equalHeight(group) {
		tallest = 0;
		group.each(function() {
			thisHeight = $(this).height();
			if(thisHeight > tallest) {
				tallest = thisHeight;
			}
		});
		group.height(tallest);
	}
	$(window).load(function() {
		equalHeight($(".column"));
		equalHeight($(".column1"));
		equalHeight($(".column2"));
		equalHeight($(".column3"));
	});
