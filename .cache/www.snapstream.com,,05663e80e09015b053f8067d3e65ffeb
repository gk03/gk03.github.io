				<script>
						"use strict";
				</script>
				<script type="text/javascript">
					var searchDemo = function () {
						var queryText = $("#initialQuery").val();
						location.href = "/SearchDemo.asp?initialQuery=" + queryText;
					};
					$("#tryItButton").on("click", function () {
						searchDemo();
					});
					$("#initialQuery").keyup(function (e) {
						if (e.which === 13) {
							searchDemo();
						}
					});
				</script>
