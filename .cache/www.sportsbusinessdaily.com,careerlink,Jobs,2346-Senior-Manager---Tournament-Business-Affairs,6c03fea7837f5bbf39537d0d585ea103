                $.getJSON('https://api.sportsbusinessdaily.com/api_careerlink/1/executiveprofiles/703118?APIKey=88777F91-A175-4EBD-9FC4-719BB2B97834', function (data) {
                    var ExPitems = ['<ul>'];
                    $.each(data, function (key, val) {
                        if (key < 4) {
                            ExPitems.push('<li>' + val.name + '</a><p class="title">' + val.position + '</p></li>');
                        }
                    });
                    ExPitems.push('</ul>');
                    $("#execProfiles").html(ExPitems.join(""));
                    
