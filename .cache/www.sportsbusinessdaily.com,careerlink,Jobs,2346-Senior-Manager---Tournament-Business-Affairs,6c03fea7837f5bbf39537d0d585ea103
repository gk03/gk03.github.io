        $('.btn-share-job.email-share, .share-via-mail').on('click', function () {
            $('.overlay-share-email-mask').fadeIn('fast');
            $('.overlay-share-via-mail').fadeIn('mediaum');
        })

        $('.overlay-share-via-mail .close-btn, .overlay-share-email-mask').on('click', function () {
            $('.overlay-share-email-mask').fadeOut('medium');
            $('.overlay-share-via-mail').fadeOut('fast');
        })

    })
</script>
