	<script type="text/javascript">
		$(document).ready(function () {

			$('.bxslider').bxSlider({
				minSlides: 1,
				maxSlides: 1,
				slideWidth: 760,
				slideMargin: 10
			});

			$("#mainvideo").on("click", function () {
				$("#videowindow").modal('show');

				var video = $('#thevideo');

				if (video.get(0).paused) {
					video.get(0).play();
					return false;
				}
				else {
					video.get(0).pause();
					return false;
				}
			});

			$('#videowindow').on('hide', function () {
				var video = $('#thevideo');
				video.get(0).pause();
			});

		});
	</script>

