	<script language="javascript">function getVisitedURL()
{var ddd=document.URL;document.getElementById("visitedPageURL").value=""+ddd+"";document.getElementById("visitedPageTitle").value=""+document.title+"";}
getVisitedURL();function showDisplay(dis)
{$('#errordivFeedBack').empty();$('#lodingImg').hide();if(dis.value==1)
{$('#feedback').hide();$('#support').show();$('#msgSubject').focus();}else
{$('#feedback').show();$('#support').hide();$('#feedbackmsg').focus();}
