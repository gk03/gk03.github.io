function CurrentPageWithMenuSelected(menuPageName,parentorderid,menuorderid,status){if(menuPageName.indexOf("|"+menuPage+"|")!=-1){if(JobOrderVal==null){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}else if(JobOrderVal==2&&menuPageName.indexOf("managejoborders")!=-1){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}else if(JobOrderVal==3&&menuPageName.indexOf("schooljoborders")!=-1){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}else if(JobOrderVal==2&&menuPageName.indexOf("|batchjoborder.do|")!=-1){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}else if(entityID==3&&JobOrderVal==3&&menuPageName.indexOf("|batchjoborder.do|")!=-1){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}else if(JobOrderVal==3&&menuPageName.indexOf("batchschooljoborder")!=-1){document.getElementById("menuid"+parentorderid).className="activeText";document.getElementById("submenuid"+parentorderid+menuorderid).className="activeText";}}}
