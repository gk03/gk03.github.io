     <script>
     jQuery(document).ready(function() {

     jQuery('.home-slider li:first-child').show().addClass('active');
     jQuery('.under-slide-con li:first-child').addClass('active');
     
     function showNextImage(){
          //get element position
          var mw_total = jQuery('.home-slider li').length;
          var mw_index = jQuery('.home-slider li.active').prevAll().length;
          var mw_target = mw_index + 2;
          
          if (mw_target > mw_total) {
               mw_target = 1;
          }
          
          //hide current image and show the target image
          jQuery('.home-slider li:visible').hide();
          jQuery('.home-slider li:nth-child('+mw_target+')').fadeIn();
          
          //remove active class and replace
          jQuery('.home-slider li').removeClass('active');
          jQuery('.home-slider li:nth-child('+mw_target+')').addClass('active');
          jQuery('.under-slide-con li').removeClass('active');
          jQuery('.under-slide-con li:nth-child('+mw_target+')').addClass('active');
     }
     
     var reload = setInterval(showNextImage, 6000);
     
     jQuery('.under-slide-con li').click(function() {
          //clear current rotation interval
          clearInterval(reload);
          
          //get button we're pressing
          var mw_index = jQuery(this).prevAll().length;
          var mw_target = mw_index + 1;
          
          //hide current image and display image associated with button
          //hide current image and show the target image
          jQuery('.home-slider li:visible').hide();
          jQuery('.home-slider li:nth-child('+mw_target+')').show();
          
          //remove active class and replace
          jQuery('.home-slider li').removeClass('active');
          jQuery('.home-slider li:nth-child('+mw_target+')').addClass('active');
          jQuery('.under-slide-con li').removeClass('active');
          jQuery(this).addClass('active');
          
          reload = setInterval(showNextImage, 6000);
     });

     });
     </script>

