<!-- Hightlight top tab -->
 <script type="text/javascript">
     $(document).ready(function () {
         current_page = document.location.href;
         if (current_page.match(/about-prometric/)) {
             $("ul.nav1st a[href*='about-prometric']").parent().addClass('navselected');
         } else if (current_page.match(/our-solutions/)) {
             $("ul.nav1st a[href*='our-solutions']").parent().addClass('navselected');
         } else if (current_page.match(/become-a-test-center/)) {
             $("ul.nav1st a[href*='become-a-test-center']").parent().addClass('navselected');
         } else if (current_page.match(/news-and-resources/)) {
             $("ul.nav1st a[href*='news-and-resources']").parent().addClass('navselected');
         } else if (current_page.match(/contact-us/)) {
             $("ul.nav1st a[href*='contact-us']").parent().addClass('navselected');
         }
         leftlinkToSet = document.location.href.toLowerCase();
         $(".QLnav1st a").each(function (index, item) {
             var href = $(item).attr("href").toLowerCase();
             if (href.indexOf(".aspx") != -1 && leftlinkToSet.indexOf(href) != -1)
                 $(item).addClass('navselected');
             if (href == 'http://nav-header') 
                $(item).removeAttr('href'); });
         $(".nav1st a[href='http://nav-header']").removeAttr('href');
         if ( leftlinkToSet.indexOf("sitemap.aspx") != -1)
            $("a[href='http://nav-header']").removeAttr('href');
     });
</script>
</body>
</html>
