                    </div><!--//login-form-container-->

                </div><!--//modal-body-->

                <div class="modal-footer">

                    <p>I want to <a class="back-to-login-link" id="back-to-login-link" href="#">return to login</a></p>                    

                </div><!--//modal-footer-->

            </div><!--//modal-content-->

        </div><!--//modal-dialog-->

    </div><!--//modal-->

    

