function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		menu_01over = newImage("images/menu_01over.gif");
		menu_02over = newImage("images/menu_02over.gif");
		menu_03menu_02over = newImage("images/menu_03menu_02over.gif");
		menu_03over = newImage("images/menu_03over.gif");
		menu_04menu_03over = newImage("images/menu_04menu_03over.gif");
		menu_04over = newImage("images/menu_04over.gif");
		menu_05menu_04over = newImage("images/menu_05menu_04over.gif");
		menu_05over = newImage("images/menu_05over.gif");
		menu_06menu_05over = newImage("images/menu_06menu_05over.gif");
		menu_06over = newImage("images/menu_06over.gif");
		menu_07menu_06over = newImage("images/menu_07menu_06over.gif");
		menu_07over = newImage("images/menu_07over.gif");
		preloadFlag = true;
	}
}

