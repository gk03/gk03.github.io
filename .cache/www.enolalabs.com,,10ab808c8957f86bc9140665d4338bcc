                    <script type="text/javascript"
                            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDXvGgJUVeZBxgFl0mlT4IxZpO96Nd-LYI"></script>
                    <script type="text/javascript">
                        function initialize() {
                            var mapTpyeID = 'custom_style';
                            var markerIcon = '/uploads/www/map-icon.png';
                            var featureOpts = [
                                {
                                    stylers: [
                                        {hue: '#266FB7'},
                                        {visibility: 'simplified'},
                                        {gamma: .5},
                                        {weight: 1}
                                    ]
                                },
                                {
                                    featureType: 'road.local',
                                    elementType: 'labels',
                                    stylers: [
                                        {color: '#266FB7'}
                                    ]
                                }
                            ];
                            var geocoder = new google.maps.Geocoder();
                            var styledMapOptions = {
                                name: 'Custom Style'
                            };
                            var marker = [];
                            var bounds = new google.maps.LatLngBounds();
                            var mapOptions = {
                                disableDefaultUI: true,
                                mapTypeId: mapTpyeID
                            };
                            var map = new google.maps.Map(document.getElementById('map'), mapOptions);

                            var addresses = ['14th Floor, 600 Congress Ave, Austin, TX 78701', ''];
                            addresses.forEach(function (address) {
                                geocoder.geocode({'address': address}, function (results, status) {
                                    if (status == google.maps.GeocoderStatus.OK) {
                                        marker.push(new google.maps.Marker({
                                            map: map,
                                            position: results[0].geometry.location,
                                            icon: markerIcon
                                        }));

                                        var customMapType = new google.maps.StyledMapType(featureOpts, styledMapOptions);
                                        map.mapTypes.set(mapTpyeID, customMapType);
                                        bounds.extend(results[0].geometry.location);
                                        map.fitBounds(bounds);
                                    }
                                });
                            });


                        }
                        google.maps.event.addDomListener(window, 'load', initialize);
                    </script>
                </div>
            </div>
