      $("select[name='input_10']").change(function(){
        var val = $(this).val();
        var match_value = "";
        $.each(data, function (k, v){
          if(k==val){
            match_value = v;
          }
        });
        if(match_value){
          $("input[name='input_54']").val(match_value);
        } else{
          $("input[name='input_54']").val('');
        }
      });

