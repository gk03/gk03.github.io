<script>
	totalImages = $("ul#SlideShow li").length;
	currentImage = 0;

	strBullets = "";
	for (i=0;i<totalImages;i++){
		strBullets += "<div class=\"BannerBullet\"></div>";
	}
	$("#BannerBullets").html(strBullets);
	
	$("ul#SlideShow li").css("opacity","0").css("display","inline");
	$("ul#SlideShow li:first").css("opacity","1");
	$("#BannerBullets div:first").css("backgroundPosition","0px 0px");
	
	bannerTimer = window.setInterval(changeImage, 4000);
	
	function changeImage() {
		if(!$("ul#SlideShow li:eq("+currentImage+")").is(":animated")){
			$("ul#SlideShow li:eq("+currentImage+")").animate({opacity:"0"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","-32px 0px");
			
			currentImage += 1;
			if(currentImage >= totalImages) {
				currentImage = 0;
			}
			
			$("ul#SlideShow li:eq("+currentImage+")").animate({opacity:"1"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","0px 0px");
		}
	}
	
	$(".BannerBullet").click(function(){
		if(!$("ul#SlideShow li:eq("+currentImage+")").is(":animated")){
			window.clearInterval(bannerTimer);
			
			$("ul#SlideShow li:eq("+currentImage+")").animate({opacity:"0"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","-32px 0px");
			
			currentImage = $(this).index();
			
			$("ul#SlideShow li:eq("+currentImage+")").animate({opacity:"1"},400);
			$("#BannerBullets div:eq("+currentImage+")").css("backgroundPosition","0px 0px");
			
			bannerTimer = window.setInterval(changeImage, 4000);
		}
	});
</script>
</div>

