<script>
function drop_down(menuitem) 
{

	if(menuitem) {
   	thisMenu = document.getElementById(menuitem).style

      	thisMenu.display = "block"
	document.getElementById(menuitem).focus();
	}  else
	{
	hideMenu()
//	window.setTimeout('hideMenu()',1000)

	}


}

function hideMenu()
{
   	thisMenu = document.getElementById('about').style
      	thisMenu.display = "none"

   	thisMenu = document.getElementById('leasing').style
      	thisMenu.display = "none"
}

</script>





