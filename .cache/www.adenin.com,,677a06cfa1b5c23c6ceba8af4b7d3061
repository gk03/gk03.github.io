<!--Makes the navigation bar sticky as you scroll down the page.-->
    <script>
        $(document).ready(function(){
            var topbarHeight = $('.navbar-header').outerHeight();
            var navHeight = $('#main-menu').height();
            if($(window).width() > 768 && window.location.pathname != '/login'){
                $(window).scroll(function(){
                    if ($(window).scrollTop() > topbarHeight){
                        $('.navbar.navbar-default').css('margin-bottom', navHeight + 'px');
                        $('#main-menu').addClass('fixed');
                    } else {
                        $('.navbar.navbar-default').css('margin-bottom','0');
                        $('#main-menu').removeClass('fixed');
                    }
                });
}
        });
</script>


