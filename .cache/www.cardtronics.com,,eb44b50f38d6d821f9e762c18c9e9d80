		<script>
		
			function round2(strStockPrice) {
				var roundPrice;
			
				roundPrice = Math.round(strStockPrice * 100) / 100;
			
				return roundPrice;
			}
		
			function getFirstGroup(strParse, strParseBy) {
				var mySplit = strParse.split(strParseBy);
			
				return mySplit;
			}

			function convertHours(strHour) {
			
				if (strHour == 0){
					strHour = 12;
				} else {
					if (strHour > 12) {
						strHour = strHour - 12;
						
					}
				}
			
				return strHour
			}
		
			function getAmPm(strHour) {
				if (strHour < 12) {
					return ' AM';	
				} else {
					return ' PM';
				}			
			}
		
			function zeroNeeded(strMinute) {
				if (strMinute < 10) {
					return '0' + strMinute;	
				} else {
					return strMinute;	
				}
			}
		
			function currMonth(strMonth) {
				var setCurrMonth = ""; 
			
				switch(strMonth) {
					case 0:
					setCurrMonth = "Jan";
					break;
				
					case 1:
					setCurrMonth = "Feb";
					break;
				
					case 2:
					setCurrMonth = "Mar";
					break;
				
					case 3:
					setCurrMonth = "Apr";
					break;
				
					case 4:
					setCurrMonth = "May";
					break;
				
					case 5:
					setCurrMonth = "June";
					break;
				
					case 6:
					setCurrMonth = "July";
					break;
				
					case 7:
					setCurrMonth = "Aug";
					break;
				
					case 8:
					setCurrMonth = "Sep";
					break;
				
					case 9:
					setCurrMonth = "Oct";
					break;
				
					case 10:
					setCurrMonth = "Nov";
					break;
							
					case 11:
					setCurrMonth = "Dec";
				
				}
			
				return setCurrMonth;
			}
				
