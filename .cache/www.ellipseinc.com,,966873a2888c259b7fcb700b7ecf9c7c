	<script type="text/javascript">
		urchinTracker();

		var obj = null;
		function checkHover() {if (obj) {obj.find('ul').fadeOut('fast');}}
		$(document).ready(function ($) {
			$('img').bind('mouseenter mouseleave', function () {if ($(this).attr('data-roll-src')) {$(this).attr({src: $(this).attr('data-roll-src'), 'data-roll-src': $(this).attr('src')});}});
			$('input, textarea').placeholder();

			$('[title!=""]').qtip({
				style: {
					classes: 'qtip-light qtip-shadow qtip-rounded'
				}
			});

			$('#headerNav > li').hover(function () {
				if (obj) {
					obj.find('ul').fadeOut('fast');
					obj = null;
				}

				$(this).find('ul').fadeIn('fast');
			}, function () {
				obj = $(this);
				setTimeout(
					"checkHover()",
					400);
			});

			$('.subNavClick').click(function () {
				$(this).children(".arrowUp").toggle();
				$(this).children(".arrowDown").toggle();
				$(this).parent().children(".subNav2").slideToggle();
			});

			$('.btnMenu').click(function () {
				$('#dropMenu').slideToggle();
			});
			$('.dropMenuSubClick').click(function () {
				$(this).children(".arrowUp").toggle();
				$(this).children(".arrowDown").toggle();
				$(this).parent().children(".dropMenuSub").slideToggle();
			});
			$('.dropMenuSub2Click').click(function () {
				$(this).children(".arrowUp").toggle();
				$(this).children(".arrowDown").toggle();
				$(this).parent().children(".dropMenuSub2").slideToggle();
			});
			$('#CeoEmailSubmit').click(function () {
			    if ($('#CeoEmail').val() != "") {
			        $.ajax({ url: 'newsletter2_proc.asp', data: 'NewsEmail=' + $('#CeoEmail').val(), cache: false, success: function (html) { $('#footerCEO').html(html); } });
			    }
			    else {
			        alert("Please enter your email address.");
			    }
			});
			$('#NewsEmailSubmit').click(function () {
			    if ($('#NewsEmail').val() != "") {
                    $.ajax({ url: 'newsletter_proc.asp', data: 'NewsEmail=' + $('#NewsEmail').val(), cache: false, success: function (html) { $('#footerUpdates').html(html); } });
			    }
			    else {
			        alert("Please enter your email address.");
			    }
			});
			$('.backToTop').click(function () {
				$("html, body").animate({ scrollTop: 0 }, "slow");
			});
		});
	</script>
