<script type="text/javascript">
	(function($){

		$(function(){

		window.animation = [
			{
				name : 'js01',
				pre : "Measure",
				start : "Everything",
				finish : "The Right Thing",
				title : 'Introducing a Revolution',
				text : 'where information is more valuable than data.  And, the right information leads to business insights.',
				href : '/peloton-website/what-we-do/enterprise-information-management/',
				link_text : 'Learn More'
			},
			{
				name : 'js02',
				pre : "Build A",
				start : "Platform of Analytics",
				finish : "Culture of Analytics",
				title : 'Introducing a Revolution',
				text : 'where users are focused on value-added analysis and management decisions are accelerated by facts.',
				href : '/peloton-website/promise-of-analytics/institutionalizing-analytics/',
				link_text : 'Learn More'
			},
			{
				name : 'js03',
				pre : "Manage Your",
				start : "Information",
				finish : "Business",
				title : 'Introducing a Revolution',
				text : 'where organizations embrace information analytics and leaders can stay fully engaged in devising strategy and driving execution.',
				href : '/peloton-website/what-we-do/enterprise-performance-management/',
				link_text : 'Learn More'
			},
			{
				name : 'js04',
				pre : "Be a Market",
				start : "Leader",
				finish : "Maker",
				title : 'Introducing a Revolution',
				text : 'where organizations readily adapt to market dynamics and companies evolve to transform their industries.',
				href : '/peloton-website/promise-of-analytics/',
				link_text : 'Learn More'
			}
		];

		// this is for dev, it will be shown at the start, moving forward
		$('.panel').show();
		var background = $('.home-background').hide();

		window.animateStep = function ( animationData, callback ) {

			var layer = $('.animation-layer .' + animationData.name );

			$('.home-background').removeClass('js01');
			$('.home-background').removeClass('js02');
			$('.home-background').removeClass('js03');
			$('.home-background').removeClass('js04');
			$('.home-background').addClass(animationData.name);

			$('.pagebuttons div').each(function(){
				$(this).removeClass('selected');
			});

			$('#' + animationData.name).addClass('selected');

			// home background always starts faded out

			// set up the step
			layer.hide();
			layer.children().hide();


			$('#text1').text(animationData.pre).hide();
			$('#text2').text(animationData.start).hide();
			$('.line').hide();
			$('.home-intro').hide();
			$('#titler').html(animationData.title);
			$('#texter').html(animationData.text);
			$('#linker').attr('href',animationData.href);
			$('#linker').html(animationData.link_text);
			//


			background.show();

			$('#text1').fadeIn(2500);
			$('#text2').fadeIn(2500);



			layer.show();

			if(layer.children().length == 3){
				layer.children(':first').fadeIn(2500);
			}

			$('#text2').css({top:0,left: $('#text1').width() + 15  }).text(animationData.start);
			$('.line').animate( { left: $('#text1').width() + 15, width : $('#text2').width()}).fadeIn(2500);

			// we're going to flip out the first text after 4 secnds

			var timer = null;;

			timer = setTimeout(function(){

				// if we're a 3 tiered anim, show :second here, otherwise :first
				if(layer.children().length == 3){

						layer.children(':first').fadeOut(2500);
						layer.children(':nth-child(2)').fadeIn(2500);

				} else {
					layer.children(':first').fadeIn(2500);
				}

				$('#text2').animate({opacity:0},750, function () {
					$(this).css({top:-138}).text(animationData.finish);

					$('.line').animate( {width : $(this).width()});
					// fade in e
					$('.home-intro').fadeIn(750);

					$(this).animate({opacity:1,top:0}, 750, function (){

						if(layer.children().length == 3){
							layer.children(':nth-child(2)').fadeOut(2500);
							layer.children(':nth-child(3)').fadeIn(2500, function (){

								var that = this;

								//setTimeout(function(){

									layer.fadeOut(1000, function(){
										$(that).hide();
									});
									$('#text1').fadeOut(1000);
									$('#text2').fadeOut(1000);
									$('.line').fadeOut(1000);

									$('.home-intro').fadeOut(1000, function (){
										if (callback) { callback.call(); }
									});
								//},4000);

							});


						} else {

							layer.children(':first').fadeOut(2500);
							layer.children(':nth-child(2)').fadeIn(2500, function (){

								var that = this;

								//timers.second = setTimeout(function(){

									layer.fadeOut(1000, function(){
										$(that).hide();
									});
									$('#text1').fadeOut(1000);
									$('#text2').fadeOut(1000);
									$('.line').fadeOut(1000);

									$('.home-intro').fadeOut(1000, function (){
										if (callback) { callback.call(); }
									});
								//},4000);

							});
						}
					});

				});

			},4000);

			return timer;

		};

		window.anim = (function () {
			var timeoutpointer = null;
			var continueon = true;



			var animate = function (start) {

				var a,b,c,d;
				if(start){
					a = start;
				} else { a = 0; }

				if (a==0){
					b=1;
					c=2;
					d=3;
				}

				if (a==1){
					b=2;
					c=3;
					d=0;
				}

				if (a==2){
					b=3;
					c=0;
					d=1;
				}

				if (a==3){
					b=0;
					c=1;
					d=2;
				}
				timeoutpointer = animateStep ( animation[a], function () {
					if (continueon) {
						timeoutpointer = animateStep ( animation[b], function () {
							if (continueon) {
								timeoutpointer = animateStep ( animation[c], function () {
									if (continueon) {
										timeoutpointer = animateStep ( animation[d], function () {
											if (continueon) {
												animate();
											}
										});
									}
								});
							}
						});
					}
				});
			};

			return {
				play : function (start){
					continueon = true;
					animate(start);
				},
				cancel : function () {
					continueon = false;
					// find all elements and call stop
					$('.animation-layer div').stop(true,true);
					$('.animation-layer div div').stop(true,true);
					$('#text1').stop(true,true);
					$('#text2').stop(true,true);
					$('.line').stop(true,true);
					$('.home-intro').stop(true,true);


					$('.animation-layer div').hide();
					$('.animation-layer div div').hide();
					$('#text1').hide();
					$('#text2').hide();
					$('.line').hide();
					$('.home-intro').hide();



					clearTimeout(timeoutpointer);
				}
			};
		})();

		anim.play();

		$('#js01').click(function(){
			anim.cancel();
			anim.play(0);
		});
		$('#js02').click(function(){
			anim.cancel();
			anim.play(1);
		});
		$('#js03').click(function(){
			anim.cancel();
			anim.play(2);
		});
		$('#js04').click(function(){
			anim.cancel();
			anim.play(3);
		});

		});
	})(jQuery);
	</script>

<div id="home-outer">
	<div class="homebg-sizeholder">
		<div class="home-background js01">
			<div class="animation-layer">
				<div class="js01" >
					<div class="anim-1"> </div>
					<div class="anim-2"> </div>
				</div>
				<div class="js02" style="display:none;">
					<div class="anim-1"> </div>
					<div class="anim-2"> </div>
					<div class="anim-3"> </div>
				</div>
				<div class="js03" style="display:none;">
					<div class="anim-1"> </div>
					<div class="anim-2"> </div>
				</div>
				<div class="js04" style="display:none;">
					<div class="anim-1"> </div>
					<div class="anim-2"> </div>
					<div class="anim-3"> </div>
				</div>
			</div>
			<div class="home-content">
				<div class="pagebuttons">
					<div id="js01" class="selected"></div>
					<div id="js02" class=""></div>
					<div id="js03" class=""></div>
					<div id="js04" class=""></div>
				</div>
				<div class="panel">
