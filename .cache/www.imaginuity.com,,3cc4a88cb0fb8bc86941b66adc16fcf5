	<script>
		$(document).foundation({
  			equalizer : {
    			equalize_on_stack: true,
    			act_on_hidden_el: true
  			}
		});

//		$(window).load(function() {
//			$(document).on('click', '.view-more', function(event) {
//				event.preventDefault();
//				loadContent('//');
//				$('.grid').isotope('layout');
//				$('.grid').isotope( 'remove', $('.grid-item.view-more') );
//			});
//		});

		$(document).ready(function(){
			//$(function() {

			// Megamenu
			$('.megamenu').megaMenuReloaded({
				menu_speed_show : 300, // Time (in milliseconds) to show a drop down
				menu_speed_hide : 200, // Time (in milliseconds) to hide a drop down
				menu_speed_delay : 200, // Time (in milliseconds) before showing a drop down
				menu_effect : 'hover_fade', // Drop down effect, choose between 'hover_fade', 'hover_slide', 'click_fade', 'click_slide', 'open_close_fade', 'open_close_slide'
				menu_easing : 'jswing', // Easing Effect : 'easeInQuad', 'easeInElastic', etc.
				menu_click_outside : 1, // Clicks outside the drop down close it (1 = true, 0 = false)
				menu_show_onload : 0, // Drop down to show on page load (type the number of the drop down, 0 for none)
				menubar_trigger : 1, // Show the menu trigger (button to show / hide the menu bar), only for the fixed version of the menu (1 = show, 0 = hide)
				menubar_hide : 0, // Hides the menu bar on load (1 = hide, 0 = show)
				menu_responsive : 1, // Enables mobile-specific script
				menu_carousel : 0, // Enable / disable carousel
				menu_carousel_groups : 0 // Number of groups of elements in the carousel
			});

			$(".footer-megamenu a.viewplus").on('click', function(e) {
				e.preventDefault();
				$(this).parents('.discipline-box').removeClass('active');
				$(this).parents('.footer-megamenu_fullwidth').css('display', 'none');
			});

			// Footer Megamenu
			$('.footer-megamenu').megaMenuFooter({
				menu_speed_show : 300,
				menu_speed_hide : 200,
				menu_speed_delay : 200,
				menu_effect : 'open_close_slide',
				menu_easing : 'jswing',
				menu_click_outside : 1,
				menu_show_onload : 0,
				menubar_trigger : 0,
				menubar_hide : 0,
				menu_responsive : 1,
				menu_carousel : 0,
				menu_carousel_groups : 0
			});

			// Awards table
			$('.awards tr').addClass('row');
			$('.awards tr td').addClass('columns small-12 medium-3');

			// Header "Tablet" menu
			$('header .viewplus').on('click', function(event) {
				event.preventDefault();
				$(this).toggleClass('active');
				$(this).siblings('.small_megamenu_fullwidth').first().fadeToggle();
			});

			/* Top Menu Knowledge Center Filter */
			$('.filters .title-heading').on('click', function() {

				var filter = $(this).data('filter');
				var form = $('.filters .filter-form');
				form.children('input').val(filter);
				form.submit();
			});

			// Newsletter Signup to Mailchimp
			$('#newsletter-form').submit(function(event) {
				event.preventDefault();
				var fname = $('input[name="firstname"]').val();
				var lname = $('input[name="lastname"]').val();
				var femail = $('input[name="email"]').val();
				$.ajax({
					url     : '/wp-content/themes/imaginuity/functions/newsletter.php',
					type    : 'POST',
					data    : { firstname : fname, lastname: lname, email : femail },
					success : function(data) {
						$('#newsletter-submit').text('Thank You!');
					}
				});
			});


			// Email form
			$('.webform-submit').submit(function(event) {
				var fullname = $('#form-fullname').val();
				var email = $('#form-email').val();
				var company = $('#form-company').val();
				var interest = $('#interest option:selected').text();
				var message = $('#form-message').val();
				$.ajax({
					url     : '/wp-content/themes/imaginuity/functions/mail.php',
					type    : 'POST',
					data    : { fname : fullname, femail : email, fcompany : company, finterest : interest, fmessage : message },
					success : function(data) {
						console.log('done');
					}
				});
				event.preventDefault();
			});


			

			

			

			


			/* Resize */

			$(window).resize(function() {

				/* Lay out the bio-detail pulldowns */
				$('.grid-item + .grid-item.full-width.bio-detail').remove();
				$('.grid .grid-item').each(function(index) {
					var bio_detail = $('#bio-detail').html();
					var cur = index+1;
					var bio_id = $(this).data('bio-detail');
					var $elem = $(bio_detail);
					$elem.addClass('detail-'+bio_id);
					if (Modernizr.mq('only screen and (max-width: 40.000em)')) {
						if (cur % 1 == 0) { $(this).after($elem); }
					}
					else if (Modernizr.mq('only screen and (max-width: 64.000em)')) {
						if (cur % 2 == 0 && index != 0) { $(this).after($elem); }
					}
					else {
						if (cur % 3 == 0 && index != 0) { $(this).after($elem); }
					}
				});

			});

//			Open Social Links in New Window
			$('.footer-social .icon').parent().attr('target', '_blank');

			//});

//			"Our Works" Featured Logo
//			$(".featured-work div")each(function() {
//				$(this).on("swipeleft",function() {
//					$(this).next().show();
//					$(this).hide();
//				})
//			});

			//add animation to Consult with Account Dev button, but only on home
							$('.dconsulting span.drop').addClass('dconsulting-button-animate');
			//});
						//});


		});
	</script>



