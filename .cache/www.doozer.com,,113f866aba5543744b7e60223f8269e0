<script type="text/javascript">
jQuery("#slideshow div:gt(0)").hide();

jQuery('.nextslide').click(function() {
    jQuery('#slideshow div:first-child').fadeOut().next().fadeIn().end().appendTo('#slideshow');
});

jQuery('.prevslide').click(function() {
    jQuery('#slideshow div:first-child').fadeOut();
    jQuery('#slideshow div:last-child').prependTo('#slideshow').fadeOut();
    jQuery('#slideshow div:first-child').fadeIn();
});


setInterval(function() { 
  jQuery('#slideshow div:first-child')
    .fadeOut(1000)
    .next()
    .fadeIn(1000)
    .end()
    .appendTo('#slideshow');
},  4000);
</script>


<script type="text/javascript">
jQuery("#smallslideshow div:gt(0)").hide();

jQuery('.nextslide').click(function() {
    jQuery('#smallslideshow div:first-child').fadeOut().next().fadeIn().end().appendTo('#smallslideshow');
});

jQuery('.prevslide').click(function() {
    jQuery('#smallslideshow div:first-child').fadeOut();
    jQuery('#smallslideshow div:last-child').prependTo('#smallslideshow').fadeOut();
    jQuery('#smallslideshow div:first-child').fadeIn();
});


setInterval(function() { 
  jQuery('#smallslideshow div:first-child')
    .fadeOut(1000)
    .next()
    .fadeIn(1000)
    .end()
    .appendTo('#smallslideshow');
},  4000);
</script>

