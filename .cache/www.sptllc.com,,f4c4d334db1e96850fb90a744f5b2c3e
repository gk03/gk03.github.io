    <script type="text/javascript">
        function setfocus() {
            document.forms[0].partno.text = "enter part no";
            document.forms[0].partno.focus();
            document.forms[0].partno.select();


        }
</script>

<script type="text/javascript">
function checkclear(what){
if(!what._haschanged)
{  what.value=''};
what._haschanged = true;
what.style.backgroundColor = "yellow";
}
function checkclearb(what) {


    document.forms(0).partnoh.value = what.value;
   // alert("cleared " + what.value);
        what._haschanged = false;
        //what.value = 'enter manufacturer part no';
        what.value = 'enter MPN';
        what.style.backgroundColor = "white";
    }



    function passvalue() {

        var frm = document.forms(0);
        var partnovalue = frm.partnoh.value;
        var url = "//remote.sptllc.com/SPTVInv/Pages/ProductDetailSearch.aspx?partno=" + partnovalue;
        
        //frm.action = "//spt01server/SPTVInv/Pages/ProductDetailSearch.aspx?partno=" + partnovalue;
        //alert(frm.action);
       // frm.action = "http://www.sptllc.com?t1=" + t1value + "&t2=" + t2value;
        // frm.submit();


        // popupWindow = window.open(url, 'popUpWindow', 'height=700,width=800,left=10,top=10,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no,status=yes');

        //popupWindow = window.open(url, 'popUpWindow', 'height=700,width=800,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no,status=yes');
        //  <a href="http://google.com/" title="Google" rel="gb_page_fs[]">Launch google.com in fullscreen window</a>

        return url;

    }

    function changeURL() {

        var frm = document.forms(0);
        var partnovalue;

        if (frm.partno.value != "enter MPN")
            partnovalue = frm.partno.value;
        else
            partnovalue = document.forms(0).partnoh.value;
            
        document.forms(0).partnoh.value = partnovalue;    
        var url = "//remote.sptllc.com/SPTVInv/Pages/ProductDetailSearch.aspx?partno=" + partnovalue;
    
        var lnk = document.getElementById('slink');
        lnk.href = "//remote.sptllc.com/SPTVInv/Pages/ProductDetailSearch.aspx?partno=" + partnovalue;


        //alert(lnk.href);

        return lnk.href;
      
        
    }


    function KeyDownHandler(btn) {
        // process only the Enter key
        if (event.keyCode == 13) {
            // cancel the default submit
            event.returnValue = false;
            event.cancel = true;
            // submit the form by programmatically clicking the specified button
            btn.click();
        }
    }



    </script>

    <script type="text/javascript">
        function Change(obj, evt) {
            if (evt.type == "focus")
                obj.style.borderColor = "red";
            else if (evt.type == "blur") {
                obj.style.borderColor = "gray";
                //document.forms[0].partno.text = "_______________________";
            }
        }
    </script>

