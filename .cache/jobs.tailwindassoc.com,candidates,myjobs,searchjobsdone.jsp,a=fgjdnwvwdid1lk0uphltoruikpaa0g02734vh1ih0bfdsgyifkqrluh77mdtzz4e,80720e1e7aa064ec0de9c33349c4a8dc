<script>
dojo.require("dijit.Dialog");
dojo.require("dijit.form.Button"); // optimize: load dijit layer

var action;
function dojoAlert(msg, type){
	if(type!=null){
		switch(type){
		case 1:
			dojo.addClass("alertDlg_alertIcon", "warningIcon");
			break;
		}
	}
	//alert("alert1");
	dojo.byId("alertDlg_msg").innerHTML=msg;
	var dlg=dijit.byId("alertDlg");
	dlg.show();
}

</script>

<script type="text/javascript" charset="utf-8">

dojo.ready(function(){	
 	var hideLoader = function() { 
		dojo.style("preloader", "display", "none"); 
 	}
		
	dojo.addOnLoad(function(){
		hideLoader();
	});
	
	
	
	if (document.Form1.div_select) {
		
		}
	if (document.Form1.div2_select) {
			
	}
	adjustMultiselect();
	if((navigator.appVersion.indexOf("Chrome")>-1 || navigator.appVersion.indexOf("Safari")>-1) && document.getElementById("logo")!=null){
		if(document.getElementById("logo").complete){
			dojo.parser.parse();
			dijit.byId("zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("rss_zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("alt_zipcode").attr("regExp", getZipcodeReg("US"));
			
		} else {
		document.getElementById("logo").onload = function() {
			dojo.parser.parse();
			dijit.byId("zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("rss_zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("alt_zipcode").attr("regExp", getZipcodeReg("US"));
			
		};
		document.getElementById("logo").onerror = function() {
			dojo.parser.parse();
			dijit.byId("zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("rss_zipcode").attr("regExp", getZipcodeReg("US"));
			dijit.byId("alt_zipcode").attr("regExp", getZipcodeReg("US"));
			
		};
		}
	} else {
		dojo.parser.parse();
		dijit.byId("zipcode").attr("regExp", getZipcodeReg("US"));
		dijit.byId("rss_zipcode").attr("regExp", getZipcodeReg("US"));
		dijit.byId("alt_zipcode").attr("regExp", getZipcodeReg("US"));
		
	}
	
})

