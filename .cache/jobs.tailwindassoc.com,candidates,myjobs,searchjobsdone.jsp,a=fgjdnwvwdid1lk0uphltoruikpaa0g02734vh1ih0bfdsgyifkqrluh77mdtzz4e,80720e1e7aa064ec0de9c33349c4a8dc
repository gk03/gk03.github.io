function submit_validation(obj) {
    var js_div="";
    var div = obj.div_select;
    if (div) {
       if (obj.div_select.options[0].selected==false) {
	    for (var i=0; i< obj.div_select.length; i++) if (obj.div_select.options[i].selected == true) js_div += obj.div_select.options[i].value+",";
       }
       obj.divisions.value = js_div;
    }
    
    var js_div2="";
    var div2 = obj.div2_select;
    if (div2) {
       if (obj.div2_select.options[0].selected==false) {
	    for (var i=0; i< obj.div2_select.length; i++) if (obj.div2_select.options[i].selected == true) js_div2 += obj.div2_select.options[i].value+",";
       }
       obj.divisions2.value = js_div2;
    }
    
    var js_positiontypes="";
    var position_types = obj.positiontype;
    if (position_types) {
       if (obj.positiontype.options[0].selected==false) {
	    for (var i=0; i< obj.positiontype.length; i++) if (obj.positiontype.options[i].selected == true) js_positiontypes += obj.positiontype.options[i].value+",";
       }
    }
    obj.jobtypes.value = js_positiontypes;    
    obj.submit();
}

function adjustSelected(obj){
	if(obj.options(0).selected){
		for(i=1;i<obj.options.length;i++){
			if(obj.options(i).selected){
				obj.options(0).selected = false;
				break;
			}		
		}
	}
}

var gr_type;
function applyLocale(obj, group){
	  var countryid = dijit.byId(obj).value;
	
	  dijit.byId(group+"zipcode").attr("regExp", getZipcodeReg(countryid));

	  gr_type = group;
	  var t = new Date();

	  var postData = "label_state~label_zipcode";

	  dojo.byId("localeKeys").value = postData;
	  var statefield = "----Select All----";
	  var options = {
	     url:     "/staticpages/getlocaledata.jsp?t="+t.getTime() + "&country="+countryid +"&statefield="+statefield+"&all=1"+"&dashes=0"+"&fullname=1",
		 dataType: "json",
		 form: "formLocale",
		 load: afterApply
	  };
	  dojo.xhrPost(options);
}

function afterApply(data, status){
	var obj = dojo.fromJson(data);
	var ziplabel = obj.label_zipcode.split("~");
	var statelabel = obj.label_state.split("~");
	var statestr = obj.state.split("~");
	var label_state = gr_type+"label_state";
	var label_zipcode = gr_type + "label_zipcode";
	var state_div = gr_type+"state_div";
	var formid = "";
	if(gr_type == "")formid = "1.";
	else if(gr_type=="alt_")formid = "2.alt_";
	else formid = "3.rss_";
	dojo.byId(label_state).innerHTML = statelabel[0]+":";
	dojo.byId(label_zipcode).innerHTML = ziplabel[0];
	var size = dojo.byId(gr_type+"states").getAttribute("size");
	dojo.byId(state_div).innerHTML = "<select id='"+gr_type+"states' name='"+gr_type+"states' multiple style='width:100%;' size='"+size+"' onchange='adjustSelected(document.Form"+formid+"states)'><option value='' selected>----Select All----</option>"+ statestr[0]+"</select>";
}

function changeSortedBy(col, dtype) {

	var url = "/candidates/myjobs/searchjobsdone.jsp?req=2&page=1&a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&StatesString=&SearchString=&divisions=&divisions2=&t=1473394335691&sortcol="+col+"&sortorder=2&sortdatatype="+dtype+"&source=";
	document.Form1.action = url;
	document.Form1.submit();
}
function changeOrder() {
	var order = 0;
	if(2==1)order = 2;
	else order = 1;
	var column = 0;
	column = column==0?4:column;
	var url = "/candidates/myjobs/searchjobsdone.jsp?req=2&page=1&a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&StatesString=&SearchString=&divisions=&divisions2=&t=1473394335691&sortcol="+column+"&sortorder="+order+"&sortdatatype=1&source=";
	document.Form1.action = url;
	document.Form1.submit();
}
var firsttime_alert = true;
var alert_h;
var alert_w;
function jobsalert_filter(){
	var dlg_alert = dijit.byId("dlg_jobs_alert");
	document.Form2.reset();
	dijit.byId("alt_country").set("value","US");
	dlg_alert.show();
	
	if(firsttime_alert){
		firsttime_alert= false;
		dlg_alert.set("title", "Create Jobs Agent");
		alert_h = dojo.style(dojo.byId("dlg_jobs_alert"),"height");
		alert_w = dojo.style(dojo.byId("dlg_jobs_alert"),"width");
	
		var x = alert_w - 15;
		var y = alert_h - 44;
	
		dojo.byId("handleRightAlert").style.height = y + "px";
		dojo.byId("handleRightAlert").style.left = x + "px";
		dojo.byId("handleCornerAlert").style.top = y + "px";
		dojo.byId("handleCornerAlert").style.left = x + "px";
		dojo.byId("handleBottomAlert").style.width = x + "px";
		dojo.byId("handleBottomAlert").style.top = y + "px";
		setDragObject("handleRightAlert","handleBottomAlert","handleCornerAlert",x,y,"dlg_jobs_alert");
	} else {
		setDragObject("handleRightAlert","handleBottomAlert","handleCornerAlert",alert_w-15,alert_h-44,"dlg_jobs_alert");
	}
}
var firsttime_rss = true;
var rss_h;
var rss_w;
function rssfeed_filter(){
	var dlg_rss = dijit.byId("dlg_rss");
	document.Form3.reset();
	dijit.byId("rss_country").set("value","US");
	dlg_rss.show();
	
