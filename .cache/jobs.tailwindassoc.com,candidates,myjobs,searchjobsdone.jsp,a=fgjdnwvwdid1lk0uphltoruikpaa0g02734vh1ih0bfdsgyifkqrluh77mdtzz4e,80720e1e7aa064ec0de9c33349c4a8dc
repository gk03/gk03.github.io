	dlg_missingjob.show();	
}
dojo.require("dijit.form.Button");

function hide_dlg_loggedin(){
	var dlg_missingjob_loggedin = dijit.byId("dlg_missingjob_loggedin");
	dlg_missingjob_loggedin.hide();
}
function hide_dlg(){
	var dlg_missingjob = dijit.byId("dlg_missingjob");
	dlg_missingjob.hide();
}
function signin_dlg(){
	//hide_dlg();
	var dlg_signin = dijit.byId("dlg_sign_in");
	dlg_signin.show();
