		firsttime_rss= false;
		dlg_rss.set("title","Jobs through RSS");
		rss_h = dojo.style(dojo.byId("dlg_rss"),"height");
		rss_w = dojo.style(dojo.byId("dlg_rss"),"width");
	
		var x = rss_w - 15;
		var y = rss_h - 44;
	
