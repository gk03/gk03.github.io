		dojo.byId("handleRightRSS").style.left = x + "px";
		dojo.byId("handleCornerRSS").style.top = y + "px";
		dojo.byId("handleCornerRSS").style.left = x + "px";
		dojo.byId("handleBottomRSS").style.width = x + "px";
		dojo.byId("handleBottomRSS").style.top = y + "px";
		setDragObject("handleRightRSS","handleBottomRSS","handleCornerRSS",x,y,"dlg_rss");
	} else {
		setDragObject("handleRightRSS","handleBottomRSS","handleCornerRSS",rss_w-15,rss_h-44,"dlg_rss");		
	}
}
function changeperpage() {
	var per = dijit.byId("perpage").get("value");
	var url = "/candidates/myjobs/searchjobsdone.jsp?req=1&pageno=1&perpage="+per+"&a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&StatesString=&SearchString=&divisions=&divisions2=&source=&t=1473394335691";
	document.Form1.action = url;
	document.Form1.submit();
}

function create_alert(){
	if(!dijit.byId("Form2").validate())return;
	var alertname = document.Form2.alertname.value;
	if(alertname == null || alertname.replace(/^\s+|\s+$/g,"").length == 0) {
		dojoAlert("Please create a title for this agent.");
		return;
	}
	else if(alertname.length > 100) {
		dojoAlert("Please use a shorter agent name.");
		return;
	}
	var candidate_email = document.Form2.email.value;
	if(candidate_email == null || candidate_email.indexOf("@") < 0) {
		dojoAlert("Invalid Email Address");
		return;
	}
	var frequency = document.Form2.frequency.value;

	var postype="";
    var positiontypes = document.Form2.alt_positiontype;
    if (positiontypes) {
       if (document.Form2.alt_positiontype.options[0].selected==false) {
	    for (var i=0; i< document.Form2.alt_positiontype.length; i++) 
	    	if (document.Form2.alt_positiontype.options[i].selected == true) 
	    		postype += document.Form2.alt_positiontype.options[i].value+",";
       }
    }
	
    var js_div="";
    var div = document.Form2.alt_div_select;
    if (div) {
       if (document.Form2.alt_div_select.options[0].selected==false) {
	    for (var i=0; i< document.Form2.alt_div_select.length; i++) 
	    	if (document.Form2.alt_div_select.options[i].selected == true) 
	    		js_div += document.Form2.alt_div_select.options[i].value+",";
       }
    }
    var states="";
    for (var i=0; i< document.Form2.alt_states.length; i++) {
	    	if (document.Form2.alt_states.options[i].selected == true) 
	    		states += document.Form2.alt_states.options[i].value+",";
    }	
    var SearchString=document.Form2.alt_SearchString.value;
    
	var zip = document.Form2.alt_zipcode.value;
    var miles = document.Form2.alt_miles.value;
        
    var country = dijit.byId("alt_country").value;

    var options = {
         url:		"/candidates/myjobs/createjobsalert_outside.jsp?a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&portalid=0&alertname="+escape(alertname)+"&email="+candidate_email+"&frequency="+frequency+"&postype="+postype+"&states="+states+"&zip="+zip+"&miles="+miles+"&country="+country+"&divisions="+js_div+"&SearchString="+escape(SearchString),
        load:	after_submit // post-submit callback  
   	};
   	
   	dojo.xhrPost(options);
}

function after_submit(val, status) {
	var reg =/^\s+|\s+$/g;
    var res = val.replace(reg,'');
    //alert(res);
	if(res == "success") {
		dojoAlert("Your job agent has been created successfully.");
		dijit.byId("dlg_jobs_alert").hide();
	}
	else dojoAlert("There was a problem creating the job agent.  Please try again later.");
}

function generate_rss_feed(){
	var ua = window.navigator.userAgent;
	var ind = ua.indexOf("Chrome");
	if(ind >-1){
		dojoAlert("Google Chrome does not currently support the generation of RSS feeds without the installation of an additional tool. To continue, either switch browsers or please refer to Google Chrome for what tools they recommend.");
	}
	
	var noofjobs = document.Form3.numberofjobs.value;

	var postype="";
    var positiontypes = document.Form3.rss_positiontype;
    if (positiontypes) {
       if (document.Form3.rss_positiontype.options[0].selected==false) {
	    for (var i=0; i< document.Form3.rss_positiontype.length; i++) 
	    	if (document.Form3.rss_positiontype.options[i].selected == true) 
	    		postype += document.Form3.rss_positiontype.options[i].value+",";
       }
    }
	
    var js_div="";
    var div = document.Form3.rss_div_select;
    if (div) {
       if (document.Form3.rss_div_select.options[0].selected==false) {
	    for (var i=0; i< document.Form3.rss_div_select.length; i++) 
	    	if (document.Form3.rss_div_select.options[i].selected == true) 
	    		js_div += document.Form3.rss_div_select.options[i].value+",";
       }
    }
    var states="";
    for (var i=0; i< document.Form3.rss_states.length; i++) {
	    	if (document.Form3.rss_states.options[i].selected == true) 
	    		states += document.Form3.rss_states.options[i].value+",";
    }	
    var SearchString=document.Form3.rss_SearchString.value;
    
	var zip = document.Form3.rss_zipcode.value;
    var miles = document.Form3.rss_miles.value;
    var country = dijit.byId("rss_country").value;
	
	var urlstring="/candidates/myjobs/getjobsrssfeed.jsp?a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&noofjobs="+noofjobs+"&postype="+postype+"&states="+states+"&zip="+zip+"&miles="+miles+"&country="+country+"&divisions="+js_div+"&SearchString="+escape(SearchString);
    window.open(urlstring,"","status=no,scrollbars=yes,resizable=yes,location=yes");
}

function adjustMultiselect() {
	var winH = 0;
	if (document.body && document.body.offsetWidth) {
		winH = document.body.offsetHeight;
	}
	if (document.compatMode=='CSS1Compat' && document.documentElement && document.documentElement.offsetWidth ) {
		winH = document.documentElement.offsetHeight;
	}
	if (window.innerHeight) {
		winH = window.innerHeight;
	}
	var miniheight = 550;
	var unit = 1+1;
	var mount = 16;
	var minisize = 6 - unit;
	var size = Math.max(Math.floor((winH-miniheight)/(unit*mount)),0)+minisize;

	if(dojo.byId("div_select")!=null)dojo.byId("div_select").setAttribute("size", size);
	if(dojo.byId("div2_select")!=null)dojo.byId("div2_select").setAttribute("size", size);
	dojo.byId("positiontype").setAttribute("size", size);
	dojo.byId("states").setAttribute("size", size);
}


</script>

