function setWebtrendsRedirectMetaTag()
{
    try
    {
        var redirectCookieValue = $.cookie("webtrendsRedirect");
        if(redirectCookieValue !== null && redirectCookieValue !== undefined && redirectCookieValue === '1')
        {
            var metaTag = $("meta[name='WT.z_r']");
            if(metaTag === undefined || metaTag === null)
            {
                $("head").append("<meta name='WT.z_r' content='1'/>");
            } else {
                $(metaTag).attr("content","1");
            }
            $.removeCookie("webtrendsRedirect");
        }
    } catch(exception)
    {
        // suppress exception and move on
    }

}
setWebtrendsRedirectMetaTag();
