    function signin(){
    	
    		url_for_register = "a="+"9xjdnwf8nt489qdiu4ab0qq7clsnet01f27n6pjaxju02yq1u697ou3dvfougsq9"+"&id=-1&SearchString=&StatesString=&from"+"COMP" ;
    		document.getElementById("form_apply_unsigned").reset();
    		document.getElementById("form_apply_unsigned").id.value = -1;
