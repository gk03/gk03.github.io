<SCRIPT LANGUAGE=VBScript>
Private i, x, MM_FlashControlVersion
On Error Resume Next
x = null
MM_FlashControlVersion = 0
var Flashmode
FlashMode = False
For i = 9 To 1 Step -1
	Set x = CreateObject("ShockwaveFlash.ShockwaveFlash." & i)
	
	MM_FlashControlInstalled = IsObject(x)
	
	If MM_FlashControlInstalled Then
		MM_FlashControlVersion = CStr(i)
		Exit For
	End If
Next
FlashMode = (MM_FlashControlVersion >= 6)
</SCRIPT>
<script language="JavaScript" type="text/javascript">
	_dw = FlashMode;
	var ISFlash = _dw;
</script>
<script language=VBScript>
If Not FlashMode Then
 document.write "<!-- eMX Globalnav, no_flash.iota $Revision: 1.8 $ -->"
 document.write "	<div id=""macromedia""><font face=Arial size=2><center><BR>We have dectect you do not have Macromedia Flash installed. <BR>Some images may not appear correctly. Please download <a href='http://www.macromedia.com/shockwave/download/download.cgi?P1_Prod_Version=ShockwaveFlash' target='_blank'>Here</a></font></center><BR></div>"
End If
</SCRIPT>

