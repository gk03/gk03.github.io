		  {
			phone = fbTfn[fbctr].split("=")[1];
			fbPage = "true";
		  }
		}
		if(fbPage == '')
		{
			className = "generic";
		}
	}
	else
	{
		for (var i=0;i<attribs.length;i++)
		{
		  var attr = attribs[i];
		  if(attr.nodeName == "class")
			className = attr.nodeValue;
		}	
	}
	for (var ctr=0;ctr < tfNumbers.length;ctr++)
	{
		if(tfNumbers[ctr].split("=")[0] == className)
			phone = tfNumbers[ctr].split("=")[1];
	}
phone = phone.split('.').join("");
    var el = document.getElementById('form1');
    el.onsubmit = function(evt) {
        var url = $(this).attr('action');
		if(url.indexOf('?') > -1 && url.indexOf('INFINITY_ID') < 0)
	      $(this).attr('action', url + '&INFINITY_ID=' + phone );
		else if(url.indexOf('INFINITY_ID') < 0)
			$(this).attr('action', url + '?INFINITY_ID=' + phone );
		document.getElementById("hdnVisitorId").value = phone;
		return true;
    };
</script>
</form>



    </div>
  </div>
</section>

