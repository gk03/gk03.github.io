<!-- Sticky Nameplate minification -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-throttle-debounce/1.1/jquery.ba-throttle-debounce.min.js"></script>
<script>
$(function(){
  function make_sticky() {
    var mn = $("#nameplate");
    mns = "be-sticky";
    hdr = $('#nameplate').height();
    if( $(this).scrollTop() > hdr ) {
      mn.addClass(mns);
    } else {
      mn.removeClass(mns);
    }
  };
  // Throttle the scroll event so it doesn't retard performance
  $(window).scroll( $.throttle( 250, make_sticky ) );
});
</script>


