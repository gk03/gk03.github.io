/* Using multiple unit types within one animation. */
$("#chat_image").click(function(){
	if ($("#live_chat").css('right') === '-198px') {
  $("#live_chat").animate({
right: '+=198'
  }, 500 );
  $("#live_chat_container").width(234);
  
	}
	if ($("#live_chat").css('right') === '0px') {
  $("#live_chat_container").animate({
width: '-=198'
  }, 500 );	
  $("#live_chat").animate({
right: '-=198'
  }, 500 );
	}
});

