			function jssInit_slider(event) {
				
				
			}
			function jssInitEnd_slider(event) {
				jssAnimText_slider(event.item.index);
			}
			function jssAnimTextExec_slider(c, x, c2, x2) {
				if (c == undefined) { c = c2; x = x2; c2 = undefined; x2 = undefined; }
			    jQuery(c).addClass(x + " animated")
					.one(jQuery.support.animation.end, function(e) {
						jQuery(this).removeClass(x + " animated");
						jQuery(this).css("opacity", "1");
						if (c2 != undefined) jssAnimTextExec_slider(c2, x2);
				});
			}
			function jssAnimText_slider(i) {
				if (!jQuery.support.animation || !jQuery.support.transition) {
					return;
				}
				jssAnimTextExec_slider("#slider .owl-item:eq(" + i + ") .jss-title > *","pulse",undefined,undefined);
			}var slider = null;

jQuery(document).ready(function(){
	var options = {
		items: 1,
		margin: 10,
		loop: true,
		center: true,
		autoplay: true,
		autoplayTimeout: 5600,
		autoplayHoverPause: 1,
		rtl: false,
		nav: true,
		navContainer: '#slider.jss-slider .jss-navigation .jss-navigation-buttons',
		dots: true,
		dotsContainer: '#slider.jss-slider .jss-navigation .jss-navigation-dots',
		autoHeight: true,
		lazyLoad: false,
		navText: [ ' ', ' ' ],
		smartSpeed: 300,
		onInitialize: jssInit_slider,
		onInitialized: jssInitEnd_slider
		
	};
	slider = jQuery("#slider.jss-slider .owl-carousel").owlCarousel(options);
	
			var slider_anim = 0;
			slider.on("translate.owl.carousel", function(event) {
				slider_anim = 1;
			});
			slider.on("translated.owl.carousel", function(event) {
				if (slider_anim) {
					
					
					jssAnimText_slider(event.item.index);
				}
				slider_anim = 0;
			});
	
});

jQuery(window).load(function() {
	slider.data('owl.carousel').refresh();
	 setTimeout(function () {
        
    }, 1000);
});
