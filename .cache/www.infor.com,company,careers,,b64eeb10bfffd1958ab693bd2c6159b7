    function isAgentAvailable()
    {
        var available = false;
        var chatLinkInfo = $("#myChatLinkInfo").attr("class");
        if (chatLinkInfo !== undefined && chatLinkInfo !== null && chatLinkInfo !== '' && chatLinkInfo === "rn_ChatAvailable")
        {
            available = true;
        } else {
            available = false;
        }
        return available;
    }
    function checkAgentAvailability()
    {
        var chatLinkInfo = $("#myChatLinkInfo").text();
        if (chatLinkInfo === '')
        {
            setTimeout(checkAgentAvailability, 1000);
        } else {
            var agentAvailable = isAgentAvailable();
            if (!agentAvailable)
            {
                applyEmailLink();
            } else {
                applyChatLink();
            }
            rightNowChat = agentAvailable;
        }
    }


    function clickChatLink()
    {
        $("#myChatLink>a").trigger("click");
    }

    
    function showProactiveChatPrompt()
    {
        if(!isChatBlocked() && !isChatPromptScheduled)
        {
            isChatPromptScheduled = true;
            $("#chatRightNowInvite").css("position","fixed");
            $("#chatRightNowInvite").delay(proactivePromptDelay).fadeIn("slow").delay(proactivePromptVisible).fadeOut("slow");
        }
    }
    function isChatBlocked()
    {
        var blocked = false;
        var chatEnabled = "yes";
        if(chatEnabled === 'no')
        {
            blocked = true;
        }
        var cookieValue = $.cookie("blockProactiveChat");
        if(cookieValue!==null)
        {
            blocked = true;
        }
        if($("#postingStatus").val()==='posting')
        {
            blocked = true;
        }
        if(!blocked)
        {
            try
            {
                if(blockChatPrompt)
                {
                    blocked = true;
                }
            } catch(exception)
            {
                // foo
            }
                
        }
        return blocked;
    }
    function blockChat()
    {
        dismissChatInvitation();
        $.cookie("blockProactiveChat", 1, {expires : 10, path    : '/',});        
    }
    function dismissChatInvitation()
    {
        $("#chatRightNowInvite").dequeue();
        $("#chatRightNowInvite").fadeOut("slow");
        isBlocked = true;
    }
    function scheduleDismissChatInvitation()
    {
        $("#chatRightNowInvite").delay(30000).fadeOut("slow");
    }
    function isRetina(){
        return ((window.matchMedia && (window.matchMedia('only screen and (min-resolution: 192dpi), only screen and (min-resolution: 2dppx), only screen and (min-resolution: 75.6dpcm)').matches || window.matchMedia('only screen and (-webkit-min-device-pixel-ratio: 2), only screen and (-o-min-device-pixel-ratio: 2/1), only screen and (min--moz-device-pixel-ratio: 2), only screen and (min-device-pixel-ratio: 2)').matches)) || (window.devicePixelRatio && window.devicePixelRatio >= 2)) && /(iPad|iPhone|iPod)/g.test(navigator.userAgent);
    }
