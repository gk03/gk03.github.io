        document.body.appendChild(bckfrm);
        function setHFrmSrc() {
            bckfrm.src = "http://www1-cdn.dell.com/flash/default/ajaxHistory/history_frame.htm";
        }
        setTimeout(setHFrmSrc, 5);
    </script>
