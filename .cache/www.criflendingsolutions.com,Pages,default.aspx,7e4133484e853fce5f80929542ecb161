<script type="text/javascript">
  function executeSearch()
  {
    var inputSearchText = window.document.getElementById('ctl00_wpSearchsearchInputBox').value;
    var valSearched = jQuery.trim(inputSearchText);
    if ( valSearched.length != '' && valSearched != 'Cerca nel sito...' && valSearched != 'Search on site...' )
    {
      window.location.href='http://www.criflendingsolutions.com/Search/?k=' + inputSearchText;
    }
    else
    {
      alert('At least one search term must be specified.');
    }
  }
  function searchEnter()
  {
    if(event.which || event.keyCode)
  {
      if ((event.which == 13) || (event.keyCode == 13)) {
        document.getElementById('ctl00_wpSearchsearchLink').click();return false;}}
      else {return true};
  }
  function clearSearch()
  {
    window.document.getElementById('ctl00_wpSearchsearchInputBox').value = '';
  }
 </script>

