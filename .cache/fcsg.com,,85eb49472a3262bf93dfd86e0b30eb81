<script>
			function revealNav(e) {
			if (document.querySelector('#banner_nav .expanded')) {
			while (document.querySelector('#banner_nav .expanded')) {
			$(document.querySelector('#banner_nav .expanded')).removeClass('expanded');
			}
			}
			$(document.body).addClass('nav_active');
			e.preventDefault();
			}
			function hideNav(e) {
			if (document.querySelector('#off_canvas_nav ul li .expanded')) {
			while (document.querySelector('#off_canvas_nav ul li .expanded')) {
			$(document.querySelector('#off_canvas_nav ul li .expanded')).removeClass('expanded');
			}
			}
			$(document.body).removeClass('nav_active');
			e.preventDefault();
			}
			function toggleOffCanvSubmenu(e) {
			e.preventDefault();
			targ = e.target;
			while (targ.tagName != "DIV") {
			targ = targ.parentElement;
			}
			if ($(targ).hasClass('expanded')) {
			collapseSubmenu(targ);
			} else {
			expandSubmenu(targ);
			}
			}
			function toggleBannerSubmenu(e) {
			e.preventDefault();
			targ = e.target;
			while (targ.tagName != "A") {
			targ = targ.parentElement;
			}
			if ($(targ).hasClass('expanded')) {
			collapseSubmenu(targ);
			} else {
			if (document.querySelector('#banner_nav .expanded')) {
			while (document.querySelector('#banner_nav .expanded')) {
			$(document.querySelector('#banner_nav .expanded')).removeClass('expanded');
			}
			}
			expandSubmenu(targ);
			}
			}
			function expandSubmenu(targ) {
			$(targ).addClass('expanded');
			}
			function collapseSubmenu(targ) {
			$(targ).removeClass('expanded');
			}
			function clickOut(e) {
			if (document.querySelector('#banner_nav .expanded')) {
			if (!isDescendant(document.getElementById("banner_nav"), e.target)) {
			while (document.querySelector('#banner_nav .expanded')) {
			$(document.querySelector('#banner_nav .expanded')).removeClass('expanded');
			}
			}
			}
			}
			function isDescendant(parent, child) {
			var node = child.parentNode;
			while (node != null) {
			if (node == parent) {
			return true;
			}
			node = node.parentNode;
			}
			return false;
			}
			$(document).ready(function() {
			$("html.touch #wrapper").on("swiperight", revealNav);
			$("html.touch #off_canvas_nav, html.touch #cover_up").on("swipeleft", hideNav);
			});
		</script>
		<script src="scripts/modernizr.custom.98069.js"></script>
