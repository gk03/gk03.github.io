                <script type="text/javascript">
                    $(document).ready(function () {

                        var menuBleed = true;

                        if (menuBleed == true) {
                            $(document).on("scroll", onScroll);
                        }
                        else{
                            $('.navbar').addClass('active');
                        }

                    });

                    function onScroll(event) {
                        var scrollPos = $(document).scrollTop();

                        if (scrollPos > 1) {
                            $('.navbar').addClass('active');
                        }
                        else {
                            $('.navbar').removeClass("active");
                        }
                    }
                </script>
            
            
