<script type="text/javascript">
    var alertmessage = '';
    if (alertmessage)
        alert(alertmessage);
    //Function to bind the list based on selected dropdon values
    function bindbyvalue() {
        try {
            var CityID = document.getElementById("CityID").value;
            var JobCategoryID = document.getElementById("JobCategoryID").value;
            if (CityID != '' && JobCategoryID == '')
                location.href = '/?cityid=' + CityID;
            else if (JobCategoryID != '' && CityID == '')
                location.href = '/?JobCategoryID=' + JobCategoryID;
            else if (JobCategoryID != '' && CityID != '')
                location.href = '/?cityid=' + CityID + '&JobCategoryID=' + JobCategoryID;
            else if (JobCategoryID == '' && CityID == '')
                location.href = '/';
        }
        catch (e) {
            alert(e);
        }
    }
</script>

