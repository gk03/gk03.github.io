<script type="text/javascript">
    $(function () {
        $('table').footable();
    });
    $(document).ready(function () {
        try
        {
            var path = 'http://jobs.centextech.com/jobs.centextech.com';
            $("#tags").autocomplete({
                source: function(request,response) {
                    $.ajax({
                        url: path+"/ContentOuter/SearchJob",
                        type: "POST",
                        dataType: "json",
                        data: { term: request.term },
                        success: function (data) {
                            response($.map(data, function (item) {
                                
                                return { label: item.NameTxt, value: item.NameTxt };
                            }))

                        }
                    })
                },
                messages: {
                    noResults: "", results: ""
                }
            });
        }
        catch (e) {
            alert(e);
        }
    })
  
</script>

