		easing: 'easeOutBounce',
		size : 200,
		animate : 2000,
		lineWidth : 2,
		lineCap : 'square',
		lineWidth : 3,
		barColor : false,
		trackColor : '#F5F5F5',
		scaleColor : false,
		onStep: function(from, to, percent) {
		$(this.el).find('.percent').text(Math.round(percent)+'%');
		}
	});
