    function preloadImages(path, pics) {

        var images = new Array;

        for (var picNum = 0; picNum < pics.length; picNum++) {
            images[picNum] = new Image(pics[picNum][1], pics[picNum][2]);
            images[picNum].src = (path + pics[picNum][0]);
        }

        return (images);
    }

    $(document).ready(function () {
        var str = location.href.toLowerCase();
        var file, n;
        $("ul#awesome-menu li a").each(function () {
            if (str.indexOf(this.href.toLowerCase()) > -1) {
                file = window.location.pathname;
                n = file.lastIndexOf('/');
                if (n >= 0) {
                    file = file.substring(n + 1);
                    if (file == "Home.aspx" || file == "home.aspx") {
                        $("ul#awesome-menu li a.Home").addClass("HomeActive");
                    }
                    if (file == "AboutUs.aspx") {
                        $("ul#awesome-menu li a.AboutUs").addClass("AboutUsActive");
                    }
                    if (file == "Products.aspx") {
                        $("ul#awesome-menu li a.Products").addClass("ProductsActive");
                    }
                    if (file == "Services.aspx") {
                        $("ul#awesome-menu li a.Services").addClass("ServicesActive");
                    }
                    if (file == "Events.aspx") {
                        $("ul#awesome-menu li a.Events").addClass("EventsActive");
                    }
                    if (file == "Jobs.aspx") {
                        $("ul#awesome-menu li a.Jobs").addClass("JobsActive");
                    }
                    if (file == "ContactUs.aspx") {
                        $("ul#awesome-menu li a.ContactUs").addClass("ContactUsActive");
                    }
                }
            }
        })
    });






