   <script type="text/javascript">

       $(document).ready(function () {


           var str = location.href.toLowerCase();

           var flag = false;

           $("nav a").each(function () {

               if (flag == false) {

                   var file = window.location.pathname;
                   if (str.indexOf(file.toLowerCase()) > -1) {

                       n = file.lastIndexOf('/');
                       if (n >= 0) {
                           file = file.substring(n + 1);
                       }
                       if (file == "AcumaticaCloudERP.aspx") {
                           $(AcumaticaCloudERP).css("background", "#58BE41");

                           flag = true;
                           return;
                       }
                       if (file == "DynamicsCrm.aspx") {

                           $(DynamicsCrm).css("background", "#58BE41");
                           flag = true;
                           return;
                       }
                       if (file == "MS_Office365.aspx") {
                           $(MS_Office365).css("background", "#58BE41");

                           flag = true;
                           return;
                       }
                       if (file == "MobileDevelopment.aspx") {
                           $(MobileDevelopment).css("background", "#58BE41");
                           flag = true;
                           return;
                       }
                  
                       if (file == "TeamProManager.aspx") {

                           $(team).css("background", "#58BE41");
                           $(team).css("width", "140px");

                           $(team).parent().parent().siblings("nav a").css("background", "#58BE41");

                           flag = true;
                           return;
                       }

                       if (file == "QB2CRM.aspx") {
                           $(qb2).css("background", "#58BE41");
                           $(qb2).parent().parent().siblings("nav a").css("background", "#58BE41");

                           flag = true;
                           return;
                       }
                       if (file == "Hoovers2CRM.aspx") {
                           $(hoover).css("background", "#58BE41");
                           $(hoover).parent().parent().siblings("nav a").css("background", "#58BE41");

                           flag = true;
                           return;
                       }
                       if (file == "ManagedServices.aspx") {
                           $(ManagedServices).css("background", "#58BE41");
                           $(ManagedServices).parent().parent().siblings("nav a").css("background", "#58BE41");

                           flag = true;
                           return;
                       }
                       if (file == "VOIP.aspx") {
                           $(VOIP).css("background", "#58BE41");
                           $(VOIP).parent().parent().siblings("nav a").css("background", "#58BE41");

                           flag = true;
                           return;
                       }


                   }

               }
           })
       });


   
</script>


