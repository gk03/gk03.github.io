<script type="text/javascript">
Supported = 1;

SubMenuImagesN = Array(10);
SubMenuImagesH = Array(10);
  
TitleMenuImagesN = Array(10);
TitleMenuImagesH = Array(10);

TellMeMore = Array(2);
   
function LoadImages()
{
	app = navigator.appName;
   	ver = navigator.appVersion;
   	mac = ver.indexOf("Macintosh") > 0;
   	win = ver.indexOf("Win") > 0;
   	vnum = parseFloat(ver);
	
   	Supported = (((app == "Netscape") && (vnum >= 3 )) ||
        ((mac) && (app == "Microsoft Internet Explorer") && (vnum >= 3.01 )) ||
        ((win) && (app == "Microsoft Internet Explorer") && (vnum >= 4 ))) 
   	if (Supported) 
   	{
  	   
		window.defaultStatus="";
  

  	TitleMenuImagesN[1]= new Image(); TitleMenuImagesN[1].src = "/assets/images/SG/SG_NavBar_1S.gif";
	TitleMenuImagesH[1]= new Image(); TitleMenuImagesH[1].src = "/assets/images/SG/SG_NavBar_1H.gif";
	TitleMenuImagesN[2]= new Image(); TitleMenuImagesN[2].src = "/assets/images/SG/SG_NavBar_2N.gif";
	TitleMenuImagesH[2]= new Image(); TitleMenuImagesH[2].src = "/assets/images/SG/SG_NavBar_2H.gif";
	TitleMenuImagesN[3]= new Image(); TitleMenuImagesN[3].src = "/assets/images/SG/SG_NavBar_3N.gif";
	TitleMenuImagesH[3]= new Image(); TitleMenuImagesH[3].src = "/assets/images/SG/SG_NavBar_3H.gif";
	TitleMenuImagesN[4]= new Image(); TitleMenuImagesN[4].src = "/assets/images/SG/SG_NavBar_4N.gif";
	TitleMenuImagesH[4]= new Image(); TitleMenuImagesH[4].src = "/assets/images/SG/SG_NavBar_4H.gif";
	TitleMenuImagesN[5]= new Image(); TitleMenuImagesN[5].src = "/assets/images/SG/SG_NavBar_5N.gif";
	TitleMenuImagesH[5]= new Image(); TitleMenuImagesH[5].src = "/assets/images/SG/SG_NavBar_5H.gif";
	TitleMenuImagesN[6]= new Image(); TitleMenuImagesN[6].src = "/assets/images/SG/SG_NavBar_6N.gif";
	TitleMenuImagesH[6]= new Image(); TitleMenuImagesH[6].src = "/assets/images/SG/SG_NavBar_6H.gif";


		TellMeMore[0]=new Image();
		TellMeMore[0].src="/assets/images/SG/TellMeMore.gif";
		TellMeMore[1]=new Image();
		TellMeMore[1].src="/assets/images/SG/TellMeMoreH.gif";
   }
}

function SetTellMeMore(id,Highlighted) 
{
   if(Supported) 
   {
      if(Highlighted)
      {
      	   if(TellMeMore[1]!=null) document.images[id].src = TellMeMore[1].src;
      }
      else
      {
      	  if(TellMeMore[0]!=null) document. images[id].src = TellMeMore[0].src;
      }
   }
}


function SetMenuImg(id,graphic,Highlighted) 
{
   if(Supported) 
   {
      if(Highlighted)
      {
      	   if(TitleMenuImagesH[graphic]!=null) document.images[id].src = TitleMenuImagesH[graphic].src;
      }
      else
      {
      	  if(TitleMenuImagesN[graphic]!=null) document. images[id].src = TitleMenuImagesN[graphic].src;
      }
   }
}
</script>
</head>
