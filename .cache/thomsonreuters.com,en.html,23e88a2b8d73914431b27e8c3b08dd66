	<script>
		$(function(){
			var modalId = "modal-" + 1474444781454;
			var $modalId = "#" + modalId;
			$($modalId).on('show.bs.modal', function() {
				$("div.modal").each(function(){
					if($(this).attr("id") != modalId){
						$(this).modal('hide');
					}
				});
			});
