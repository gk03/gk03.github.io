  <script>
function Validate()
{
    var emailRegEx = /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\.[A-Z]{2,4}$/i;
	//var illegalChars = ;
    if (document.cont_us.fullname.value == '') 
    {
        alert('Please enter your full name!');
		document.cont_us.fullname.focus();
        return false;
    }
	if (document.cont_us.email.value == '') 
    {
       alert('Please enter your email address!');
	   document.cont_us.email.focus();
       return false;
    }
	if (document.cont_us.email.value.search(emailRegEx) == -1) 
    {
       alert('Please enter a valid email address!');
       return false;
    }
		return true;
}
</script>
<script>
function validate_My_form()
{
if(document.getElementById('my_resume').value == '')
{
document.getElementById('myresume_error').innerHTML = 'Please select your resume and upload';
document.getElementById('my_resume').focus();
return false;
}
else if(document.getElementById('my_resume').value != '')
{
var file_value = document.getElementById('my_resume').value;
var checkfile = file_value.toLowerCase();
if (!checkfile.match(/(\.doc|\.DOC)$/))
{
document.getElementById('myresume_error').innerHTML = 'Only doc files are allowed';
document.getElementById('my_resume').focus();
return false;
}
}
document.myForm.submit();
}
</script>

