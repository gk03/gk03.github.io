<!-- matchHeight -->
<script src="http://itconsortium.net/wp-content/themes/7/js/jquery.matchHeight-min.js" type="text/javascript"></script>
<script type="text/javascript">
    /* This matches the height of the elements that are inline with eachother */
    jQuery(function($) {
    	$('#mainNav ul#menu-main-navigation-menu>li>ul.sub-menu>li').matchHeight();
    	$('#mainNav ul#menu-main-navigation-menu>li>ul.sub-menu>li').each( function(){
    		$(this).find('a').eq(0).matchHeight( false );
    	});
    	$('#mainNav ul#menu-main-navigation-menu>li>ul.sub-menu>li>ul.sub-menu').matchHeight();
        $('.homeboxes .homebox').matchHeight();
        $('#preFooter .boxy').matchHeight();
    });
</script>
<script type="text/javascript">
	$('ul#menu-main-navigation-menu>li').on('click', function(e){
		var $this = $(this);
		var target = e.target;
		$('ul#menu-main-navigation-menu>li>ul.sub-menu').hide();
		var $menu = $this.find('ul.sub-menu');
		$menu.show();
		$menu.css({
			left: $this.position().left
		});
	});
	$('body').on('click', function(e){
		var target = e.target;
		var found = false;
        $(target).parents().each(function(){
            if($(this).hasClass('menu-main-navigation-menu-container')){
                found = true;
            }
        });
        if( !found ){
        	$('ul#menu-main-navigation-menu>li>ul.sub-menu').hide();
        }
	});
</script>
<!-- mobile nav -->
<script src="http://itconsortium.net/wp-content/themes/7/js/mobileNav.js"></script>
<!-- nav bottom icons hover 
<script type="text/javascript">
    jQuery(function($){
        /* This will add the navBottom to each top level nav item */
        var $navBottom  = $('.navBottom');
        $('#mainNav ul.menu > li').each(function(){
            $navBottom.clone().appendTo($(this));
        });
    	/* Click - Open - Menu */
    	var $opened = null;
    	$('#mainNav ul.menu > li a').on('click', function(e){
    		//e.preventDefault();
    		$opened = $(this).parent();
    		$('#mainNav ul.sub-menu').hide();
    		$opened.find('ul.sub-menu').show();
            //$navBottom.show();
            $('#mainNav ul li .navBottom').hide();
            if($opened.hasClass('contact')){
                return;    
            }
            posSub($opened);
            posBottom($opened);
    	});
        /* Click - Hide - Menu */
        $('body').on('click', function(e){
            var target = e.target;
            var found = false;
            $(target).parents().each(function(){
                if($(this).hasClass('menu-main-navigation-menu-container')){
                    found = true;
                }  
            }).promise().done(function(){
                if(!found){
                    $('#mainNav ul.sub-menu').hide();
                    $('#mainNav ul li .navBottom').hide();
                }
            });
        });
        /* This position the subNav lvl one items relatively, to be under the main/top level nav  */
        function posSub($this){
            //var $this = $(this);
            var $bottom = $this.find('.navBottom');
            $('#mainNav ul li .navBottom').hide();
            $bottom.show();
            var $rEl = $('#navBg');
            var relativeX = $this.offset().left - $rEl.offset().left;
            var $subNavOne = $this.find('ul.sub-menu').eq(0);
            if(($this.outerWidth()+relativeX) > $subNavOne.outerWidth()){
                var left = (relativeX-$subNavOne.outerWidth()+$this.outerWidth());
                $subNavOne.css('left', left+'px');
        		$bottom.css('left', left+'px');
            }
        }
        /* This positions the navBottom to fit inside the subNav level one */
        function posBottom($this){
            var navBottomOffset = $('#mainNav ul.sub-menu:visible').height();
            var $bottom = $this.find('.navBottom');
            var navBottomHeight = $bottom.outerHeight();
            var subWidth = $this.find('ul.sub-menu').eq(0).outerWidth();
            $bottom.css({
                'bottom': '-'+ ( navBottomOffset + navBottomHeight - 3 ) +'px',
                'right': '',
                'width': subWidth+'px'
            });
            if( subWidth < 450 && subWidth != null ){
                $bottom.find('img').css({
                    'width': '40%',
                    'height': 'auto',
                    'margin-left': '2%',
                    'margin-right': '2%'
                });
            } else if ( subWidth < 620 && subWidth != null ){
                $bottom.find('img').css({
                    'width': '20%',
                    'height': '',
                    'max-height': '42px',
                    'margin-left': '2%',
                    'margin-right': '2%'
                });
            }else{
                $bottom.find('img').css({
                    'width': '',
                    'height': '',
                    'max-height': '',
                    'margin-left': '',
                    'margin-right': ''
                });
            }
        }
        $('#mainNav ul.menu > li').on('mousemove', function(){
            if($opened === null){
                return false;
            }
            if($opened.hasClass('contact')){
                return false;    
            }
            posBottom($(this));
        });
    });
</script>
