			<script type="text/javascript">
		// Get Site Title
		var site = document.title; // Insert Site Title
		// Cookie Settings
		var expDays = 1;
		var exp = new Date();
		exp.setTime(exp.getTime() + (expDays*24*60*60*1000));

		function createCookie(name,value,days) {
			if (days) {
				var date = new Date();
				date.setTime(date.getTime()+(days*24*60*60*1000));
				var expires = "; expires="+date.toGMTString();
			} else var expires = "";
			document.cookie = name+"="+value+expires+"; path=/";
		}
		function readCookie(name) {
			var nameEQ = name + "=";
			var ca = document.cookie.split(';');
			for(var i=0;i < ca.length;i++) {
				var c = ca[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
			}
			return null;
		}
		function eraseCookie(name) {
			createCookie(name,"",-1);
		}
		function incCookie(name){
			var count = readCookie(name);
			if(count == null) {
				createCookie(name,1,exp)
			} else {
				var newcount = parseInt(count) + 1;
				eraseCookie(name)
				createCookie(name,newcount,exp)
			}
			console.log('cssErrors = ' + readCookie('CheckCSS'));
		}
		function errEmail() {
			// Send Email
			var email 		= new XMLHttpRequest(),
				URL			= document.URL;

			email.open('POST', 'http://www.nettango.com/wp-content/themes/nettango/blocks/cssCheck/cssEmail.php', true);
			email.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
			email.send("sendemail=true&site="+site+"&url="+URL);
			console.log('Email Sent');
		}
		// Rewrite Body
		function reBody(ver){
			var message = '';
			if (ver == 'refresh') {
				// Script Recommending Refreshing
				message = '<p>It appears as though some elements did not load properly.</p><p>Please try <a href="#" onClick="window.location.reload()" style="color:#4285f4; text-decoration:none !important;">refreshing</a> the page.</p>';
			} else {
				// Script telling the visitor we've been notified and are looking into it
				message = '<p>The site is currently unavailable. We are working to get it back online as soon as possible. We apologize for any inconvenience.</p>';   
			}
			
			document.body.style.margin = "0";
			document.body.style.backgroundColor = "#f2f2f2";
			document.body.innerHTML = '<div style="padding: 30px;margin: 60px auto;width: 90%;max-width: 600px;text-align: center;background-color: white;border: 1px solid #ccc;font-family: sans-serif; line-height:1.5;"><h1>Our Apologies</h1>'+message+'<h3>- '+site+' -</h3></div>';
		}

		function cssERROR() {
			try {
				// eraseCookie('CheckCSS');
				incCookie('CheckCSS');
				var cssErrors = readCookie('CheckCSS');

				if (cssErrors < 3) {
					reBody('refresh');
				} else if (cssErrors == 3) {
					reBody();
					errEmail();
				} else if (cssErrors > 3) {
					reBody();
				}

			} catch(err) {
				console.log('No Cookies');
				reBody();
			} finally {
			   console.log('cssERROR Script Executed');
			}
		}

		// Check for Maintenance Mode
		var maintMode = false;
		if(maintMode){ reBody(); }
		
		// Check for CSS file if no Maintenance Mode
		css = document.getElementById('main-css');
		if(css.sheet.cssRules.length == 0 && !maintMode ) {
			cssERROR();
		}

	</script>
