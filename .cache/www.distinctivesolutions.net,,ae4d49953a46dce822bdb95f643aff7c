          jQuery('#contentflexslider1666').flexslider({
              animationLoop: true,
              animation: "slide",
              slideshowSpeed: 3000,
              itemWidth: 130,
              controlNav: false,
              directionNav: true,
              maxItems: 5,
              move: 1,
              itemMargin: 10
          });

          jQuery(".flex-image a[rel^='prettyPhoto']").prettyPhoto({
              animation_speed: 'normal',
              theme: 'pp_default',
              slideshow: 3000,
              autoplay_slideshow: false,
              overlay_gallery: false /* If set to true, a gallery will overlay the fullscreen image on mouse over */
          });
          // ---------------------------------------
          // PHOTO
          // ---------------------------------------

          function base_photo_hover() {
              // Photo Hover
              jQuery('.image-zoom, .image-link').unbind().hoverIntent({
                  over: function () {
                      jQuery('a img', jQuery(this)).stop().animate({
                          opacity: 0.4
                      }, 150);
                  },
                  out: function () {
                      jQuery('a img', jQuery(this)).stop().animate({
                          opacity: 1
                      }, 150);
                  }
              });
          }
      });
</script>

 
