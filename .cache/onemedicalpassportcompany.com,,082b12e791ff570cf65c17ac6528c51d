		<script>
	
	
$(document).ready(function(){
	function IsNumeric(sText) {
        var ValidChars = "0123456789.";
        var IsNumber = true;
        var Char;
        for (i = 0; i < sText.length && IsNumber == true; i++) {
            Char = sText.charAt(i);
            if ((i == 0) && (Char == "-")) // check first character for minus sign
                continue;
            if (ValidChars.indexOf(Char) == -1) {
                IsNumber = false;
            }
        }
        return IsNumber;
    }
	
function checkIt() {
		var id = $("#numOfSurg").attr('id');
		var value = $("#numOfSurg").val();
        if (id == 'numOfSurg') {
            if (value=='' || IsNumeric(value) == false)
            {
                alert('Please enter a numeric value!');
                $("#numOfSurg").focus();
                $("#numOfSurg").select();
                return false;
            }
        }
        return true;
    }
    function calculate() {
        var numOfSurg = document.getElementById('numOfSurg').value;
        var perPatAllScr = (100 - document.getElementById('percOfPatScr').options[document.getElementById('percOfPatScr').selectedIndex].value) / 100;
        var perPatPhScr = document.getElementById('percOfPatPhScr').options[document.getElementById('percOfPatPhScr').selectedIndex].value / 100;
        var percPatFacScr = document.getElementById('percOfPatFacScr').options[document.getElementById('percOfPatFacScr').selectedIndex].value / 100;
        var numFTE = document.getElementById('numFTE').options[document.getElementById('numFTE').selectedIndex].value;
        var wageFTE = document.getElementById('wageFTE').options[document.getElementById('wageFTE').selectedIndex].value;
        var lenPreopCall = document.getElementById('lenPreopCall').options[document.getElementById('lenPreopCall').selectedIndex].value / 60;
        var lenPreopVisit = document.getElementById('lenPreopVisit').options[document.getElementById('lenPreopVisit').selectedIndex].value / 60;
       var calc1_ans = (numOfSurg * perPatAllScr * (perPatPhScr * lenPreopCall * wageFTE)) + (numOfSurg * perPatAllScr * (percPatFacScr * lenPreopVisit * wageFTE) * 0.5) - (2 * numOfSurg);
	   document.getElementById('result').style.display = "block";
		//var calc1_ans = (numOfSurg * (perPatPhScr * lenPreopCall * wageFTE)) + (numOfSurg * (percPatFacScr * lenPreopVisit * wageFTE) * 0.5) - (2 * numOfSurg);
		
        var calc2_ans = (numFTE * wageFTE * .5 * 2000) - (2 * numOfSurg);
              
        document.getElementById('calc1').innerHTML = '$' + CommaFormatted(calc1_ans);        
        document.getElementById('calc2').innerHTML = '$' + CommaFormatted(calc2_ans);
        //document.getElementById('calcs').style.display = '';
    }
    function CommaFormatted(amount)   
    {   
        var delimiter = ",";   
        var i = parseInt(amount);   
        if(isNaN(i)) { return ''; }   
        var minus = '';   
        if(i < 0) { minus = '-'; }   
        i = Math.abs(i);   
        var n = new String(i);   
        var a = [];   
        while(n.length > 3)   
        {   
            var nn = n.substr(n.length-3);   
            a.unshift(nn);   
            n = n.substr(0,n.length-3);   
        }   
        if(n.length > 0)   
        {   
            a.unshift(n);   
        }   
        n = a.join(delimiter);   
        amount = n;   
        amount = minus + amount;   
        return amount;   
    }
    function printWindow() {
        var numOfSurg = document.getElementById('numOfSurg').value;
        var perPatAllScr = document.getElementById('percOfPatScr').options[document.getElementById('percOfPatScr').selectedIndex].value;
        var perPatPhScr = document.getElementById('percOfPatPhScr').options[document.getElementById('percOfPatPhScr').selectedIndex].value;
        var percPatFacScr = document.getElementById('percOfPatFacScr').options[document.getElementById('percOfPatFacScr').selectedIndex].value;
        var numFTE = document.getElementById('numFTE').options[document.getElementById('numFTE').selectedIndex].value;
        var wageFTE = document.getElementById('wageFTE').options[document.getElementById('wageFTE').selectedIndex].value;
        var lenPreopCall = document.getElementById('lenPreopCall').options[document.getElementById('lenPreopCall').selectedIndex].value;
        var lenPreopVisit = document.getElementById('lenPreopVisit').options[document.getElementById('lenPreopVisit').selectedIndex].value;
        var calc1 = document.getElementById('calc1').innerHTML;
        var calc2 = document.getElementById('calc2').innerHTML;
        //msgWindow = open('ROIReport.html?numOfSurg=' + numOfSurg + '&perPatAllScr=' + perPatAllScr + '&perPatPhScr=' + perPatPhScr + '&percPatFacScr=' + percPatFacScr + '&numFTE=' + numFTE + '&wageFTE=' + wageFTE + '&lenPreopCall=' + lenPreopCall + '&lenPreopVisit=' + lenPreopVisit + '&calc1='+ calc1 +'&calc2='+ calc2, 'Report', 'scrollbars=yes,resizable=yes,status=no,toolbar=no,location=no,width=750,height=600');
		msgWindow = open('roi-print?numOfSurg=' + numOfSurg + '&percOfPatScr='+perPatAllScr+'&perPatPhScr=' + perPatPhScr + '&percPatFacScr=' + percPatFacScr + '&numFTE=' + numFTE + '&wageFTE=' + wageFTE + '&lenPreopCall=' + lenPreopCall + '&lenPreopVisit=' + lenPreopVisit + '&calc1='+ calc1 +'&calc2='+ calc2, 'Report', 'scrollbars=yes,resizable=yes,status=no,toolbar=no,location=no,width=750,height=600');
        if (msgWindow.opener == null) msgWindow.opener = self;
	}
        $(".btn-annual, .btn-savings").bind('click', function(){
		if(checkIt()) {calculate();}
         })
		 $(".print-me, .print").bind('click', function(){
			 printWindow();
			 })
