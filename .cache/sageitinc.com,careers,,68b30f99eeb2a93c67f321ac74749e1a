                required: true,

                email: true

            },

			message: {

				required: true,

				minlength:50

			},

		},

		

            messages: {

				 customer_name: {

					 required: "Please enter your name",

					 minlength: "Your name must consist of at least 2 characters"

				 },

				email: {

					email: "Please enter a valid email address"

				},

				message: {					 

					 required: "Please enter your message",

					 minlength: "your message must consist of at least 50 characters"

				},

				

            

			},

        

       

	   

	    submitHandler: function (form) {

			

			 	$.ajax({  



    				type: "POST",  



      			  	url: "http://sageitinc.com/process_email.php",  



      			  	data: $( "form" ).serialize(),



	      			success: function(result,status) { 



	      				if(result == "1"){



					$('.status_success').text("Message Sent").fadeIn(2500);



	      			$('.status_success').text("Message Sent").fadeOut(2500);							 



							change_captcha_form('captcha');							



						$("#customer_name").val("");	



						

						$("#email").val("");



						$("#message").val("");						



						$("#word").val("");



	      				} else if(result == 'false')



						{



							$('.status').text("Captcha Wrong").fadeIn(2500);



							$('.status').text("Captcha Wrong").fadeOut(2500);



							



									change_captcha_form('captcha');



									



						$("#word").val("");



						}



						else if(result == "0")



						{



	      					$('.status').text("Message Failed to Send");



							



	      				}



					



	      			}, 



	      			error: function(result,status){



	      				$('.status').text("Message Failed to Send");



	      			}  



				});	



      			



	  

			}

		

		

		

		

		

		

		

		

    });



});

        



	

    </script>

