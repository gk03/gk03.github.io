    <script type="text/javascript">
        var timeout = 500;
        var closetimer = 0;
        var ddmenuitem = 0;

        function jsddm_open() {
            jsddm_canceltimer();
            jsddm_close();
            ddmenuitem = $(this).find('ul').css('visibility', 'visible');
        }

        function jsddm_close()
        { if (ddmenuitem) ddmenuitem.css('visibility', 'hidden'); }

        function jsddm_timer()
        { closetimer = window.setTimeout(jsddm_close, timeout); }

        function jsddm_canceltimer() {
            if (closetimer) {
                window.clearTimeout(closetimer);
                closetimer = null;
            }
        }

        $(document).ready(function () {
            $('#homeMenu > li').bind('mouseover', jsddm_open)
            $('#homeMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#companyMenu > li').bind('mouseover', jsddm_open)
            $('#companyMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#practicesMenu > li').bind('mouseover', jsddm_open)
            $('#practicesMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#industriesMenu > li').bind('mouseover', jsddm_open)
            $('#industriesMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#clientsMenu > li').bind('mouseover', jsddm_open)
            $('#clientsMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#newsMenu > li').bind('mouseover', jsddm_open)
            $('#newsMenu > li').bind('mouseout', jsddm_timer)
        });

        $(document).ready(function () {
            $('#careersMenu > li').bind('mouseover', jsddm_open)
            $('#careersMenu > li').bind('mouseout', jsddm_timer)
        });

        document.onclick = jsddm_close;
</script>

