						<script>

							window.__scrollLoad = false;
							$(document).ready(function() {
								var transitionEndPos = 300; // how far documentwn before the header becomes a floating header
								var hashPadding = 160; // how far the padding is on a hash
								$(window).scroll(function() {
									if (window.__scrollLoad == false) {
										window.__scrollLoad = true;
										if (location.hash.length) {
											var scrolly = $(document).scrollTop() - hashPadding;
											if (scrolly > 0){
												window.scrollTo(0, scrolly);
											}

										}
									}
									var scrollTop = $(document).scrollTop();
									if (scrollTop >= transitionEndPos) {
										$(".theFloater").removeClass("limbo");
										$(".theFloater").addClass("active");
										} else if (scrollTop <= transitionEndPos && scrollTop > 140) {
										$(".theFloater").addClass("limbo");
										} else {
										$(".theFloater").removeClass("limbo");
										$(".theFloater").removeClass("active");
									}

								});
							});

							$('html').on('touchstart mouseup', function(e) {
								
								//close login
								var containerLogin = $(".loginBox");
								if (!containerLogin.is(e.target) && containerLogin.has(e.target).length === 0){
									containerLogin.removeClass("active");
									$("#loginContainer").hide(100);
								} 

								// close search
								var containerSearch =  $('.searchWrapper');
								if (!containerSearch.is(e.target) && containerSearch.has(e.target).length === 0){
									$('.siteSearchBox').removeClass('open');
									$(".searchWrapper").removeClass('active');
									$('.siteSearchDropDown').slideUp('fast');
								} 

							})	
						</script>

						
					</body>
				</html>
