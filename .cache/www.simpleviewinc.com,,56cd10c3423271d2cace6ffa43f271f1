											$(".overlayLinkHeader").on("click", function() {
												if($(".loginBox").hasClass("active")){
													$("#loginContainer").hide(100);
													$(".loginBox").removeClass("active");
													return false;
												}
												else{
													$("#loginContainer").show(100, function(){
														$(".loginBox").addClass("active");
													});
													return false;
												}
