<SCRIPT LANGUAGE="JavaScript">

function Is() {
   var agent = navigator.userAgent.toLowerCase() ;
	this.major = parseInt(navigator.appVersion) ;
	this.minor = parseFloat(navigator.appVersion) ;
	this.ns = ((agent.indexOf('mazilla')!=-1) &&
	          ((agent.indexOf('spoofer')== -1) &&
				  (agent.indexOf('compatible') == -1))) ;
	this.ns2 = (this.ns && (this.major == 2)) ;
	this.ns3 = (this.ns && (this.major == 3)) ;
	this.ns4 = (this.ns && (this.major >= 4)) ;
	this.ie  = (agent.indexOf("msie") != -1) ;
	this.ie3 = (this.ie && (this.major == 2)) ;
	this.ie4 = (this.ie && (this.major >= 4)) ;
	this.op3 = (agent.indexOf("opera") != -1) ;
}

var is = new Is() 

function AccessWeb() {
setTimeout("SelectWeb()",6000)  ;
}

function SelectWeb() {
if (screen.height >= 768)
   {
	   if (is.ie4)
		{
		   if (confirm('This site uses Microsoft Agent Technology for an interactive interface. \n\r\n\rWould you like to enable this feature?') )
  			{
  		      if (confirm('The Microsoft Agent Requires software from Microsoft, \n\rif it is not already present, to be loaded on your computer.  \n\r\n\rDo you wish to install the software now if necessary?') )
				{
               window.location = "hires/default.htm" ;
				}
				else	
				{
               window.location = "hires/noagent.htm" ;
				}
			}
			else
			{
            window.location = "hires/noagent.htm" ;
			}	
		}
		else if (is.ie3)
		{
         window.location = "hires/noagent.htm" ;
		}		
		else
		{
         window.location = "hires/netscape.htm" ;
		}		
   }
   else
   {
	   if (is.ie4)
		{
		   if (confirm('This site uses Microsoft Agent Technology for an interactive interface. \n\r\n\rWould you like to enable this feature?') )
  			{
  		      if (confirm('The Microsoft Agent Requires software from Microsoft, \n\rif it is not already present, to be loaded on your computer.  \n\r\n\rDo you wish to install the software now if necessary?') )
				{
               window.location = "lores/default.htm" ;
				}
				else	
				{
               window.location = "lores/noagent.htm" ;
				}
			}
			else
			{
            window.location = "lores/noagent.htm" ;
			}	
		}
		else if (is.ie3)
		{
         window.location = "lores/noagent.htm" ;
		}		
		else
		{
         window.location = "lores/netscape.htm" ;
		}		
   }
}
