jQuery(document).ready(function(){

    jQuery('.contact-pullout a.pull-left').click(function(){
		if(jQuery('.contact-pullout').hasClass('pulled')){
			jQuery('.contact-pullout').animate({right: "-=320"}, 800);
			jQuery('.contact-pullout').removeClass('pulled');
		}else{
			jQuery('.contact-pullout').animate({right: "+=320"}, 800);
			jQuery('.contact-pullout').addClass('pulled');
		}
	})
});
