<script type="text/javascript">
$(document).ready(function(){
	
	$(".slidingDiv").hide();
	
	$(".show_hide").click(function(){
	
		$(this).next(".slidingDiv").slideToggle("slow",function() {
			
			if ($(this).is(':hidden')) {
					$(this).parent("li").css("listStyleImage", "url('/sites/all/themes/isf/images/icon_plus.gif')");
				} else {
					$(this).parent("li").css("listStyleImage", "url('/sites/all/themes/isf/images/icon_subtract.gif')");
				}
		});
	});

});
</script>
   
