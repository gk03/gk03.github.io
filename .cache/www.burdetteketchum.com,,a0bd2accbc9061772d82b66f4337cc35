    <script type="text/javascript">
	// FORM VALIDATION FUNCTIONS
	function IsValidEmail(email) {
		var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
		return filter.test(email);
	}

	$(function() {

		$("input[name='Email']").change(function() {
			var email = $("input[name='Email']").val();
			if (email == "" || (!IsValidEmail(email))) {
				$("input[name='Email']").addClass("error");
			}
			else
				$("input[name='Email']").removeClass("error");
		});

		$("#submit").click(function() {

			var email = $("input[name='Email']").val();

			if(email == '' || (!IsValidEmail(email))) {
				if (email == "" || (!IsValidEmail(email))) {
					$("input[name='Email']").addClass("error");
				}
				else
					$("input[name='Email']").removeClass("error");

				return false;
			}

			var name = $("input[name='FullName']").val();
			var company = $("input[name='Company']").val();
			var phone = $("input[name='Phone']").val();
			var comments = $("textarea[name='Comments']").val();

			var dataString = 'Email=' + email + '&FullName='+ name + '&Company=' + company + '&Phone=' + phone  + '&Comments=' + comments;

			$.ajax({

				type: "POST",
				url: "/wp-content/themes/gamechangers/scripts/collect.php",
				data: dataString,
				success: function(data) {
					$('#form_confirm').html(data);

				},
				error: function(data) {
					var error_msg = '<p class="error">Sorry, your message could not be submitted. Please refresh your page and try again or email us at <a href="mailto:info@burdetteketchum.com">info@burdetteketchum.com</a>.</p>';
					$('#form_confirm').html(error_msg);
				}
			});

			return false;

		});
	});
	</script>

    <script type="text/javascript">
	// BACKGROUND IMAGE RESIZE
	function resize_bg(){
	   $("#hero").css("left","0");
	   var doc_width = $(window).width();
	   var doc_height = $(window).height();
	   var image_width = $("#hero").width();
	   var image_height = $("#hero").height();
	   var image_ratio = image_width/image_height;
	   var new_width = doc_width;
	   var new_height = Math.round(new_width/image_ratio);
	   if(new_height<doc_height){
			new_height = doc_height;
			new_width = Math.round(new_height*image_ratio);
			var width_offset = Math.round((new_width-doc_width)/2);
			$("#hero").css("left","-"+width_offset+"px");
	   }
	   $("#hero").width(new_width);
	   $("#hero").height(new_height);
	   $("#hero").css('display', 'block');
	}
	</script>

