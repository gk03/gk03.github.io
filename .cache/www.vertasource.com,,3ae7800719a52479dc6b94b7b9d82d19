			var validateForm = new FormValidator.Inline('getQuote', {
				evaluateFieldsOnBlur: false,
				evaluateFieldsOnChange: false,
				onFormValidate: function(passed, form, event) {
					if (passed) {
						event.stop();
						form.getElement('.statevalue').set('value', '10800');

						form.set('send', {
							onComplete: function(response) {
								var jsonResp = JSON.decode(response);
								if (jsonResp.error) {
									var errorP = new Element("p", {
										"id": "error",
										"html": jsonResp.error[0].Message
									});

									var quoteFormContent = $('quoteFormContent');

									quoteFormContent.set('morph', {
										duration: 500,
										transition: 'linear'
									});

									quoteFormContent.morph({ height: 60 });

									quoteFormContent.empty();

									var func = function() { errorP.replaces($('quoteFormContent')); };
									func.delay(1000);
								}
							}
						});
						form.send();
					}
				}
			});
		});
	</script>

	
