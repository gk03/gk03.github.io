<script>
   $(document).ready( function () {
       var imageSize = NI.utils.browserSize();
       if (imageSize === "xs") imageSize = "sm";
       
       $.each($("span.section-media"), updateImageSource);
       updateBGImageSource();
       /**
       * updates the padth needed for the release?
       * @param none
       * @returns true
       */
       function updateImageSource(){
           var source = $(this).data(imageSize);
           $("img", this).attr("src", source);
           return true;
       }
   
       
       function updateBGImageSource(){
           var fileSrc =$(".section-media-background-partial").data("src");
           if (imageSize=="lg"|| imageSize=="md"){
        filepath= "background: url(http://s7d5.scene7.com/is/image/ni/feature-shop?$layer_1_src="+fileSrc+"&wid=1176) no-repeat center;"
           }else if(imageSize=="sm"){
            var windowsize = $(window).width();
               if(windowsize >= 768 && windowsize <= 991){
                   filepath= "background: url(http://s7d5.scene7.com/is/image/ni/feature-shop?$layer_1_src="+fileSrc+"&wid=1176) no-repeat center;"
               }else{
                   filepath="background: url(http://s7d5.scene7.com/is/image/ni/feature-shop_1?$layer_1_src="+fileSrc+"&wid=671) no-repeat center;"
               }
           }else {filepath = "background: url(http://s7d5.scene7.com/is/image/ni/"+fileSrc+"?hei=162&wid=320) no-repeat center;"}
           
           $(".section-media-background-partial").attr("style", filepath);
           
           return true;
       }
   }); 
   
</script>

