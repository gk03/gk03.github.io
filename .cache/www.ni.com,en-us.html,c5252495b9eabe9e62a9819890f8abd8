<script>

  $(document).ready( function () {

    var imageSize = NI.utils.browserSize();
    if (imageSize === "xs") imageSize = "sm";

    $.each($(".section-media"), updateImageSource);
   	$.each($(".carousel-media"), updateCarouselImg);
    /**
     * updates the padth needed for the release?
     * @param none
     * @returns true
     */
    function updateImageSource(){
      var source = $(this).data(imageSize);
      $("img", this).attr("src", source);
      return true;
    }


    function updateCarouselImg(){
        var fileSrc =$(this).data("src");
        var filepath="";

  if (imageSize=="lg"){
         filepath= "background: url(http://s7d5.scene7.com/is/image/ni/"+fileSrc+"?wid=588&hei=396&fit=stretch) no-repeat center;"}
        else if(imageSize=="md"){
            filepath="background: url(http://s7d5.scene7.com/is/image/ni/"+fileSrc+"?wid=378&hei=295&fit=stretch) no-repeat center;"
        }else {filepath = "background: url(http://s7d5.scene7.com/is/image/ni/"+fileSrc+"?wid=320&hei=177&fit=stretch) no-repeat center;"}

      $(".section-media-carousel-background",this).attr("style", filepath);
      return true;
    }

  }); 

</script>
 
  
  
 
