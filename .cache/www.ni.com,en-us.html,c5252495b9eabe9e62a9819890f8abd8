        var NI = NI || {};
        NI.utils = NI.utils || {};
        NI.utils.browserSize = function() {

            var envs = ['xs', 'sm', 'md', 'lg'];

            var $el = $('<div>');

            $el.appendTo($('body'));

            for (var i = envs.length - 1; i >= 0; i--) {
                var env = envs[i];

                $el.addClass('hidden-'+env);
                if ($el.is(':hidden')) {
                    $el.remove();
                    return env;
                }
            }

        }
    </script>

	
	
