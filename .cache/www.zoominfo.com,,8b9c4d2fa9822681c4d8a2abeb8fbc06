		url: "http://subscriber.zoominfo.com/bizservices/analysis/leadscore?emailAddress=" + email,
		method: "GET",
		data: {
			emailAddress: email
		},
		dataType: "json"
	}).done(function( data ) {
		
		var score = -1; // error by default
		score = data.value.score;

		document.getElementById("score").value = score;


		if( data && data.resultCode < 0 ) {

			score = data.resultCode;
		
		} 
		else if(score == "D") {
						returnUrlElem.val( "http://www.zoominfo.com/business/free-trial-additional?email=" + email + "&camp_id=" + document.getElementById("camp_id").value);
		}
		else {

			//score = data.value.score;
		
			returnUrlElem.val( "http://www.zoominfo.com/business/zoominfo-free-trial-thanks" );
			
		}

	 	//returnUrlElem.val( "http://www.zoominfo.com/business/free-trial" );
		form.submit();
	
		
		
	}).fail(function( jqXHR, textStatus ) {

		returnUrlElem.val( "http://www.zoominfo.com/business/free-trial" );
		form.submit();

	});
	
	return false;
});
</script>
