    <script>
        $(document).ready(function() {
            // hide #back-top first
            $("#back-top").hide();

            // fade in #back-top
            $(function() {
                $(window).scroll(function() {
                    if ($(this).scrollTop() > 100) {
                        $('#back-top').fadeIn();
                    } else {
                        $('#back-top').fadeOut();
                    }
                });

                // scroll body to 0px on click
                $('#back-top a').click(function() {
                    $('body,html').animate({
                        scrollTop: 0
                    }, 800);
                    return false;
                });
            });

        });
    </script>

    <script type="text/javascript">
        $(document).ready(function() {
            var pageTitle = document.title; //HTML page title
            var pageUrl = location.href; //Location of the page

            //user hovers on the share button  
            $('#share-wrapper li').hover(function() {
                var hoverEl = $(this); //get element

                //browsers with width > 699 get button slide effect
                if ($(window).width() > 699) {
                    if (hoverEl.hasClass('visible')) {
                        hoverEl.animate({ "margin-left": "-148px" }, "fast").removeClass('visible');
                    } else {
                        hoverEl.animate({ "margin-left": "0px" }, "fast").addClass('visible');
                    }
                }
            });

            //user clicks on a share button
            $('.button-wrap').click(function(event) {
                var shareName = $(this).attr('class').split(' ')[0]; //get the first class name of clicked element

                switch (shareName) //switch to different links based on different social name
                {
                    case 'facebook':
                        var openLink = 'https://www.facebook.com/BullseyeEvaluationSoftware';
                        break;
                    case 'twitter':
                        var openLink = 'https://twitter.com/bullseyepm';
                        break;
                    case 'linkedin':
                        var openLink = 'http://www.linkedin.com/companies/bullseyeevaluation';
                        break;


                    case 'google':
                        var openLink = 'https://plus.google.com/+GraceAtkinsbullseyeengagement/posts';
                        break;

                    case 'youtubeleft':
                        var openLink = 'https://www.youtube.com/bullseyeperformance';
                        break;

                    case 'email':
                        var openLink = 'mailto:mgr@bepms.com?subject=' + pageTitle + '&body=Found this useful link for you : ' + pageUrl;
                        window.location.href = openLink;
                        return false;
                        break;
                }

                //Parameters for the Popup window
                winWidth = 650;
                winHeight = 450;
                winLeft = ($(window).width() - winWidth) / 2,
                winTop = ($(window).height() - winHeight) / 2,
                winOptions = 'width=' + winWidth + ',height=' + winHeight + ',top=' + winTop + ',left=' + winLeft;

                //open Popup window and redirect user to share website.
                window.open(openLink, 'Share This Link', winOptions);
                return false;
            });
        });
    </script>
     <script type="text/javascript">
         function validateSId(txt, e) {

             if (e.keyCode == 13) {
                 if (txt.val() == "") {
                     alert("Please enter some text for search...");
                     e.preventDefault();
                     return;
                 }
                 var str = txt.val();
                 var address = '/search.aspx';
                window.location = address + "?search=" + str;
                e.preventDefault();
            }
        }
    </script>
    <script type="text/javascript">
        $(document).ready(function ()
        {
            var session = 'en-US';
            if (session == "fr-FR") {
                $(".footer-email").append('<a href="mailto:befrance@bepms.com">mgr@bepms.com</a>');
                $(".head-email").append('<a href="mailto:befrance@bepms.com">mgr@bepms.com</a>');
                $("#liIQ").hide();
                $("#liABC").hide();
            }
            else if (session == "es-ES")
            {
                $(".footer-email").append('<a href="mailto:bemexico@bepms.com">mgr@bepms.com</a>');
                $(".head-email").append('<a href="mailto:bemexico@bepms.com">mgr@bepms.com</a>');

            }
            else {
                $(".head-email").append('<a href="mailto:mgr@bepms.com">mgr@bepms.com</a>');
                $(".footer-email").append('<a href="mailto:mgr@bepms.com">mgr@bepms.com</a>');
            }
        });
    </script>
	
	
