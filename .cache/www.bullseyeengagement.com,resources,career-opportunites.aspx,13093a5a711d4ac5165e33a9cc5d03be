        function Restrict(event) {
            if (document.all)
                return ((event.keyCode >= 48 && event.keyCode <= 57) || (event.keyCode == 0 || event.keyCode == 8 || event.keyCode == 13 || event.keyCode == 9))
            else
                return ((event.which >= 48 && event.which <= 57) || (event.which == 0 || event.which == 8 || event.which == 13 || event.which == 9))
        }
        function RestrictPrice(event) {
            if (document.all)
                return ((event.keyCode >= 48 && event.keyCode <= 57) || (event.keyCode == 0 || event.keyCode == 8 || event.keyCode == 13 || event.keyCode == 9 || event.keyCode == 46))
            else
                return ((event.which >= 48 && event.which <= 57) || (event.which == 0 || event.which == 8 || event.which == 13 || event.which == 9 || event.which == 46))
        }   
    </script>

