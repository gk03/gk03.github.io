  <script type="text/javascript">
function getCookie(name) { 
	var re = new RegExp(name + "=([^;]+)"); 
	var value = re.exec(document.cookie); 
	return (value != null) ? unescape(value[1]) : null; 
	}
 
var $j = jQuery.noConflict();
	$j(document).ready(function() {
		
		var date = new Date();
		date.setTime(date.getTime() + (1 * 24 * 60 * 60 * 1000));
		expires = "; expires=" + date.toGMTString();
		
		var isCookie = getCookie('offer');
		
		if(isCookie == 1)
		{ 
			document.getElementById('offerdisplay').style.bottom = "-550px";
			$j(".show-btn").show();
			/*document.getElementById('offerdisplay').style.right = "20px";
			document.getElementById('offerdisplay').style.bottom = "0px"; */
		}
		else
		{
			 $j(".show-btn").show();
			//document.getElementById('offerdisplay').style.right = "20px";
			//document.getElementById('offerdisplay').style.bottom = "0px";
			document.getElementById('offerdisplay').style.bottom = "-550px";
		}
		
		/*alert(leftposition);*/
		
			
		$j(".close-btn").click(function(){
			$j(".inquiy_popup").animate({
				bottom:"-550px",
			
			},700);
			
			name = "offer = 1";
			document.cookie = name + expires + "; path=/";
			//document.getElementById('offerdisplay').style.bottom = "-300px";
			/*
			
			document.getElementById('offerdisplay').style.left = "-750px";
			alert('hello');*/
			setTimeout(function(){$j(".show-btn").show();}, 500);

		});
		$j(".show-btn").click(function(){
			$j(".show-btn").hide();
			$j(".inquiy_popup").animate({
				left:"0px",bottom:"0",
			},700);
			
			name = "offer = 0";
			document.cookie = name + expires + "; path=/";
			//document.getElementById('offerdisplay').style.bottom = "0px";

		});
		
	});
</script>
