          jQuery.noConflict();
		  jQuery(document).ready(function() {
		  		jQuery('#filter-all').on('click', function() {
					var selector = jQuery(this).attr('data-filter');
					jQuery('.bef-select-as-links a').removeClass('active');
					jQuery(this).addClass('active');
					jQuery.container.isotope({
						filter: selector
					});
					return false;
			 });
			    setTimeout(function() {
        			jQuery("#filter-all").trigger('click');
    		    },5);
				
                jQuery(".menufication-menu-level-1 li span:nth-child(2)" ).hide();
                
                jQuery(".custome2 span:nth-child(2)").hide();
                jQuery(".custome3 span:nth-child(2)").hide();
                jQuery(".custome4 span:nth-child(2)").hide();
                
                jQuery(".menufication-menu-level-1 li.hasChildren" ).click(function(){
					
                });
				
				
				setTimeout(function() {
        			jQuery(".menufication-menu-level-1" ).each(function( index ) {
						
						jQuery.parent_li = jQuery(this).parent();
						jQuery.prev_atag = jQuery(this).prev();
						var tag_html = jQuery.prev_atag.html();
						var tag_text = jQuery.prev_atag.text();
						tag_html = tag_html.trim();
						tag_text = tag_text.trim();
					});
    		    },20);
				
				if(jQuery("h1#page-title").html() != null){
				
					var blog = jQuery("h1#page-title").html().trim();
					if(blog === "Blog")
					{
					  //jQuery("h1#page-title").next().andSelf().wrapAll('<div class="float_L" />');
					   jQuery(".portfolio_page_content .pagecontentContent div h1").addClass('blog_L');
					 }
				} 
				// Resize page banner titles
    			if (jQuery(".pagewrap.innerbanner.portfolio_page_content  .pagecontentContent").length > 0) {
    			}
    			function bannerHeaderResize() {
				   	var leftPush = 0;
        			jQuery(".pagewrap.innerbanner.portfolio_page_content .pagecontentContent").width(jQuery(".content:last").offset().left + jQuery(".pagewrap.innerbanner.portfolio_page_content .pagecontentContent div h1").width() + leftPush);
    			}
				if(jQuery("nav#menufication-nav ul li:nth-child(3)").children('a.portfolio').html() != null)
				{
				  jQuery("nav#menufication-nav ul li:nth-child(3)").children('a.portfolio').parent().removeClass( "menufication-has-child-menu hasChildren" );
				 
				  jQuery("nav#menufication-nav ul li:nth-child(3)").children('a.portfolio').parent().click(function(){
				   jQuery(this).removeClass( "menufication-child-menu-open" );
				   jQuery(this).children('ul').removeClass( "menufication-child-menu" );
				 });
				  	
				}
				
				if(jQuery("nav#menufication-nav ul li:nth-child(4)").children('a.blog').html() != null)
				{
				  jQuery("nav#menufication-nav ul li:nth-child(4)").children('a.blog').parent().removeClass( "menufication-has-child-menu hasChildren" );
				  
				  jQuery("nav#menufication-nav ul li:nth-child(4)").children('a.blog').parent().click(function(){
				  
				     jQuery(this).removeClass( "menufication-child-menu-open" );
				     jQuery(this).children('ul').removeClass( "menufication-child-menu" );
				  });		
				}
				
				if(jQuery("nav#menufication-nav ul li:nth-child(5)").children('a.contact').html() != null)
				{
				  jQuery("nav#menufication-nav ul li:nth-child(5)").children('a.contact').parent().removeClass( "menufication-has-child-menu hasChildren" );
				  
				  jQuery("nav#menufication-nav ul li:nth-child(5)").children('a.contact').parent().click(function(){
				   jQuery(this).removeClass( "menufication-child-menu-open" );
				   jQuery(this).children('ul').removeClass( "menufication-child-menu" );
				 });			
				}
				
				
		  });
