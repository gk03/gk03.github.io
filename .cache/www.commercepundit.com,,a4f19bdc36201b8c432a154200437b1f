<script type="text/javascript">
function validate_form(thisform){
     with (thisform){
		if (validate_required("FirstName_Step2","vld_FirstName_Step2","First Name must be filled out!")==false)
		{
			document.getElementById("FirstName_Step2").focus();return false;
		}
		if (validate_required("Email_Step2","vld_Email_Step2","Email must be filled out!")==false)
		{
			document.getElementById("Email_Step2").focus();return false;
		}
		if (validate_maxLength("Notes_Step2","vld_Notes_Step2","Notes maximum length should be 4095 characters!",4095)==false)
		{
			document.getElementById("Notes_Step2").focus();return false;
		}
		var v = grecaptcha.getResponse();
		if(v.length == 0)
		{
			document.getElementById('captcha1').innerHTML="You can't leave Captcha Code empty";
			return false;
		}
        document.getElementById('submit_web_form').disabled=true;
     }

}


$(document).ready(function(){
	$("#zohogetintouch").submit(function(){
		var name = $("#FirstName_Step2").val();
		var email = $("#Email_Step2").val();
		var company_step2 = $("#CompanyName_Step2").val();
		var notes_Step2 = $("#Notes_Step2").val();
		var v = grecaptcha.getResponse();
		// Returns successful data submission message when the entered information is stored in database.
		var dataString = 'name1='+ name + '&email1='+ email + '&company='+ company_step2 + '&notes='+ notes_Step2;
		if(name=='')
		{
			document.getElementById("FirstName_Step2").focus();
			alert("Please enter first name");
			return false;
		}else if(email=='')
		{
			document.getElementById("Email_Step2").focus();
			alert("Please enter email");
			return false;
		}else if(company_step2=='')
		{
			document.getElementById("CompanyName_Step2").focus();
			alert("Please enter company");
			return false;
		}else if(notes_Step2=='')
		{
			document.getElementById("Notes_Step2").focus();
			alert("Please enter description");
			return false;
		}else if(v.length == 0)
		{
			document.getElementById('captcha1').innerHTML="You can't leave Captcha Code empty";
			return false;
		}
		else
		{
			$("#getinloadimg").removeClass("term-class");
			// AJAX Code To Submit Form.
			$.ajax({
			type: "POST",
			url: "getintouch.php",
			data: dataString,
			cache: false,
			success: function(result)
			{
				if(result)
				{
					$("#divgetintouch").removeClass("term-class");
					$('#divgetintouch').delay(5000).fadeOut('slow');
					$("#getinloadimg").addClass("term-class");
					$('#zohogetintouch')[0].reset();
					$("#captcha1").hide();
					grecaptcha.reset();
					
				}	
			}
		});
	}
return false;
});
});
 

 


</script>
