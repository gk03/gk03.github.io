		<script type="text/javascript">
			$j(document).ready(function(){
				initNewsletterCta();
			});
		</script>
	
<script type="text/javascript">
	var newsletterCtaType = 'Modal';
	var publicationID = '44';
	
	var serializationKey = '20160908336787408';
	var channel = 'globalspec';
	var ctaIsInited = false;

	// create the js functions for dw cta tracking
	
			function assertSeenCtaToken() {
				if (document.images) { tokenImage = new Image(); tokenImage.src = 'http://www.globalspec.com/Tokens?token=cta-sub-seen:Modal&P1=&r=2807&srcWebHitId=336787408'; }
			}
			
			function assertOpenCtaToken() {
				if (document.images) { tokenImage = new Image(); tokenImage.src = 'http://www.globalspec.com/Tokens?token=cta-sub-open:Modal&P1=&r=2206&srcWebHitId=336787408'; }
			}
			
			function assertSubmitCtaToken() {
				if (document.images) { tokenImage = new Image(); tokenImage.src = 'http://www.globalspec.com/Tokens?token=cta-sub-submit:Modal&P1=&r=7940&srcWebHitId=336787408'; }
			}
			
			function assertRegCtaToken() {
				if (document.images) { tokenImage = new Image(); tokenImage.src = 'http://www.globalspec.com/Tokens?token=cta-sub-reg:Modal&P1=&r=3662&srcWebHitId=336787408'; }
			}
			

	function isCtaEnabled() {
		
			if (getCookie("subscriberid") || getCookie("gsuser")) { return false; }
			if ($j("html").hasClass("phone")) { return false; }
			
				if (getCookie("noModalCta") || getCookie("noWelcomeAdPrd")) { return false; }
			
		return true;
	}

	function initNewsletterCta() {
		ctaIsInited = true;

	var isCtaSeen = false;	// Flag when a cta is "seen" for omniture/dw cta tracking purposes
		if (!isCtaEnabled()) {
			// already signed up for something. abort.
			return;
		}

		$j('select#country').chosen({
			inherit_select_classes: true,
			width: "100%"
		});

		
			showCtaAsModal();
			isCtaSeen = true;
		

		if (isCtaSeen) {
			// assert the "seen" token for dw cta tracking
			assertSeenCtaToken();

			// record the "seen" event for omniture cta tracking
			
		}

		

		$j("#newsletter-cta .close").click(function(e) {
			closeCta();
		});

		$j('#newsletter-signup').validate();
	}

	function showCtaAsModal() {
		if (!ctaIsInited) {
			initNewsletterCta();
		}
	
	
		if (isCtaEnabled()) {
			setCookie("noModalCta", "1", 1, true);
			setCookie("noWelcomeAdPrd", "1", 1, true);
			showSimpleModal('#newsletter-cta');
			$j("#overlay").click(closeOverlayTracking);
		}
	
	}

	function closeOverlayTracking() {
		trackOmniClick(window.s_account, 'event47', {channel: s.channel, eVar1: s.channel, eVar5: s.pageName, list1: publicationID, eVar57: newsletterCtaType}, 'NL CTA Closed');
	}

	function closeCta() {
	
		hideSimpleModal();
	
	}

	function newsletterCrossPromotionSubmit(form) {
		$j("#newsletter-cta").css("height","auto");
		if ($j("#newsletter-cta form").valid()) {
			$j.ajax({
				url: "http://www.globalspec.com/ajax/newsletterCrossPromotionSignUp",
				dataType: "jsonp",
				data: $j(form).serialize() + "&trackEvar5=" + s.pageName + "&trackEvar56=" + publicationID + "&trackEvar57=" + newsletterCtaType + "&trackChannel=" +  encodeURIComponent(channel) + "&serializationKey=" + serializationKey,
				timeout: 20000,
				crossDomain: true,
				success: function(result) {
					
					if ((typeof result.NEWUSER !== 'undefined') && (result.NEWUSER)) {
					assertRegCtaToken();
					}
					if(result.SUCCESS) {
						assertSubmitCtaToken();
						$j("#close-cta-prompt").removeAttr("onclick");
						$j("#overlay").unbind("click");
			
						$j("#overlay").click(hideSimpleModal);
			
	                	showSubscriptionConfirmation();
	                	setCookie("noSliderAd", "1", 1, true);	
					} else {
						showErrorMessage();
					}
				},
				error: function() {
					showErrorMessage();
				}
			});
		}

		return false;
	}
</script>



<script type="text/javascript">
	function widgetTease() {
		var dividerTopMargin = 15;
		var widgetTopPadding = 15;

		var widget = $j("#newsletter-cta.Widget");
		widget.show();

		var widgetHeight = widget.height();
		var widgetLabelBar = $j("#newsletter-cta.Widget .label-bar");
		var widgetMinimizedHeight = widgetLabelBar.height();
		var widgetTableHeight = widget.find("table.call-to-action").height();

		var bottomValue = -widgetHeight + widgetMinimizedHeight + widgetTableHeight + dividerTopMargin + widgetTopPadding;
		widget.delay(2000).animate({bottom: bottomValue + "px"}, 500);

		showMinimizedWidget();
	}

	function setInitialWidgetPosition() {
		var widget = $j("#newsletter-cta.Widget");
		var widgetHeight = widget.height();

		widget.css("bottom", -widgetHeight);

		setRightValueWidget();

		widget.addClass("minimized");
		widget.hide();
	}

	function showMinimizedWidget() {
		var widget = $j("#newsletter-cta");
		widget.show();

		var widgetHeight = widget.height();
		var widgetLabelBar = $j("#newsletter-cta .label-bar");
		var widgetMinimizedHeight = widgetLabelBar.height();

		var bottomValue = -widgetHeight + widgetMinimizedHeight;

		widget.delay(2000).animate({bottom: bottomValue + "px"}, 1000);

		widget.removeClass("shown");
		widget.addClass("minimized");
	}

	function setRightValueWidget() {
		var windowWidth = $j(window).width();
		var pageWidth = $j("#page-wrapper").width();

		var rightValue = 0;

		if (windowWidth > pageWidth) {
			rightValue = ($j(window).width() - $j("#page-wrapper").width())/2;
		}

		var widget = $j("#newsletter-cta.Widget");
		widget.css("right", rightValue + "px");
	}

	function showWidget() {
		var widget = $j("#newsletter-cta.Widget");
		widget.show().stop(true, true);

		widget.animate({bottom: 0}, 500);

		widget.addClass("shown");
		widget.removeClass("minimized");
	}

	function onSliderAdRenderComplete(event) {
		
		if (event && !event.isEmpty) {
			// do nothing
		} else {
			// render!
			initNewsletterCta();
		}
	}

	function minimizeWidget() {
		var widget = $j("#newsletter-cta.Widget");
		var widgetHeight = widget.height();
		var widgetLabelBar = $j("#newsletter-cta.Widget .label-bar");
		var widgetMinimizedHeight = widgetLabelBar.height();

		var bottomValue = -widgetHeight + widgetMinimizedHeight;

		widget.stop(true, true).animate({bottom: bottomValue + "px"}, 500);

		widget.removeClass("shown");
		widget.addClass("minimized");
	}

	function minimizeOnpagetop() {
		var cta = $j("#newsletter-cta");
		cta.removeClass("shown");

		var ctaLabelBar = $j("#newsletter-cta .label-bar");
		var ctaMinimizedHeight = ctaLabelBar.height();

		cta.stop(true, false).attr("data-height", cta.height());
		cta.animate({height: ctaMinimizedHeight + "px"}, 500, function() {
			cta.css('overflow', 'hidden');
			cta.addClass("minimized");
		});
	}

	function showOnpagetop() {
		var cta = $j("#newsletter-cta");
		cta.removeClass("minimized");
		cta.addClass("shown");

		var ctaMaximizedHeight = $j(cta).attr("data-height");
		cta.stop(true, false).animate({height: ctaMaximizedHeight + "px"}, 500, function() {
			cta.css('overflow', 'visible');
		});
	}

	function onpagetopTease() {
		var cta = $j("#newsletter-cta.Onpagetop");
		cta.css('overflow', 'hidden');
		cta.delay(2000).animate({height: "170px"}, 500, function() {
			cta.delay(2000).animate({height: "40px"}, 500);
		});


		//var widgetHeight = widget.height();
		//var widgetLabelBar = $j("#newsletter-cta.Widget .label-bar");
		//var widgetMinimizedHeight = widgetLabelBar.height();
		//var widgetTableHeight = widget.find("table.call-to-action").height();

		//var bottomValue = -widgetHeight + widgetMinimizedHeight + widgetTableHeight + dividerTopMargin + widgetTopPadding;
		//widget.delay(2000).animate({bottom: bottomValue + "px"}, 500);

		//showMinimizedWidget();
	}

	function showSubscriptionConfirmation() {
		var switchDiv = $j("#newsletter-cta .switch-on-confirmation");
		var confirmDiv = $j("#newsletter-cta .switch-on-confirmation .slide-two");

		// match the size of the switch div (this may vary in height)
		confirmDiv.css("height", switchDiv.height()).css("width", switchDiv.width());
		confirmDiv.show();
	}

	function showErrorMessage() {
		var switchDiv = $j("#newsletter-cta .switch-on-confirmation");
		var confirmDiv = $j("#newsletter-cta .switch-on-confirmation .slide-three");

		// match the size of the switch div (this may vary in height)
		confirmDiv.css("height", switchDiv.height()).css("width", switchDiv.width());
		confirmDiv.show();
	}

	function canadaCheck() {
		$j("#country_chosen a.chosen-single span").addClass("changed");
		$j("#newsletter-cta").css("height", "auto");


		if (($j('#country').val() == "Canada") || ($j('#email-address').val().toLowerCase().substr(-3) == '.ca')){
			$j("#newsletter-cta").addClass("canada");
			$j("#newsletter-cta #cb_subConsent").attr("required", "");
		} else {
			$j("#newsletter-cta").removeClass("canada");
			$j("#newsletter-cta #cb_subConsent").removeAttr("required");
		}
	}
</script> 

