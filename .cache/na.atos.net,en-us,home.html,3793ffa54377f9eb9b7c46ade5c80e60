String.prototype.trunc =
Â Â Â Â  function( n, useWordBoundary ){
Â Â Â Â Â Â Â Â  var isTooLong = this.length > n,
Â Â Â Â Â Â Â Â Â Â Â Â  s_ = isTooLong ? this.substr(0,n-1) : this;
Â Â Â Â Â Â Â Â  s_ = (useWordBoundary && isTooLong) ? s_.substr(0,s_.lastIndexOf(' ')) : s_;
Â Â Â Â Â Â Â Â  returnÂ  isTooLong ? s_ + '...' : s_;
Â Â Â Â Â  };


