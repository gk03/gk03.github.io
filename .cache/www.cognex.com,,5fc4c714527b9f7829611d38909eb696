<script type="text/javascript">
    var timerId = 0;
    var timerId0 = 0;
    var timerId1 = 0;

    $(function () {
        /**
        * the menu
        */
        var $menu = $('#cmm_menu');

        /**
        * for each list element,
        * we show the submenu when hovering and
        * expand the span element (title) to 510px
        */
        $menu.children('li').each(function () {
            var $this = $(this);
            var $span = $this.children('span');
            $span.data('width', $span.width());

            $this.bind('mouseenter', function () {
                clearTimeout(timerId);
                clearTimeout(timerId0);

                if ($this.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').css("display") == "none") {
                    timerId = setTimeout(function () {
                        $menu.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').slideUp('fast');
                    }, 275);
                    timerId0 = setTimeout(function () {
                        $span.animate({ 'width': $span.data('width') + 'px' }, 25, function () {
                            $this.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').slideDown();
                        });
                    }, 350);
                }
            }).bind('mouseleave', function () {
                clearTimeout(timerId);
                clearTimeout(timerId0);

                timerId = setTimeout(function () {
                    $menu.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').slideUp('fast');
                }, 300);
            });
        });
    });

        /*
