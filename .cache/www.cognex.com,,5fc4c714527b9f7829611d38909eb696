    // Show #topOfPage link when necessary
    $(document).scroll(function () {
        var y = $(this).scrollTop();
        if (y > 400) {
            $('.linkToTop').fadeIn();
        } else {
            $('.linkToTop').fadeOut();
        }
    });
</script>


