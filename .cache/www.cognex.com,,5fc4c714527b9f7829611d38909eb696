    <script language="javascript" type="text/javascript">
        $(document).ready(function () {
            if ($("#ctl00_Hidden_CookieEU").val() == "true") {
                $("#EuroCookieDiv").show();

                // GH's stuff displays cookie notice, now add class to make header & global nav scrollable (normal inline elements)
                $(".header_container").addClass('static');
                $(".topnav_container").addClass('static');
                $(".sectionhead_container").addClass('static');
                $(".content_container").addClass('static');
                $(".footer_container").addClass('default');
            }

            // Hide the search box in China.
            var domainNameHide = "cognex.cn";
            var domainName = window.location.hostname.toLowerCase();
            if (domainName.indexOf(domainNameHide) != -1) {
                $(".topnav_search").hide();
            }
        });

        function CloseCookieEU() {
            // onClick reset header & global nav to a fixed position
            $(".header_container").removeClass('static').addClass('sticky');
            $(".topnav_container").removeClass('static').addClass('stickyNav');
            $(".sectionhead_container").removeClass('static').addClass('sticky');
            $(".content_container").removeClass('static').addClass('sticky');
            $(".footer_container").removeClass('default');

            $.cookie("eucog", "false");
            $("#EuroCookieDiv").slideUp('100');
        }
    </script>
	

