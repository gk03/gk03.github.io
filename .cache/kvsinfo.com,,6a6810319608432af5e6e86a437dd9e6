<script type="text/javascript">
    function switchPanel(clickItem)
    {
        var panelName = clickItem.getAttribute("id");
        var onPanelName = panelName + "_display";
        var offPanelName = "";
        if (panelName.indexOf("left") >= 0) 
        {
            offPanelName = onPanelName.replace("left", "right");
        } else {
            offPanelName = onPanelName.replace("right", "left");
        }
        var onPanel = document.getElementById(onPanelName);
        var offPanel = document.getElementById(offPanelName);
        onPanel.style.visibility = "visible";
        offPanel.style.visibility = "hidden";
        return false;
    }
</script>
