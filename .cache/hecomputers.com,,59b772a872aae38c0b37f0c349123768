            var $M  =  YAHOO.util.Motion;
            var $EA =  YAHOO.util.Easing;
            var $DD =  YAHOO.util.DD;
            var $C  =  YAHOO.util.Connect;
            var $   =  $D.get;

            YAHOO.namespace ("Smb.Asteroids.Logger");
            YAHOO.Smb.Asteroids.Logger = {
                Log : function(e) {
