<script type="text/javascript">
$(window).on('load resize', function() {
	//$.log('trying');
	var dw = $(document).innerWidth(),
		breakpoint = 768;
	if (dw < breakpoint) {
		//$.log('this should work');
		$(window).on('scroll', function() {
			var masthead = $('#MastHead').outerHeight();
			if ($(window).scrollTop() > masthead) {
				$('#NavZone').addClass('fixed');
			} else {
				$('#NavZone').removeClass('fixed');
			}
		});
	} else {
		$(window).off('scroll');
		$('#NavZone').removeClass('fixed');
	}
});</script>
