<script type="text/javascript">
//Extra code to find position:
function findPos(){
  if(bw.ns4){   //Netscape 4
    x = document.layers.layerMenu.pageX
    y = document.layers.layerMenu.pageY
  }else{ //other browsers
    x=-3; y=2; var el,temp
    el = bw.ie4?document.all["divMenu"]:document.getElementById("divMenu");
    if(el.offsetParent){
      temp = el
      while(temp.offsetParent){ //Looping parent elements to get the offset of them as well
        temp=temp.offsetParent; 
        x+=temp.offsetLeft
        y+=temp.offsetTop;
      }
    }
    x=el.offsetLeft
    y+=el.offsetTop
  }
  //Returning the x and y as an array
  return [x,y]
}

pos = findPos()

//Start Menu
var oM=new makeCM("oM")
oM.pxBetween=2
oM.fromLeft=pos[0]
oM.fromTop=pos[1]
oM.menuPlacement=0
oM.wait=600
oM.fillImg=""
//We also need to "re place" the menu on resize. So:
oM.onresize="pos = findPos(); oM.fromLeft=pos[0]; oM.fromTop=pos[1]"
oM.resizeCheck=1 
oM.zIndex=1 
oM.onlineRoot=""
oM.offlineRoot=""
oM.rows=01
oM.arrow="/images/arrow_red.gif"
oM.arrowWidth=10
oM.arrowHeight=10
oM.level[0]=new cm_makeLevel()
oM.level[0].width=392
oM.level[0].height=220
oM.level[0].regClass="clT" 
oM.level[0].overClass="clTover"  
oM.level[0].borderX=0
oM.level[0].borderY=0
oM.level[0].margin=0
oM.level[0].borderClass="clB" 
oM.level[0].rows=0
oM.level[0].align="bottom"
oM.level[0].offsetX=0
oM.level[0].offsetY=0
oM.level[0].arrow=""
oM.level[0].arrowWidth=10
oM.level[0].arrowHeight=10
oM.level[1]=new cm_makeLevel()
oM.level[1].borderX=2
oM.level[1].borderY=2
oM.level[1].regClass="clLevel1" 
oM.level[1].overClass="clLevel1over"  
