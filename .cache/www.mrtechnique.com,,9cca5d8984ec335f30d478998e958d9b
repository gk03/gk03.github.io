				<article id="post-3528" class="post-3528 page type-page status-publish hentry">

				
					<div class="entry-content">
					<div id="piedmont-park" class="et_pb_section et_pb_section_parallax et_pb_fullwidth_section  et_pb_section_0 et_pb_with_background et_section_regular">
				
				
					
					<div id="home-slider" class="et_pb_module et_pb_slider et_pb_slider_parallax  et_pb_fullwidth_slider_0">
