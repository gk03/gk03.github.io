<script type="text/javascript">

    var firstbig = false;

    function ytBig() {
        if (!firstbig) return;
        var pl = document.getElementById("ytplayer");
        pl.height = 380;
        pl.width = 750;
        document.getElementById("allfon").style.display = 'block';
    }


    function ytNormal() {
        if (!firstbig) return;
        var pl = document.getElementById("ytplayer");
        pl.height = 250;
        pl.width = 350;
        document.getElementById("allfon").style.display = 'none';
        firstbig = false;
    }

    function onytplayerStateChange(newState) {
        if (newState == 2) ytNormal();
        if (newState == 0) { ytNormal(); window.scrollTo(0, document.body.scrollHeight); }
    }


    function onYouTubePlayerReady(pid) {
        var pl = document.getElementById("ytplayer");
        pl.addEventListener("onStateChange", "onytplayerStateChange");
        ytBig();
    }



    function _run() {
        var videoID = "-aodoBtizXw"
        videoID = "7Pgx445bGms"
        var params = { allowScriptAccess: "always" };
        var atts = { id: "ytplayer" };
        swfobject.embedSWF("http://www.youtube.com/v/" + videoID + "&enablejsapi=1&playerapiid=utplayer&rel=0&hl=en_US&autoplay=0",
                           "divplayer", "485", "295", "8", null, null, params, atts);


    }
    google.setOnLoadCallback(_run);



</script>  



