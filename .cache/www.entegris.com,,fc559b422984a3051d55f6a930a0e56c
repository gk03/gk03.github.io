<script language="javascript" type="text/javascript">

var homepageModuleLeftCnt = $('#leftModuleCnt').val();
var homepageModuleCenterCnt = $('#centerModuleCnt').val();
var homepageSlideShowCnt = $('#slideShowCnt').val();

$(document).ready(function () {
    startTimer();
    moduleStartTimer();
    homepageModuleLeftCnt = $('#leftModuleCnt').val();
    homepageModuleCenterCnt = $('#centerModuleCnt').val();
    homepageSlideShowCnt = $('#slideShowCnt').val();
});
    
var timer;
var timerOn = 0;
var moduleTimer;
var moduleTimerOn = 0;
var curSlide = 1;
var nSlide = 2;
var curHomepageModuleLeft = 1;
var nHomepageModuleLeft = 2;

var curHomepageModuleCenter = 1;
var nHomepageModuleCenter = 2;


function timedCount() {
    if (timerOn) {
        LoadSlide(nSlide,false);
        timer = setTimeout("timedCount()", 5000);
    }
}

function startTimer() {
    timerOn = 1;
    timer = setTimeout("timedCount()", 5000);
}

function clearTimer() {
    clearTimeout(timer);
}

function moduleTimedCount() {
    if (moduleTimerOn) {
        if (curHomepageModuleLeft < homepageModuleLeftCnt) {
            HomepageModuleLeftSwitch(nHomepageModuleLeft,false);
            moduleTimer = setTimeout("moduleTimedCount()", 5000);
            nHomepageModuleLeft++;
        }
        else if (curHomepageModuleCenter < homepageModuleCenterCnt) {
            HomepageModuleCenterSwitch(nHomepageModuleCenter,false);
            moduleTimer = setTimeout("moduleTimedCount()", 5000);
            nHomepageModuleCenter++;
        }
        else {
            moduleClearTimer();
        }
    }
}

function moduleStartTimer() {
    moduleTimerOn = 1;
    moduleTimer = setTimeout("moduleTimedCount()", 5000);
}

function moduleClearTimer() {
    clearTimeout(moduleTimer);
}

function LoadSlide(nextSlide,selected) {
    if (selected)
        clearTimer();
    if (nextSlide != curSlide) {
        $('#Slide' + curSlide).fadeOut();
        $('#Slide' + nextSlide).fadeIn();
        $('#SlideLink' + curSlide).css('font-weight','normal');

        $('#SlideLink' + nextSlide).css('font-weight','bold');

        curSlide = nextSlide;
        if (curSlide == homepageSlideShowCnt) {
            clearTimer();
            //nSlide = curSlide + 1;
        }
        else {
            nSlide = curSlide + 1;
            //nSlide = 1;
        }
    }
}

function HomepageModuleLeftSwitch(nextHomepageModule,selected) {
    if (selected)
        moduleClearTimer();
    $('#HomePageModuleLeft' + curHomepageModuleLeft).hide();
    $('#HomePageModuleLeft' + nextHomepageModule).show();
    $('#btnHomepageModuleOnLeft' + curHomepageModuleLeft).hide();
    $('#btnHomepageModuleOffLeft' + curHomepageModuleLeft).show();
    $('#btnHomepageModuleOnLeft' + nextHomepageModule).show();
    $('#btnHomepageModuleOffLeft' + nextHomepageModule).hide();
    curHomepageModuleLeft = nextHomepageModule;
}

function HomepageModuleCenterSwitch(nextHomepageModule,selected) {
    if (selected)
        moduleClearTimer();
    $('#HomePageModuleCenter' + curHomepageModuleCenter).hide();
    $('#HomePageModuleCenter' + nextHomepageModule).show();
    $('#btnHomepageModuleOnCenter' + curHomepageModuleCenter).hide();
    $('#btnHomepageModuleOffCenter' + curHomepageModuleCenter).show();
    $('#btnHomepageModuleOnCenter' + nextHomepageModule).show();
    $('#btnHomepageModuleOffCenter' + nextHomepageModule).hide();
    curHomepageModuleCenter = nextHomepageModule;
}
var curTabBox = 1;
function HomepageFlipTab(nextTab) {
    if (nextTab != curTabBox) {
        $('#HomepageTabBox' + curTabBox).hide();
        $('#HomepageTabBox' + nextTab).show();
        $('#HomepageTabOn' + curTabBox).hide();
        $('#HomepageTabOff' + curTabBox).show();
        $('#HomepageTabOff' + nextTab).hide();
        $('#HomepageTabOn' + nextTab).show();
        curTabBox = nextTab;
    }
}

$(document).ready(function() {
    if ($('#quickLinksCnt').val() > 3)
        $('#HomepageTabBox1').jScrollPane();
});
</script>

