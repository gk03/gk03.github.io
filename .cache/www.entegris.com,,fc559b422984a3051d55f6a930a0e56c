<script language="javascript" type="text/javascript">
var curTab;
$(document).ready(function() {
   curTab = 0;
 });
function ExpandFlyout(nextTab) {
    if (nextTab == curTab) {
        nextTab = 0;
    }
    $('#FlyoutContentContainer' + nextTab).animate({height:140}, 500);
    $('#FlyoutContent' + nextTab).fadeIn();
    $('#FlyUpLinkOff' + nextTab).hide();
    $('#FlyUpLinkOn' + nextTab).show();
    $('#FlyoutContentContainer' + curTab).animate({height:0}, 500);
    $('#FlyoutContent' + curTab).fadeOut();
    $('#FlyUpLinkOff' + curTab).show();
    $('#FlyUpLinkOn' + curTab).hide();
    curTab = nextTab;
}
</script>

