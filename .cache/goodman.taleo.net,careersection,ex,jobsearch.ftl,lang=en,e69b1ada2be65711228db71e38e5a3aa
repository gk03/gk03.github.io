<script>

var BUTTONS_DEFINITION = {
	applyButtonDefinition : new applyButtonDefinition(
				RESOURCES.getResource("resultListPanel.applyButton"),
				RESOURCES.getResource("resultListPanel.finishDraftSubmissionButton"),
				RESOURCES.getResource("resultListPanel.viewEditSubmissionButton"),
				RESOURCES.getResource("resultListPanel.reApplyButton"),
				'result-list-button'),			
	referCandidateButtonDefinition : new referCandidateButtonDefinition(
				RESOURCES.getResource("resultListPanel.referACandidateButton"), 
				'result-list-button'),		
	addToJobCartButtonDefinition : new addToJobCartButtonDefinition(
				RESOURCES.getResource("resultListPanel.addToJobCartAction"),
				'add-to-jobcart'),
	addThisButtonDefinition : new AddThisButtonDefinition(STATIC_RESOURCES.shareRes, 
				STATIC_RESOURCES.commonJobDesc)					
}

var ICONS_DEFINITION = createIconsDefinitions()
var LOGGED_IN = false
var IS_AGENCY_PORTAL = false
var IS_APPLICATION_FLOW_SET = true
var FACETED_SEARCH_SETTINGS = new FacetedSearchSettings(true,
		true)

var MULTILINE_DEFINITION = [[{type: "field", value: "", index: 0},{type: "staticText", value: " ", index: 0},{type: "staticText", value: "-", index: 0},{type: "staticText", value: " ", index: 0}],[{type: "field", value: "", index: 1}],[{type: "label", value: "Referral Bonus", index: 0},{type: "staticText", value: " ", index: 0},{type: "field", value: "", index: 2}]]
var URGENT_JOB_DISPLAY = true
var IS_REQUISITION_DESCRIPTION_PAGE_DISPLAYED = true

$(document).ready(function() {
	
	var resultListPanel = new ResultListPanel(SEARCH_HANDLER, false)
 	
 	if(IS_AGENCY_PORTAL) {
 		$('#refer-a-friend-link').attr('href', URL_BUILDER.getReferCandidateProfileURL())	
 	} else {
 		$('#refer-a-friend-link').attr('href', URL_BUILDER.getReferFriendProfileURL())	
 	}

});

function createIconsDefinitions() {
	var hotJobIcon = new iconDefinition('hot-job', 
						RESOURCES.getResource("icons.urgentNeedJob"),
						function(job, isUserLogged, isAddToJobCartAction) {
							if(job.hotJob && URGENT_JOB_DISPLAY) {
								return true;
							}
							return false;
						});
	var addedToJobCart = new iconDefinition('added-to-job-cart', 
						RESOURCES.getResource("icons.addedToJobCartResource"),
							function(job, isUserLogged, isAddToJobCartAction) {
								if(isUserLogged == true && isAddToJobCartAction && job.addedToJobCart) {
									return true;
								}
								return false;
							});
	var draft = new iconDefinition('draft', 
						RESOURCES.getResource("icons.draftSubmission"),
							function(job, isUserLogged, isAddToJobCartAction) {
								if(isUserLogged  == true && job.draft) {
									return true;
								}
								return false;
							});
	var alreadyAppliedOn = new iconDefinition('already-applied-on', 
						RESOURCES.getResource("icons.completedSubmission"),
							function(job, isUserLogged, isAddToJobCartAction) {
								if(isUserLogged  == true && job.alreadyAppliedOn) {
									return true;
								}
								return false;
							});
	
	return new Array(hotJobIcon, addedToJobCart, draft, alreadyAppliedOn);
}

$('#addToJobCartErrorOk').click(function() {
	POPUP_MANAGER.closePopup($('#addToJobCartError'));
});
</script>


