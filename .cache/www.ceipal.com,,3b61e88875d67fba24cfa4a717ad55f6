        <script type="text/javascript" src="js/jquery.validation.js"></script>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <!-- <script src="js/jquery-1.11.1.min.js"></script> -->
    <!-- Bootstrap -->
    <script src="js/bootstrap.min.js"></script>
    <script>
    var url = '';
    if(url == 'talenthire-features' || url == 'workforce-features'){
        $('.dropdown-toggle').addClass('active');
        $('.navbar-nav li a[href="'+url+'"]').addClass('active');
        // console.log(url);
    }else if(url=='' || url=='index'){
        $('.homepage').addClass('active');
    }else if(url !=''){
        // console.log(url);
        $('.navbar-nav li a[href="'+url+'"]').addClass('active');
    }
        $(window).scrollTop(0);
        $(window).scroll(function () {
            if ($(this).scrollTop() > 20) {
                $('.navbar').addClass("navbar_stick");
            } else {
                $('.navbar').removeClass("navbar_stick");
            }
        });
        $('.read_more_btn').click(function (e) {
            $('html,body').animate({
                scrollTop: $(this.hash).offset().top
            }, 1000);
            return false;
        });
        $(document).on('click', '.free_trail_submit', function(){
        var validator_reg = $('.free_trail').validate({
            rules: {
                'email_id': {
                    required: true,
                    email:true
                },
                'mobile_id': {
                    required: true,
                     number: true
                }
            },
            messages:{
                'email_id':'',
                'mobile_id':''
            }
        });
            y=validator_reg.form();
        $('label[for="email_id"]').hide();
        $('label[for="mobile_id"]').hide();
            if(y) {
                $.ajax({
                        url: "mail_send.php",
                        type: "Post",
                        data: $(".free_trail").serialize(),
                        success: function (res) {
                        if(res=='1'){
                           // window.location= 'http://localhost/ceipal.com/ceipal/resend';
						   window.location= 'https://www.ceipal.com/resend';
                            // alert('Your request has been received. We will contact you soon');
                            validator_reg.resetForm();
                        }
                        }
                    });
                }
            return y;
 
    });
    $(document).on('click', '.freetrail_contact', function(){
        var validator_reg = $('.free_trailform').validate({
            rules: {
                'email_id': {
                    required: true,
                    email:true
                },
                'mobile_id': {
                    required: true,
                     number: true
                }
            },
            messages:{
                'email_id':'',
                'mobile_id':''
            }
        });
            y=validator_reg.form();
        $('label[for="email_id"]').hide();
        $('label[for="mobile_id"]').hide();
            if(y) {
                $.ajax({
                        url: "mail_send.php",
                        type: "Post",
                        data: $(".free_trailform").serialize(),
                        success: function (res) {
                        if(res=='1'){
                            //window.location= 'http://localhost/ceipal.com/ceipal/resend';
                            window.location= 'https://www.ceipal.com/resend';

                            // alert('Your request has been received. We will contact you soon');
                            validator_reg.resetForm();
                        }
                        }
                    });
                }
            return y;
 
    });
	
	//videos stop & play script 
	var iframe_src = "https://www.youtube.com/embed/qgWQUSyMQG8?feature=player_detailpage&rel=0";
    $('#ceipal-intro').on('shown.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src + '&autoplay=1');
    });
    $('#ceipal-intro').on('hide.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src + '&autoplay=0');
    });

    //var iframe_src_th = "https://www.youtube.com/embed/BJtJXS-KHkA?feature=player_detailpage&rel=0";
    var iframe_src_th = "https://www.youtube.com/embed/8Be4Vlg9ej8?feature=player_detailpage&rel=0";
    $('#talenthire-features').on('show.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src_th + '&autoplay=1');
    });
    $('#talenthire-features').on('hide.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src_th + '&autoplay=0');
    });

   // var iframe_src_wf = "https://www.youtube.com/embed/8-fgU1Amj1w?feature=player_detailpage&rel=0";
    var iframe_src_wf = "https://www.youtube.com/embed/CA6rz9JzNOU?feature=player_detailpage&rel=0";
    $('#workforce-features').on('show.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src_wf + '&autoplay=1');
    });
    $('#workforce-features').on('hide.bs.modal', function() {
        $(this).find('iframe').attr('src', iframe_src_wf + '&autoplay=0');
    });
	
	
	
    // https://www.youtube.com/embed/8-fgU1Amj1w
    // $('#workforce-features').on('hide.bs.modal', function() {
    //     var iframe_src = $(this).find('iframe').attr('src');
    //     $(this).find('iframe').attr('src', iframe_src + '&autoplay=0');
