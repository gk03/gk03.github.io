<script>
$(window).load(function () {
    $(window).resize(bnnerResize);
    bnnerResize();

});
function bnnerResize() {
    if ($(window).width() < 477) {
        bannerHeight = ($(window).height()) - 60;
        $('.banner').height(bannerHeight);
    } else {
        $('.banner').removeAttr('style');

    }


}

if ($('.emd_dl_red_darker').hasClass('emd_dl_red_darker')) {
    $(".emd_dl_red_darker").addClass("download-button").removeClass("emd_dl_red_darker");

}
