    jQuery(document).ready(function () {
        jQuery('.panel-default').hover(function () {
            jQuery(this).find('.panel-collapse').stop(true, true).slideDown();
            jQuery(this).find('.panel-heading a').addClass('hoverColor');
        }, function () {
            jQuery(this).find('.panel-collapse').stop(true, true).slideUp();
            jQuery(this).find('.panel-heading a').removeClass('hoverColor');
