        <script src="/bundles/jquery?v=DoLGHW9uWXOIg1xDaO29ViiH8PvEjfkdqc7dWPYhWHQ1"></script>

        
    <script type="text/javascript">
        $(document).ready(function () {
            //$("#mgmtConsultText").style.display = "block";
            //$("#projectConsultText").style.display = "block";
            //$("#itConsultingText").style.display = "block";

            
            var idx = 2;

            $("#slideshow > div:gt(0)").hide();

            setInterval(function () {
                $('#slideshow > div:first')
                  .fadeOut(1000)
                  .next()
                  .fadeIn(1000)
                  .end()
                  .appendTo('#slideshow');
                switch (idx) {
                    case 1:
                        $("#srvIt").css("font-weight", "Normal");
                        $("#srvManage").css("font-weight", "Bold");
                        $("#itConsultingText").hide(250);
                        $("#mgmtConsultText").show(500);
                        break;
                    case 2:
                        $("#srvManage").css("font-weight", "Normal");
                        $("#srvProject").css("font-weight", "Bold");
                        $("#mgmtConsultText").hide(250);
                        $("#projectConsultText").show(500);
                        break;
                    case 3:
                        $("#srvProject").css("font-weight", "Normal");
                        $("#srvIt").css("font-weight", "Bold");
                        $("#projectConsultText").hide(250);
                        $("#itConsultingText").show(500);
                        break;
                }//switch
                idx++;
                if (idx > 3) idx = 1;
            }, 6000);

            $("h2")
                .wrapInner("<span>")
            
            $("h2 br")
                .before("<span class='spacer'>")
                .after("<span class='spacer'>");
        });
    </script>

