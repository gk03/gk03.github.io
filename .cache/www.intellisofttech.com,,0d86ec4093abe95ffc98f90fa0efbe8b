   $(document).ready(function() {
    $("div.bhoechie-tab-menu>div.list-group>a").click(function(e) {
        e.preventDefault();
        $(this).siblings('a.active').removeClass("active");
        $(this).addClass("active");
        var index = $(this).index();
        $("div.bhoechie-tab>div.bhoechie-tab-content").removeClass("active");
        $("div.bhoechie-tab>div.bhoechie-tab-content").eq(index).addClass("active");
    });
});
        // Set Fixed Header options
        var options = {
            offset: '#showHere',
            classes: {
                clone:   'banner--clone',
                stick:   'banner--stick',
                unstick: 'banner--unstick'
            }
        };
        // Initialise with options
        var banner = new Headhesive('.fixed_header', options);
        // Headhesive destroy
        // banner.destroy();
		
		//Add active class to testimmonials
		$('.testi').click(function()
	   {
		   $('.main_testi_container .testimoials_bg').removeClass('active');
		   $('.testimonials .testi').removeClass('active');
		   //Add class to image
		   $(this).addClass('active');
		   
		   //Add active class to Content
		   var content_id = $(this).attr('content_id');
		   $('#testi_content_'+content_id).addClass('active');
	   });
});

</script>


	
</body>
</html>