			<!-- Form Validation Script -->

<script language="JavaScript" type="text/JavaScript">

<!--

// Function to Strip HTML out of all fields 11/21/2011 - SPO

function stripHTML(){ 
var re= /<\S[^><]*>/g
for (i=0; i<arguments.length; i++) 
arguments[i].value=arguments[i].value.replace(re, "") 
} 


function Guildernet_Validator(theForm)
{

stripHTML(theForm.nametext,theForm.phonetext,theForm.emailtext,theForm.messagetext); 


if (theForm.nametext.value == "")
  {
    alert("Please enter your name into the name field of the form.");
    theForm.nametext.focus();
    return (false);
  }
  
  if (theForm.nametext.value == "Name")
  {
    alert("Please enter your name into the name field of the form.");
    theForm.nametext.focus();
    return (false);
  }

if (theForm.phonetext.value == "")
  {
    alert("Please provide a phone number so that we may reach you.");
    theForm.phonetext.focus();
    return (false);
  }
  
  if (theForm.phonetext.value == "Phone")
  {
    alert("Please provide a phone number so that we may reach you.");
    theForm.phonetext.focus();
    return (false);
  }
   
  if (theForm.emailtext.value == "")
  {
    alert("Please enter your email address into the email field of the form so that we can contact you.");
    theForm.emailtext.focus();
    return (false);
  }
  
  if (theForm.emailtext.value == "Email")
  {
    alert("Please enter your email address into the email field of the form so that we can contact you.");
    theForm.emailtext.focus();
    return (false);
  }
  
  // 11/21/2011 Added extra error checking to email field per cutomer request
  //check for @ and . in email address
  var at_pos = -1;
  var dot_pos = -1;
  var tmp_pos;
  for (tmp_pos = 0; ((tmp_pos < theForm.emailtext.value.length) && (at_pos == -1)); tmp_pos++)
    if (theForm.emailtext.value.substr(tmp_pos, 1) == '@')
      at_pos = tmp_pos;
  for (tmp_pos = 0; ((tmp_pos < theForm.emailtext.value.length) && (dot_pos == -1)); tmp_pos++)
    if (theForm.emailtext.value.substr(tmp_pos, 1) == '.')
      dot_pos = tmp_pos;
  if ((at_pos < 1) || (dot_pos < 0))
  {
    alert("The Email address you entered is not in a valid format. Please re-enter your Email address.");
    theForm.emailtext.focus();
    return false;
  }
  
  
    return (true);
}
  
  
//-->

</script>

<!-- End of Client Side Form Validation -->
			
			
