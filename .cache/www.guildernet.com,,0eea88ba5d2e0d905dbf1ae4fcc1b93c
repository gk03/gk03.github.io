<!-- Twitter Script -->

<script type="text/javascript">
	$(document).ready(function() {
	var twitterusername = "guildernet";
    var notweets = "1";
	$.get('noapitwitter/notwitterapi.php?endpoint=timeline&twitterusername=' + twitterusername + '&count=' + notweets, function (jsondata) {
	var data = $.parseJSON(jsondata);
	var i = 0;
	$.each(data, function() {
	i++;
	$('#twitter-feed1').append('<div class="twitter-article" id="tw1"><div class="twitter-text"><p><span class="tweetprofilelink"><a href="https://twitter.com/' + this['username'] + '" target="_blank"><strong>@' + this['username'] + '</strong></a></span><span class="tweet-time"><BR><STRONG>' + this['date'] + '</STRONG></span><br/>' + this['tweet'] + '</p></div></div>');
	});
	});	
	});
	</script>

	
