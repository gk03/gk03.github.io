    var bCancel = false; 

    function validatePublicRequirementSearchForm(form) { 
        if (bCancel) { 
            return true; 
        } else { 
            var formValidationResult; 
            formValidationResult = validateRequired(form); 
            return (formValidationResult == 1); 
        } 
    } 

