		var count = 0;
		/* instantiate primary dhtml objects */
		var mnu = new MenuBar();
	 	var ro = new Rollover();
		
		/* assign init() function to onload event handler */
		onload = init;
		
		/* initialize primary dhtml objects. the page is fully loaded, so isReady must now be true */
		function init() {
			ro.init();
			mnu.init();
			swap()
			isReady = true;
		}
		
		function swap() {
			count++
			if (count == 3) ro.on("anim");
			setTimeout("swap()", 1000)
		}
		
		
	</script>
