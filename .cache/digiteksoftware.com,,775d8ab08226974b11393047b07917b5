         var ip;
         window.onload = function () {
        var script = document.createElement("script");
        script.type = "text/javascript";
        script.src = "http://www.telize.com/jsonip?callback=DisplayIP";
        document.getElementsByTagName("head")[0].appendChild(script);
    };
    function DisplayIP(response) {
        ip=response.ip;
          $.get("http://ipinfo.io/"+ip, function (response) {
                 if(response.country=='IN'){
                 $.sessionStorage('Digi_Logo', 'India');
                 updateHeader();
                 }
            }, "jsonp");
    }
     </script>

    <script type="text/javascript">
        $(function () {
            $('.m-carousel').carousel();
            $("a.clicker").click(function () {
                $("a.clicker").find(".icon-replacer").removeClass("active-icon");
                $(this).find(".icon-replacer").addClass("active-icon");
            });
        });
        var ie = 0;
        if ($.browser.msie && (parseInt($.browser.version, 10) === 8 || $.browser.msie && parseInt($.browser.version, 10) === 7)) {
            ie = 1;
        }
        var current = 0;
        var previous = 0;
        function bannerreset(b) {
            var j = 1;
            while (document.getElementById('b' + j)) {
                var i = 1;
                $('#b' + j).hide();
                while (document.getElementById('b' + j + i)) {
                    $('#b' + j + i).hide();
                    $('#b' + j + i).clearQueue();
                    $('#b' + j + i).stop();
                    i++;
                }
                j++;
            }
        }
        var myVar = '';
        function startanimation() {
            myVar = setInterval("shownext(0)", 5000);
        }
        var pauseflag = 1;
        function resetpauseflag() {
            pauseflag = 1;
        }
        function shownext(i) {
            if (pauseflag == 1) {
                pauseflag = 0;
                setTimeout('resetpauseflag()', 500);
            }
            else
            { return }
            if (current != 0) {
                hb('b' + previous);
            }
            if (i == 0) {
                current++;
            } else {
                current = i;
            }
            if (document.getElementById('b' + current)) {

            }
            else {
                current = 0;
                current++;
            }
            previous = current;
            var currentdate = new Date();
            var str = currentdate.getMinutes() + ":" + currentdate.getSeconds();

            setTimeout("sb('b" + current + "')", 600);
        }
        function sb(b) {
            var w = $(window).width();
            var block = '980';
            var ml = (w - block) / 2;
            var mlpx = ml + 'px';
            wpx = w + 'px';
            bannerreset(b);
            blinormal();
            $('#' + b + 'li').addClass('selected');
            //below code for active tail
            $('#' + b + 'li').find(".tail-place").addClass("selected-tail");
            $('#' + b + 'li').find(".icon-placer").addClass("active-icon");
            if (b == 'b1') {
                $('#bannerdiv').addClass("it-staff-bg");
                $("#b11").css('right', wpx);
                $('#b1').show();
                $('#b11').show();
                if (ie) {
                    $("#b11").animate({ right: (ml - 80) + 'px' }, 900).animate({ right: (ml - 60) + 'px' }, 50);
                    var lf = (parseInt(ml) + parseInt(580)) + 'px';
                    $('#b12').css('right', lf).css('margin-top', 110).show();
                    $("#b12").delay(400).animate({ marginTop: 110 }, 900);
                    $('#b13').css('right', lf).css('margin-top', 230).show();
                    $("#b13").delay(600).animate({ marginTop: 230 }, 800);
                }
                else {
                    $("#b11").animate({ right: (ml - 80) + 'px', opacity: 1 }, 1200).animate({ right: (ml - 60) + 'px' }, 50);
                    var lf = (parseInt(ml) + parseInt(580)) + 'px';
                    $('#b12').css('right', lf).css('opacity', '0').css('margin-top', 110).show();
                    $("#b12").delay(400).animate({ opacity: 1, marginTop: 110 }, 900);
                    $('#b13').css('right', lf).css('opacity', '0').css('margin-top', 300).show();
                    $("#b13").delay(600).animate({ opacity: 1, marginTop: 230 }, 800);
                }
            }
            if (b == 'b2') {
                $('#bannerdiv').addClass("wrds-bg");
                $('#b2').show();
                if (ie) {
                    $('#b21').css('top', 10).css('left', 210).show();
                    $('#b22').css('top', 120).css('left', -120).show();
                    $('#b23').css('top', 260).css('left', w).show();
                    $('#b24').css('top', 284).css('left', -500).show();

                    $("#b21").delay(800).animate({ opacity: 0, left: '-=20' }, 150);
                    $("#b22").delay(150).animate({ left: ml }, 400);
                    $("#b23").delay(200).animate({ left: ml }, 500);
                    $("#b24").delay(500).animate({ left: parseInt(ml) + 5 }, 300);

                }
                else {
                    $('#b21').css('top', 10).css('left', 210).show();
                    $('#b22').css('top', 120).css('left', -120).css('opacity', 1).show();
                    $('#b23').css('top', 260).css('left', w).css('opacity', 1).show();
                    $('#b24').css('top', 284).css('left', -500).css('opacity', 1).show();

                    $("#b21").delay(500).animate({ opacity: 1, left: '-=20' }, 150);
                    $("#b22").delay(150).animate({ opacity: 1, left: ml }, 400);
                    $("#b23").delay(200).animate({ opacity: 1, left: ml }, 500);
                    $("#b24").delay(500).animate({ opacity: 1, left: parseInt(ml) + 5 }, 300);

                }
            }
            if (b == 'b3') {
                $('#bannerdiv').addClass("wrdsmobi-bg");

                $("#b31").css('right', wpx);
                $('#b3').show();
                $('#b31').show();
                if (ie) {
                    $("#b31").animate({ right: (ml - 0) + 'px' }, 1000).animate({ right: (ml - -70) + 'px' }, 50);
                    var lf = (parseInt(ml) + parseInt(390)) + 'px';
                    $('#b32').css('right', lf).css('margin-top', 10).show();
                    $("#b32").delay(400).animate({ marginTop: 170 }, 400);
                    $('#b33').css('right', lf).css('margin-top', 250).show();
                    $("#b33").delay(500).animate({ marginTop: 300 }, 500);
                }
                else {
                    $("#b31").animate({ right: (ml - 0) + 'px', opacity: 1 }, 1000).animate({ right: (ml - -70) + 'px' }, 50);
                    var lf = (parseInt(ml) + parseInt(390)) + 'px';
                    $('#b32').css('right', lf).css('opacity', '0').css('margin-top', 170).show();
                    $("#b32").delay(400).animate({ opacity: 1, marginTop: 10 }, 400);
                    $('#b33').css('right', lf).css('opacity', '0').css('margin-top', 300).show();
                    $("#b33").delay(500).animate({ opacity: 1, marginTop: 250 }, 500);
                }
            }
            if (b == 'b4') {
                $('#bannerdiv').addClass("it-services-bg");
                $('#b4').show();
                if (ie) {
                    $('#b41').css('left', parseInt(ml) + 590).show();
                    $('#b42').css('top', 105).css('left', -500).show();
                    $('#b43').css('top', 205).css('left', w).show();
                    $('#b44').css('top', 284).css('left', -500).show();

                    $("#b42").animate({ left: ml }, 400);
                    $("#b43").animate({ left: ml }, 500);
                    $("#b44").delay(500).animate({ left: parseInt(ml) + 5 }, 300);
                    $("#b41").delay(200).animate({ left: '+=20' }, 300);
                }
                else {
                    $('#b41').css('left', parseInt(ml) + 590).css('opacity', 0).show();
                    $('#b42').css('top', 105).css('left', -500).css('opacity', 1).show();
                    $('#b43').css('top', 205).css('left', w).css('opacity', 1).show();
                    $('#b44').css('top', 284).css('left', -500).css('opacity', 1).show();

                    $("#b42").animate({ opacity: 1, left: ml }, 400);
                    $("#b43").animate({ opacity: 1, left: ml }, 500);
                    $("#b44").delay(500).animate({ opacity: 1, left: parseInt(ml) + 5 }, 300);
                    $("#b41").delay(200).animate({ opacity: 1, left: '+=20' }, 300);
                }
            }
           
//            if (b == 'b5') {
//                $('#bannerdiv').addClass("it-staff-bg");
//                $('#b5').show();
//                if (ie) {
//                    $('#b51').css('left', parseInt(ml) + 590).show();
