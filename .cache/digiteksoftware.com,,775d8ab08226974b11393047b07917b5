            if (b == 'b5') {
                $('#bannerdiv').css('background-color', '#FEB900');
                $('#b5').show();
                if (ie) {
                    $('#b51').css('top', -140).css('left', parseInt(ml) + 250).show();
                    $('#b52').css('top', 130).css('left', parseInt(ml) + 270).show();
                    $('#b53').css('top', 150).css('left', -400).show();
                    $('#b54').css('top', 150).css('left', parseInt(ml) + 320).show();
                    $('#b55').css('top', 130).css('left', parseInt(ml) + 620).show();
                    $('#b56').css('top', 142).css('left', w).show();

                    $('#b51').animate({ top: 40 }, 300);
                    $('#b53').animate({ left: parseInt(ml) + 40 }, 300);
                    $('#b56').animate({ left: parseInt(ml) + 660 }, 300);
                }
                else {
                    $('#b51').css('top', -140).css('left', parseInt(ml) + 250).css('opacity', 1).show();
                    $('#b52').css('top', 130).css('left', parseInt(ml) + 270).css('opacity', 0).show();
                    $('#b53').css('top', 150).css('left', -400).css('opacity', 1).show();
                    $('#b54').css('top', 150).css('left', parseInt(ml) + 320).css('opacity', 0).show();
                    $('#b55').css('top', 130).css('left', parseInt(ml) + 620).css('opacity', 0).show();
                    $('#b56').css('top', 142).css('left', w).css('opacity', 1).show();

                    $('#b51').animate({ opacity: 1, top: 40 }, 300);
                    $('#b52').animate({ opacity: 1 }, 500);
                    $('#b53').animate({ opacity: 1, left: parseInt(ml) + 40 }, 300);
                    $('#b54').animate({ opacity: 1 }, 500);
                    $('#b55').animate({ opacity: 1 }, 500);
                    $('#b56').animate({ opacity: 1, left: parseInt(ml) + 660 }, 300);
                }
            }
        }
        function hb(b) {
           
            if (ie) {
                b
                if (b == 'b1') {
                    $('#b11').animate({ right: '-=20' }, 100);
                    $('#b12').animate({ right: '-=20' }, 150);
                    $('#b13').animate({ right: '+=20' }, 200, function () { bannerreset(b) });
                }
                if (b == 'b2') {
                    $('#b21').animate({ right: '+=20' }, 200);
                    $('#b22').animate({ left: '-=1000' }, 200);
                    $('#b23').animate({ left: '+=1000' }, 200);
                    $('#b24').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
                }
                if (b == 'b3') {
                    $('#b31').animate({ right: '-=20' }, 200);
                    $('#b32').animate({ right: '-=20' }, 200);
                    $('#b33').animate({ right: '+=20' }, 200, function () { bannerreset(b) });
                }
                if (b == 'b4') {
                    $('#b41').animate({ left: '+=20' }, 200);
                    $('#b42').animate({ left: '-=1000' }, 200);
                    $('#b43').animate({ left: '+=1000' }, 200);
                    $('#b44').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
                }
//                if (b == 'b5') {
//                    $('#b51').animate({ left: '+=20' }, 200);
//                    $('#b52').animate({ left: '-=1000' }, 200);
//                    $('#b53').animate({ left: '+=1000' }, 200);
//                    $('#b54').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
//                }
                if (b == 'b5') {
                    $('#b51').animate({}, 200);
                    $('#b52').animate({}, 200);
                    $('#b54').animate({}, 200);
                    $('#b55').animate({}, 200);
                    $('#b53').delay(150).animate({ left: '-=50' }, 50).animate({ left: '+=450' }, 200);
                    $('#b56').delay(250).animate({ left: '+=50' }, 50).animate({ left: '-=450' }, 200, function () { bannerreset(b) });
                }
            }
            else {
                if (b == 'b1') {
                    $('#b11').animate({ right: '-=20', opacity: 0 }, 100);
                    $('#b12').animate({ right: '-=20', opacity: 0 }, 150);
                    $('#b13').animate({ right: '+=20', opacity: 0 }, 200, function () { bannerreset(b) });
                }
                if (b == 'b2') {
                    $('#b21').animate({ left: '+=20', opacity: 0 }, 200);
                    $('#b22').animate({ left: '-=1000', opacity: 0 }, 200);
                    $('#b23').animate({ left: '+=1000', opacity: 0 }, 200);
                    $('#b24').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
                }
                if (b == 'b3') {
                    $('#b31').animate({ right: '-=20', opacity: 0 }, 200);
                    $('#b32').animate({ right: '-=20', opacity: 0 }, 200);
                    $('#b33').animate({ right: '+=20', opacity: 0 }, 200, function () { bannerreset(b) });
                }
                if (b == 'b4') {
                    $('#b41').animate({ left: '+=20', opacity: 0 }, 200);
                    $('#b42').animate({ left: '-=1000', opacity: 0 }, 200);
                    $('#b43').animate({ left: '+=1000', opacity: 0 }, 200);
                    $('#b44').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
                }
//                if (b == 'b5') {
//                    $('#b51').animate({ left: '+=20', opacity: 0 }, 200);
//                    $('#b52').animate({ left: '-=1000', opacity: 0 }, 200);
//                    $('#b53').animate({ left: '+=1000', opacity: 0 }, 200);
//                    $('#b54').animate({ left: '-=1000' }, 200, function () { bannerreset(b) });
//                }
                if (b == 'b5') {
                    $('#b51').animate({ opacity: 0 }, 200);
                    $('#b52').animate({ opacity: 0 }, 200);
                    $('#b54').animate({ opacity: 0 }, 200);
                    $('#b55').animate({ opacity: 0 }, 200);
                    $('#b53').delay(150).animate({ left: '-=50' }, 50).animate({ left: '+=450', opacity: 0 }, 200);
                    $('#b56').delay(250).animate({ left: '+=50' }, 50).animate({ left: '-=450', opacity: 0 }, 200, function () { bannerreset(b) });
                }
            }
        }
        function blinormal() {
            $('.bli').removeClass('selected');
            //below code for remove active tail
            $('.bli').find(".tail-place").removeClass("selected-tail");
            $('.bli').find(".icon-placer").removeClass("active-icon");
            $('#bannerdiv').removeClass("it-staff-bg");
            $('#bannerdiv').removeClass("wrds-bg");
            $('#bannerdiv').removeClass("wrdsmobi-bg");
            $('#bannerdiv').removeClass("it-services-bg");


        }
        function bliclick(i) {
            window.clearInterval(myVar);
            shownext(i);
            startanimation();

        }
        function next() {
            window.clearInterval(myVar);
            shownext(0);
            startanimation();
        }
        function pre() {
            window.clearInterval(myVar);
            var cur = current - 1;
            if (cur == 0) {
                cur = 5;
            }
            shownext(cur);
            startanimation();
        }
        function starting() {
            shownext(0);
            startanimation();
        }
        setTimeout('starting()', 1000);
    </script>

