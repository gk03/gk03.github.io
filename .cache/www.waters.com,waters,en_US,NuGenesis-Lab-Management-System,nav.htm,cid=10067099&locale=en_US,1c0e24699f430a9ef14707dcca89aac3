	// 3 column layout fix
	var cBody = $j("#ContentBody");
	if ($j("#LeftNav").is(':visible') && $j("#RightNav").is(':visible') && $j("#ContentBody").length ){   
	    cBody.hasClass('twoColLayout')?cBody.removeClass('twoColLayout'):'';
	    cBody.addClass('centerColumn');
	}
	/*if(cBody.is(':visible') && cBody.hasClass('centerColumn')){
		cBody.removeClass('centerColumn');
	}*/
	
	if ($j("#LeftNav").is(':hidden') && $j("#RightNav").is(':visible')){ 
		//old hack for IE6....
		//if ($j.browser.msie && $j.browser.version.substr(0,1)<7) {return;} //do not size if in IE
		cBody.addClass('auto');
	}
});
function fixCenterCol(){
	if ($j('#LeftNav').is(":hidden")) {
		$j('#ContentBody').removeClass('centerColumn');
	}
}
//OMNITURE STUFF //
function trackClick(reporting_pageName, link_analytics_prop52, link_analytics_prop53, link_analytics_prop54, link_analytics_prop55, link_analytics_prop57, link_analytics_prop58) {
	//handle the interaction here
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop52,prop53,prop54,prop55,prop56,prop57,prop58,eVar51,eVar52,eVar53,eVar54,eVar55,eVar56,eVar57,eVar58,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop52 = s.eVar52 = link_analytics_prop52;
	s.prop53 = s.eVar53 = link_analytics_prop53;
	s.prop54 = s.eVar54 = link_analytics_prop54;
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop57 = s.eVar57 = s.prop52 + ':' + s.prop53 + ':' + s.prop54 + ':' + s.prop55;
	s.prop58 = s.eVar58 = link_analytics_prop58;
	s.tl(this, 'o', 'megamenu');
};
function trackTimer(s_prop61, s_prop62, s_prop63) {
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop61,prop62,prop63,eVar61,eVar62,eVar63,events';
	s.linkTrackEvents = 'event51';
	s.events = 'event51';
	s.prop61 = s.eVar61 = s_prop61;
	s.prop62 = s.eVar62 = s_prop62;
	s.prop63 = s.eVar63 = s_prop63;
	s.tl(true, 'o', 'mouseover');
};
function trackReelClick(reporting_pageName, link_analytics_prop55,
		link_analytics_prop58, link_analytics_prop59) {
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop54,prop55,prop58,prop59,prop60,eVar51,eVar54,eVar55,eVar58,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "Promo";
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop58 = s.eVar58 = link_analytics_prop58;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop59 + ':' + s.prop55 + ':' + s.prop58;
	s.tl(this, 'o', 'Promo Click');
};

function trackPromoClick(reporting_pageName, link_analytics_prop55,
		link_analytics_prop58, link_analytics_prop59){
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop54,prop55,prop58,prop59,prop60,eVar51,eVar54,eVar55,eVar58,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "Promo";
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop58 = s.eVar58 = link_analytics_prop58;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop59 + ':'+s.prop54+':' + s.prop55 + ':' + s.prop58;
//alert(s.prop60);
	s.tl(this, 'o', 'Promo Click'); 
};
		
		
function trackQuickClick(reporting_pageName, link_analytics_prop55,
		link_analytics_prop58, link_analytics_prop59) {
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop54,prop55,prop58,prop59,prop60,eVar51,eVar55,eVar58,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "Promo";
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop58 = s.eVar58 = link_analytics_prop58;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop59 + ':' + s.prop55 + ':' + s.prop58;
	s.tl(this, 'o', link_analytics_prop59 + ' Link Click');
};

function trackNonpromoClick(reporting_pageName, link_analytics_prop55, link_analytics_prop59) {
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop54,prop55,prop59,prop60,eVar51,eVar54,eVar55,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "link";
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop59 + ':'+s.prop54+':' + s.prop55;
	s.tl(this, 'o', 'Link Click'); 
};

function trackTabDocClicks(reporting_pageName, link_analytics_prop55, link_analytics_prop59) {
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop54,prop55,prop59,prop60,eVar55,eVar54,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "Tab";
	s.prop55 = s.eVar55 = link_analytics_prop55;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop54+':' +s.prop59 + ':' + s.prop55;
	s.tl(this, 'o', link_analytics_prop59 + ' Link Click');
};

function trackFeedClick(reporting_pageName, link_analytics_prop55,
		link_analytics_prop58, link_analytics_prop59){
	s = s_gi(omniture_account);
	s.linkTrackVars = 'prop51,prop54,prop55,prop58,prop59,prop60,eVar51,eVar54,eVar55,eVar58,eVar59,eVar60,events';
	s.linkTrackEvents = 'event52';
	s.events = 'event52';
	s.prop51 = s.eVar51 = reporting_pageName;
	s.prop54 = s.eVar54 = "Feed Link";
	s.prop55 = s.eVar55 = link_analytics_prop55.substring(0,149);
	s.prop58 = s.eVar58 = link_analytics_prop58;
	s.prop59 = s.eVar59 = link_analytics_prop59;
	s.prop60 = s.eVar60 = s.prop59 + ':'+s.prop54+':' + s.prop55 + ':' + s.prop58;
	s.tl(this, 'o', 'Feed Click'); 
};


