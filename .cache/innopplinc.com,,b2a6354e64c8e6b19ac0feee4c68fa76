<script>
jQuery(document).ready(function($) {

var file = "";
 
// Hubspot Form Submission 
	
	function submitToHubSpot(data) {
  var $form = $('#hubspot-form form'),
      k;
  for (k in data) {
    $form.find("input[name='" + k + "']").val(data[k]);
  }
 $form.submit();
 }

function validateEmail(email) { 
   
    var re = /^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(?!aaa)(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
    return re.test(email);
}

// Here's how you'd use this.
$('#my-custom-form').on('submit', function() {
 var email = $('#my-custom-form #email').val();

 if(email =='' || email == ' '){  
                    
             alert("Please enter email");
                $('#my-custom-form #email').css('border','1px solid red').focus();
                return false;
            }
            else if(validateEmail(email) == false){
              alert("Please enter valid email");
               // $('#main-msg').html('Please enter valid email');
               $('#my-custom-form #email').css('border','1px solid red').focus();
              return false;           
            }
   else{   
   	
  var formData = {};

  $(this).serializeArray().forEach(function(data) {
    formData[data.name] = data.value;
  });  
  console.log(file);
window.location = 'http://innopplinc.com/wp-content/themes/nouveau/download-pdf.php?file='+file;
// window.location ="http://innopplinc.com/whitepaper/download-pdf.php";
 alert('Thank you for downloading our Case Study'); 
 submitToHubSpot(formData);
 $('#my-custom-form #email').css({'border' : '1px solid #ddd'}).val("");
return false;
 }});
});
</script>
 
