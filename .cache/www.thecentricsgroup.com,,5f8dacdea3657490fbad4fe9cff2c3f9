<script type="text/javascript">
    jQueryCS(document).ready(function () {
        var newTwitMessage = 'Type in your message here...';
        jQueryCS('textarea.cs_messageField').maxlength({ 'feedback': '.cs_charsLeft' });
        if (jQueryCS('#dnn_ctr2481_CS_TwitterView_NewTwitTB').val() == newTwitMessage) {
            jQueryCS('.cs_charsLeft').html("0");
        }
    });
    
    function addNewTwitTextboxHandlers() {
        var newTwitMessage = 'Type in your message here...';
        jQueryCS('#dnn_ctr2481_CS_TwitterView_NewTwitTB').click(function () {
            if (this.value == newTwitMessage) {
                this.value = '';
            }
        });
    }
</script>
