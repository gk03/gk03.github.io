<script type="text/javascript">

     

     function validationFile1215(){
        var b = true;
         jQuery(".form_div_1215 input[type='file']").filter(".ajaxUpload").each(function(i){
            var fileid =  $(this).attr("id");
            jQuery.ajax({
            async:false,
            url: "/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx",
            cache: false,
            data: "ModuleID=1215&type=AjaxCustomUpload&fieldValue="+jQuery(this).val(),
            success: function(Result){
                        if(Result != "true")
                        {
                        jQuery(".form_div_1215 #png_upload_"+fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/no.png").show();
                        }else
                        {
                            //jQuery(".form_div_1215 #png_upload_"+ fileid).attr("src","/DesktopModules/DNNGo_PowerForms/Resource/images/yes.png").show();
                            jQuery(".form_div_1215 #png_upload_"+ fileid).hide();
                        }
                          if( Result != "true") {    b= false; } 
                }
            });
        });
        return b;
     }



    jQuery(function ($) {
        var sisForm = $(".form_div_1215").sisyphus({ customKeyPrefix: 'bg', timeout: 0, autoRelease: true });
        $("#Form").submit(function () {sisForm.manuallyReleaseData();});

        $("input.other_textbox").bind("keyup change",function(){
           var forid = "#" + $(this).data("forid");
           $(forid).val($(forid).data("value")+ $(this).val());
        });


        tinymce.init({
            selector: "textarea.tinymce",
            entity_encoding: "raw",
            convert_urls: false,
             menubar : false
        });

        
       
       $(".form_div_1215 input[lang='Submit']").click( function () { 

            if(!validationFile1215())
            {
                return false;
            }
            $(".form_div_1215 .group_title span").addClass("min").removeClass("max");
            $(".form_div_1215 div[class='group_inside']").show(function () {
               
            });
       });


        
    


        

           
    });
</script>

 
</div>

 <script type="text/javascript">
     jQuery(function (q) {
         q(".form_div_1215").validationEngine({
             promptPosition: 'topRight',
             ajaxFormValidationURL: '/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx?ModuleID=1215'
         });

         q(".form_div_1215 input[lang='Submit']").click(function () {

             if (!$('.form_div_1215').validationEngine('validate')) {
                 return false;
             }

             if($("#g-recaptcha-1215").is("div") )
             {
                 if ( !$("#g-recaptcha-1215").data("GRecaptcha"))
                 {
                     $("#g-recaptcha-msg-1215").show("fast",function(){
                         $(this).fadeOut(5000);
                     });
                     return false;
                 }
             }
             
             

         });

         q(".form_div_1215 input[lang='reset']").click(function () {
             $('.form_div_1215').validationEngine('hideAll');
         });

         q(".form_div_1215 .jquery-datepick").datepick({ dateFormat: 'mm/dd/yyyy' });
   






     });
</script>


