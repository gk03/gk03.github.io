                    function validateForm() {
                      var error_string = "";
                      if(document.mainForm.q.value == ""){
                        error_string += "Please Enter a Keyword.\n";
                      }

                      if(error_string == "") {
                        return true;
                      } else {
                        alert(error_string);
                        return false;
                      }
                    }
</script>
