    $(function () {

        $('.player').brightcoveVideo({
            'bgcolor': '',
            'playerID': '1156866486001',
            'playerKey': 'AQ~~,AAAABI8vj4E~,grBfPD3eLB1VEmxUKeFEV3Gcxde4xwve',
            'width': '396',
            'height': '282',
            'flashID': 'myExperience5067339025001',
            '@videoPlayer': '5067339025001',
            'videoID': '',
            'templateReadyHandler': onTemplateReady
        });

        var overlay = "<h3>CAREERS AT &nbsp;<span class=\"light-font\">BROADCAST MUSIC, INC.</span></h3>"
            + "<div class=class=\"overlay-container\"><div class=\"arrow-right\" /></div>";

        function onTemplateReady(event) {
            var $player = $(this);
            applyOverlay();
            $player.brightcoveVideo('onMediaEvent', 'COMPLETE', onMediaStopEvent);

        }

        var overlayObject;
        function applyOverlay() {
            $('.player').brightcoveVideo("overlay", overlay)
				.on("click", function () {
				    overlayObject = $(this);
				    $(this).fadeOut();
				    $('.player').brightcoveVideo('play');
				});
        }

        function onMediaStopEvent(event) {
            overlayObject.fadeIn();
        }
    });
</script>
