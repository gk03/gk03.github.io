<script type="text/javascript">
        function ValidaMail(e) {
   			 var exr = /^[0-9a-z_\-\.]+@[0-9a-z\-\.]+\.[a-z]{2,4}$/i;
    			return exr.test(e);
    				}
		function validarcorreo(){
			if(ValidaMail(document.getElementById('email').value)){
				return true;
			}else{
				alert ("Invalid Mail");
				return false;	
			}
		}
		
        </script>  <!-- end  footer --></div>
