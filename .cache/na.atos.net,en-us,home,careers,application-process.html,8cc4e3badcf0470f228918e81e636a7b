<script type="text/javascript">
var sUrlForGAAccept='/acceptCookies';
var sUrlForGADecline='/declineCookies';
$(document).ready(function() { 
    var is_chrome = navigator.userAgent.toLowerCase().indexOf('chrome') > -1;
    var  sHostName = window.location.hostname;
    var cookieid = 'CookiePolicy' + sHostName.replace(".","");
    var sessid1 =(is_chrome) ? $.Storage.get(cookieid): $.cookies.get(escape(cookieid));
    
    if(sessid1 == null){
        $('.cookieinfo').css('display','block');
    }
    
    function setCookieJS(name,value,expires){
        document.cookie=name+"="+value+";path=/"+((expires==null)?"":";expires="+expires.toGMTString())
    }
    
    function setCookie(event) 
    {      
        if( sessid1 == null)
             {          
                if(is_chrome) {
                    $.Storage.set(cookieid, 'acceptedCookie',{expires: 9999});
                } else { 
                    //$.cookies.set(cookieid, 'acceptedCookie',{expires: 9999});
                    //<![CDATA[
                    // create a value for the cookie
                    var cookieData= escape(cookieid);
                    // create a new Date object
                    var expiryDate=new Date();
                    // set the expiry date of the cookie to 100 days in the future: a persistent cookie
                    expiryDate.setTime(expiryDate.getTime() + (1000000*60*60*24*100));
                    // call the setCookie() function to create the cookie
                    setCookieJS(cookieData,cookieData,expiryDate);
                }
                sessid1 = 'acceptedCookie';
                $('.cookieinfo').hide();            
        }
        pageTracker._trackPageview(sUrlForGAAccept);
    }
    
    function declineCookie(event)
    {
        pageTracker._trackPageview(sUrlForGADecline);
    }
    $('#acceptCookie').click(setCookie);
    $('#declineCookie').click(declineCookie);    
});

</script>





