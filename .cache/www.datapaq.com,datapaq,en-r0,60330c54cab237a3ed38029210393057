<script language="javascript">
   function searchValidate(locale, searchInput, mode)
    {
     
		__CMS_PostbackFormBeenReset = true;
        //var SrchTxt = eval('document.all.' + searchInput + '.value');
        //document.getElementById(
        var SrchTxt = eval('document.getElementById(searchInput).value');
        
         if (SrchTxt != ''){
    
                 if(mode=="header")
                        document.forms[0].action = '/Datapaq/' + locale + '/Search/search.htm?mode=' + mode;
          
                else if (mode=="faq")
                document.forms[0].action = '/Datapaq/' + locale + '/Literature And Downloads/Faq/FaqSearch.htm?mode=' + mode + '&searchstring='+ SrchTxt;
             
        }
        else {
        return false;
        } 
    } 
    
    
</script>

