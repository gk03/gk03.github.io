<script type="text/javascript">

    //set global variables to use in all functions. 
    currentTitle = document.title;

    //withtout escaping, if there is an "&" in the title or URL, it, and everything after, will not get sent to the bookmark site	
    currentTitle = escape(currentTitle);

    favesTitle = unescape(document.title);

    currentURL = window.location.href;
    currentURL = escape(currentURL);

    function openNewWin() {
        var emailWin = window.open("/share/bookmark_mailit.aspx?pageurl=" + currentURL + "&title=" + currentTitle, "mailform", "resizable,scrollbars=no,location=no,width=350,height=325,top=450,left=425");
    }
    function bookmarkURL(bookmark) {

        var urlLength = currentURL.length;

        // fixes bug where if "#" is the last char of the URL, the title will not get passed. This line strips the "#". Occurs on a page where an href may just be a "#" like /events/webseminars.cfm after you click a "+" to expand listing.
        if (window.location.href.charAt(urlLength - 1) == '#') {
            currentURL = window.location.href.substring(0, urlLength - 1);
        }

        if (currentTitle == "") {
            currentTitle = "AspenTech Page";
        }

        switch (bookmark) {

            case 'delicious':
                var deliciousurl = "http://del.icio.us/post?url=" + currentURL + "&title=" + currentTitle + "&jump=no";
                document.getElementById("delicioushref").setAttribute("href", deliciousurl);
                break;
            case 'digg':
                var diggurl = "http://digg.com/submit?phase=2&url=" + currentURL + "&title=" + currentTitle;
                document.getElementById("digghref").setAttribute("href", diggurl);
                break;
            case 'google':
                var googleurl = "http://www.google.com/bookmarks/mark?op=edit&bkmk=" + currentURL + "&title=" + currentTitle;
                document.getElementById("googlehref").setAttribute("href", googleurl);
                break;
            case 'linkedin':
                var linkedinurl = "http://www.linkedin.com/shareArticle?mini=true&url=" + currentURL + "&title=" + currentTitle;
                document.getElementById("linkedinhref").setAttribute("href", linkedinurl);
                break;
            case 'twitter':
                var twitterurl = "http://twitter.com/home?" + "status=Reading " + currentTitle + " " + currentURL;
                document.getElementById("twitterhref").setAttribute("href", twitterurl);
                break;

            case 'facebook':
                var facebookurl = "http://www.facebook.com/sharer.php?u=" + currentURL + "&t=" + currentTitle;
                document.getElementById("facebookhref").setAttribute("href", facebookurl);
                break;

            case 'stumbleupon':
                var stumbleuponurl = "http://www.stumbleupon.com/submit?url=" + currentURL + "&title=" + currentTitle;
                document.getElementById("stumbleuponhref").setAttribute("href", stumbleuponurl);
                break;

            case 'reddit':
                var redditurl = "http://reddit.com/submit?url=" + currentURL + "&title=" + currentTitle;
                document.getElementById("reddithref").setAttribute("href", redditurl);
                break;

            case 'buzzup':
                var buzzupurl = "http://buzz.yahoo.com/buzz?targetUrl=" + currentURL + "&headline=" + currentTitle + "&src=" + currentTitle;
                document.getElementById("buzzuphref").setAttribute("href", buzzupurl);
                break;


            case 'msnlive':
                var msnliveurl = "https://favorites.live.com/quickadd.aspx?marklet=1&mkt=en-us&url=" + currentURL + "&title=" + currentTitle + "&top=1";

                document.getElementById("msnlivehref").setAttribute("href", msnliveurl);
                break;

            default:
                document.write("Unable to process link");
                break;
        }

        //                <a href="http://favorites.live.com/quickadd.aspx?marklet=1&mkt=en-us&url=http://www.aspentech.com&title=testingtitle" target="_blank">Live</a>
    }


    function addBookmark() {
        if (window.sidebar) { // firefox
            window.sidebar.addPanel(favesTitle, self.location, "");
        } else if (document.all) { //MSIE
            window.external.AddFavorite(self.location, favesTitle);
        } else {
            alert("Sorry, your browser doesn't support this function.");
        }
    }

    //Added by Gautam
    function clear_textbox() {
        if (document.getElementById('ctl00_main_ShareAndContactUs1_txtSearch').value == "Search AspenTech")
            document.getElementById('ctl00_main_ShareAndContactUs1_txtSearch').value = "";
    }
    function set_textbox() {
        if (document.getElementById('ctl00_main_ShareAndContactUs1_txtSearch').value == "")
            document.getElementById('ctl00_main_ShareAndContactUs1_txtSearch').value = "Search AspenTech";
    }

    // On Enter key press, trigger search click
    $(document).ready(function () {
        $("#ctl00_main_ShareAndContactUs1_txtSearch").keypress(function (event) {
            if (event.which == 13) {
                window.location = "/Search.aspx?searchtext=" + encodeURIComponent($('#ctl00_main_ShareAndContactUs1_txtSearch').val());
                return false;
            }
        });
    });

    function clickButton(e, buttonid) {
        var evt = e ? e : window.event;
        var bt = document.getElementById(buttonid);
        if (bt) {
            if (evt.keyCode == 13) {
                bt.click();
                return false;
            }
        }
    }
    //End
</script>

