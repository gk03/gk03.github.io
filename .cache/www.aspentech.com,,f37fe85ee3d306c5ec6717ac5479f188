    <script type="text/javascript">
        function updateMarket(position) {
            $("#content_left > div").each(function (index) {
                if (!$(this).hasClass("content_" + position)) {
                    $(this).css("display", "none");
                }
                else {
                    $(this).css("display", "block");
                }
            });

            $("#content_right a").each(function (index) {
                if (!$(this).hasClass("link_" + position)) {
                    $(this).removeClass("active");
                }
                else if (!$(this).hasClass("titleLink")) {
                    $(this).addClass("active");
                }
            });
        }

        $(document).ready(function () {
            $("#right_promo a").click(function () {
                if (window._trackEvent) {
                    _trackEvent("TContent", $(this).attr("href"), window.userId);
                }
                else if (window._gaq && _gaq.push) {
                    _gaq.push(["_trackEvent", "TContent", $(this).attr("href"), window.userId]);
                }
            });
        });
    </script>
