    //var JsonObj = JSON && JSON.parse(BannerData) || $.parseJSON(BannerData);
    var banners = JSON && JSON.parse(BannerData) || $.parseJSON(BannerData);

    $(document).ready(function () {
        if (getCookie("AckPrivacyNotice") == "") {
            $.getJSON(window.location.protocol+"//freegeoip.net/json/198.143.38.25?callback=?",
                function (json) {
                    //document.write("Country : ", json.country);
                    if (banners[json.country_code] != undefined) {
                        $('#LanguageBannerContainer').append("<a class='bannerlink' style='float:left;width:95%;' href='"+ banners[json.country_code].LinkUrl + "'>"+ banners[json.country_code].BannerContent +"</a><div style=\"float:right;cursor:pointer;\" OnClick=\"Fade();\">X</div><div style=\"clear:both;\"></div>");
                        LanguageInFade();
                    }
                }
            );
        }
    });
</script>
