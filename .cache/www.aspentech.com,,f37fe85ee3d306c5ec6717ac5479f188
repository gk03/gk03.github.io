<script type="text/javascript">
    function InFade() {
        $('#BannerContainer').animate({ 'max-height': '150', 'padding-top': '0.5em', 'padding-right': '0.5em', 'padding-bottom': '0.5em', 'padding-left': '0.5em' }, 750, function () { $('#BannerContainer').animate({ opacity: '1' }, 500); });
    }

    function FadeAndSetCookie() {
        var d = new Date();
        d.setTime(d.getTime() + (90 * 24 * 60 * 60 * 1000));
        document.cookie = "AckPrivacyNotice=Y; expires="+ d.toUTCString() +"; path=/";
        $('#BannerContainer').animate({ opacity: '0' }, 500, function () { $('#BannerContainer').animate({ 'max-height': '0', 'padding-top': '0.5em', 'padding-right': '0.5em', 'padding-bottom': '0.5em', 'padding-left': '0.5em' }, 500, function () { $('#BannerContainer').remove(); }); });
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
        }
        return "";
    }
    var JsonCountries;
    $(document).ready(function () {
        //var JsonObj = JSON && JSON.parse(BannerData) || $.parseJSON(BannerData);
        JsonCountries = JSON && JSON.parse(BannerCountries) || $.parseJSON(BannerCountries);
        if (getCookie("AckPrivacyNotice") == "") {
            $.getJSON(window.location.protocol+"//freegeoip.net/json/198.143.38.25?callback=?",
                function (json) {
                    //alert("Country : " + json.country_code + " and index is " + JsonCountries.indexOf(json.country_code));
                    //countrycode = json.country_code;
                    if (JsonCountries.indexOf(json.country_code) > -1) { 
                        InFade();
                    }
                    if (json.country_code == "CN") {
                        showBing();
                    }
                }
            );
        }
    });
</script>
