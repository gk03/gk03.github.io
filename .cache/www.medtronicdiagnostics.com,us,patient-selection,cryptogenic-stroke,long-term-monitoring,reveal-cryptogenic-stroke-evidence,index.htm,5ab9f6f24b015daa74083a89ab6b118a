	<script type="text/javascript">
	$(document).ready(function(){
	var hFooterSiteInfoNavGlobal
	// Get heights of blocks at bottom of screen and make room for #nav-global
	function navPositioning(){
		if (Modernizr.mq('only all and (max-width: 767px)')) {
			// target the relevant DIVs
			var elSiteInfo = $("#site-info");
			var elFooter = $("#footer");
			var elNuevoFooter = $("footer");
			var elNavGlobal = $("#nav-global");
			var elContainer = $("#container");
			var hSiteInfo = elSiteInfo.height();
			var hFooter = elFooter.height();
			var hNeuvoFooter = elNuevoFooter.height();
			var hFooterSiteInfo = hFooter + hSiteInfo+ hNeuvoFooter ;
			hFooterSiteInfoNavGlobal = elNavGlobal.height() + hFooterSiteInfo;
			// take action
			hFooterSiteInfo +=10;
			$("#nav-global").css("bottom",hFooterSiteInfo+"px");

			//$("footer").css("bottom",hNeuvoFooter+"px");

			/*get height of subnav too */
			var subNavHeight = $('#nav-local').height();
			hFooterSiteInfoNavGlobal += subNavHeight + 95;
			$("#container").css("padding-bottom",hFooterSiteInfoNavGlobal+"px");
			$('#nav-global li a.selected').after($( "#nav-local" ));
			$('#column1').hide();
			}
		else{
			$('#column1').show();
			$('#column1').prepend($( "#nav-local" ));
			$("#container").css("padding-bottom","0");
			}
	};

		navPositioning();
		// Adjust heights when window gets resized
		$(window).resize(function(){
		navPositioning();
	});

	});

	</script><![endif]-->
