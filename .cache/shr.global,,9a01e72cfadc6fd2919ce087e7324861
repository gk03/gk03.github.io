piAId = '73042';
piCId = '1684';
 
(function() {
                function async_load(){
                                var s = document.createElement('script'); s.type = 'text/javascript';
                                s.src = ('https:' == document.location.protocol ? 'https://pi' : 'http://cdn') + '.pardot.com/pd.js';
                                var c = document.getElementsByTagName('script')[0]; c.parentNode.insertBefore(s, c);
                }
                if(window.attachEvent) { window.attachEvent('onload', async_load); }
                else { window.addEventListener('load', async_load, false); }
})();
</script>

<script>
jQuery().ready(function($) {

	$('.viewall').click(function(e) {
		e.preventDefault();

//		$('.additional').slideToggle().toggleClass('hidden').toggleClass('shown');
		$('.additional').slideToggle().toggleClass('hidden');
//		$('.viewall').slideToggle().toggleClass('hidden');
		var text = $('.viewall').text();
		if ( text == 'View all testimonials' ) {
			$('.viewall').text('View less');
		} else {
			$('.viewall').text('View all testimonials');
		}
		
	});
	
    $('a.open-nav').on( 'click', function() {
        $('#header').addClass('show-nav');
        return false
    });
    $('a.close-nav').on( 'click', function() {
        $('#header').removeClass('show-nav');
        return false
    });

	$('.lightbox-ajax').magnificPopup({
		type:'ajax',
		midClick: true, // Allow opening popup on middle mouse click. Always set it to true if you don't provide alternative source in href.
		callbacks: {
		  updateStatus: function(data) {
				setTimeout(function(){ $('iframe').iFrameResize(); }, 300);
		  }
		}
	});
	
});

jQuery(window).load(function() {

	jQuery('iframe').iFrameResize();
	
//	jQuery.getScript('http://rsc.shr.global/shr/js/vimeo.ga.min.js');

});
</script>
