<script src="http://insurancethoughtleadership.com/wp-content/themes/insurancethoughtleadership/js/itl-api.js" type="text/javascript"></script>
<script>

jQuery(document).ready(function() {
	
                       //add this when you want to add the lead box again
//	jQuery('.photo-courtesy').next().next().next().after('<section class="bio grid-15-padded"><table><tr><td width="85%"> <b>Want to connect with a thought leader on this topic?</b></td><td width="15%"><a type="button"class="btn btn-primary" id="question-button" href="#" onclick="overlay(); return false;">Ask Question</a></td></tr></table></section>');
                       
                       //add this when you want the lead box to popup again
//                       setTimeout(
//                                  function()
//                                  {
//                                  overlay();
//                                  }, 60000);
                       
                       
                       jQuery(".btn-question").click(function(event) {
                                                     event.preventDefault();
                                                     
                                                     
                                                     
                                    
                                                     
                                                     
                                                     var data = new Object();
                                                     data.name = jQuery.trim(jQuery("#q-name").val());
                                                     data.email = jQuery("#q-email").val();
                                                     data.phone = jQuery("#q-phone").val();
                                                     data.company = jQuery("#q-company").val();
                                                     data.job_title = jQuery("#q-job-title").val();
                                                     
                                                     data.inquiry = jQuery("#q-inquiry").val();
                                                     
                                                     data.article = jQuery("#article-title").text();
                                                     data.article_url = jQuery('link[rel="canonical"]').attr("href");
                                                     
                                                     
                                                     jQuery(".error-box").text( "" );
                                                     
                                                     if(data.name.length == 0){
                                                     jQuery(".error-box").append( "Name field is required" );
                                                     return false;
                                                     }else{
                                                     if ((data.name.match(/ /g) || []).length != 1){
                                                     jQuery(".error-box").append( "Enter first AND last name" );
                                                     return false;
                                                     }
                                                     }
                                                     
                                                     if(data.email.length == 0){
                                                     jQuery(".error-box").append( "Email field is required" );
                                                     return false;
                                                     }else{
                                                     var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
                                                                                   if (pattern.test(data.email)){
                                                                                   
                                                                                   }else{
                                                                                   jQuery(".error-box").append( "Email is not valid" );
                                                                                   return false;
                                                                                   }
                                                                                   }
                                                                                   
                                                                                   if(data.phone.length == 0){
                                                                                   jQuery(".error-box").append( "Phone field is required" );
                                                                                   return false;
                                                                                   }else{
                                                                                   if (!jQuery.isNumeric(data.phone)){
                                                                                   jQuery(".error-box").append( "Phone must only contain numbers" );
                                                                                   return false;
                                                                                   }
                                                                                   }
                                                                                   
                                                                                   if(data.company.length == 0){
                                                                                   jQuery(".error-box").append( "Company field is required" );
                                                                                   return false;
                                                                                   }
                                                                                   
                                                                                   if(data.job_title.length == 0){
                                                                                   jQuery(".error-box").append( "Job Title field is required" );
                                                                                   return false;
                                                                                   }
                                                                                   
                                                                                   if(data.inquiry.length == 0){
                                                                                   jQuery(".error-box").append( "Message field is required" );
                                                                                   return false;
                                                                                   }
                                                                                   
                                                                                   itlapi.postQuestion(itlapi.postQuestionCallback, data); 
                                                                                   
                                                                                   
                                                                                   return false; 
                                                                                   });
                                                                                  
                                                                                  
                                                                                  
                                                                                  });
                                                                                 
                                                                                 function overlay() {
                                                                                 
                                                                                 el = document.getElementById("overlay");
                                                                                 el.style.visibility = (el.style.visibility == "visible") ? "hidden" : "visible";
                                                                                 
                                                                                 }
                                                                                 
                                                                                 </script>

