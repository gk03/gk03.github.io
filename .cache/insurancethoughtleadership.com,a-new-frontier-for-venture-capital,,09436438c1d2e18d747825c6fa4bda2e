<script language="javascript">




jQuery(document).ready(function(e) {
	var val = '';
	var cname = "wp11674";
	var name = cname + "=";
    var ca = document.cookie.split(';');

    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) val = c.substring(name.length,c.length);
    }

    if (val.indexOf(':') === -1){
    	var ival = '';
    	var icname = "itl_last";
    	var iname = icname + "=";
    	var ica = document.cookie.split(';');

    	for(var j=0; j<ica.length; j++) {
        	var ic = ica[j];
        	while (ic.charAt(0)==' ') ic = ic.substring(1);
        	if (ic.indexOf(iname) == 0) ival = ic.substring(iname.length,ic.length);
        }

        var now = new Date();
        var ddnow = now.getDate();
        var mmnow = now.getMonth()+1; //January is 0!
        var hnow = now.getHours();
        var yyyynow = now.getFullYear();

        if(ddnow<10){
        	ddnow='0'+ddnow
        } 

        if(mmnow<10){
        	mmnow='0'+mmnow
        } 

        var nowstr = ddnow+'-'+mmnow+'-'+yyyynow+':'+hnow;
        if (ival != nowstr){
			var today = new Date();
			var dd = today.getDate();
			var mm = today.getMonth()+1; //January is 0!
			var h = today.getHours();
			var yyyy = today.getFullYear();

        	if(dd<10){
        		dd='0'+dd
        	} 

        	if(mm<10){
        		mm='0'+mm
        	} 

        	var todaystr = dd+'-'+mm+'-'+yyyy+':'+h;
        	var d = new Date();
        	d.setTime(d.getTime() + (1*24*60*60*1000));
        	var expires = "expires="+d.toUTCString();
        	document.cookie = "itl_last" + "="+todaystr+"; " + expires;
                       
                       
            if(typeof window.orientation !== 'undefined'){
                
            }else{
                DisplayFrom();
            }


    
        }
    }
});

