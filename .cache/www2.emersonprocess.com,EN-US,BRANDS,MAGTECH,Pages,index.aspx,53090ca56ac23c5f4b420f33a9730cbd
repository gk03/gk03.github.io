{
    var initInfo = {editable: false,isEditMode: false,allowWebPartAdder: false,listId: "{30db37ca-a67e-44f3-b621-5f3c1640c660}",itemId: 6,recycleBinEnabled: true,enableMinorVersioning: true,enableModeration: true,forceCheckout: true,rootFolderUrl: "\u002fen-US\u002fbrands\u002fMagtech\u002fPages",itemPermissions:{High:16,Low:196673}};
