<!--Force Lowercase Code-->
<script language = "JavaScript">
$(document).ready(function() {
// Begin: this function is to make the href attribute value from combination of upper and lower cases into lower case only
// excluding youtube href value
var alink = document.getElementsByTagName("a");
	for (var i = alink.length - 1; i >= 0; i--)
	{
		var strHref = alink[i].href;
		var matched = strHref.indexOf("youtube");
		var matched2 = strHref.indexOf("youtu");
		var matched3 = strHref.indexOf("liveperson");
		if(matched != -1) {
			alink[i].href = alink[i].href;
		} 
		else if(matched2 != -1) {
			alink[i].href = alink[i].href;
		}
		else if(matched3 != -1) {
			alink[i].href = alink[i].href;
		}
		else
		{
			alink[i].href = strHref.toLowerCase();
		}	
	}
// End: 
});
</script>
<script>
$(document).ready(function() {
	$(".EmrsnFindItFastDropdownSelection").removeAttr("href");
});
</script>
