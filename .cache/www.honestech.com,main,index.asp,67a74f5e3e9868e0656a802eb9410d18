<script language="javascript" type="text/javascript">
function pop_scroll()
	{               
		interval = setInterval("pop_view()", 5000);                      
	}

	function pop_view()
	{
		var id = "";
		var maxcnt = $("#divRollBanner div").length;
		var nowRoll = $("#nowRollBanner").val();
		for(j = 1; j <= maxcnt; j++)
		{
			id = "divRoll"+ j;                             
			$("#"+id).hide();
		}
		$("#divRoll"+nowRoll).show();

		if (nowRoll == maxcnt)
			$("#nowRollBanner").val("1");
		else
		{
			var nextRoll = parseInt(nowRoll);
			$("#nowRollBanner").val(nextRoll+1);		
		}
	}	

	pop_scroll();
</script>
