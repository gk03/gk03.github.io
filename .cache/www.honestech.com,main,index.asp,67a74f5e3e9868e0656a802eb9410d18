		function showimage() 
		{
			if (i_strngth <= 110) 
			{
				testimage.innerHTML="<img id='img"+i_image+"'  onfocus='this.blur();' style='filter:alpha(opacity="+i_strngth+")' src="+imageurl[i_image]+" border=0>";
				i_strngth = i_strngth + 10;
				var timer=setTimeout("showimage()",100);
			}
			else 
			{
				clearTimeout(timer);
				var timer=setTimeout("hideimage()",5000);
			}

			if(document.layers) 
			{
				alert(2);
				clearTimeout(timer)
				document.testimage.document.write("<img src="+imageurl[i_image]+" border=0>")
				document.close()
				i_image++
				if (i_image >= imageurl.length) {i_image=0}
				var timer=setTimeout("showimage()",100)
			}

			if(typeof(document.getElementById('img'+i_image))  =="object" ) 
			{
				var img = document.getElementById('img'+i_image);
				img.useMap ="#m00"+(i_image+1);
			}
		}


		function hideimage() 
		{
			if (i_strngth >= -10) 
			{
				testimage.innerHTML="<img style='filter:alpha(opacity="+i_strngth+")' src="+imageurl[i_image]+" border=0>";
				i_strngth = i_strngth - 17;
				var timer = setTimeout("hideimage()",50);
			}
			else 
			{
				clearTimeout(timer);
				i_image++;
				if (i_image >= imageurl.length)
					i_image = 0;
				i_strngth=1;
				var timer=setTimeout("showimage()",15);
			}
		}
