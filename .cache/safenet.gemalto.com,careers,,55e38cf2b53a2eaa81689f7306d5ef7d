    $(function () {
        // Center nav-sections...
        $(".nav-1.nav-submenu > .nav-section").each(function () {
            var newLeft = (($(this).parent("li").outerWidth() / 2) - $(this).outerWidth() / 2);
            $(this).css({ "left": newLeft + "px" });
        });
    });
