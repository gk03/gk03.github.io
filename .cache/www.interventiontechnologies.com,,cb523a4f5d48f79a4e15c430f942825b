<SCRIPT language=JavaScript type=text/javascript><!--
			delay = 60;
			imgs = new Array();
			counter = new Array();
			peelingup = new Array();
			peelingdown = new Array();
			currentsection = 'news'
			initializing = true;
			lastover = '';
			oldsection = 'news';

			function preloadmenu(section) {
				if (document.images) {
					imgs[section] = new Array();
					for (i=0; i < 6; i++) {
						imgs[section][i] = new Image();
						imgs[section][i].src = "images/" + section + i + ".gif";
					}
					counter[section] = 1;
				}
			}


			preloadmenu('faq');



			peelingup['faq'] = 1;


			if (currentsection) {
				peelingup[currentsection] = 0;
				peelingdown[currentsection] = 1;

			}

			function startpeelup(section) {
				if (section != currentsection) {
					startpeeldown(lastover);
					lastover = section;
					peelingup[section] = 1;
					peelingdown[section] = 0;
					peelup(section);
				}
			}

			function startpeeldown(section) {
				if (section != currentsection) {
					peelingdown[section] = 1;
					peelingup[section] = 0;
					peeldown(section);
				}
			}

			function peelup(section) {
				if (section != currentsection) {
					if (! peelingdown[section]) {
						if (counter[section] < 5) {
							document.images[section].src = imgs[section][counter[section]].src;
							if (counter[section] < 4) {
								counter[section] = counter[section] + 1;
								setTimeout("peelup('" + section + "')",delay);
							}
						}
					}
				}
			}

			function peeldown(section) {
				if (section != currentsection) {
					if (! peelingup[section]) {
						if (counter[section] > -1) {
							document.images[section].src = imgs[section][counter[section]].src;
							if (counter[section] > 0) {
								counter[section] = counter[section] - 1;
								setTimeout("peeldown('" + section + "')",delay);
							}
						}
					}
				}
			}



		// -->
		</SCRIPT>

