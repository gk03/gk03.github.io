<SCRIPT language=javascript><!--

			function slide() {
				var block = document.blockDiv;
				block.xpos = parseInt(document.blockDiv.left);
				block.ypos = parseInt(document.blockDiv.top);
				if (block.ypos > 10) {
				block.ypos -= 4
				document.blockDiv.top = block.ypos
				setTimeout("slide()",30);
			}
				else if (block.ypos == 10) {
					show('staticform')
				}
			}

			function slideAway() {
				var block = document.blockDiv;
				block.xpos = parseInt(document.blockDiv.left);
				block.ypos = parseInt(document.blockDiv.top);
				if (block.ypos < 50) {
					block.ypos += 4
					document.blockDiv.top = block.ypos
					setTimeout("slideAway()",30);
				}
				else if (block.ypos == 50) {
					show('backdropdown');
					startpeeldown('mailinglist');
					setsection(oldsection);
				}
			}

