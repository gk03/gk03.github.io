<script>
$(function(){
	$('#toggle-left-nav').click(function(){
		btn = $(this);
		$('#left-nav').slideToggle("slow",function(){
			if($(this).is(':visible')){
				btn.find('.fa').removeClass('fa-angle-right').addClass('fa-angle-down');
			}else{
				btn.find('.fa').removeClass('fa-angle-down').addClass('fa-angle-right');
			}
		});
	});
});
</script>			</div>		
