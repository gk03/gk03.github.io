        $(document).ready(function () {

            setTimeout(function() {
                $("#subscribeBar").fadeIn(1000);

                $.ajax({
                    url: '/Home/CloseUnsubscribe',
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8'
                });
            }, 3000);
            
            $("#closeSubscribeBar").click(function() {

                $.ajax({
                    url: '/Home/CloseUnsubscribe',
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8'
                });

                $("#subscribeBar").hide();
            });
        });
