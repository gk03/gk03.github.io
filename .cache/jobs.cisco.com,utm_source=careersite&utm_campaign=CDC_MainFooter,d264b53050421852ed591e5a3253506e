    <script type="text/javascript">
        //<![CDATA[
        $(function () {
            // Initialize modal.
            $('[id*=Modal],[id*=modal]').modal({
                show: false
            });

            $('a[href*=Modal],a[href*=modal]').click(function () {
                var
                        target = $(this).attr('href'),
                        $thisModal = $(target + '.modal'),
                        $iframe = $thisModal.find('iframe');

                function iframeSRC(autoStart) {
                    if (autoStart == 'autoStartTrue') {
                        var src = $iframe.attr('src').replace('autoStart=false', 'autoStart=true');
                    }
                    else if (autoStart == 'autoStartFalse') {
                        var src = $iframe.attr('src').replace('autoStart=true', 'autoStart=false');
                    }
                    $iframe.attr('src', src);
                }

                $thisModal.on({
                    'show': function () {
                        iframeSRC('autoStartTrue');

                        var modalHeight = $(this).outerHeight();
                        var modalWidth = $(this).outerWidth();
                        $(this).css({
                            'position': 'fixed',
                            'top': '50%',
                            'left': '50%',
                            'margin-top': (-(modalHeight / 2)),
                            'margin-left': (-(modalWidth / 2))
                        });
                    },
                    'hide': function () {
                        iframeSRC('autoStartFalse');
                    }
                });
            });
        });
        //]]>
    </script>
