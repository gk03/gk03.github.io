            //<![CDATA[
            // Modules Height
            $(window).load(function () {
                var
                        $a = $('.modules.b'),
                        aHeight = $a.height(),

                        $b = $('.modules.b > .module > .inner'),
                        bPadding = parseInt($b.css('padding-top')) + parseInt($b.css('padding-bottom')),
                        bHeight = aHeight - bPadding;

                $b.each(function () {
                    $(this).height(bHeight);
                });
            });

