	<script type="text/javascript">
		var saveUnRegAgentCallback;
		var saveUnRegAgentCallback2;

		function displayUnRegJobAlertForm() {
			var div = document.getElementById("divToShowHide").value;
			var authenticated = "false";
			if (authenticated == "true") {
				document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
				document.NAVSUMMARY.submit();
			} else {
				if (document.getElementById(div).style.display == "none") {
					document.getElementById(div).style.display = "inline-block";
					if (div == "unRegisteredJobAlertSignUp") {
						document.getElementById("unRegEmailAddress").focus();
					}
				} else {
					document.getElementById(div).style.display = "none";
				}
			}
		}

		function displayUnRegJobAlertForm2() {
			var div = document.getElementById("divToShowHide2").value;
			var authenticated = "false";
			if (authenticated == "true") {
				//document.getElementById("SAVESEARCH2").value = "Save as a Search Agent";
				document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
				document.NAVSUMMARY.submit();
			} else {
				if (document.getElementById(div).style.display == "none") {
					document.getElementById(div).style.display = "inline-block";
					if (div == "unRegisteredJobAlertSignUp2") {
						document.getElementById("unRegEmailAddress2").focus();
					}
				} else {
					document.getElementById(div).style.display = "none";
				}
			}
		}

		function saveUnRegSearchAgent() {
			//perform Ajax call
			var emailAddress = document.getElementById("unRegEmailAddress").value;
			var query = "N%3D0%26Ntk%3DJobSearchRanking%26Ntt%3Dh3-technologies%26Ntx%3Dmode%2Bmatchall%26Nty%3D1%26Nf%3Dp_PostedAge%7CLTEQ+30";
			var form_data = "%2Fjobsearch%2Fservlet%2FJobSearch%3Fop%3D300%26rel_code%3D1102%26N%3D0%26Hf%3D0%26NUM_PER_PAGE%3D30%26Ntk%3DJobSearchRanking%26Ntx%3Dmode%2Bmatchall%26AREA_CODES%3D%26AC_COUNTRY%3D1525%26QUICK%3D1%26ZIPCODE%3D%26RADIUS%3D64.37376%26ZC_COUNTRY%3D0%26COUNTRY%3D1525%26STAT_PROV%3D0%26METRO_AREA%3D33.78715899%252C-84.39164034%26TRAVEL%3D0%26TAXTERM%3D0%26SORTSPEC%3D0%26FRMT%3D0%26DAYSBACK%3D30%26LOCATION_OPTION%3D2%26FREE_TEXT%3Dh3-technologies%26WHERE%3D%26SEARCH.x%3D40%26SEARCH.y%3D9";

			if (emailAddress == null || emailAddress == "") {
				alert("Please provide a valid email address");
				document.getElementById("unRegEmailAddress").select();
				return false;
			}

			var x = "?op=331&email=" + emailAddress + "&query=" + query + "&form_data=" + form_data;
			submitAsyncRequest('GET',
					'/jobsearch/servlet/JobSearch' + x,
					saveUnRegAgentCallback,
					"");
		}

		function saveUnRegSearchAgent2() {
			//perform Ajax call
			var emailAddress = document.getElementById("unRegEmailAddress2").value;
			var query = "N%3D0%26Ntk%3DJobSearchRanking%26Ntt%3Dh3-technologies%26Ntx%3Dmode%2Bmatchall%26Nty%3D1%26Nf%3Dp_PostedAge%7CLTEQ+30";
			var form_data = "%2Fjobsearch%2Fservlet%2FJobSearch%3Fop%3D300%26rel_code%3D1102%26N%3D0%26Hf%3D0%26NUM_PER_PAGE%3D30%26Ntk%3DJobSearchRanking%26Ntx%3Dmode%2Bmatchall%26AREA_CODES%3D%26AC_COUNTRY%3D1525%26QUICK%3D1%26ZIPCODE%3D%26RADIUS%3D64.37376%26ZC_COUNTRY%3D0%26COUNTRY%3D1525%26STAT_PROV%3D0%26METRO_AREA%3D33.78715899%252C-84.39164034%26TRAVEL%3D0%26TAXTERM%3D0%26SORTSPEC%3D0%26FRMT%3D0%26DAYSBACK%3D30%26LOCATION_OPTION%3D2%26FREE_TEXT%3Dh3-technologies%26WHERE%3D%26SEARCH.x%3D40%26SEARCH.y%3D9";

			if (emailAddress == null || emailAddress == "") {
				alert("Please provide a valid email address");
				document.getElementById("unRegEmailAddress").select();
				return false;
			}

			var x = "?op=331&email=" + emailAddress + "&query=" + query + "&form_data=" + form_data;
			submitAsyncRequest('GET',
					'/jobsearch/servlet/JobSearch' + x,
					saveUnRegAgentCallback2,
					"");
		}

		saveUnRegAgentCallback = {
			cache:false,
			success: function(o) {
				if (o.responseXML) {
					var returnCode = o.responseXML.getElementsByTagName("returnCode")[0].firstChild.nodeValue;
					var innerHTML;
					if (returnCode == "0") {
						document.getElementById("existingDiceAccount").style.display = "inline-block";
						document.getElementById("unRegisteredJobAlertSignUp").style.display = "none";
						document.getElementById("getEmailUpdatesLink").style.display = "none";
						document.getElementById("divToShowHide").value = "existingDiceAccount";
					} else if (returnCode == "1") {
						document.getElementById("confirmationJobAlert").style.display = "inline-block";
						document.getElementById("unRegisteredJobAlertSignUp").style.display = "none";
						document.getElementById("divToShowHide").value = "confirmationJobAlert";
					} else if (returnCode == "2") {
						document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
						document.NAVSUMMARY.submit();
					} else if (returnCode == "3") {
						document.getElementById("maxSearchAgentDiv").style.display = "inline-block";
						document.getElementById("unRegisteredJobAlertSignUp").style.display = "none";
						document.getElementById("divToShowHide").value = "maxSearchAgentDiv";
					} else if (returnCode == "4") {
						alert("Please provide a valid email address");
						document.getElementById("unRegEmailAddress").select();
					}
				}
				wait.hide();
			},
			failure: function(o) {
				wait.hide();
			}
		};

	saveUnRegAgentCallback2 = {
			cache:false,
			success: function(o) {
				if (o.responseXML) {
					var returnCode = o.responseXML.getElementsByTagName("returnCode")[0].firstChild.nodeValue;
					var innerHTML;
					if (returnCode == "0") {
						document.getElementById("existingDiceAccount2").style.display = "inline-block";
						document.getElementById("unRegisteredJobAlertSignUp2").style.display = "none";
						document.getElementById("getEmailUpdatesLink2").style.display = "none";
						document.getElementById("divToShowHide2").value = "existingDiceAccount2";
					} else if (returnCode == "1") {
						document.getElementById("confirmationJobAlert2").style.display = "inline-block";
						document.getElementById("unRegisteredJobAlertSignUp2").style.display = "none";
						document.getElementById("divToShowHide2").value = "confirmationJobAlert2";
					} else if (returnCode == "2") {
						document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
						document.NAVSUMMARY.submit();
