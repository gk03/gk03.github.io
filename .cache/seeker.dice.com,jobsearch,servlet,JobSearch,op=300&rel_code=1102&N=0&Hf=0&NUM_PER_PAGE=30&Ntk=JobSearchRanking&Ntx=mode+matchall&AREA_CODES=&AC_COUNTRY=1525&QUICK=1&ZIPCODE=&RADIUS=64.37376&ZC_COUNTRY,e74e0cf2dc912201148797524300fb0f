<script type="text/javascript">
    jQuery.noConflict();
    jQuery(document).ready(function($) {
        $(".aboutDiceDU").click(function(e){
            e.preventDefault();
            $("fieldset#ftMenu_1").toggle();
            $(".aboutDiceDU").toggleClass("menu-open");
        });
        $("fieldset#ftMenu_1").mouseup(function() {
            return false
        });
        $(document).mouseup(function(e){
            if($(e.target).parent(this).hasClass("menu-open") || $(e.target).hasClass("menu-open")){
         		//do nothing, the regular toggle will fire.
            }else{
            	$(".aboutDiceDU").removeClass("menu-open");
                $("fieldset#ftMenu_1").hide();
            }
        });
        $(".relatedSitesDU").click(function(e) {
            e.preventDefault();
            $("fieldset#ftMenu_2").toggle();
            $(".relatedSitesDU").toggleClass("menu-open");
        });
        $("fieldset#ftMenu_2").mouseup(function() {
            return false
        });
        $(document).mouseup(function(e) {
            if($(e.target).parent(this).hasClass("menu-open") || $(e.target).hasClass("menu-open")){
         		//do nothing, the regular toggle will fire.
            }else{
                $(".relatedSitesDU").removeClass("menu-open");
                $("fieldset#ftMenu_2").hide();
            }
        });
    });
</script>
