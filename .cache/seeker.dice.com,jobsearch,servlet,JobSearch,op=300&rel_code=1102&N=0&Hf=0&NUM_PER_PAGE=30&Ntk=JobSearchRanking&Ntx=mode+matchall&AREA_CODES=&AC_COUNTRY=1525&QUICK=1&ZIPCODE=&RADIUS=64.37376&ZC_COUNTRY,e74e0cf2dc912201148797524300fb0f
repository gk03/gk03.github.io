<script type="text/javascript">
    //jQuery.noConflict();
    jQuery(document).ready(function($) {
        $(".myDiceDD").click(function(e) {
            e.preventDefault();
            $("fieldset#menu_1").toggle();
            $(".myDiceDD").toggleClass("menu-open");
        });
        $("fieldset#menu_1").mouseup(function() {
            return false
        });
        $(document).mouseup(function(e) {
            if($(e.target).parent(this).hasClass("menu-open") || $(e.target).hasClass("menu-open")){
                //do nothing, the regular toggle will fire.
            }else{
                $(".myDiceDD").removeClass("menu-open");
                $("fieldset#menu_1").hide();
            }
        });
        $(".talentCommDD").click(function(e) {
            e.preventDefault();
            $("fieldset#menu_2").toggle();
            $(".talentCommDD").toggleClass("menu-open");
        });
        $("fieldset#menu_2").mouseup(function() {
            return false
        });
        $(document).mouseup(function(e) {
            if($(e.target).parent(this).hasClass("menu-open") || $(e.target).hasClass("menu-open")){
                //do nothing, the regular toggle will fire.
            }else{
                $(".talentCommDD").removeClass("menu-open");
                $("fieldset#menu_2").hide();
            }
        });
        $(".techNewsDD").click(function(e) {
            e.preventDefault();
            $("fieldset#menu_3").toggle();
            $(".techNewsDD").toggleClass("menu-open");
        });
        $("fieldset#menu_3").mouseup(function() {
            return false
        });
        $(document).mouseup(function(e) {
            if($(e.target).parent(this).hasClass("menu-open") || $(e.target).hasClass("menu-open")){
                //do nothing, the regular toggle will fire.
            }else{
                $(".techNewsDD").removeClass("menu-open");
                $("fieldset#menu_3").hide();
            }
        });
    });
</script>

	
