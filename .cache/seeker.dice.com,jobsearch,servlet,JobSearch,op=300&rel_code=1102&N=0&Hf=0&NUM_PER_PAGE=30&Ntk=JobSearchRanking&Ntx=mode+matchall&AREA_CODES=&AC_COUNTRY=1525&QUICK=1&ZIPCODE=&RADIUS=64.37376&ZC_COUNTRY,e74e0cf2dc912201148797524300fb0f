<script type="text/javascript">
	var submitAsyncRequest;
	var wait;

function joinNetwork(diceID) {
	var joinNetworkHref = "/jobsearch/servlet/JobSearch?op=327&diceID=" + diceID + "&joinNetwork=1&fromNavSum=1";
	setJoinNetworkHref(joinNetworkHref);
}

function reloadRegister() {
	document.getElementById("registerRemSw").value = '1';
	document.NAVSUMMARY.submit();
	return true;
}

function showHiddenJobs() {
	document.location.href = document.location.href + '&showall=1';
	return false;
}

function oldHackHideSomething(id){
	var hide = document.getElementById(id);
	var header = document.getElementById(id + '_header');
	if(hide.style.display == 'none'){
		hide.style.display = 'block';
		header.style.backgroundImage = "url(/assets/stylesheets/images/ICO16px_hideRemoveV2.gif)";
	} else {
		hide.style.display = 'none';
		header.style.backgroundImage = "url(/assets/stylesheets/images/ICO16px_showAddV2.gif)";
	}
}

function expandNavigation(id) {
	
	document.getElementById(id).style.display = 'block';
	document.getElementById(id + '_header').style.backgroundImage = "url(/assets/stylesheets/images/ICO16px_hideRemoveV2.gif)";
	document.NAVSUMMARY.EXPANDED_NE.value = id;
	return true;
}

function collapseNavigation(id) {
	document.getElementById(id).style.display = 'none';
	document.getElementById(id + '_header').style.backgroundImage = "url(/assets/stylesheets/images/ICO16px_showAddV2.gif)";
	document.NAVSUMMARY.EXPANDED_NE.value = '';
	return true;
}

function toggleNavigationState(id){
	if (document.NAVSUMMARY.EXPANDED_NE.value == '') {
		expandNavigation(id);
	} else if (document.NAVSUMMARY.EXPANDED_NE.value == id) {
		collapseNavigation(id);
	} else {
		collapseNavigation(document.NAVSUMMARY.EXPANDED_NE.value);
		expandNavigation(id);
	}
	return true;
}



// Grab the email option values from the URL and replace them with user's selections before executing the link.
function changeEmailOptionsInURL(href) {


	// Always submit the form even if we're not editing a search agent so that the dimension value changes will be reflected.
	document.location.href = href;
	return false;
}

var init = function () {
	function onButtonClick2(p_oEvent) {
		if(this.get("id") == "unRegJobAlertExisting" ){
			document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
			document.NAVSUMMARY.submit();
		}
		if(this.get("id") == "unRegJobAlertExisting2" ){
			document.getElementById("SAVESEARCH").value = "Save as a Search Agent";
			document.NAVSUMMARY.submit();
		}
		if (this.get("id") == "unRegJobAlertRegister") {
			var seekerDomain = "http://www.dice.com";
			var registerURL  = "/regman/register.html"
			var registrationURL = seekerDomain + registerURL;
			window.location = registrationURL;
		}
		if (this.get("id") == "unRegJobAlertRegister2") {
			var seekerDomain2 = "http://www.dice.com";
			var registerURL2  = "/regman/register.html"
			var registrationURL2 = seekerDomain2 + registerURL2;
			window.location = registrationURL2;
		}
		if (this.get("id") == "manageSearchAgents") {
			var seekerDomain = "http://www.dice.com";
			var searchAgentURL  = "/profman/servlet/ProfMan?op=3003"
			var searchAgentManagement = seekerDomain + searchAgentURL;
			window.location = searchAgentManagement;
		}
		if (this.get("id") == "manageSearchAgents2") {
			var seekerDomain = "http://www.dice.com";
			var searchAgentURL  = "/profman/servlet/ProfMan?op=3003"
			var searchAgentManagement = seekerDomain + searchAgentURL;
			window.location = searchAgentManagement;
		}
	}

	wait = new YAHOO.widget.Panel("wait",
        { width:"240px",
            fixedcenter:true,
            close:false,
            draggable:false,
            zindex:4,
            modal:true,
            visible:false
        });

        wait.setHeader("Please wait");
        wait.setBody('<img src="/assets/images/progress.gif" />');
        wait.render(document.body);

	// Grab the email option values from the form and replace them with user's selections before submitting.
	function changeEmailOptionsOnForm(p_oEvent) {
	

		document.NAVSUMMARY.submit();
	}

	YAHOO.util.Event.onContentReady("navSummSearch", function () {
		var oSubmitButton1 = new YAHOO.widget.Button("search", { value: "Search",  onclick: { fn: changeEmailOptionsOnForm } });
		var oSubmitButton2 = new YAHOO.widget.Button("searchAg", { value: "Save as a Search Agent",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton3 = new YAHOO.widget.Button("saveUnRegSearchAgent", { value: "Save Search Agent", onclick: { fn: saveUnRegSearchAgent} } );
		var oSubmitButton4 = new YAHOO.widget.Button("unRegJobAlertExisting", { value: "Login to save your search agent",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton5 = new YAHOO.widget.Button("unRegJobAlertRegister", { value: "Register with Dice",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton6 = new YAHOO.widget.Button("manageSearchAgents", { value: "Manage Search Agent",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton7 = new YAHOO.widget.Button("unRegJobAlertExisting2", { value: "Login to save your search agent",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton8 = new YAHOO.widget.Button("unRegJobAlertRegister2", { value: "Register with Dice",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton9 = new YAHOO.widget.Button("manageSearchAgents2", { value: "Manage Search Agent",  onclick: { fn: onButtonClick2 } });
		var oSubmitButton10 = new YAHOO.widget.Button("saveUnRegSearchAgent2", { value: "Save Search Agent", onclick: { fn: saveUnRegSearchAgent2} } );
		var oLinkButton1 = new YAHOO.widget.Button("RSSfeed");
	});



	
submitAsyncRequest = function(type, url, callback, postData) {
	wait.show();

	if (postData == "") {
		postData = 'random=' + generateRandom();
	}
	else {
		postData += '&random=' + generateRandom();
	}
	if (type == 'POST') {
		YAHOO.util.Connect.asyncRequest(type, url, callback, postData);
	}
	else {
		YAHOO.util.Connect.asyncRequest(type, url, callback);
	}
}

	function generateRandom() {
	var d = new Date();
	var bigNumber = " " + d.getYear() + d.getDay() + d.getHours() + d.getMinutes() + d.getSeconds() + d.getMilliseconds();
	return Math.floor(Math.random() * bigNumber);
}

	// Catch the click event and determine if we've clicked outside of the report or rate candidate panels.
	// If so, hide the panel.
	document.onclick=checkClick;
	function checkClick(e) {
		var obj;
		var target = (e && e.target) || (event && event.srcElement);

		if (target.id == "getEmailUpdatesAnchor") {
			if (document.getElementById("getEmailUpdatesLink").className == "SApanelActive" ) {
				document.getElementById("getEmailUpdatesLink").className = "SApanelInactive";
			} else {
				document.getElementById("getEmailUpdatesLink").className = "SApanelActive";
			}
		} else if (target.id == "getEmailUpdatesAnchor2") {
			if (document.getElementById("getEmailUpdatesLink2").className == "SApanelActive" ) {
				document.getElementById("getEmailUpdatesLink2").className = "SApanelInactive";
			} else {
				document.getElementById("getEmailUpdatesLink2").className = "SApanelActive";
			}
		} else {
			if (document.getElementById("unRegisteredJobAlertSignUp").style.display == "inline-block") {
				obj = "unRegisteredJobAlertSignUp";
			}
			if (document.getElementById("confirmationJobAlert").style.display == "inline-block") {
				obj = "confirmationJobAlert";
			}
			if (document.getElementById("existingDiceAccount").style.display == "inline-block") {
				obj = "existingDiceAccount";
			}
			if (document.getElementById("maxSearchAgentDiv").style.display == "inline-block") {
				obj = "maxSearchAgentDiv";
			}
			if (document.getElementById("unRegisteredJobAlertSignUp2").style.display == "inline-block") {
				obj = "unRegisteredJobAlertSignUp2";
			}
			if (document.getElementById("confirmationJobAlert2").style.display == "inline-block") {
				obj = "confirmationJobAlert2";
			}
			if (document.getElementById("existingDiceAccount2").style.display == "inline-block") {
				obj = "existingDiceAccount2";
			}
			if (document.getElementById("maxSearchAgentDiv2").style.display == "inline-block") {
				obj = "maxSearchAgentDiv2";
			}

			if (checkParent(target, obj)) {
				closeDiv(obj);
			}
		}
	}

	function closeDiv(obj) {
		if (obj!=undefined) {
			document.getElementById(obj).style.display = "none";
			document.getElementById("getEmailUpdatesLink").className = "SApanelInactive";
			document.getElementById("getEmailUpdatesLink2").className = "SApanelInactive";
		}
	}

	// Used by checkClick method to crawl up parental elements from the one clicked looking for the panel div name.
	function checkParent(target, panelDiv) {
		if (document.getElementById(panelDiv) != null) {
		}
		while(target.parentNode) {
			if (target == document.getElementById(panelDiv)) {
				return false;
			}
			target = target.parentNode;
		}
		return true;
	}
} ();
</script>

