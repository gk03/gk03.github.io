	imagearray: [
		['homeslide1.jpg', ' '], //["image_path", "optional description"]
		['homeslide2.jpg', ' '] //<--no trailing comma after very last image element!
	],
	displaymode: {type:'auto', pause:7000, cycles:99, stoponclick:false, pauseonmouseover:true},
	navbuttons: ['left.png', 'right.png', 'up.gif', 'down.gif'], // path to nav images
	activeslideclass: 'selectedslide', // CSS class that gets added to currently shown DIV slide
	orientation: 'h', //Valid values: "h" or "v"
	persist: true, //remember last viewed slide and recall within same session?
	slideduration: 500 //transition duration (milliseconds)
})

