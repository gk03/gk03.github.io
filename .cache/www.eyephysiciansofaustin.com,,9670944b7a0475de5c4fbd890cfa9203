<script style="javascript/text">



	jQuery(window).load(function(){



		var is_close = readCookie('isclose');



		if(!is_close==1){



			jQuery('#overlay_div').fadeIn('slow');



			jQuery('#popup_image').fadeIn('slow');



		}



		jQuery('#overlay_div').click(function(){



				writeCookie('isclose',1,'');



				jQuery('#popup_image').fadeOut('slow');



				jQuery('#overlay_div').fadeOut('slow');



			  });



		jQuery('#close').click(function(){



				writeCookie('isclose',1,'');



				jQuery('#popup_image').fadeOut('slow');



				jQuery('#overlay_div').fadeOut('slow');



			  });



	});



</script>



