<script  type="text/javascript">

    $(document).ready(function() {


      $("ul.menu-footer li:first-child").addClass("menu-footer-first");


      /*contact form fix v2*/
      if ($('select[name="subject"]').html() !== 'undefined') {

        /*hide real destination combo*/
        $('select.split_hidden').parent().parent().hide();
        
        /*get GET param*/
        param = $('input[name="hidden_subject"]').val();
        
        /*
         * 
         * parse options in select (fake select & real select):
         * 
         * */

        /*from select to be used from GET param:*/
        $('select[name="subject"] option').each(function(ix, e) {

          /*get Text & Val */
          op = $(e).text().split('*'); 
          
          /*save param in option for on(change) event*/
          $(e).attr('related', op[1]); 
          
          /*put cleared text & value in fake select*/
          $(e).text(op[0]).val(op[1]); 

          /*option matches with GET param?*/
          if (param === op[1]) {
            /*set "selected" for visible(fake) & hidden select fields*/
            $('select[name="subject_email"] option[value="' + op[1] + '"]').prop('selected', true);
            $('select[name="subject"] option[value="' + op[1] + '"]').prop('selected', true);
          }
        });

        /*
         * 
         * end parsing
         * 
         * */

        /*when user change the visible select*/
        $('select[name="subject"]').on('change', function() {
          $('select[name="subject_email"] option[value="' + $('option:selected', this).attr('related') + '"]').prop('selected', true);
        });


      }
    });

  </script>
