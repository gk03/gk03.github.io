        // required: first name, last name, company, email, phone
        if (frm.FirstName.value == ''
            || frm.LastName.value == ''
            || frm.Company.value == ''
            || frm.Email.value == ''
            || frm.Phone.value == '') {
            alert('Please complete all required fields.');
            return false;
        }

        var re = /^(\S)+@(\S)+\.(\S)+$/;
        if (!re.test(frm.Email.value)) {
            alert('Please include a valid email address.');
            return false;
        }

        for (i = 0; i < frm.elements.length; i++) {
            sVal = frm.elements[i].value;
            sVal = sVal.toLowerCase();
            if (sVal.indexOf('http://') > -1 && frm.elements[i].name != 'redirect') {
                alert('Please do not include hyperlinks.');
                return false;
            }
