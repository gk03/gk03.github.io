	<script>
		// HOME PAGE SCROLLERS
		// ===================
		// News Scroller
		$(function() {
			$(".verticalScroller").scrollable({ vertical: true});
		});
        // REMOVING THE GAP-AFTER ISSUE
        $(function() {
            var thisCarousel = $('.verticalScroller');  			// var this scroller
            var size = 2; 											// how many should display before the scrolling stops
            var newsitemCount = $('.verticalScroller ul li').length;// get number of items in this scroller
            var thisNext = $(thisCarousel).parent().find('a.next'); // var this next controller
            if (newsitemCount < (size + 1)) { 						// hide next controller if less than size
                thisNext.addClass('disabled');
            }
            $(thisCarousel).scrollable();    
            var scrollable = $(thisCarousel).data("scrollable");
            scrollable.onSeek(function () {
                if (this.getIndex() >= this.getSize() - size) {
                    thisNext.addClass('disabled');
                }
            });
            scrollable.onBeforeSeek(function (event, index) {
                if (this.getIndex() >= this.getSize() - size) {
                    if (index > this.getIndex()) {
                        return false;
                    }
                }
            });
        });
    </script>
