		$(window).load(function() {
				// Hide the video
				$('.nivo-nextNav, .nivo-prevNav').click(function() {
					$('#hiddenVideo').removeClass('ieActive');
					$('#hiddenVideo').animate({opacity: 0},1000);
					$('#hiddenVideo').css({'z-index': 0});
					$('.nivo-controlNav').css({display:'block'});
					$('#iFramedVideo').fadeOut(400);
					//var player=$f($('#iFramedVideo')[0]);
					//player.api('pause');
					vimeoWrap = $('#centerVideoCont');
					vimeoWrap.html( vimeoWrap.html() );
				});
				
				// show the video
				$('a#showVideo').click(function() {
					$('#hiddenVideo').addClass('ieActive');
					$('#hiddenVideo').animate({opacity: 1, width:"100%"},1000);
					$('#hiddenVideo').css({'z-index': 500,width:"100%"});
					$('.nivo-controlNav').css({display:'none'});
					$('#iFramedVideo').show();
				});
				$('#iFramedVideo').hide();
		});	
	</script>     


