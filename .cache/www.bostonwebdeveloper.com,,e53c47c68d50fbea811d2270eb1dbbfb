    <!-- jCarousel skin stylesheet -->
    <link rel="stylesheet" type="text/css" href="css/skin.css" />

    <style type="text/css">

        /**
         * Additional styles for the controls.
         */
        .jcarousel-control {
	        margin-bottom: 10px;
	        text-align: center;
	        padding: 0 0 0 400px;
        }

        .jcarousel-control a { float:left;
            font-size:1px; color:#fff;text-indent:15px;_text-indent:-5px;
            text-decoration: none;
            padding: 0 5px;
            margin: 5px 0 5px 0;
            border:0;
            background: url(img/jcontrol.jpg) no-repeat;height:17px; width:17px; display:block;
        }

        .jcarousel-control a:focus, .jcarousel-control a:active {
	        outline: none;
	        background:url(img/jcontrol_sel.jpg) no-repeat;
	        text-decoration: none;
        }

        .jcarousel-scroll {
        width:880px; margin-left:10px;
	        text-align: center;
        }

        .jcarousel-scroll form {
            margin: 0;
            padding: 0;
        }

        .jcarousel-scroll select {
            font-size: 75%;
        }

        #mycarousel-next{
            cursor: pointer;
            font-size:1px; line-height:1px;display:block; width:23px; height:23px;background:url(img/next-horizontal.jpg) no-repeat; float:right;
	        background:url(img/next-horizontal.jpg) no-repeat;
        }

        #mycarousel-prev {
            cursor: pointer;
	        font-size:1px; line-height:1px;display:block; width:23px; height:23px;background:url(img/prev-horizontal.jpg) no-repeat;
	         float:left
        }

    </style>

    <script type="text/javascript">
        /**
        * We use the initCallback callback
        * to assign functionality to the controls
        */
        function mycarousel_initCallback(carousel) {
            jQuery('.jcarousel-control a').bind('click', function () {
                carousel.scroll(jQuery.jcarousel.intval(jQuery(this).text()));
                return false;
            });

            jQuery('.jcarousel-scroll select').bind('change', function () {
                carousel.options.scroll = jQuery.jcarousel.intval(this.options[this.selectedIndex].value);
                return false;
            });

            jQuery('#mycarousel-next').bind('click', function () {
                carousel.next();
                return false;
            });

            jQuery('#mycarousel-prev').bind('click', function () {
                carousel.prev();
                return false;
            });
        };

        // Ride the carousel...
        jQuery(document).ready(function () {
            jQuery("#mycarousel").jcarousel({
                scroll: 3,
                initCallback: mycarousel_initCallback,
                // This tells jCarousel NOT to autobuild prev/next buttons
                buttonNextHTML: null,
                buttonPrevHTML: null
            });
        });

    </script>

