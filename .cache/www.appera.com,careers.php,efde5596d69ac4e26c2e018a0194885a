<script>
function setresume()
{
var companyname=document.resupload.comname.value;
var filename=document.resupload.reup.value;
var yemail=document.resupload.emailid.value;
var error="";
var errorc="";
var errormail="";
var errorresume="";
if(companyname=="")
{
error+="<br/>Please Select Title";
errorc="Please Select Title";
}
if(filename=="")
{
error+="<br/>Please Upload Your Resume";
errorresume="Please Upload Your Resume";

} 
if(yemail.length==0)
{
error+="<br/>Please enter valid Email Address";
errormail="Please enter valid Email Address";
}
else
{
if((yemail.indexOf('@')==-1))
{
error+="<br/>Please enter valid Email Address";
errormail="Please enter valid Email Address";
}
else
{
if(yemail.indexOf('.')==-1)
{
error+="<br/>Please enter valid Email Address";
errormail="Please enter valid Email Address";
}
else if((yemail.substr(yemail.lastIndexOf('.'))).length>4)
			{
			  error+="<br/>Please enter valid Email Address";
			  errormail="Please enter valid Email Address";
			}
}
}



if(error=="")
{
document.resupload.submit();
}
else
{
document.getElementById("error").style.display="none";
document.getElementById("error").innerHTML= error;
document.getElementById("scr").style.display="block";
document.getElementById("scr").innerHTML= errorc;
document.getElementById("ur").style.display="block";
document.getElementById("ur").innerHTML= errorresume;
document.getElementById("mailerror").style.display="block";
document.getElementById("mailerror").innerHTML= errormail;

return false;
} 
}
</script>
<!-- InstanceEndEditable -->
