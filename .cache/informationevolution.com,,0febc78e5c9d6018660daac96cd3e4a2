		<script type="text/javascript" charset="utf-8">

			// INTIALIZE CLIENT LOGO FADER
			function faderSwitch() {
				var $active = $('.custom.home #homeclient_fader IMG.active');
	
				if ( $active.length == 0 ) $active = $('.custom.home #homeclient_fader IMG:last');
			
				var $next =  $active.next().length ? $active.next()
					: $('.custom.home #homeclient_fader IMG:first');
			
				$active.addClass('last-active');
			
				$next.css({opacity: 0.0})
					.addClass('active')
					.animate({opacity: 1.0}, 1000, function() {
						$active.removeClass('active last-active');
					});
			}
			
			$(document).ready(function(){
				$('#homeslider_area').loopedSlider({
					containerClick: false,
					container: '.homeslider_container',
					slides: '.homeslider_slides',
					autoStart: 20000,
					slidespeed: 200,
					addPagination: true,
					fadespeed: 0
				});	
				$('.pagination').wrap('<div class="pagination_area" />');

				// Fader and Record Counters
				$(function() {
					setInterval( "faderSwitch()", 5000 );
				});
								//alert('11115000:11/14/2013:4341:1125:5712:163');
				var nowDate = new Date();
				var startDate = new Date('11/14/2013');
				var msPerDay=1000*60*60*24;
				var msSinceStart = nowDate.getTime() - startDate.getTime();
				var daysSinceStart = msSinceStart / msPerDay;
				var recordsPerDay = parseInt(4341) + parseInt(1125) + parseInt(5712) + parseInt(163);
				
				//alert(nowDate.toString() + ':' + startDate.toString() + ':' + msPerDay.toString() + ':' + msSinceStart.toString() + ':' + daysSinceStart.toString());
				//alert(recordsPerDay.toString());
				
				var recordsNow = Math.round(parseInt(11115000) + (recordsPerDay * daysSinceStart));
				//alert(recordsNow.toString());			
				
				var recordsPerSec = recordsPerDay/24/60/60;	
				//alert(recordsPerSec.toString());
				
				var delayTime = Math.round(1000/recordsPerSec);	
				//alert(delayTime.toString());
			
				$('.home-record-counter').jOdometer({increment: 1, delayTime: delayTime, counterStart:recordsNow.toString(), numbersImage:'http://informationevolution.com/wp-content/themes/thesis_185/custom/images/counter_numbers_rev2.png', formatNumber: true, spaceNumbers: 0, widthDot: 7, heightNumber: 65, widthNumber: 26 });
			});			
		</script>
			<script type="text/javascript" charset="utf-8">
			
		// page height adjustments
		$(window).load(function(){
			// adjust height of page to match content
							var blogAreaTop = $('#homeblogarea').css('top');
				blogAreaTop = parseInt(blogAreaTop.substring(0, blogAreaTop.length-2));
				
				var blogAreaHeight = parseInt($('#homeblogarea').height());
				
				var clientAreaTop = $('#homeclientarea').css('top');
				clientAreaTop = parseInt(clientAreaTop.substring(0, clientAreaTop.length-2));
				
				var clientAreaHeight = parseInt($('#homeclientarea').height());					
				//alert(blogAreaTop+":"+blogAreaHeight+":"+clientAreaTop+":"+clientAreaHeight);
				
				var newClientAreaTop = (512 + blogAreaHeight + 30);					
				//alert(newClientAreaTop.toString());			
				$('#homeclientarea').css('top', newClientAreaTop.toString() +'px' );
				var newClientAreaBottom = newClientAreaTop + clientAreaHeight;
				//alert(newClientAreaBottom.toString());			
				
				var rsbAdAreaTop = $("#homersbadspace").css('top');
				rsbAdAreaTop = parseInt(rsbAdAreaTop.substring(0, rsbAdAreaTop.length-2));
				//alert(rsbAdAreaTop.toString());			
				var rsbAdAreaHeight = parseInt($('#homersbadspace').height());
				var rsbAdAreaBottom = rsbAdAreaTop + rsbAdAreaHeight;
				//alert(rsbAdAreaBottom.toString());			
				
				var newFooterTop = Math.max((Math.max(rsbAdAreaBottom, newClientAreaBottom) + 40), 1200);
									
				//alert(newFooterTop.toString());
				$('#footer').css('top', newFooterTop.toString() +'px' );
				
						});		
		
		$(document).ready(function(){
			// email subscription form
			$("#es_txt_name_pg").attr("placeholder","Name");
			$("#es_txt_email_pg").attr("placeholder","Email Address");
			$("#es_msg_pg").text("Never miss an update.");
			$("#es_txt_button_pg").val("Subscribe now");
		});		

	</script>
	
	<script type="text/javascript" charset="utf-8">
		// IMAGE ROLLOVER FUNCTION
		$(function() {
			$('img[data-hover]').hover(function() { 
				$(this).attr('tmp', $(this).attr('src')).attr('src', $(this).attr('data-hover')).attr('data-hover', $(this).attr('tmp')).removeAttr('tmp');
			}).each(function() {
				$('<img />').attr('src', $(this).attr('data-hover'));
			});;
		});
	</script>
