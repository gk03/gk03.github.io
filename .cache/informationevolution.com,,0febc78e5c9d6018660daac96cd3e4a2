<div id="homersbcalculator">
<script language="javascript" type="text/javascript">
function CalculatorApiClass(psApiObjectName) {
	var mTypeClassName = "CalculatorApiClass";
	var mApiObjectName = psApiObjectName;
	var mApiObject = null;
	var mInitialized = false;
	var mRequestInSubmit = false;
	var mRequestHandlerForm = null;
	var mRequestHandlerFrame = null;
	
	this.isInitialized = function() {
		return mInitialized;
	};

	this.initialize = function () {
		try {
			mInitialized = true;

			mApiObject = eval(mApiObjectName);

			// Request Handler
			mRequestHandlerForm = $("#xcCalculatorForm");
			mRequestHandlerFrameId = "xcCalculatorFrame";
			mRequestHandlerFrame = $("#" + mRequestHandlerFrameId);
			
			// setup the action handlers
			mRequestHandlerForm.submit(
            function (event) {
            	var el = $(this);
            	mApiObject.onRequestHandlerSubmit(event, el);
            });

			mRequestHandlerFrame.load(
            function (event) {
            	var el = $(this);
            	mApiObject.onRequestHandlerFrameLoad();
            });
			
			// set the watermarks
			$('#txtNorec').watermark('Enter number of records');
			$('#txtFields').watermark('Enter number of fields');
			
		} catch (e) {
			this.display(mTypeClassName, e, "initialize");
		}
	};
	
	this.onRequestHandlerSubmit = function () {
		try {
			//alert("debug: onRequestHandlerSubmit");

			mRequestInSubmit = true;

		} catch (e) {
			this.display(mTypeClassName, e, "onRequestHandlerSubmit");
		}
	};

	this.onRequestHandlerFrameLoad = function () {
		try {
			//alert("debug: onRequestHandlerFrameLoad");

			if (!mRequestInSubmit) {
				return;
			}

			// process upload post results
			//alert("debug: onFrameLoad - Processing Results");

			var frameDoc = this.getFrameDocument(mRequestHandlerFrameId);
			var data = $(frameDoc.body).text();
			//alert(data);
			
			$("#xcCalcResultsText").text(data);
			$("#xcCalcMessage").show();

			// clean up
			mRequestInSubmit = false;


		} catch (e) {
			this.display(mTypeClassName, e, "onRequestHandlerFrameLoad");
		}
	};

	this.display = function(psMessage,poError,psFunction) {
		var info = "";
		info = poError.message;
		info = " (" + psFunction + ") " + info;
		alert(psMessage + ": " + info);
	};
	
	this.getFrameDocument = function(psFrameName) {
		try {
			var frameName = psFrameName;
			var frameEl = document.getElementById(frameName);
			if (frameEl) {
				if (frameEl.contentWindow) {
					return frameEl.contentWindow.document;
				} else {
					//frameName = frameEl.getAttribute("name");
					//frameName = psFrameName;
				}
			}
			
			if (document.frames) {
				var frame = document.frames[frameName];
				if (frame) {
					return frame.document;
				}
			}
	
		} catch (e) {
		}
		return null;
	};

	this.validateForm = function() {
		
		var calcRecords = document.getElementById("txtNorec").value;
		
		if(calcRecords == "") {
			alert("Number of Records is Missing.");
			document.getElementById("txtNorec").focus();
			return false;
		} else if (!this.isNumeric(calcRecords)) {
			alert("Number of Records is Invalid.");
			document.getElementById("txtNorec").focus();
			return false;
		} else if (parseFloat(calcRecords) > 100000) {
			if (confirm('Please use our inquiry form for projects with more than 100K records.')) {
				window.location = "/contact/inquiry-form/";
				return false;
			}
		}
		
		var calcFields = document.getElementById("txtFields").value;
		if(calcFields == "") {
			alert("Fields per Record is Missing.");
			document.getElementById("txtFields").focus();
			return false;
		} else if (!this.isNumeric(calcFields)) {
			alert("Fields per Record is Invalid.");
			document.getElementById("txtFields").focus();
			return false;
		}
		
		var calcMethod = $("input[name=xcCalcMethod]:checked");
		if (calcMethod.val() == undefined || calcMethod.val() == "") {
			alert("Updating Methodology selection is Missing.");
			return false;
		}
		
		return true;
	};
	
	this.isNumeric = function(n) {
  		return !isNaN(parseFloat(n)) && isFinite(n);
	};

	
	return this;
};

// jquery document onload function
$(document).ready(function() { fnDocumentOnReady(); });
var gCalculatorApi = null;
function fnDocumentOnReady() {
	try {
		// setup the controllers
		gCalculatorApi = new CalculatorApiClass("gCalculatorApi");
		gCalculatorApi.initialize();

	} catch (e) {
		//
	}
}

</script>
