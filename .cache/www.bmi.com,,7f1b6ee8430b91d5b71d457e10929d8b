  	// load sharethis and set options on success
		$.getScript("https://ws.sharethis.com/button/buttons.js")
			.done(function() {
				stLight.options({publisher: "5dd426b3-e4c8-4713-b04d-67d9918fd66a", doNotHash: true, doNotCopy: false, hashAddressBar: false});
