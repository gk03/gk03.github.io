<script>
	$(function() {
		var base_url = 'https://custlogin.audiovideoweb.com/';
		$('.login-with-form').magnificPopup({
			type: 'inline',
			mainClass: 'box ui-widget',
			preloader: false,

			// When elemened is focused, some mobile browsers in some cases zoom in
			// It looks not nice, so we disable it:
			callbacks: {			
				beforeOpen: function() {
		    		$("#login-form").hide();
					$("#login-res").html("Please Wait...");
		    		$("#login-res").show();	

		    		var mag = this;

			        $.ajax({
			          url: base_url + "cred/check",
			          type: "GET",
			          dataType: "json",
					  xhrFields: {
					  	withCredentials: true
					  },
			          success: function(json) {
			          	if (json.logged_in) 
			          	{
							if (top != self) 
							{
								top.location = base_url + 'base';	    
							}
							else
							{
								window.location = base_url + 'base';
							}               
						}
						else
						{
				    		$("#login-form").show();
				    		$("#login-res").hide();	

							if($(window).width() < 700) {
								mag.st.focus = false;
							} else {
								mag.st.focus = '#login-username';
							}				    		
						}
			          },
			          error: function(xhr, status, errorThrown) {
			            /*
			            alert( "Sorry, there was a problem!" );
			            console.log( "Error: " + errorThrown );
			            console.log( "Status: " + status );
			            console.dir( xhr );
			            */
			          }
			        }); 
				},
				close: function() {
					$("#login-res").hide();
					$("#login-form").show();
					$("#login-form")[0].reset();
				}
			}
		});

	    $('#login-btn').button({
	      label: 'Login'
	    }).click(function() {
	        //$('#login-form').submit();
	    }); 		

    	$('#login-form').submit(function(e) {
    		e.preventDefault();
    		
    		if ($.trim($('#login-username').val()) == '') {
    			alert('Please enter your username.');
    			return;
    		}

    		if ($.trim($('#login-pass').val()) == '') {
    			alert('Please enter your password.');
    			return;
    		}    		

 			var formdata = $(this).serialize();
    		$("#login-form").hide();
			$("#login-res").html("Please Wait...");
    		$("#login-res").show();	

	        $.ajax({
	          url: base_url + "cred/login",
	          type: "POST",
	          data: formdata,
	          dataType: "json",
			  xhrFields: {
			  	withCredentials: true
			  },	          
	          success: function(json) {
	          	if (!json.success) {
	          		if (json.error) {
						$("#login-res").html('<div class="ui-state-error ui-corner-all">' + json.error + '</div>');
						$("#login-res").show();
						$("#login-form").show();    			
	          		}
	          		return;
	          	}

				if (top != self) 
				{
					top.location = base_url + 'base';	    
				}
				else
				{
					window.location = base_url + 'base';
				}               	
	          },
	          error: function(xhr, status, errorThrown) {
	            /*
	            alert( "Sorry, there was a problem!" );
	            console.log( "Error: " + errorThrown );
	            console.log( "Status: " + status );
	            console.dir( xhr );
	            */
	          }
	        });    		
    	});
	});
</script></body>
</html>