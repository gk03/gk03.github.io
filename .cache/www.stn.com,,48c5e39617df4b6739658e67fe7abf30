var whichp = 'index';
var currimg = '';
function domouseover(butelemi) {
 butelemi.style.backgroundColor = '#adadc1'; }
function domouseout(butelemo) {
 butelemo.style.backgroundColor = '#70708d'; }
function dofocus(tdelemidi) {
 document.getElementById(tdelemidi).style.borderColor = '#ffffff'; }
 function doblur(tdelemido) {
 document.getElementById(tdelemido).style.borderColor = '#adadc1'; }
 
function applyImageTransition (obj, imgsrc)
{
    obj.filters(0).Apply();
    obj.src=imgsrc;
    obj.filters(0).Play();
}
function fftest(imggid,spcamnt) {
var shouldbe = 'images/' + imggid;
var newwid = spcamnt + 'px';
if((navigator.appName == "Microsoft Internet Explorer") && ((currimg != imggid) && (currimg != ''))) { myimg.style.filter='progid:DXImageTransform.Microsoft.Slide(duration=0.3,slidestyle=push)'; applyImageTransition(myimg,shouldbe); } 
else {document.getElementById('myimg').src = shouldbe; }
currimg = imggid;
document.getElementById('spacertd').style.width = newwid; 
}
function PrintTheFrame() { 
thefr.focus(); 
thefr.print(); 
}

