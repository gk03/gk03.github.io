    <script type="text/javascript">
        var metaslider_78 = function($) {
            $('#metaslider_78').addClass('flexslider'); // theme/plugin conflict avoidance
            $('#metaslider_78').flexslider({ 
                slideshowSpeed:5000,
                animation:"fade",
                controlNav:true,
                directionNav:true,
                pauseOnHover:true,
                direction:"horizontal",
                reverse:false,
                animationSpeed:600,
                prevText:"&lt;",
                nextText:"&gt;",
                slideshow:true,
                init: function(slider) {
                                $('#metaslider_78 .msHtmlOverlay').each(function() {
                $(this).css('display', 'block');
            });
                },
                before: function(slider) {
                    $('li:not(".flex-active-slide") .animated', slider).not('.disabled').each(function(index) {
                        var el = $(this);
                        var cloned = el.clone();
                        el.before(cloned);
                        $(this).remove();
                    });
                }
            });            
            $(window).resize(function(){
               $('#metaslider_78').metaslider_scale_layers({
                   downscale_only: false,
                   orig_width: 1198
               });
            });
            $('#metaslider_78').metaslider_scale_layers({
                downscale_only: false,
                orig_width: 1198
            });

            $('#metaslider_78 .msHtmlOverlay .layer[data-link], #metaslider_78 .msHtmlOverlay[data-link]').each(function() {
                var layer = $(this);

                layer.css('cursor', 'pointer').on('click', function(e) {
                    if( ! $(e.target).closest('a').length) {
                        window.open(layer.attr('data-link'), layer.attr('data-target'));
                    }
                 });
            });

            $('#metaslider_78 .msHtmlOverlay').each(function() {
                $(this).css('display', 'block');
            });
        };
        var timer_metaslider_78 = function() {
            var slider = !window.jQuery ? window.setTimeout(timer_metaslider_78, 100) : !jQuery.isReady ? window.setTimeout(timer_metaslider_78, 1) : metaslider_78(window.jQuery);
        };
        timer_metaslider_78();
    </script>
