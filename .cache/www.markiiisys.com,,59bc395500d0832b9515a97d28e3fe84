<!--Create active state of secondary navigation (Primary Nav Controlled through CSS)-->    
<script>
jQuery(document).ready( function(){
    var thispage = location.pathname.substring(2);
    //document.write(thispage);
    $('.sub-menu ul li a[href*="'+ thispage + '"]').parent().addClass('active');
	$('.side-nav ul li a[href*="'+ thispage + '"]').parent().addClass('current');
});
</script>

<script>
$(document).ready(function() {
   var pathname = window.location.pathname;
   var pathname = pathname.split('/');
   var tester = pathname[pathname.length-2];
   
   $('.sub-menu ul li a').each(function(){
       var test = $(this).attr('href');
       if (test == tester){
           $(this).parent().addClass('active');
       }
   });
});
</script>

