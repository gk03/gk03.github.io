<script>
  var SideSlides_Total = 3;
  var SideSlides_Current = 1;  
  var SideSlides_Delay = 6000;  
  var SideSlides_Fade = 300;  

  $(document).ready(function(){SideSlides_Start();});
  
  function SideSlides_Start(){
    if (SideSlides_Total > 1){
      SideSlides_HideAllButCurrent();
      setTimeout(function(){SideSlides_HideCurrent(); }, SideSlides_Delay );
    }

  }

  function SideSlides_HideCurrent(){
    id = '#SideSlide_' + SideSlides_Current;
    $(id).fadeOut(SideSlides_Fade);
    setTimeout(function(){SideSlides_ShowNext(); }, SideSlides_Fade + 10);
  }

  function SideSlides_ShowNext(){
    SideSlides_Current++;
    if (SideSlides_Current > SideSlides_Total){SideSlides_Current = 1; }
    id = '#SideSlide_' + SideSlides_Current;
    $(id).fadeIn(SideSlides_Fade);

    setTimeout(function(){SideSlides_HideCurrent(); }, SideSlides_Delay );
  }

  function SideSlides_HideAllButCurrent(){
    for (i=1; i<= SideSlides_Total; i++){
      if (i == SideSlides_Current){ continue; }
      id = '#SideSlide_' + i;
      $(id).hide();
    }
  }

</script>


</td>
