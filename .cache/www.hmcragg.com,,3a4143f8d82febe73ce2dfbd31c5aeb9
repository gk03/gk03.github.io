	#Slideshow_RightContents {background-color:black; height:240px; width:628px; float:right; position:relative;}
	#Slideshow_RightContents div {position:absolute; left:0; top:0; height:240px; width:628px;}
	#Slideshow_RightContents div div {position:absolute; color:white;}
