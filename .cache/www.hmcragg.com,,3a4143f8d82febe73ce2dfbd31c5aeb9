<script type='text/javascript'>
	$(document).ready(function(){LoadSlideShow();});
	
	var SlideShow = new Object();
	var SlideShow_CurrentSlide = 1;
	var SlideShow_Timer = setTimeout(function(){void(0);},100);
	var SlideShow_Delay = 7000;
	var SlideShow_LongDelay = 9000;
	var SlideShow_TipIsOn = false;
	var SlideShow_TipNeedToShow = true;
	var SlidShow_ImagesReady = new Array();
	
	function LoadSlideShow(){
		$('#SlideShow_LoadingPercent').html('Loading Frames');
		$.ajax({
				cache: true,
				url: "/assets/snippets/HomePageSlideShow.php?",
				type: "POST",
				data: 'LoadFull=true',
				success: function (data){
					$('#SlideShow').html(data);
					$('#SlideShow_LoadingPercent').html('Loading Images ');
					$('#preload_1').bind('load', function(){SlideShowImageReady(1);});
					$('#preload_2').bind('load', function(){SlideShowImageReady(2);});
					$('#preload_3').bind('load', function(){SlideShowImageReady(3);});
					$('#preload_4').bind('load', function(){SlideShowImageReady(4);});
					$('#preload_5').bind('load', function(){SlideShowImageReady(5);});
					$('#preload_6').bind('load', function(){SlideShowImageReady(6);});
					$('#preload_7').bind('load', function(){SlideShowImageReady(7);});
					$('#preload_8').bind('load', function(){SlideShowImageReady(8);});
					$('#preload_9').bind('load', function(){SlideShowImageReady(9);});
					$('#preload_10').bind('load', function(){SlideShowImageReady(10);});
					$('#preload_11').bind('load', function(){SlideShowImageReady(11);});
					$('#preload_12').bind('load', function(){SlideShowImageReady(12);});
				}
			});
	}
	
	
	function SlideShowImageReady(imgNum){
		TotalImages = 12;
		SlidShow_ImagesReady.push(imgNum);
		
		msg = 'Loading Images ' + Math.round((SlidShow_ImagesReady.length / TotalImages) * 100) + '%';
		$('#SlideShow_LoadingPercent').html(msg)
		
		if (SlidShow_ImagesReady.length == TotalImages){Slideshow_Init();}
	}
	
	function Slideshow_Init(){
		//Show Intro Frame the Begin Second INIT
		$('#SlideShow_LoadingPercent').html('Loading Images 100%');
		$('#SlideShow_LoadingPercent').fadeOut(1800);
		$('#SlideShow_Loading').fadeOut(2000, function(){setTimeout(function(){SlideShow_Start()}, 300);} );
	}
		
	function SlideShow_Start(){
		//Hide Intro Frame and show Main Contents
		$('#SlideShow_Loading').html('');
		$('#SlideShow_Intro').fadeOut(600);
		$('#SlideShow_MainContents').fadeIn(600);
		
		//Start the slide show timer and INIT Show Topics
		SlideShow_Timer = setTimeout(function(){SlideShow_ShowNextSlide()}, SlideShow_Delay);
		ShowDebug ("Slideshow_Init - Timer Started " + SlideShow_Delay);
		$('.SlideShowHoverLI').hover(function(){SlideShow_ShowTopic(this);}, function(){SlideShow_HideTopic(this);});
		
		//Set timer to show tool tip
		//setTimeout(function(){SlideShow_ShowTooltip();}, 1500);
	}
	
	function SlideShow_ShowTooltip(){
		if (SlideShow_TipNeedToShow==false){return;}
		$('#SlideShow_ToolTip').fadeIn(300);
		SlideShow_TipIsOn = true;
		SlideShow_TipNeedToShow = false;
		setTimeout(function(){SlideShow_HideTooltip();}, SlideShow_Delay - 3000); 
	}
	
	function SlideShow_HideTooltip(){
		if (SlideShow_TipIsOn==false){return;}
		SlideShow_TipIsOn = false;
		$('#SlideShow_ToolTip').fadeOut(300);
	}
	
	
		
	function FadeOutIntroFrame2(){
		$('#SlideShow_Intro').fadeOut(300);
		$('#SlideShow_MainContents').fadeIn(400);
		SlideShow_FadeIn(1);
	}

	
	function SlideShow_ShowSlide(SlideNum){
		$('#SlideTopics_' + SlideShow_CurrentSlide).hide();
		$('#Slide_' + SlideShow_CurrentSlide).hide();
		SlideShow_FadeIn(SlideNum);
				
		//Restart the slide show timer
		clearTimeout(SlideShow_Timer);
		ShowDebug ("SlideShow_ShowSlide - Timer Stopped");
	}
	
	function RestartShow(){
		SlideShow_Timer = setTimeout(function(){SlideShow_ShowNextSlide()}, SlideShow_LongDelay);
		ShowDebug ("RestartShow - Timer Started " + SlideShow_LongDelay);
	}
	
	
	function SlideShow_ShowTopic(obj){
		SlideShow_TipNeedToShow = false;
		SlideShow_HideTooltip();
		
		//Stop slide show timer
		clearTimeout(SlideShow_Timer);
		ShowDebug ("SlideShow_ShowTopic - Timer Stopped");
				
		//Hilight Bullet
		imgId = 'img_' + obj.id.replace('_li','');
		$('#' + imgId)[0].src = '/assets/images/HomePageSlideShow/Bullet_Hover.png';
		
		//Show Additional Info Topic
		ShowDiv = obj.id.replace("_li", "_div");
		$('#' + ShowDiv).show();
	}
	
	 
	function SlideShow_HideTopic(obj){
		//Restart timer
		SlideShow_Timer = setTimeout(function(){SlideShow_ShowNextSlide()}, SlideShow_Delay);
		ShowDebug ("SlideShow_HideTopic - Timer Started " + SlideShow_Delay);
		
		//un-Hilight Bullet
		imgId = 'img_' + obj.id.replace('_li','');
		$('#' + imgId)[0].src = '/assets/images/HomePageSlideShow/Bullet_Normal.png';
		
		//Hide Additional Info Topic
		ShowDiv = obj.id.replace("_li", "_div");
		$('#' + ShowDiv).fadeOut('fast');
	}
	
	function SlideShow_ShowNextSlide(){
		
		//Get Next slide #
		NextSlide = SlideShow_CurrentSlide + 1;
		NextSlide = (NextSlide > 6)?0:NextSlide;
		
		//Fade out the current slide, wait and faid in the next slide
		SlideShow_FadeOut (NextSlide);
		
		//Restart the slide show timer
		SlideShow_Timer = setTimeout(function(){SlideShow_ShowNextSlide()}, SlideShow_Delay)
		ShowDebug ("SlideShow_ShowNextSlide - Timer Started " + SlideShow_Delay);
	}
	
	function SlideShow_FadeOut(NextSlide){
		//Fade out the current Slide
		$('#SlideTopics_' + SlideShow_CurrentSlide).fadeOut(150, function(){SlideShow_FadeOut2(NextSlide);});
	}
	
	function SlideShow_FadeOut2(NextSlide){
		$('#Slide_' + SlideShow_CurrentSlide).fadeOut(300, function(){SlideShow_FadeIn(NextSlide);});
	}
	
	function SlideShow_FadeIn(SlideNum){
		//Set the current slide number to this slide number
		SlideShow_CurrentSlide = SlideNum;
		if (SlideNum == 0){
			$('#SlideShow_MainContents').fadeOut(300);
			$('#SlideShow_Intro').fadeIn(400, function(){setTimeout(function(){FadeOutIntroFrame2();}, SlideShow_Delay - 2000)});
			return;
		}
		
		//Change left menu bar from blue to orange
		for (i=1; i<=6; i++){$('#LeftMenu_Image' + i)[0].src = '/assets/images/HomePageSlideShow/LeftMenu_Blue.png';}
		$('#LeftMenu_Image' + SlideNum)[0].src = '/assets/images/HomePageSlideShow/LeftMenu_Orange.png';
		
		//Fade in the Slide as requested
		$('#Slide_' + SlideNum).fadeIn(150, function(){SlideShow_FadeIn2(SlideNum);});
	}
	
	function SlideShow_FadeIn2(SlideNum){
		$('#SlideTopics_' + SlideNum).fadeIn(150);
	}
	
	var DebugMsg = 0;
	function ShowDebug(msg){
		DebugMsg ++
		temp = $('#DebugWindow').html();
		temp = DebugMsg + ') ' + msg + "\n" + temp;
		//$('#DebugWindow').html(temp);
		pausecomp(25);
	}
	
	function pausecomp(millis){
		var date = new Date();
		var curDate = null;
		do { curDate = new Date(); }
		while(curDate-date < millis);
	} 
	
</script>
<textarea id=DebugWindow rows=10 cols=60 style='font-family:arial; font-size:10pt; display:none;'></textarea>



