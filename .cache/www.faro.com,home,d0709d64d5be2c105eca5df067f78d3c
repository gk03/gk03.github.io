		    function getCountryForLanguage(lang) {
		        if (lang == 'en-US') return 'United States';
		        if (lang == 'en-GB') return 'United Kingdom';
		        if (lang == 'de-DE') return 'Germany';
		        if (lang == 'es-ES') return 'Spain';
		        if (lang == 'fr-FR') return 'France';
		        if (lang == 'it-IT') return 'Italy';
		        if (lang == 'nl-NL') return 'Netherlands';
		        if (lang == 'pl-PL') return 'Poland';
		        if (lang == 'tr-TR') return 'Turkey';
		        if (lang == 'ja-JP') return 'Japanese';
		        if (lang == 'ko-KR') return 'Korean';
		        if (lang == 'zh-CN') return 'Chinese';
		        if (lang == 'en-SG') return 'Singapore';
		        if (lang == 'en-IN') return 'India';

		        return '';
		    }

		    function getRegion(country) {
		        // Copied from FormCMS - We can remove the one in FormCMS probably and use this only
		        var region = "EUROPE";

		        if (country == "Australia" || country == "Brunei" || country == "Bangladesh" || country == "China" || country == "Hong Kong" || country == "India" || country == "Indonesia" || country == "Japan" || country == "Korea" || country == "Malaysia" || country == "Myanmar" || country == "New Zealand" || country == "Philippines" || country == "Singapore" || country == "South Korea" || country == "Sri Lanka" || country == "Taiwan" || country == "Thailand" || country == "Vietnam") {
		            region = "ASIA";
		        }
		        if (country == "USA" || country == "United States" || country == "Canada" || country == "Mexico" || country == "Brazil" || country == "Costa Rica" || country == "Guatemala" || country == "Honduras" || country == "Puerto Rico" || country == "Chile" || country == "Argentina" || country == "Panama" || country == "Peru" || country == "Bolivia" || country == "Ecuador" || country == "Colombia" || country == "Venezuela") {
		            region = "AMERICAS";
		        }
		        return region;
		    }

		    function isProduct(name) {
		        // Until we find a better way how to identify a page being a product, we have to match the title for all languages.
		        return (name == "products") || (name == "produkte") || (name == "produits") || (name == "prodotti") || (name == "productos");
		    }

		    function isOverviewTab(name) {
		        return (name == "overview") || (name == "überblick") || (name == "aperçu") || (name == "panoramica") || (name == "información general");
		    }

		    /* You may give each page an identifying name, server, and channel on the next lines. */

