    							    $(".LocationsBottomContainer .sf_colsOut .sf_colsIn .sf_cols .sf_colsOut .sf_colsIn .sf_cols:visible").first().removeClass("LocationsHeaderNoBorder");
    							</script>
						    </div>
					    </div>
				    </footer>
                
</div>
			</div>
            

