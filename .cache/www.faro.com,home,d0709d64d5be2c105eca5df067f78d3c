<script type="text/javascript" >
	var defaultTextNewsletter = $("#tbNewsletter")[0].defaultValue;
	$("#tbNewsletter").blur(function () {
		if ($(this).val() == "") { $(this).val(defaultTextNewsletter); }
	});
	$("#tbNewsletter").focus(function () {
		if (defaultTextNewsletter === null) { defaultTextNewsletter = $(this).val(); }
		if ($(this).val() == defaultTextNewsletter) { $(this).val(""); }
	});

	var defaultTextSearch = $("#tbFullTextSearch")[0].defaultValue;
	$("#tbFullTextSearch").blur(function () {
		if ($(this).val() == "") { $(this).val(defaultTextSearch); }
	});
	$("#tbFullTextSearch").focus(function () {
		if (defaultTextSearch === null) { defaultTextSearch = $(this).val(); }
		if ($(this).val() == defaultTextSearch) { $(this).val(""); }
	});
</script>

            
		

