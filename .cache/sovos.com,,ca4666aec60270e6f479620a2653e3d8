               if ( $j(window).scrollTop() > 25 && $pop != 'true' ) { 
                    $j('#featurepop').stop(true).animate({ bottom : '-445px' }, 250); 
               } else if ( $j(window).scrollTop() <= 25 && $pop != 'true' ) { 
                    $j('#featurepop').stop(true).animate({ bottom : '-570px' }, 400); 
               }
          });
          $j('.learnmore').click( function() {
               $pop = 'true';
               //$j('#learner').stop(true).slideUp(400, function() { $j('#featurepop').animate({ bottom : '-30px' }, 400); });
               $j('#featurepop').animate({ bottom : '-30px' }, 400);
               $j('#learner').stop(true).slideUp(200);
          });
          $j('.closebutton').click( function() {
               $pop = 'false';
               //$j('#learner').stop(true).slideDown(400, function() { $j('#featurepop').animate({ bottom : '-445px' }, 400); });
               $j('#featurepop').animate({ bottom : '-445px' }, 400);
               $j('#learner').stop(true).slideDown(200);
          });

      // video popup
          $j('.videopop').magnificPopup({
              disableOn: 700,
              type: 'iframe',
              mainClass: 'mfp-fade',
              removalDelay: 160,
              preloader: false,
              fixedContentPos: false,
              iframe: {
                  markup: '<div class="mfp-iframe-scaler">'+
                  '<div class="mfp-close"></div>'+
                  '<iframe class="mfp-iframe" frameborder="0" allowfullscreen></iframe>'+
                  '</div>'+ // HTML markup of popup, `mfp-close` will be replaced by the close button
                  '<div class="button-white small" style="position: absolute; left: 50%; margin: 15px 0 0 -140px; width: 280px;"><a style="padding: 20px;" href="/aca">Learn more about Taxport ACA</a></div>',
             }
          });
     });</script> </div></div></div></div></div></div></div><div id="top"     class="vc_row wpb_row section vc_row-fluid" style=' text-align:left;'><div class=" clearfix"><div class="topleft wpb_column vc_column_container vc_col-sm-12"><div class="vc_column-inner "><div class="wpb_wrapper"><div class="wpb_text_column wpb_content_element "><div class="wpb_wrapper"><h1 class="header" style="text-align: center;">Compliance is a full-time job.</h1><p class="header" style="text-align: center;">Save money, reduce risk, and focus on growing your business.<br /> Simplify tax and compliance with Sovos.</p><div class="button-white large"><a href="/solutions">Learn More</a></div></div></div></div></div></div></div></div><div id="arrow"     class="vc_row wpb_row section vc_row-fluid" style=' text-align:left;'><div class=" clearfix"><div class="wpb_column vc_column_container vc_col-sm-12"><div class="vc_column-inner "><div class="wpb_wrapper"><div class="wpb_text_column wpb_content_element "><div class="wpb_wrapper"><p><a href="#solutions">▼</a></p></div></div></div></div></div></div></div><div id="solutions"     class="vc_row wpb_row section vc_row-fluid vc_custom_1448306011823" style=' text-align:left;'><div class=" clearfix"><div class="wpb_column vc_column_container vc_col-sm-12"><div class="vc_column-inner "><div class="wpb_wrapper"><div class="wpb_text_column wpb_content_element "><div class="wpb_wrapper"><h2 style="text-align: center;">Accurate. Secure. Efficient.</h2><h3 style="text-align: center;">This is what Sovos is all about. Here’s what we excel at:</h3></div></div></div></div></div></div></div><div      class="vc_row wpb_row section vc_row-fluid education-row" style=' text-align:left;'><div class=" clearfix"><div class="wpb_column vc_column_container vc_col-sm-4"><div class="vc_column-inner "><div class="wpb_wrapper"><div class="wpb_text_column wpb_content_element "><div class="wpb_wrapper"><div style="text-align: center;"><a href="/solutions/aca"><img class="alig