		function validateForm()
                 {
			var x=document.forms["form"]["first_name"].value;
			if (x==null || x=="" || x=="First name *" )
			  {
			  alert("First Name must be filled out");
                       document.form.first_name.focus();
			  return false;
			  }
			  var x=document.forms["form"]["last_name"].value;
			if (x==null || x=="" || x=="Last name *" )
			  {
			  alert("Last Name must be filled out");
                       document.form.last_name.focus();
			  return false;
			  }
			  var x=document.forms["form"]["log"].value;
			  	var atpos=x.indexOf("@");
			var dotpos=x.lastIndexOf(".");
			if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length)
			  {
			  alert("Please enter a valid E-mail address");
                       document.form.log.focus();
			  return false;
			  }
			  var x=document.forms["form"]["user_email"].value;
			var atpos=x.indexOf("@");
			var dotpos=x.lastIndexOf(".");
			if (atpos<1 || dotpos<atpos+2 || dotpos+2>=x.length)
			  {
			  alert("Please Re-enter your E-mail address");document.form.user_email.focus();
			  return false;
			  }


