            (function(){

                // Listen for the ready event for any vimeo video players on the page
                var vimeoPlayers = document.querySelectorAll('iframe'),
                    player;

                for (var i = 0, length = vimeoPlayers.length; i < length; i++) {
                    player = vimeoPlayers[i];
                    $f(player).addEvent('ready', ready);
                }

                
                function addEvent(element, eventName, callback) {
                    if (element.addEventListener) {
                        element.addEventListener(eventName, callback, false);
                    }
                    else {
                        element.attachEvent(eventName, callback, false);
                    }
                }

              

                function ready(player_id) {
//alert("Player ID: " + player_id);
                    // Keep a reference to Froogaloop for this player
                    var container = document.getElementById(player_id).parentNode.parentNode,froogaloop = $f(player_id),apiConsole = container.querySelector('.console .output');

/*alert("CONTAINER Level 0: " + document.getElementById(player_id).className);
alert("CONTAINER Level 1: " + document.getElementById(player_id).parentNode.className);
alert("CONTAINER Level 2: " + document.getElementById(player_id).parentNode.parentNode.className);
alert("CONTAINER Level 3: " + document.getElementById(player_id).parentNode.parentNode.parentNode.className);
alert("CONTAINER Level 4: " + document.getElementById(player_id).parentNode.parentNode.parentNode.parentNode.className);
alert("CONTAINER Level 5: " + document.getElementById(player_id).parentNode.parentNode.parentNode.parentNode.parentNode.className);
alert("CONTAINER Level 6: " + document.getElementById(player_id).parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.className);*/


                    
                    function setupSimpleButtons() {
                       // var buttons = container.querySelector('div ul.simple'),
                         //   playBtn = buttons.querySelector('.play'),
                            pauseBtn = document.getElementById('allthumbsup');
                            
                        // Call play when play button clicked
                       /* addEvent(playBtn, 'click', function() {alert("play1");Test();
                            froogaloop.api('play');
                        }, false);*/

                        // Call pause when pause button clicked
                        addEvent(pauseBtn, 'click', function() {
                            alert("pause1");

                            froogaloop.api('pause');
                        }, false);

                       
                    }




                   function setupEventListeners() {
                       

                        function onLoadProgress() {
                           
                                froogaloop.addEvent('loadProgress', function(data) {
                                    apiLog('loadProgress event : ' + data.percent + ' : ' + data.bytesLoaded + ' : ' + data.bytesTotal + ' : ' + data.duration);
                                });
                           
                        }

                        function onPlayProgress() {
                           
                                froogaloop.addEvent('playProgress', function(data) {
                                    apiLog('playProgress event : ' + data.seconds + ' : ' + data.percent + ' : ' + data.duration);
                                });
                           
                        }

                        function onPlay() {
                          
                            
                                froogaloop.addEvent('play', function(data) {localStorage.setItem("Val",1);//alert("Play");
                                  // var localstore = localStorage.getItem("localstore");
//alert("LOCAL STORAGE = " + localstore);
					//play.pauseRotation();                                                                        
				        apiLog('play event');
                                });
                            
                        }


                        function onPause() {
                           
                                froogaloop.addEvent('pause', function(data) {
                                    apiLog('pause event');
                                });
                           
                        }

                        function onFinish() {
                            
                                froogaloop.addEvent('finish', function(data) { localStorage.setItem("Val",0); 
                                              var ref= localStorage.getItem("Val2");
                                                 // alert("finish1"+ref);
                                         initSlideShow();
                                   //alert("finish");
					apiLog('finish');
                                });
                            
                        }

                        function onSeek() {
                           
                                froogaloop.addEvent('seek', function(data) {
                                    apiLog('seek event : ' + data.seconds + ' : ' + data.percent + ' : ' + data.duration);
                                });
                           
                        }

                       

                        // Calls the change event if the option is checked
                        // (this makes sure the checked events get attached on page load as well as on changed)
                        onLoadProgress();
                        onPlayProgress();
                        onPlay();
                        onPause();
                        onFinish();
                        onSeek();
                    }

                  
                    
                    setupSimpleButtons();
                     setupEventListeners();

                   
                }
            })();

