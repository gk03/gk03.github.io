	  <script type="text/javascript">
	  $("#job_searchform").submit(function(e){
		e.preventDefault();
		var keyword = $("#cs_jobsearchforminput_q").val();
		if (keyword.match("^[0-9]{3}")) {
		  $("#cs_jobsearchforminput_q").val("");
		  var input = $("<input>").attr("type", "hidden").attr("name", "cf[JobDisplayID]").val(keyword);
		  input.appendTo( $("#job_searchform") ); 
		}
		var lstate = $("#location_state").val();
		if(lstate=="remote"){
		  $("#location_state").val("");
		  var input = $("<input>").attr("type", "hidden").attr("name", "location").val("Remote");
		  input.appendTo( $("#job_searchform") );
		}
		var query = "?" + $("#job_searchform").serialize();
		document.location.href = "/jobs/search" + query;
		return false;
	  });
	</script>

