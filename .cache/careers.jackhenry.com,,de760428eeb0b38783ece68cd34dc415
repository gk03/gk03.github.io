      	/* set copyright year ***************************************************************/
      	Date.prototype.getRealYear = function() { 
		  if(this.getFullYear)
			  return this.getFullYear();
		  else
			  return this.getYear() + 1900; 
		};
		var myDate = new Date();
		var currYear = myDate.getRealYear();
		$("#pnlCurrentYear").html("&copy; " + currYear);      
