	var message = "You are about to be redirected to the new eMIS Employee Portal.  When you are prompted to login, if you are not currently on the MUNIZ domain (the MEIT network), be sure to preface your login with 'MUNIZ\\'.  Thank you";
	return confirm(message);
}
