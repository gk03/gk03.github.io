<script type="text/javascript">
	$(document).ready(function(){
		/*$('#contractLink').mouseover(function(e){
			$('.contractDiv').css('display','');
			$('.contractDiv').css('top',(e.pageY-30)+'px');
			$('.contractDiv').css('left',(e.pageX-30)+'px');
		});*/
		$('#contractLink').click(function(e){
			$('.contractDiv').css('display','');
			$('.contractDiv').css('top',(e.pageY-30)+'px');
			$('.contractDiv').css('left',(e.pageX+60)+'px');
		});
		$('#contractLink').bind("touchstart",function(e){
			$('.contractDiv').css('display','');
			$('.contractDiv').css('top',(e.pageY-30)+'px');
			$('.contractDiv').css('left',(e.pageX+60)+'px');
		});
		$('contractDiv').mouseout(function(){
			$('contractDiv').hide();
		});
	});
</script>
