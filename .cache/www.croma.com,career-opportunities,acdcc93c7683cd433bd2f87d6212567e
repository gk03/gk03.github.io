<script type="text/javascript">

 	cmCreatePageviewTag("Career Opportunities","Static Page");

 
 window.mediator.subscribe('trackAddToCartCoreMetrics', function(data) {
		if (data.productCode && data.quantity)
		{
                 /*	cmCreatePageviewTag("Cart", "CART"); */
			cmCreateShopAction5Tag(data.productCode, data.productDesc, data.quantity,data.price);
			cmDisplayShops();
		}
	});

	window.mediator.subscribe('trackUpdateCartCoreMetrics', function(data) {
		if (data.productCode && data.initialCartQuantity && data.newCartQuantity)
		{
			cmCreatePageviewTag("Cart", "CART");
			cmCreateShopAction5Tag(data.productCode,data.productDesc, data.newCartQuantity-data.initialCartQuantity,data.price);
			cmDisplayShops();
		}
	});

	window.mediator.subscribe('trackRemoveFromCartCoreMetrics', function(data) {
		if (data.productCode && data.initialCartQuantity)
		{
			cmCreatePageviewTag("Cart", "CART");
			cmCreateShopAction5Tag(data.productCode, data.productDesc,-data.quantity,data.price);
			cmDisplayShops();
		}
	});
	
	window.mediator.subscribe('successPincodeCheckForProduct', function(data) {
		
if (data.productCode && data.pincode)
		{
			cmCreatePageviewTag("PRODUCT:"+data.productDesc+"("+data.productCode+")", data.categoryCode);
			cmCreateConversionEventTag (data.pincode,"2","Success:Pincode Check","10","PRODUCT:"+data.productCode,"DESC:"+data.productDesc);
			
		}
	});

window.mediator.subscribe('failPincodeCheckForProduct', function(data) {
		
if (data.productCode && data.pincode)
		{
			cmCreatePageviewTag("PRODUCT:"+data.productDesc+"("+data.productCode+")", data.categoryCode);
			cmCreateConversionEventTag (data.pincode,"2","Failure:Pincode Check","10","PRODUCT:"+data.productCode,"DESC:"+data.productDesc);
			
		}
	});

window.mediator.subscribe('trackEMI',function(data) {
	if(data.productCode && data.bank && data.amount)
		{
			cmCreateConversionEventTag ("EMI Check ","2"," Product Details Page:"+data.productName,"10","Bank Selected :"+data.bank,"Amount :"+data.amount+" Product Code :"+data.productCode);
		}
});
window.mediator.subscribe('trackPDPButton',function(data) {
	if(data.buttonName && data.productName && data.code)
		{
				
				cmCreateConversionEventTag (data.productName+" :"+data.buttonName,"2","Product Details Page ("+data.code+"):Button Track","10");
		}
});
window.mediator.subscribe('trackCartPincode',function(data) {
	if(data.pincode)
		{
			
			cmCreateConversionEventTag ("CartPage Pincode:"+data.pincode,"2","Pincode","10","CartId :"+data.cartCode);
		}
});
window.mediator.subscribe('trackCouponCode',function(data) {
	if(data.couponCode && data.cartCode)
		{
			
			cmCreateConversionEventTag ("Coupon Code ","2","CartPage Coupon :"+data.couponCode,"10","CartId :"+data.cartCode);
		}
});
window.mediator.subscribe('trackPaymentMode',function(data){
	if(data.paymentMode && data.cartCode)
		{
			
			cmCreateConversionEventTag ("Payment Mode","2","Payment Page:"+data.paymentMode,"10","CartId :"+data.cartCode);
		}
});
window.mediator.subscribe('trackQuickViewPopup',function(data){
	if(data.productCode && data.categoryCode && data.productName)
		{		
			
			cmCreateConversionEventTag ("Quick View","2",data.categoryCode,"10",data.productName+"("+data.productCode+")");
			cmCreateProductviewTag(data.productCode, data.productName,data.categoryCode);
		}
});
window.mediator.subscribe('trackAddToCartFromListAndGridCoremetrics',function(data){

	if(data.productCode && data.productName)
		{		
/*			cmCreateConversionEventTag ("Add To Cart","2",data.productName,"10","Category Code :"+data.categoryCode); */
			cmCreateShopAction5Tag(data.productCode, data.productName,"1",data.price,data.categoryCode);
			cmDisplayShops();	
		}
});
window.mediator.subscribe('trackLogin',function(data){
	if(data.emailId)
		{		
	/*	cmCreatePageviewTag("Login",data.emailId); */
		cmCreateRegistrationTag(data.emailId,data.emailId,null,null,null,null,data.pageType);

		}
});
window.mediator.subscribe('trackHeaderLinkClick',function(data){	
		if(data.eventCategory)
			{		
			cmCreatePageviewTag(data.eventCategory);

			}
});
window.mediator.subscribe('trackAddtoCartPDP',function(data){

	if(data.productCode && data.productName)
		{		
/*			cmCreateConversionEventTag ("Add To Cart","2",data.productName,"10","Category Code :"+data.categoryCode); */
			cmCreateShopAction5Tag(data.productCode, data.productName,"1",data.price,data.categoryCode);
			cmDisplayShops();	
		}
});
window.mediator.subscribe('trackRegisterCoremetrics',function(data){

	if(data.email && data.phone)
		{		
		cmCreateRegistrationTag(data.email,data.email,data.city,data.state,data.pincode,data.country);	
		}
});
