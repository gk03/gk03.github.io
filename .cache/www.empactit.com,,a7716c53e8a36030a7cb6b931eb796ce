      documentReady(function() {
        
        window.__gaq__ = window['_gaq'];
        
        window["_gaq"] = {
          push: function(commandArray) {
            var method = commandArray[0];
  
            if (method === "_link") {
              var target = commandArray[1];
              if (target) {
                // For Universal, we need to create a linker from default tracker,
                // and use it to decorate the href, since `ga('linker:decorate', target)`
                // requires an anchor as target, not just the link, but essentially
                // perform the same operations.
                var defaultTracker = _gau.getByName('t0');
                var linker = new window.gaplugins.Linker(defaultTracker);
                var decorated = linker.decorate(target);
                setTimeout(function() { location.href = decorated; }, 200);
              }
            }
  
            if (method != "_trackEvent") return;
  
            var type = commandArray[1],
                action = commandArray[2],
                target = commandArray[3],
                extras = commandArray[4];
  
            if (type === "RankTracker" && extras === true) {
              // For RankTracker, legacy GA uses `true` while universal GA uses `{nonInteraction: 1}`
              commandArray[4] = true;
              extras = {'nonInteraction': 1};
            }
  
            _gau('send', 'event', type, action, target, extras);
          }
        };
        
        
        if (document.referrer.match(/google\.com/gi) && document.referrer.match(/cd/gi)) {
          var myString=document.referrer,r=myString.match(/cd=(.*?)&/);
          var rank=parseInt(r[1]), kw=myString.match(/q=(.*?)&/);
          var keyWord=kw[1].length>0?decodeURI(kw[1]):"(not provided)";
          var p=document.location.pathname;

          _gau('send', 'event', 'RankTracker', keyWord, p, rank, {'nonInteraction': 1});
        }
        
        jQuery('body').on('click', 'a.tracking-button', function(e) {
          var $this = jQuery(e.target)
            , targetCheck = $this.attr('target')
            , lightboxCheck = $this.attr('id')
            , hrefCheck = $this.attr('href')
            , category = "Button Click";

          var action = $this.text() || "Graphic Button";
          var opt_label = $this.attr('rel') || $this.attr('href') || "n/a";

          _gau('send', 'event', category, action, opt_label);

          //condition for target _blank
          if (targetCheck != '_blank' && hrefCheck) {
            e.preventDefault();
            setTimeout('document.location = "' + $this.attr('href') + '"', 200);
            return false;
          }
        });
        
        var filetypes = /\.(zip|exe|pdf|doc*|xls*|ppt*|mp3)$/i;
        var baseHref = '';
        if (jQuery('base').attr('href') != undefined) baseHref = jQuery('base').attr('href');
        jQuery('a').each(function() {
          var href = jQuery(this).attr('href');
          if (href && (href.match(/^https?\:/i)) && (!href.match(document.domain))) {
            
            jQuery(this).click(function() {
              var extLink = href.replace(/^https?\:\/\//i, '');

              _gau('send', 'event', 'External', 'Click', extLink);

              if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
                setTimeout(function() { location.href = href; }, 200);
                return false;
              }
            });
            
          }
          
          else if (href && href.match(/^mailto\:/i)) {
            jQuery(this).click(function() {
              var mailLink = href.replace(/^mailto\:/i, '');

              _gau('send', 'event', 'Email', 'Click', mailLink);

              if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
                setTimeout(function() { location.href = href; }, 200);
              }
            });
          }
          
          else if (href && href.match(filetypes)) {
            jQuery(this).click(function() {
              var extension = (/[.]/.exec(href)) ? /[^.]+$/.exec(href) : undefined;
              var filePath = href;
              var action = 'Click-'+extension;

              _gau('send', 'event', 'Download', action, filePath);

              if (jQuery(this).attr('target') != undefined && jQuery(this).attr('target').toLowerCase() != '_blank') {
                setTimeout(function() { location.href = baseHref + href; }, 200);
                return false;
              }
            });
          }
          
        });
        
