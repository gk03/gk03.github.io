<script>
	function deleteTrackingCookies() {
		var cookiesNamesWhitelist = ["AVGCookiePercentage","avg_eshop_cart","AVGSIDM","F5AVG","TSSID","avg_toolbar","avgualpw","avguareg","upl_ssid_c","geoip","spideroak","avg_eshop_cart_ubrowser","avg_eshop_ctysel","avg_myaccount","avg_eshop_chsel","avg_eshop_chsel_force","avg_eshop_chsel_force","avg_eshop_pattempts","avg_eshop_empty_cart_redir","avg_eshop_no_expiry_date","avg_eshop_bkpcd_","checkout_type_","avg_eshop_use_license_data","affid","forceGateway","rbsFail","gbcFail","TC_orderInfo_Fail","TC_getKeyRequest_Fail","checkout_type","autobilling_variant","debugbar","AVGSCHELPREQUEST","avgLocale","avg_plt","avg_thr","avg_mobile","SESSIONID","JSESSIONID","SessionPersistence-publish-prod","AParam","app-param-A","avg_app_params","avg_app_params2","SessionPersistence-publish","two_step_acc","AVGSurvey","avgLocale","PHPSESSID","s_zen"];
		var cookiesStubsWhitelist = ["AVGCSC","AVGRandom"];
		var cookies = document.cookie.split("; ");
		for (var i = 0; i < cookies.length; i++) {
			var cookie = cookies[i];
			var parts = cookie.split("=");
			var cookieName = parts[0];
			if (typeof cookieName !== "undefined") {
				var notFound = true;
				for (var j = 0; j < cookiesNamesWhitelist.length; j++) {
					if (cookiesNamesWhitelist[j] === cookieName) {
						notFound = false;
						break;
					}
				}
				if (notFound) {
					for (j = 0; j < cookiesStubsWhitelist.length; j++) {
						if (cookieName.indexOf(cookiesStubsWhitelist[j]) == 0) {
							notFound = false;
							break;
						}
					}
				}
				if (notFound) {
					if (document.domain.indexOf(".avg.com", document.domain.length - 8) !== -1) {
						document.cookie = cookieName + "=; expires=Thu, 01-Jan-70 00:00:00 UTC; path=/; domain=.avg.com;";
					} else {
						document.cookie = cookieName + "=; expires=Thu, 01-Jan-70 00:00:00 UTC; path=/;";
					}
				}
			}
		}
	}

	function setDeclineCookie() {
		var optOutCookies = ["sat_track:false"];
		var name = "sat_track";
		var value = "false";
		if (optOutCookies.length > 0) {
			var declineCookie = optOutCookies[0].split(":");
			name = declineCookie[0];
			if (declineCookie.length > 1) {
				value = declineCookie[1];
			} else {
				value = name;
			}
		}
		var cookieDate = new Date();
		cookieDate.setFullYear(cookieDate.getFullYear( ) + 2);
		if (document.domain.indexOf(".avg.com", document.domain.length - 8) !== -1) {
			document.cookie = name + "=" + value +"; expires=" + cookieDate.toUTCString() + "; path=/; domain=.avg.com;";
		} else {
			document.cookie = name + "=" + value +"; expires=" + cookieDate.toUTCString() + "; path=/;";
		}
	}

	(function checkOptOutCookies() {
		var value = "; " + document.cookie;
		var optOutCookies = ["sat_track:false"];
		for (var i = 0; i < optOutCookies.length; i++) {
			var cookie = optOutCookies[i].split(":");
			var parts = value.split("; "+ cookie[0] + "=");
			if (parts.length == 2) {
				var value = parts.pop().split(";").shift();
				if (typeof value  !== "undefined") {
					if (cookie.length > 1) {
						if (value === cookie[1]) {
							deleteTrackingCookies();
							var cookieDate = new Date();
							cookieDate.setFullYear(cookieDate.getFullYear() + 2);
							if (document.domain.indexOf(".avg.com", document.domain.length - 8) !== -1) {
								document.cookie = cookie[0] + "=" + value +"; expires=" + cookieDate.toUTCString() + "; path=/; domain=.avg.com;";
							} else {
								document.cookie = cookie[0] + "=" + value +"; expires=" + cookieDate.toUTCString() + "; path=/;";
							}
						}
					}
				}
			}
		}
	}());

</script>
        



	


