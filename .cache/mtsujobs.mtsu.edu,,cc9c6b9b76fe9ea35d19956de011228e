$(document).ready(function() {
    
	$(".gallery .item:not(:first)").hide();

	var count = 1;

	function imageRotator(){

		$(".gallery .item:nth-child("+count+")").fadeOut(1000);

			count++;

			if(count > 18)
			{
				count = 1;
			}

		$(".gallery .item:nth-child("+count+")").fadeIn(1000);

	}

	setInterval(function(){

		imageRotator();

	},5000);

});

</script>
