	  }

	  ////
	  // Build an array of test data.
	  var m_aDataNoty = [];
	  
	   //----auto-check
	   	var m_dtTSFirstMsg = "full" //'3/13/2014 4:19:45 PM' //"full";
    	var m_AutoCheckTimeNoty = 25000;
    	var m_AutoCheckTimeNotyID = null;
    	
    	var m_AutoTimeNoty = 1400;
    	var m_AutoTimeNotyID = null;
    	
    	var m_bFreshNoty = true;

	 	//:::::::
	 	//:::::::   
	    function AutoNoty()
	    {
	    
			if ( (m_dtTSFirstMsg == '') || (m_dtTSFirstMsg == 'full') )
			{
				var mydate = new Date();
				var myyear = mydate.getYear();
				if (myyear < 200) myyear = myyear + 1900;
				m_dtTSFirstMsg = (mydate.getMonth() + 1) + "/" + mydate.getDate() + "/" + myyear + " " + formatAMPM(mydate)  
				
			}
 
	    	window.clearTimeout(m_AutoCheckTimeNotyID);
	        var sURL = '../../mail/ms_ajax.asp?folder=/Inbox&pg=1&msgno=25&sortby=Received&sort_order=DESC&dtTS=' + m_dtTSFirstMsg + '&JSON=yes';
	        var obj_callback = new clsAjaxcallback();
	        obj_callback.Ajaxcallback.success = fShowNotyModule;
	        makeRequestInbox(sURL, obj_callback.Ajaxcallback);
	    }

		//:::::::
		//:::::::
	    function fPopNoty()
	    {
	    	if (m_aDataNoty.length > 0)
	    	{
	    		generateAll('alert', m_aDataNoty.pop());
	    		window.setTimeout("fPopNoty()",m_AutoTimeNoty);
	    	}
	    
	    } 
	    
	    //:::::::
		//:::::::
    	function fShowNotyModule(o) {
    	
    			//stop prefetch...
    			m_iPreFetchMsgIndex = 100;
    	 
    			//start notifier ::::::::::
		   		m_AutoCheckTimeNotyID = window.setTimeout("AutoNoty()",m_AutoCheckTimeNoty);
 
    	 
    			if (m_bFreshNoty)
    			{
    				//$.noty.closeAll();
    				m_bFreshNoty = false;
    			}
    			
    			var m_CDelimiter	="_#c|-";
    			var m_RDelimiter	="_#r|-";
    			
				var sNotyMsg = '';
				
		        if(o.responseText !== undefined)
		        {
		        	sNotyMsg = '';
		            var sHTTPData = o.responseText; 
		            m_sFolderRecords = sHTTPData;
		            o = null;
				
		            var row_arr= sHTTPData.split(m_RDelimiter);
		            sHTTPData = "";
			        var i =0;
			           
			        while (i < (row_arr.length - 1))
		            {
		                cell_arr = row_arr[i].split(m_CDelimiter);
		                
				        msg_id = cell_arr[0];  //msgid
				        msg_su = cell_arr[1];  //subject
				        msg_rd = cell_arr[2];  //received
				        msg_fr = cell_arr[3];  //from
				        msg_fe = cell_arr[4];  //fromemail
				        msg_re = cell_arr[9];  //read
				        msg_mc = cell_arr[17]; //messagecount
				        
				        if (i==0)
		                {
		                    m_dtTSFirstMsg = msg_rd;
		                }
		                
				        
				        if  (msg_re == "True")
				        {
				        	i++;
				        	continue;
				        }
				        
		                
		                if (msg_fr.length == 0)
				            msg_fr = msg_fe;
			
			            if (msg_fr.length == 0)
				            msg_fr = "(none)";
				            
			            if (msg_su.length == 0)
				            msg_su = "(no subject)";	
				            

						if (msg_su.length > 29)
							msg_su = msg_su.substr(0, 29) + '...';
	
						if (msg_fr.length > 16)
							msg_fr = msg_fr.substr(0, 16) + '...';
		            
			            sNotyMsg = '<div style="cursor:pointer" onclick="fnOpneNotyMsg(\'' + msg_id + '\');" style="font-family:Tahoma, Arial, serif; width:337px; text-overflow: ellipsis;  white-space: nowrap"><img src="../../app/noty/img/mail.png" style="width:24px; height:24px" align="absmiddle">&nbsp;&nbsp;&nbsp;' + mcLang.Text('lang_mail_118',1) + ' <b>' + fnEscapeHtml(msg_fe) + '</b>:<br><img src="../../images/common/trans.gif" style="width:24px; height:24px" align="absmiddle">&nbsp;&nbsp;&nbsp;' + fnEscapeHtml(msg_su) + '</div>';
			            m_aDataNoty.unshift(sNotyMsg);
			            			            
		                i++;   
		                
		                
		            }
		            

		           	if (m_aDataNoty.length > 0)
	    			{
	    				//Are we in inbox?
	    				if ( (m_iCurrentAppIndex == 0) && (sort_by.toLowerCase() == 'received') && (sort_order.toLowerCase() == 'desc') && (m_sInboxMode == '') && (v_sFolder.toLowerCase() == '/inbox') && (CurentPage == 1) )
	    				{
	    						m_bAutoCheckCall = true;  
								fShowInboxModule();	 
						}

	    			}
		            
		            window.setTimeout("fPopNoty()",m_AutoTimeNoty);
	 	
		        }
		        
		        AutoCheckMailCount();
		        
		    }

