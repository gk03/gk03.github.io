function fnInitFeatherEditor() 
{

			   var sToolLang = slang;
			   
				if (sToolLang == 'av')
					sToolLang = 'es';	

		   	   featherEditor = new Aviary.Feather({
		       apiKey: 'rhrkapod5tcueco2',
		       maxSize: 800,
		       language: sToolLang,
		       theme: 'dark',
		       apiVersion: 2,
		       tools: 'enhance,frames,crop,warmth,focus,contrast,splash,text,whiten,blemish,redeye,draw,sharpness,saturation,brightness,orientation,resize,stickers,effects',
		       appendTo: '',
		       onSave: function(imageID, newURL) {
		           //var img = document.getElementById(imageID);
		           //img.src = newURL;
		           //alert(newURL);
		           
		           featherEditor.close();
		           
		           if (m_sEditToolMode == 'composer')
		           {
		           		fnUpdateComposerAttachPic(newURL);
		           }
		           else
		           		location = '../../app/download_url.asp?sURL=' + escape(newURL) + "&UpdatePicURL="
			
		           
		           
		       },
		       onError: function(errorObj) {
		           alert(errorObj.message);
		       }
		   });


}



