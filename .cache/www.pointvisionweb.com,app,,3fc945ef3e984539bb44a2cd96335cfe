	function ResizeToContent() 
	{
	
		try {
				
			//console.log('Msg_body height before:' + $("#Msg_body").height() );
			//console.log('$(#Msg_body).contents():' + $("#Msg_body").contents().find("#Ifrm_Msg_body").height() );
			
			$('#Msg_body').css({'height':'0px'});
			
			var Msg_Main_height = $("#Msg_Main").height();
			
			$('#Msg_body').css({'height':'100%'});
			
			var Ifrm_Msg_body_height = $("#Msg_body").contents().height() + 100;
		
			
			$("#mMsgBodyScroll").height(Ifrm_Msg_body_height);
			$("#Msg_body").height(Ifrm_Msg_body_height);
			
			ReSizeMsgPanel();
			
			if (m_bMsgPreFullScreen)
			{
				var m_ClientHeight = $(window).height();
				var m_ClientWidth = $(window).width();
				var iMsgDivTop = 80;
				var iMsgDivLeft= 130;
				var iMsgDivHeight = m_ClientHeight - (iMsgDivTop * 2);
				var iMsgDivWidth  = m_ClientWidth - (iMsgDivLeft * 2);
				$('#Msg_Panel_Msg').width(iMsgDivWidth);
				$('#Msg_Panel_Msg').height(iMsgDivHeight);
				$('#Msg_Main').height(iMsgDivHeight);
				$("#mMsgBodyScroll").height(iMsgDivHeight - 30);
			}


		
		}
		catch (x) {
				//alert("Jerr with ResizeToContent() ");
				return;
		}

    }
    
    
