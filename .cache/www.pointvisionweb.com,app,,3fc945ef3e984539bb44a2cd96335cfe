//:::::::
function UpdateInboxAlert(f_sText, f_sIcon)
{
	window.clearTimeout(m_InfoInboxBarTimeID);
	m_InfoInboxBarTimeID = window.setTimeout("HideInfobarInbox()",7000);

	document.getElementById("MUserInboxAlert").innerHTML = f_sText;
	document.getElementById("MUserInboxAlert").style.display = "";

	var iAlertDivWidth = parseInt(document.getElementById('MUserInboxAlert').offsetWidth);
	var xy = YAHOO.util.Dom.getXY('MUserInboxAlert');
	xy[0] =  ((mimMainPanelWidth /2) - (iAlertDivWidth / 2)) + 170;
	//xy[1] += 0;
	YAHOO.util.Dom.setXY('MUserInboxAlert', xy);


	var attributes = { height: { from: 0, to: (40) } };   
	var anim = new YAHOO.util.Anim('MUserInboxAlert', attributes, .4, YAHOO.util.Easing.easeIn);
	anim.animate(); 

/*
    var div=$("MUserInboxAlert");
    div.animate({height:'300px',opacity:'0.4'},"slow");
    div.animate({width:'300px',opacity:'0.8'},"slow");
    div.animate({height:'100px',opacity:'0.4'},"slow");
    div.animate({width:'100px',opacity:'0.8'},"slow");
	*/
}
    
//:::::::
//:::::::
function HideInfobarInbox()
{

	//var attributes = { height: { to: 0 } , width: { from: m_ClientWidth, to: 0 } };   
	var attributes = { height: { to: 0 } };
	var anim = new YAHOO.util.Anim('MUserInboxAlert', attributes, .5, YAHOO.util.Easing.easeIn);
	anim.onComplete.subscribe(function () {   
	 						QHideInfobarInbox();
							});
	 anim.animate(); 
}


//:::::::
//:::::::
function QHideInfobarInbox()
{
	document.getElementById("MUserInboxAlert").style.display = "none";
}
