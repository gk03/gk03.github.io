<script type="text/javascript">
	  
	//window.setTimeout("NotyServerAlert()",34000);
	//window.setTimeout("NotyUserOnline()",42000);
	//window.setTimeout("NotyWarning()",57000);

	  
	 var g_NotyID = null;
	
	  //:::::::
	  //:::::::
	  function generate(layout, v_sType, v_stxt) {
	  	var n = noty({
	  		text: v_stxt,
	  		type: v_sType,
	      dismissQueue: true,
	  		layout: layout,
	  		 animation: {
			    open: {height: 'toggle'},
			    close: {height: 'toggle'},
			    easing: 'swing',
			    speed: 200 // opening & closing animation speed
			  },

	  		theme: 'default'
	  	});
	  	//console.log('html: '+n.options.id);
	  	
	  	//setTimeout(function() {
     	// $.noty.setText(n.options.id, 'I\'m closing now!'); // same as alert.setText('Text Override')
    	//}, 1000);
 
	  //:::::::
	  //:::::::
       setTimeout(function() {
      $.noty.close(n.options.id);
    	}, 10100);


	  	g_NotyID = n.options.id;
	 
	  }
	
	  //:::::::
	  //:::::::
	  function generateAll(sType, sText) {
	  
	  		var sNotyBody = sText;
	  		
	    generate('bottomRight', sType, sNotyBody);
	    
	    /*
	    var snd = new Audio("../../app/sound/Ring03.wav"); // buffers automatically when created
		snd.play();
		*/
	    
	  }

	  //:::::::
	  //:::::::	  	  
	  function NotyServerAlert()
	  {
	  	generateAll('warning', '<img src="../../app/noty/img/1348072346_001_11.png" style="width:24px; height:24px" align="absmiddle">&nbsp;&nbsp;You are running out of disk space!');

	  }
	  
	  //:::::::
	  //:::::::
	  function NotyUserOnline()
	  {
	  	generateAll('alert', '<img src="../../app/noty/img/1348082709_User.png" style="width:24px; height:24px" align="absmiddle">&nbsp;&nbsp;<b>Khalid Abed</b> is online');

	  }
	 

	  //:::::::
	  //::::::: 
	  function NotyWarning()
	  {
