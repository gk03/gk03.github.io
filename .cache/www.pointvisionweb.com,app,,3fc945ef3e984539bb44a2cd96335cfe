<script>

//:::::::
var m_tSiteTimeout 	 = null;
var m_bSiteTimeout 	 = false;
var m_iSiteTimeoutMS = 3600000;
var m_sTimeoutMsg = 'Your mail session has timed out. As a security precaution, sessions end after 30 minutes of inactivity.\n\nYou can login again to resume using your account.';

//:::::::
//:::::::
function fnInitialAutoSiteTimeout()
{
	m_tSiteTimeout = setTimeout(function() {
		
		$(document.body).text('');
		alert(m_sTimeoutMsg);
		fnSignoutUser();
	}, m_iSiteTimeoutMS);

	$(document).on('mousemove', function() {
	    if (m_tSiteTimeout !== null) {
	        clearTimeout(m_tSiteTimeout);
	    }
	
	    m_tSiteTimeout = setTimeout(function() {
			$(document.body).text('');
			alert(m_sTimeoutMsg);
			fnSignoutUser();

	    }, m_iSiteTimeoutMS);
	});

}
//:::::::


//:::::::
//:::::::
function fnConfirmBrowserReq()
{
	if ($.browser.name == "msie" && $.browser.versionNumber < 9)
	{
		fnYUIDialog("V12 requires advanced browsers that support HTML5. Older browsers just don’t have the chops to provide you with the same high-quality experience.<br><br>We’ll support the current and prior major release of Chrome, Firefox, Internet Explorer and Safari on a rolling basis.", 'OK, lets upgrade', 'Close')
		return(false);
	}


}

//:::::::
//:::::::
function fnSignin(sPageUsername, sPagePassword, sPageDomain, sPageRememberMe)
{

	if (mDOMReady == false)
		return;
	
	m_bFirstLogin = true;	//uad_korney 11/02/2015 Set the option "First login"						
	
	var obj_callback = new clsAjaxcallback(); 
	obj_callback.Ajaxcallback.success = fAXSignInResponse;
		
	var sURL = 'api=signin&username=' + encodeURI(sPageUsername) + '&password=' + encodeURIComponent(sPagePassword) + '&domaincode=enommail2worldcom&RememberMe=' + sPageRememberMe + '&domain=' + sPageDomain;
	
	//:::: For timezone
	var ctime = "9/13/2016 3:40:25 AM";
	var tz = null;
	if (new Date())
	{
		var mydate = new Date();
		tz = ( (mydate.getTimezoneOffset()/60) * -1 );
		if ((tz != null) && (tz > -15) && (tz < 15)) 
			sURL += "&tz=" + tz;
	
		var myyear = mydate.getYear();
		if (myyear < 200) myyear = myyear + 1900;
		ctime = (mydate.getMonth() + 1) + "/" + mydate.getDate() + "/" + myyear + " " + mydate.getHours() + ":" + mydate.getMinutes() + ":" + mydate.getSeconds();
	}
	
	if (ctime.length > 0)
		sURL += "&ctime=" + escape(ctime);
	//////

	
	makePOSTRequest('../../app/pageapi.asp', obj_callback.Ajaxcallback, sURL );
}

//:::::::
//:::::::
function escapeRegExp(string) {
    return string.replace(/([.*+?^=!:${}()|\[\]\/\\])/g, "\\$1");
}

//:::::::
//:::::::
function replaceAll(string, find, replace) {
  return string.replace(new RegExp(escapeRegExp(find), 'g'), replace);
}

//:::::::
//:::::::
function fnAutoSignin()
{	
	if (mDOMReady == false)
		return;
		
	var obj_callback = new clsAjaxcallback(); 
	obj_callback.Ajaxcallback.success = fAXSignInResponse;
	var sURL = '../../app/pageapi.asp?api=autosignin';
	
	//:::: For timezone
	var ctime = "9/13/2016 3:40:25 AM";
	var tz = null;
	if (new Date())
	{
		var mydate = new Date();
		tz = ( (mydate.getTimezoneOffset()/60) * -1 );
		if ((tz != null) && (tz > -15) && (tz < 15)) 
			sURL += "&tz=" + tz;
	
		var myyear = mydate.getYear();
		if (myyear < 200) myyear = myyear + 1900;
		ctime = (mydate.getMonth() + 1) + "/" + mydate.getDate() + "/" + myyear + " " + mydate.getHours() + ":" + mydate.getMinutes() + ":" + mydate.getSeconds();
		
	}
	
	if (ctime.length > 0)
		sURL += "&ctime=" + escape(ctime);
	//////

	makeRequest(sURL, obj_callback.Ajaxcallback);
	
}

//::::::
//::::::
function formatAMPM(date) {
  var hours = date.getHours();
  var minutes = date.getMinutes();
  var ampm = hours >= 12 ? 'PM' : 'AM';
  hours = hours % 12;
  hours = hours ? hours : 12; // the hour '0' should be '12'
  minutes = minutes < 10 ? '0'+minutes : minutes;
  var strTime = hours + ':' + minutes + ':' + date.getSeconds() + ' ' + ampm;
  return strTime;
}


//:::::::
//:::::::
function fAXSignInResponse(o)
{	
	//setup Date & GUID  :::::
	mIntDate = Date();
	mCurrentDate = Date();
	mGGUID = guid();
	//:::::::::::::::::::::::
	
	var objSAM = eval("(" + o.responseText + ")");
    
    //::::special HTTP reditect?
	if (objSAM.msg == "http_redirect")
	{
		location = unescape(objSAM.redirect_url);
		return;
	}
	//:::::::
		
	if (objSAM.msg == "0")
	{	
	
	    //check the last lang match the one the user have
		if ( (slang.toLowerCase() == '' ) && (objSAM.lang.toLowerCase() == 'en') )
		{
		
		}
		else
		{    
			if ( slang.toLowerCase() != objSAM.lang.toLowerCase() )
			{
				location = location;
				return;
			}
		}
	
		m_sUsername = objSAM.username.toLowerCase(); 
		m_sDomain = objSAM.domain.toLowerCase();   
		m_bPublicContacts = (objSAM.bpubliccontacts == 'True');
		m_bSigendin = true;
		
        //begin uad_korney 11/02/2015 Reload Page after first login to update MailSearchForm
        if (m_bFirstLogin)
        {
           location.reload();
           return;
        }
        //end uad_korney 11/02/2015		
		sName  =  objSAM.FirstName + ' ' +  objSAM.LastName;
		
		fnUpdateFullName(sName);
		
		asktologout = objSAM.asktologout;
		delemessageconf = objSAM.delemessageconf;
		autoaddsig = objSAM.autoaddsig;
		sendingdisabled = objSAM.sendingdisabled;

		fnLoadApp();
		
		//default open cal. widget?
		if (m_sCalWidgetDefault == 'open')
		{
			fnShowMailMiniCal(true);
		}
			
			
		//site has auto-timeout?
		if (m_bSiteTimeout)
		{
			fnInitialAutoSiteTimeout()
		}
		else
		{
			m_tSiteTimeout = null;
		}
	
		
	}
	else
	{		
		$('#ifm_login')[0].contentWindow.sLoginfailed();
	}
			
}


//:::::::
//:::::::
function fnUpdateFullName(sName)
{
	sEmail = m_sUsername + '@' + m_sDomain;
	
	if (sName.length < 2) 
		sName = sEmail;

	sFullLengthName = sName;
		
	m_ClientWidth = $(window).width();
	if ( m_ClientWidth < 1024)
	{
			if (sName.length > 10) 
				sName= sName.substr(0, 10) + '...'; 
	}

	if (sName != sEmail )
		sEmail = sFullLengthName + '\n' + sEmail
		
	fnInnerHTML('LoginEmail', ('<span id="LoginEmailSpan" style="cursor:default" sfullname="' + sFullLengthName + '" title="' + sEmail + '" class="jqTootltip">' + sName + '</span>') );

}


//:::::::
//:::::::
function fnUGUID()
{
    mCurrentDate = new Date();
    //var seconds = Math.round((mCurrentDate - mIntDate )/1000);
    //var minutes = Math.round(seconds / 60);
    
    var hours = Math.abs(mCurrentDate - mIntDate) / 36e5;
    hours = parseInt(hours);
    
    if ( hours > 4 )
    {
    	mGGUID = guid();
    	mIntDate = Date();
    }
        
    return(mGGUID);
}


//:::::::
//:::::::
function fnLoadApp()
{

			//KKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKKK
	//mMainBanner
	//MainHeader
	/*
	
	var mbTopBannerURL 			= "";
var mbTopBanner				= "false";
var miTopPanel_CustomHeight = ;

		
	*/
		if (mbTopBanner == 'true')
		{	
			var iMainHeader_height = parseInt($('#mMainHeader').css("height"));
			$('#mMainHeader').height(iMainHeader_height + miTopPanel_CustomHeight);
			$('#mMainBanner').height(miTopPanel_CustomHeight);
			var sHTMLBanner = '<iframe onload="" id="ifm_topbanner" style="background-color:#fff; height:100%; width:100%; border: #ffffff 0px solid;" scrolling="no" frameborder="0" border=0 name="ifm_topbanner" src="/TopMenu.asp" width="100%" height="100%" allowTransparency  borderwidth="0"></iframe>';
			$('#mMainBanner').html(sHTMLBanner);
			
			//

		}
	//////////////////////////////

		//::::::::::
		var sLoginEmail = m_sUsername + '@' + m_sDomain;
		sLoginEmail = sLoginEmail.toLowerCase(); 
	
		sStorageLoginEmail = fnSotrageLoad_LoginEmail();
		
		if ( (sStorageLoginEmail == false) || (sStorageLoginEmail != sLoginEmail) )
		{
			fnSotrageReset();
		}

		fnSotrageSave_LoginEmail(sLoginEmail);
		//::::::::::
		
		//check storage version
		data = fnSotrageLoad_Version();
		
		if (data == false)
		{
			//fnSotrageReset();
			fnSotrageSave_Version('1.0');
		}
		//:::::
		        
		HideInfobarInbox();
		
		//read folders from storage
		data = fnSotrageLoad_MailFolders();
		if (data == false)
		{
			//load mail-folders
			$.ajax({
		     type: "GET",
		     url: '../app/folders.asp',
		     data: "",
		     success: function(data) {
		        	fnSotrageSave_MailFolders(data);
					fInitFoldersModule(data); 
				}
		   });
	   }
	   else
	   {
	   		fInitFoldersModule(data);
	   		m_bStorageCall = true;
	   		m_iTotalIncomingMsg = 0;
	   		m_ObjCurretnSelectedDir = document.getElementById('udir' + 0);
	   		fnRefreshFolders(); 
	   }
	   //::::


		if (mSkinUI == 'telmex')
		{
			preload(
					"../../app/ress/theme/Telmex/IMG/main_correo.png",
					"../../app/ress/theme/Telmex/IMG/main_contacto.png",
					"../../app/ress/theme/Telmex/IMG/main_drive.png",
					"../../app/ress/theme/Telmex/IMG/main_calendario.png"
				)
		}
		else
		{
			preload(
				m_sRessIMGURL + "grippy.png",
				m_sRessIMGURL + "a.png",
				m_sRessIMGURL + "star_off.png"
			)
		
		}

	//grap and store the Drive storage
	fnBackpackGetStorage(true);

}


//:::::::
//:::::::	
function fInitFoldersModule(sHTTPData) 
{
				
	document.getElementById('spanLeftNavFolders').innerHTML = fnBuildMailDolder(sHTTPData);
	
	AutoCheckMailCount();
		
	$("#mLeftPanelBody").bind("contextmenu",function(e) {
		    e.preventDefault();
		    // create and show menu
		    fnDirContextMenu(e.pageX, e.pageY);
		
	});
	

		data = fnSotrageLoad_Inbox();
		
		if (data == false)
		{
			fnGetInbox('/Inbox');
		}
		else
		{
			m_bInboxSotrage = true;
			m_sFolderRecords = data;
			sCheckFolderREcords('');
			fnGetInbox('/Inbox');
			m_iTotalIncomingMsg = 0;
		}

	
	fnInitRTE();
	
	$(function(){
	    mInlineEditMode = 'inbox';
        $('.editable').inlineEdit();
    });

}

