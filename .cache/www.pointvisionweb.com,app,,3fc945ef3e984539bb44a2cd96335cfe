function fnOpenFeedback()
{

	sURL = "/app/feedback.asp"

	var iMsgDivTop = 80;
	var iMsgDivLeft= 120;
	
	var iCenterPos = (m_ClientWidth / 2) - (650 / 2) ;
	
	$("#feedbackForm-fullscreen").css( "display", "" );
	$("#divFeedbackFormView").css( {'position' : 'absolute', 'left' : (iCenterPos + 'px'), 'top' : (iMsgDivTop + 'px'), 'display' : 'none'} );

    
    var iMsgDivHeight = m_ClientHeight - (iMsgDivTop * 2);
    if (iMsgDivHeight > 550)
    	iMsgDivHeight = 540;
	var iMsgDivWidth  = m_ClientWidth - (iMsgDivLeft * 2);
		
	$('#divFeedbackFormView').width(650);
	$('#divFeedbackFormView').height(iMsgDivHeight + 35);
	
	$("#divFeedbackFormView").fadeIn( "fast", function() { });
	
	//set iframe height
	$('#ifm_FeedbackFormView').height(iMsgDivHeight - 5);
	
	$('#ifm_FeedbackFormView').attr('src','about:blank');
	$('#ifm_FeedbackFormView').fadeOut(200, function() 
	{ 	
		$("#ifm_FeedbackFormView").css( {'display' : ''} );
		$('#ifm_FeedbackFormView').attr('src',sURL);
	});	

}

function fnFeedbackFormExitFullscreen()
{
	$("#feedbackForm-fullscreen").css( "display", "none" );
	$("#divFeedbackFormView").css( "display", "none" );
	$('#ifm_FeedbackFormView').attr('src','about:blank');
}



