var ResizeIDTimer;
var m_smFolderDataHTML = '';
var m_MsgPanelHeadHTML = '';

//:::::::
//:::::::
function g_fInitPage()
{
	//reading panel resize fnOnResize
	
	mDOMReady = true;
	QHideInfobarInbox();
	
	//@@@@ ::::::::::: read and set panel mode and size ////
	sReadingPane = fnSotrageLoad_ReadingPane();
	if ( sReadingPane != false )
	{
		Msg_Panel_Type = sReadingPane.split("|")[0];
	}
	
	fnResizePage();
	
	m_smFolderDataHTML = $('#mFolderData').html();

	//@@@@ :::::::::::

	g_fInitReadingPane();
	
	//@@@@ ::::::::::: read and set panel mode and size ////
	if ( sReadingPane != false )
	{
		Msg_Panel_Size = sReadingPane.split("|")[1];
		
		if (Msg_Panel_Type == 'r')
		{
			mFolderDataWidth = Msg_Panel_Size;
			
			if (mFolderDataWidth < 345)
				mFolderDataWidth = 345;
				
			Msg_Panel_Inbox.resize(null, null, mFolderDataWidth, 0, 0, true); 
		}
	
		if (Msg_Panel_Type == 'b')
		{
			mFolderDataHeight = Msg_Panel_Size;
			
			if (mFolderDataHeight < 200)
				mFolderDataHeight = 200;
		
			Msg_Panel_Inbox.resize(null, mFolderDataHeight, null, 0, 0, true); 
		}

		
		
	}
	//@@@@ :::::::::::

	
	document.getElementById('mdivApps').ondragstart = function() { return false; };
	
	//@@@@ :::::::::::
	$(window).resize(function() {
		clearTimeout(ResizeIDTimer);
  		ResizeIDTimer = setTimeout(fnResizePage, 100);	
	});
	
	//@@@@ :::::::::::
	$(document).click(function(event) {
	  
	  //var target = $(event.target);
	
	  //if (!target.attr('id').match(/^mydiv/) && target.parents('#mydiv').length == 0) {
	  //  $('#mydiv').hide();
	  //}
	  	fnDocOnClick();
	  	
	  	if ( $('#MUserInboxAlert') !== undefined )
	  		QHideInfobarInbox();
	});
	
	fnNewYUIUploader();
		
	
	$.ajax({
	
		type: "GET",
		url: '../../sql/ws.asp',
		data: "Function=MemberIDV12Login", 
		success: function(data) { 
		
			//check for mobile devices and updates m_bMobileDevice 
			isMobileDevice();
		     
		    if ( (data.length > 0) && (data != -99) ) 
			{
				if (m_bMobileDevice == false)
					fnAutoSignin();
				else
				{
					window.location = "../../touch/home.asp"
				}
			}
			else
			{
				
				//if mobile device, redirect to touch
				if (m_bMobileDevice == false)
					$('#ifm_login')[0].contentWindow.fnLoginNoSession();
				else
				{
					window.location = "../../touch/"
				}

				
			}
		}
	});

	
}

//:::::::
//:::::::
function fnSignout()
{
	if (asktologout == "nah")
	{
		fnSignoutUser();
	}
	else
	{

	if ( fnYUIPrompt('<div>' + mcLang.Text('lang_Gen_12',1) + '</div>', mcLang.Text('lang_Gen_13',1), mcLang.Text('lang_Gen_14',1), 'message icon-bubble', fnSignoutUser, arguments) )
			return;
	}
}

//:::::::
//:::::::
function fnSignoutUser()
{

	var cookies = $.cookie();
	for(var cookie in cookies) {
	   $.removeCookie(cookie, { path: '/' });
	}
	
	$.cookie('lang', slang, { expires: 7, path: '/' });
	
	var sPageURL = window.location.search.substring(1);
	var sDomaincode = fnGetUrlParameter(sPageURL, 'domaincode');
	
	//load mail-folders
	$.ajax({
			 type: "GET",
		     url: '../app/rac.asp',
		     data: "",
		     success: function(data) {

					if (typeof sDomaincode != 'undefined')
						location.href = '../../app/?ss=' + guid() + '&domaincode=' + sDomaincode;
					else
						location.href = '../../app/?ss=' + guid();


				}
	});

}

//:::::::
//:::::::
function fnResizePage()
{
	if (mDOMReady != true)
		return;

	var lPanelmargin = 2;
	var imMainPanelBannerWidth = 173;
	if (mbRightPanelBanner == false)
		imMainPanelBannerWidth = 0;

	m_ClientHeight = $(window).height();
	m_ClientWidth = $(window).width();
	
	//console.log(m_ClientWidth)
	
	
	if ( $('#LoginEmailSpan') !== undefined )
	{
		sFullName = $('#LoginEmailSpan').attr('sfullname');
		fnUpdateFullName(sFullName);
	}
	

	if ( m_ClientWidth <= 800)
	{
		m_ClientWidth = 800;		

	}
	
	if (mbRightPanelBanner)
	{
		if ( m_ClientWidth < 1024)
		{
			imMainPanelBannerWidth = 0;		
			
		}
	}
	
	var mMainHeader_height = parseInt($('#mMainHeader').css("height"));
	var mMainPanel_height = m_ClientHeight - mMainHeader_height - (lPanelmargin * 2);
	var mLeftPanel_width = 210 +  mLeftPanel_Customwidth;
	var mRightPanel_width = 0//150;
	var mMainPanel_width = (m_ClientWidth - (mLeftPanel_width)) - (lPanelmargin * 1);
	
	mMainPanel_width -= imMainPanelBannerWidth;
	
	$('#mLeftPanel').width(mLeftPanel_width);
	$('#mMainPanel').width(mMainPanel_width);

	mimLeftPanelWidth = mLeftPanel_width;
	mimMainPanelWidth = mMainPanel_width;
	mimMainPanelHeight = mMainPanel_height;
	
	$('#mMainPanel').width(mMainPanel_width);
	$('#mLeftPanel').width(mLeftPanel_width);
	
	if (mbRightPanelBanner)
		$('#mMainPanelBanner').height(mMainPanel_height);
	
	$('#mMainPanel').height(mMainPanel_height);
	$('#mLeftPanel').height(mMainPanel_height);
	$('#mRightPanel').height(mMainPanel_height);
	$('#MainWrapper').height(mMainPanel_height);
	$('#mFolderData').height(mMainPanel_height - 22);
		
	/*
	$("#MainWrapper").css({
	     "min-width": "1300px"
	 });
	*/		
	//slog = 'mMainPanel:' + $('#MainWrapper').css("min-width") + '  mimLeftPanelWidth:' + mimLeftPanelWidth + '  m_ClientWidt:' + m_ClientWidth + ' both:' + (m_ClientWidth - mimLeftPanelWidth)
	//console.log(slog)
	
	try
   {
   		//close any  menus
   		fnDocOnClick();
   		
   		fnCheckFolderScroll();
   		

				var mPOPOpt_Height = $('#mdivApps').outerHeight();
				var MainWrapperHeight = $('#MainWrapper').height();
				$('#mdivApps').height(MainWrapperHeight -32);;
				$('#mdivApps').css('overflow-y', 'auto');
				
		fnResetPanel();

   		//Run some code here
   		fnREsizeInboxCol();
   		       
       if (document.getElementById("raptor-wrapper").style.visibility == "visible")
			fnResizeRTE();
			
		ReSizeMsgPanel();
		
		//Backpack
		if (m_iCurrentAppIndex == 3)
		{
			YAHOO.util.Dom.setStyle('mdivApps', 'height', ((mimMainPanelHeight - 31) + "px"));
		}
	
   }
   catch(err)
   {
   		//Handle errors here
   }


}


