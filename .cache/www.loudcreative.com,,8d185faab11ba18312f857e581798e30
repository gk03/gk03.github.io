$(document).ready(function() {
   $("#email-txt-input input").focus(function(e){
   	if ($(this).val() == "Enter email address") $(this).val("");
   });
   $("#email-txt-input input").blur(function(e){
   	if ($(this).val() == "") $(this).val("Enter email address");
   });
 });
</script>
