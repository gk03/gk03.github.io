<script type="text/javascript">
$(document).ready( function(){
var gotoTop = $('#goToTop');
var func_gotoTop = function(){
                if($(this).scrollTop() == 0){
                                gotoTop.hide();                                                               
                }
                else{
                                gotoTop.show();                             
                }
}



func_gotoTop();

$(window).scroll(func_gotoTop);

//Click event to scroll to top

$('#goToTop').click(function(){
                $('html, body').animate({scrollTop : 0},500);
                return false;
}) });
</script>

