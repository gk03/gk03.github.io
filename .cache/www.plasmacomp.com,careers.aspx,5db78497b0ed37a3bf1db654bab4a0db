    <script type="text/javascript">
        /* New Job */
        $(document).ready(function () {
            var animTime = 300,
                clickPolice = false;

            $(document).on('touchstart click', '.acc-btn', function () {
            
            if ($(this).find('h1').hasClass("selected")) {
                
                if (!clickPolice) {
                    clickPolice = true;

                    var currIndex = $(this).index('.acc-btn'),
                        targetHeight = $('.acc-content-inner').eq(currIndex).outerHeight();

                    $('.acc-btn h1').removeClass('selected');
                    $(this).find('h1').removeClass('selected');

                    $('.acc-content').stop().animate({ height: 0 }, animTime);
                    $('.acc-content').eq(currIndex).stop().animate({ height: 0 }, animTime);

                    setTimeout(function () { clickPolice = false; }, animTime);
                }
            }
            else {
                if (!clickPolice) {
                    clickPolice = true;

                    var currIndex = $(this).index('.acc-btn'),
                        targetHeight = $('.acc-content-inner').eq(currIndex).outerHeight();

                    $('.acc-btn h1').removeClass('selected');
                    $(this).find('h1').addClass('selected');

                    $('.acc-content').stop().animate({ height: 0 }, animTime);
                    $('.acc-content').eq(currIndex).stop().animate({ height: targetHeight }, animTime);

                    setTimeout(function () { clickPolice = false; }, animTime);
                }
            }
        });


        });
        function HideModelBackGroundForWaivers() {
            $('#ModelBackGroundForWaivers').hide();
            $('#waiversbodypart').hide();
            $('#iframeforwaivers').attr('src', '');
        }

        function ShowModelBackGroundForWaivers(newurl) {
            var url = "ApplyJob.aspx";
            newurl = url;
            $('#ModelBackGroundForWaivers').show();
            $('#waiversbodypart').show();
            $('#iframeforwaivers').attr('src', newurl);
        }

        function Bind(val) {
            $("#hdnChk").val(val);            
        }

        
        function validateFileSize() {

            var uploadControl = document.getElementById('FileUploader');
            if (uploadControl.files[0].size > 1024000) { //
                 //alert('Uploaded file should not be larger than 2MB.');
                 $('#btnConnect').prop('disabled', true);
                 $('#lblmsg1').text('Uploaded file should not be larger than 1 MB.')
                 return false;
             }
             else {
                 //alert('valid');
                 $('#btnConnect').prop('disabled', false);
                 $('#lblmsg1').text('')
                 return true;
             }
        }

        function showModelViewDiv(Title, Location) {            
            $('#lblTitle1').text(Title);            
            $("#ddlLocation1").val(Location);
            //document.getElementById('div_contact').style.display = '';
            return false;
        }

        function HideDeviceAssociate(CarrerId) {
            document.getElementById('DivContact').style.display = 'none';
            return false;
        }
     
    </script>


    
