		<script>
		// Checks for any open menus, closes them, and toggles the menu clicked on
		$('.dropdown').mouseenter(function() {
			if ($('.dropdown-menu', this).css('display') == 'none') {
				$('.dropdown-menu').not(this).fadeOut('slow');
			}
			$('.dropdown-menu',this).fadeIn('slow')
		});
		
		// Prevents menu from closing when an item link is clicked
		$('.dropdown-menu').mouseenter(function(event){
			event.stopPropagation();
		});
		
		// Closes menu(s) if the mouse leaves the menu
		$('.main-menu').mouseleave(function() {
			$('.dropdown-menu').delay(250).fadeOut('slow');
		});
		// Closes menu(s) when link that is not a dropdown is entered 'not-dropdown' class needs to be assigned to element
		$('.nodrop').mouseenter(function() {
			$('.dropdown-menu').fadeOut('slow');
		});
		</script>
	</div>
