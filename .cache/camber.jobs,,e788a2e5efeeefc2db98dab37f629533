<script>
$(window).scroll(function(){
	var sticky = $('.sticky'),
	stickyLogo = $('#logo'),
	mainMenu = $('#mainMenu > ul > li > a'),
	homeIcon = $('.main-menu ul li i'),
	scroll = $(window).scrollTop();
	if (scroll >= 42) {
		sticky.addClass('fixed');
		stickyLogo.addClass('width-101');
		mainMenu.css({'line-height':'60px', 'padding':'0 10px'});
		homeIcon.css('line-height', '60px');
	} else {
		sticky.removeClass('fixed');
		stickyLogo.removeClass('width-101');
		mainMenu.css({'line-height':'70px', 'padding':'10px 10px'});
		homeIcon.css('line-height', '70px');
	}
});

function toggleDiv(divId) {
	$("#"+divId).slideToggle("fast");
}

function toggleDiv2(divId2) {
	$("#"+divId2).slideToggle("fast");
}
</script> 

    

