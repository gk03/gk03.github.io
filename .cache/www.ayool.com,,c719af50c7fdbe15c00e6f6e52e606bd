	var slideShowSpeed = 5000;
	// Duration of crossfade (seconds)
	var crossFadeDuration = 3;
	// Specify the image files
	var Pic = new Array();
	var linklist	= "index.cfm,index.cfm,index.cfm,index.cfm,index.cfm";
	var Link		= linklist.split(',');
	// to add more images, just continue the pattern, adding to the array below
	Pic[0] = 'images/index_0.jpg'
	Pic[1] = 'images/index_1.jpg'
	Pic[2] = 'images/index_2.jpg'
	Pic[3] = 'images/index_3.jpg'
	Pic[4] = 'images/index_4.jpg'
