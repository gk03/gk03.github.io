<script type="text/javascript">

var clients=["true_course_logo.png", "WMS.png", "TheHealthCollaborativeOfCincinnati.png", "JPD.png", "JataraSolutions.png", "icsi.png", "HMCT.png", "HealthInsight.png", "HCIF.png", "DayOneCornerstone.png", "Aligning_Forces_for_Quality_South_Central_Pennsylvania.png", "C3Partners.png", "mncm.png", "phcqa.png", "wchq.png", "quality_quest_small_logo.png", "MHQP.png", "new_hampshire.png", "wapc.png"];
var clientweight=[1,5,5,1,5,1,10,5,5,1,1,5,10,5,10,1,10,5,10];
var images = new Array();

for (a=0; a<3; a++){

var totalweight=eval(clientweight.join("+"));
var weighedclients=new Array();
var currentclient=0;

while (currentclient<clients.length){
	for (i=0; i<clientweight[currentclient]; i++){
		weighedclients[weighedclients.length]=clients[currentclient];
	}
	currentclient++;
}
var randomnumber = Math.floor(Math.random()*totalweight);
images[a] = weighedclients[randomnumber];
var removeme = clients.indexOf(weighedclients[randomnumber]);
clients.splice(removeme,1);
clientweight.splice(removeme,1);
}

</script>

