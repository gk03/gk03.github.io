	loadSuccesses = function(myHrefQS){
		var flashvars = {}; flashvars.successString = $("#successString").text(); flashvars.urlString = $("#urlString").text(); flashvars.titleString = $("#titleString").text(); flashvars.pageString = $("#pageString").text();
		var params = {}; params.allowScriptAccess = "always"; params.wmode = "transparent";
		swfobject.embedSWF("/swf/successes.swf?vc=1776974", "successes2", "1000", "250", "9.0.0", "expressInstall.swf", flashvars, params);
	};
	$("ul.sf-menu").superfish({
		animationOpen:	 {height:"show"},
		animationClose:	 {height:"hide"},
		speed:           "normal",
		delay:            300,
		dropShadows:      false,
		autoArrows:       false
	});
	setStaticLinks = function(){
		staticLinks = new Array();
		$("#logo, #menu a, #featured a, #navBtm a").each(function(i){
			staticLinks[i] = $(this).attr("href");
		});
	}
	getStaticLinks = function(){
		$("#logo, #menu a, #featured a, #navBtm a").each(function(i){
			$(this).attr("href", staticLinks[i]);
		});
	}
	setStaticLinks();
	reloadPage = function(){
		$("#videoBg").show();
		$("#content").css({margin:0});/*Remove JS-less margin*/
		getStaticLinks();
		$("a:not(.g4ImgA)").click(function(){
			var myHref = $(this).attr("href");
			if(myHref != "javascript:void(0)" && myHref != "#" && myHref.substring(myHref.length - 4) != ".pdf"){
				var myHrefQS = ""; var myHrefHash = "";
				var myHrefDomain = myHref.substring(0, 24);
				if(myHrefDomain == "http://www.inet-web.com/" && myHref.lastIndexOf("/") > 24){myHrefDomain = "";}
				if(myHrefDomain == "http://www.inet-web.com/"){
					myHref = myHref.substring(23); /*Added because IE retrieves full URL*/
				}
				$(this).attr("href", "javascript:void(0)");
				if(myHref.substring(0, 1) == "."){myHref = myHref.substring(5);} /*Check for ../.. in Site Edit*/
				if(myHref == "/"){
					myHref = "/default.asp";
				}else if(myHref.substring(0, 28) == "/winningWebsiteSolutions.asp"){
					myHrefQS = myHref.substring(28);
					myHref = myHref.substring(0, 28);
					$("#successNav").load(myHref + myHrefQS + " #successNav2");
				}else if(myHref.substring(0, 27) == "winningWebsiteSolutions.asp"){
					myHrefQS = myHref.substring(27);
					myHref = "/" + myHref.substring(0, 27);
					$("#successNav").load(myHref + myHrefQS + " #successNav2");
				}else if(myHref.substring(0, 26) == "/websiteSuccessDetails.asp"){
					myHrefQS = myHref.substring(26);
					myHref = myHref.substring(0, 26);
				}else if(myHref.substring(0, 30) == "/webDeveloperCareersDetail.asp"){
					if(myHref.search("#") != -1){
						myHrefQS = myHref.substring(30, myHref.indexOf("#"));
					}else{
						myHrefQS = myHref.substring(30);
					}
					myHref = myHref.substring(0, 30);
				}else if(myHref.substring(0, 29) == "webDeveloperCareersDetail.asp"){
					myHrefQS = myHref.substring(29);
					myHref = myHref.substring(0, 29);
				}else if(myHref.substring(0, 32) == "/creativeGeniusWebDevelopers.asp"){
					myHrefQS = myHref.substring(32);
					myHref = myHref.substring(0, 32);
				}else if(myHref.substring(0, 26) == "/webDevelopersGlossary.asp"){
					myHrefHash = myHref.substring(26);
					myHref = myHref.substring(0, 26);
				}else if(myHref.search("#") != -1){
					myHref = myHref.substring(myHref.indexOf("#"));
				}
				if(myHref.substring(0, 7) == "http://" && myHrefDomain != "http://www.inet-web.com/"){
					var myTarget = $(this).attr("target");
					if(myTarget != ""){
						window.open(myHref, myTarget);
					}else{
						top.location.href = myHref;
					}
					var notHere = true;
					$(this).attr("href", myHref);
				}else if(myHref.search("#") != -1){
					top.location.href = myHref;
					$("#content").jScrollPane();
					$(this).attr("href", myHref);
				}
				if($(this).attr("class") != "btnL" && myHref.search("#") == -1 && notHere != true){
					$("#ajaxBox1").load(myHref + myHrefQS + " #ajaxBox2", function(){
						resizeContent(myHref, myHrefQS, myHrefHash);
						if(myHref != "/winningWebsiteSolutions.asp" && myHref != "/websiteSuccessDetails.asp"){reloadPage();}
						$("#content").jScrollPane();
					});
				}
			}
		});
	};
	reloadPage();
	$("#successes").hide();
	resizeContent = function(myHref, myHrefQS, myHrefHash){
		$("#meetCreativeGeniuses, #successes").hide();
		if(myHref != "/websiteSuccessDetails.asp"){$("#featured").hide(); $("#successesNavSearch").hide();}
		$("#content").jScrollPaneRemove();
		var top1, right1, width1, height1, width2, height2;
		if(myHref == "/" || myHref == "/default.asp"){
			top1 = 207; right1 = 0; width1 = 595; height1 = 358; width2 = 555; height2 = 318;
			$("#featured").show();
		}else if(myHref == "/creativeGeniusWebDevelopers.asp"){
			top1 = 640; right1 = 110; width1 = 862; height1 = 150; width2 = 822; height2 = 110;
			var flashvars = {};
			var params = {}; params.wmode = "transparent";
			swfobject.embedSWF("/swf/sphere_as3.swf", "meetCreativeGeniuses", "500", "500", "9.0.0", "expressInstall.swf", flashvars, params);
				$("#meetCreativeGeniuses").show();
		}else if(myHref == "/winningWebsiteSolutions.asp"){
			top1 = 407; right1 = 110; width1 = 862; height1 = 170; width2 = 822; height2 = 130;
			$("#successesNavSearch, #successes").show();
			$("#successNav").load(myHref + myHrefQS + " #successNav2", function(){
				$("#successes").load(myHref + myHrefQS + " #successes2", function(){
					loadSuccesses(myHrefQS);
					reloadPage();
				});
			});
		}else if(myHref == "/websiteSuccessDetails.asp"){
			$("#success_details img").hide(); /* Added so alt image doesn't display first */
			top1 = 240; right1 = 110; width1 = 862; height1 = 340; width2 = 822; height2 = 300;
			$.post("/includes/successDetailsPost.asp" + myHrefQS, function(successDetails){
				$("#featured").hide(); $("#successesNavSearch").show();
				$("#successDetails").load(myHref + myHrefQS + " #successDetails2", function(){
					$("#successNav").load(myHref + myHrefQS + " #successNav2", function(){
						reloadPage();
						var flashvars = {}; flashvars.successDetails = successDetails;
						var params = {}; params.wmode = "transparent";
						swfobject.embedSWF("/swf/success_details.swf?vc=56799391", "success_details", "400", "311", "9.0.0", "expressInstall.swf", flashvars, params);
						$("#success_details img").show(); /* Added so alt image doesn't display first */
					});
				});
			});
		}else if(myHref == "/videoMarketing.asp"){
			var flashvars1 = {}; flashvars1.allowfullscreen = "true"; flashvars1.file = "http://www.inetshow.com/inet/MDAF_1510_SM2.flv";
			flashvars1.image = "http://www.milwaukeedomesartfestival.com/swf/tvPreview.jpg"; flashvars1.autostart = "false";
			var params1 = {}; params1.wmode = "opaque"; params1.allowfullscreen = "true";
			swfobject.embedSWF("/files/mediaplayer.swf?vc=53792435", "spot2", "533", "300", "9.0.0", "expressInstall.swf", flashvars1, params1);
			var flashvars = {}; flashvars.allowfullscreen = "true"; flashvars.file = "http://www.inetshow.com/inet/TV_Spot9.flv";
			flashvars.image = "http://www.inetshow.com/inet/placeholder2.jpg"; flashvars.autostart = "false";
			var params = {}; params.wmode = "opaque"; params.allowfullscreen = "true";
			swfobject.embedSWF("/files/mediaplayer.swf?vc=53792435", "spot1", "533", "300", "9.0.0", "expressInstall.swf", flashvars, params);
			top1 = 240; right1 = 110; width1 = 862; height1 = 340; width2 = 822; height2 = 300;
			$("#featured, #successesNavSearch").hide();
		}else{
			top1 = 240; right1 = 110; width1 = 862; height1 = 340; width2 = 822; height2 = 300;
			$("#featured, #successesNavSearch").hide();
		}
		$("#wrapWrap #contentBg, #wrapWrap #contentWrap").animate({
			top:       top1,
			right:     right1,
			width:     width1,
			height:    height1
		}, 1000);
		$("#wrapWrap #content").css({
			width:     width2,
			height:    height2,
			overflow:  "auto"
		});
		$("#content").jScrollPane();
		if(myHref == "/webDevelopersGlossary.asp"){
			var scrollOffset = 0;
			if(document.all){scrollOffset = parseInt($("#content").offset().top);}/*IE-Specific Offset*/
			$("#content")[0].scrollTo(parseInt($(myHrefHash).offset().top) - scrollOffset);
		}
		fixPNG();
	};
	reloadPageFlash = function(myHref){
		var myHrefQS = "";
		if(myHref.substring(0, 26) == "/websiteSuccessDetails.asp"){
			myHrefQS = myHref.substring(26);
			myHref = myHref.substring(0, 26);
		}
		$("#ajaxBox1").load(myHref + myHrefQS + " #ajaxBox2", function(){
			resizeContent(myHref, myHrefQS);
			if(myHref != "/winningWebsiteSolutions.asp" && myHref != "/websiteSuccessDetails.asp"){reloadPage();}
		});
		$("#ajaxBox1").ajaxError(function(){$("#ajaxBox1").replaceWith("");});
	};
	doSearch = function(){
		var whichSearch = "";
		if($("#radios").size() == 1){whichSearch = "1";}
		myHrefQS = "?SQ=" + escape($("#SQ" + whichSearch).val()) + "&M=";
		var searchMethod = $("#radios input:checked").attr("value");
		switch(searchMethod){
			case "0":	myHrefQS += "0"; break;
			case "2": 	myHrefQS += "2"; break;
			default: 	myHrefQS += "1"; break;
		}
		$("#ajaxBox1").load("/successResults.asp" + myHrefQS + " #ajaxBox2", function(){
			resizeContent("/successResults.asp", myHrefQS);
			reloadPage();
		});
	};
	checkSearchKey = function(event){
		if(event.keyCode == 13){doSearch();}
	};
	checkContactKey = function(event, serialized){
		if(event.keyCode == 13){submitContactForm(serialized);}
	};
	checkCareerKey = function(event, serialized){
		if(event.keyCode == 13){submitCareerForm(serialized);}
	};
	submitContactForm = function(){
		var ccStatus;
		$.ajax({
			async: false,
			url: "/shared_inc/FormProtect/incCaptchaFunctionsAjax.asp",
			type: "GET",
			data: {
				CS: "",
				TR: document.forms[0].TuringKey.value,
				TC: document.forms[0].yeKgniruT.value
			},
			success: function(responseText) {ccStatus = responseText;},
			dataType: "text"
		});
		if (ccStatus == 0) {
			document.forms[0].TuringKey.value = "** Please Try Again **";
			return false;
		} else {
			formData = $("#contactInet").serialize();			
			$.post("/includes/contactSend.asp", formData, function(){
				$("#ajaxBox1").load("/contactResponse.asp #ajaxBox2", function(){
					$("#content").jScrollPane();
					reloadPage();
				});
			});
		}
	};
	submitCareerForm = function(){
		var ccStatus;
		$.ajax({
			async: false,
			url: "/shared_inc/FormProtect/incCaptchaFunctionsAjax.asp",
			type: "GET",
			data: {
				CS: "",
				TR: document.forms[0].TuringKey.value,
				TC: document.forms[0].yeKgniruT.value
			},
			success: function(responseText) {ccStatus = responseText;},
			dataType: "text"
		});
		if (ccStatus == 0) {
			document.forms[0].TuringKey.value = "** Please Try Again **";
			return false;
		} else {
			formData2 = $("#contactInet").serialize();
			$.post("/includes/careerSend.asp", formData2, function(){
				$("#ajaxBox1").load("/careerResponse.asp #ajaxBox2", function(){
					$("#content").jScrollPane();
					reloadPage();
				});
			});
		}
	};
	checkTab = function(event){
		if(event.keyCode == 9){$("#content").jScrollPane();}
	}
	fixPNG = function(){
		$(document).pngFix();	
	}
});

</script>
</body>
</html>

