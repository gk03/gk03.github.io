<script type="text/javascript">
   jQuery( window ).load(function () {
    var $wcontainer = jQuery('#carouselcontainer');
    var $container = jQuery('#home-custom-carousel');
        function initCarousel_custom() {
          $container.carouFredSel({
              scroll: {items:1, easing: "swing", duration: 700, pauseOnHover : true},
              auto: {play: true, timeoutDuration: 9000},
              prev: '#prevport_custom_carousel',
              next: '#nextport_custom_carousel',
              pagination: false,
              swipe: true,
                items: {visible: null
                }
            });
        }
        setWidths();
        initCarousel_custom();
        jQuery(window).on("debouncedresize", function( event ) {
            $container.trigger("destroy");
            setWidths();
            initCarousel_custom(); 
            });
          function getUnitWidth() {
          var width;
          if(jQuery(window).width() <= 480) {
          width = $wcontainer.width() / 2;
          } else if(jQuery(window).width() <= 767) {
          width = $wcontainer.width() / 3;
          } else if(jQuery(window).width() <= 990) {
          width = $wcontainer.width() / 4;
          } else {
          width = $wcontainer.width() / 5;
          }
          return width;
          }
          function setWidths() {
          var unitWidth = getUnitWidth() -1;
          $container.children().css({ width: unitWidth });
          }

});
</script>    
