<script type="text/javascript">
    function subSearchForm(subtype) {
        var URL = 'http://www.adp.com/site-search.aspx?q=' + document.getElementById("site-search").value;
        document.location.href = URL;
        return false;
    }
    function checkReturn(e) {
        if (window.event) // IE
        {
            keynum = e.keyCode
        }
        else if (e.which) // Netscape/Firefox/Opera
        {
            keynum = e.which
        }
        if (keynum == 13) {
            return subSearchForm('rec');
        }
    }
	</script>
                    
