				<DIV class="ms-rte-embedcode ms-rte-embedwp"><script type="text/javascript">
        $(document).ready(function () {
            $("#DeltaPlaceHolderMain").addClass("home-image-banner");

$.ajax({

        url: getRelativeUrl() + "/news/_api/Lists/getByTitle('Pages')/items?$select=Title,EncodedAbsUrl,NewsMediaSource&$orderby=ArticleStartDate desc&$filter=ContentType eq 'News Page' and substringof('News',FileRef)",
        type: "GET",
        headers: { "accept": "application/json; odata=verbose" },
        success: function (data) {

            var listData = data.d.results;
          var val;
		  val="	<div class='caption-wrapper container row'>";
		  for(var i=0;i<listData.length;i++)
		  {
			var media= listData[i].NewsMediaSource;
			var res= media.split('-');	
			val += "<p class='caption container row'>"+listData[i].Title + "-" + res[1]+ "<span><a href='"+listData[i].EncodedAbsUrl+"' >Read More</a></span></p>";
break;
		  }
          val+="</div>"
            $('.home-slider').append(val);
          
        },
        error: function () {
            //alert(errorCode + ErrorEvent + Error);
        }
    });
        });
    </script></DIV>
