                                var Searchtxt;
                                var url;
                                function SubmitFrm() {
                                    Searchtxt = document.getElementById("txtSearch").value;
                                    url = getRelativeUrl() + "/search?keyword=" + Searchtxt;
                                    window.location = url;
                                }
                                function getRelativeUrl() {
                                    var url = "";
                                    if (_spPageContextInfo.siteServerRelativeUrl == "/") {
                                        url = "";
                                    }
                                    else {
                                        url = _spPageContextInfo.siteServerRelativeUrl;
                                    }
                                    return url;
                                }
                            </script>
                     
