                        $(function () {
                            



                       

                            $("#assetDownload").submit(function (e) {
                                e.preventDefault();
                                var dataString = 'elqSiteID=1716500231&elqFormName=industryResourcesDocumentDownloadForm-132807644210&firstName=' + $('input#firstName').val() + '&lastName=' + $('input#lastName').val() + '&emailAddress=' + $('input#emailAddress').val() + '&company=' + $('input#company').val() + '&AssetTitle=' + $('input#AssetTitle').val();
                                //alert('WHEN NEW SITE IS LIVE DATA IS SENT TO ELOQUA...I TURNED THIS OFF FOR TESTING');
								//alert($('input#AssetTitle').val());
								//alert(dataString)

                                $('#OptInForm').fadeOut(500);
								$('#assetImage').fadeOut();
                               
                                setTimeout(function () {
                                    $('#responseMessage').fadeIn();
                                }, 600);

                          
								
								
								
								

                                    $.ajax({

                                       type: "POST",
                                       url: "/PHP/eloquaProxy.php",
                                       data: dataString,
                                        success: function () {
                                           //$('#responseMessage').html("<h2>Contact Form Submitted!</h2>");
                                           $('#OptInForm').fadeOut();
										  
                                           
                                       // OptInForm
                                           setTimeout(function () {
												
                                              $('#responseMessage').fadeIn();
                                           }, 1000);
                                       }

                                    });



                                return false;
                            });


                        
                          

                        });

                      


                    </script>
