<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script type="text/javascript" src="/js/vendor/modernizr-2.6.2.min.js?v3"></script>
<script type="text/javascript" src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.8/jquery.validate.min.js"></script>
<script type="text/javascript" src="/css/custom/owl/owl.carousel.js"></script>

<script src="/css/custom/nav.jquery.min.js?v3"></script>
<script src="/css/custom/progressbar/jquery-scrollbar.js?v3"></script>
<script type="text/javascript" src="/css/custom/navigation/global.js?v3"></script>
<script type="text/javascript">
    $('.nav').nav();
    $("#progress-bar").onscroll({backgroundColor: '#FC6D58', height: '5px', position: 'fixed'});
    var float = $('#main_navigation').offset().top;

    $(window).on("scroll touchstart", function () {
        if ($(window).scrollTop() > float)
        {
            $('#helpline-header').addClass('hide');
            $('#main_navigation').addClass('main_floatdiv');
            $('#hori_logo').removeClass('hide');
            $('#small_logo').addClass('hide');
            $('nav.nav').addClass('top12');

        } else
        {
            $('#main_navigation').removeClass('main_floatdiv');
            $('#helpline-header').removeClass('hide');
            $('#hori_logo').addClass('hide');
            $('#small_logo').removeClass('hide');
            $('nav.nav').removeClass('top12');
        }
    });

</script>

<script type="text/javascript">
   

        $('#headerform').validate({
            rules: {
                txtname: {
                    required: true

                },
                txtemail: {
                    required: true,
                    email: true
                },
                txtphne: {
                    required: true,
                    minlength: 10,
                    maxlength: 14

                }
            },
            messages: {
                txtname: {
                    required: "Please enter your name"
                },
                txtphne: {
                    required: "Please enter your phone number"
                },
                txtemail: {
                    required: "Please enter your email address"
                }
            },
              submitHandler: function(form) {
                // do other things for a valid form
                $.ajax({
                    type: 'post',
                    url: '/inserts/insertrequest',
                    data: $('#headerform').serialize(),
                    beforeSend: function(){
                        $('#headerform .homeSearchBtn').val('Sending..');
                    },
                    error: function(){
                        $('#headerform .homeSearchBtn').val('SUBMIT');
                    },
                    success: function(data) {
                    $('#headerform .homeSearchBtn').val('SUBMIT');
                        if (data == "3" || data == "2" || data == "5" || data == "6") {
                            $('#headermsg').html("<div class='alert alert-success'>Thank you for your interest in our case study, we will be emailing it to you right away!</div>");
                        } else if (data == "1" || data == "4") {
                            $('#headermsg').html("<div class='alert alert-success'>Thank you for downloading our Whitepaper. We have sent it to your email.</div>");
                        } else {
                            $('#headermsg').html("<div class='alert alert-success'>We appreciate your interest. Your white paper / case study has been sent to the email address you provided.</div>");
                        }
                        $('#txtname,#txtemail,#txtphne,#txtarea').val('');
                    }
                });
            }
   });

</script>
 <script>
            jQuery(function ($) {
                $(".ivm-web-design ").prev().addClass("ivm-active-header");
            });
        </script> 
        

