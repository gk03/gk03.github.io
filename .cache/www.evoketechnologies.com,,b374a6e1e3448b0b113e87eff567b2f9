<section class="portfolio_section mainpage_portfolio_section">
     <h2 class="news_title">News<span> and</span> Events</h2>
       
     
    <div id="demo">
       
            <div class="col-xs-12">

              <div id="owl-demo" class="owl-carousel">
                <div class="item twi">
                    <a class="twitter-timeline"  href="https://twitter.com/EvokeUS" data-widget-id="540500137433247744">Tweets by @EvokeUS</a>
            <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+"://platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
                </div>
                <div class="item whitepaper">
