<script>

function isEmail(str){ 

	res = /^\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/; 

	var re = new RegExp(res); 

	return !(str.match(re) == null); 

}

function getBlogAlert(name){

	$email = jQuery('input[name=' + name + ']').val();

	if (isEmail($email)) {

		jQuery.post(

			"/blog/subscribe-2/",

			{

				email: $email,

				ip: '200.35.152.52',

				subscribe: "Subscribe",

				uri: "http://"

			},

			function(result){

				alert('A confirmation message is on its way!');

			}

		)

	}

}



jQuery('.side_category').hover(function(){

	jQuery('.side_category ul').css('display', 'block')

}, function(){

	jQuery('.side_category ul').css('display', 'none')

})



	

jQuery('.nav-item').hover(function(){

	jQuery(this).find('.nav-item-dropdown').show();

}, function(){

	jQuery(this).find('.nav-item-dropdown').hide();

});

</script>
