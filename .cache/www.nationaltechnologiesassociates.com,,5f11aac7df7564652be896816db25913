<script type="text/javascript">
	$(document).ready(function(){
		//fx interval: the rate at which animations fire 
	$("#featured").tabs({fx:{opacity: "toggle"}}).tabs("rotate", 3000, true); // this line is neccessary
	$("#featured").hover( // these lines neccessary?
	function() {
	$("#featured").tabs("rotate",0,true); 
	},
	function() {
	$("#featured").tabs("rotate",3000,true);
	}
	);
});

$(function() {
$('.external_link').click(function() {
	//Get the value of an attribute for the first element in the set of matched elements.
	//The .attr() method gets the attribute value for only the first element in the matched set.
	//data-accordion is the number of the accordion panel
var dataAccordion = $(this).attr('data-accordion');
//window.location object can be used to get the current page address (URL) and to redirect the browser to a new page.
//The location.href property returns the URL of the current page.
//question mark in the URL specifies that anything coming after the question mark is what is called a "querystring"
//This URL is telling the website to open up a page, and to load up the specific data and output results. 
//It passes variables from a link into a script that produces output or stores the data in a database. 
window.location.href = 'programs.html#'+ dataAccordion;
return false;
})
})

</script>

