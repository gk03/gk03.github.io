                                            // count the number of promos on the page
                                            var promoCount2 = $('script.js--promo2').length;
                                            var promorand2 = Math.floor(Math.random() * 100);
                                            var promoIdx2 = 1;
                                            if (promorand2 < 40) {
                                                // Events - 40%
                                            } else if (promorand2 < 65) {
                                                // Jobs - 25%
                                                promoIdx2 = 2;
                                            } else {
                                                // POTM - 35%
                                                promoIdx2 = 3;
                                            }
                                            // handle if not 3 promos (native)
                                            if (promoIdx2 > promoCount2) {
                                                promoIdx2 = promoCount2;
                                            }
                                            smartPromo = $("#module2content" + promoIdx2).attr('data-smartpromo2-id');
                                            $('#module2Mbox').html($("#module2content" + promoIdx2).text());
                                        </script>
