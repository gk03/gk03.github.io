<div class="cell-wrapper layout-widget-wrapper">
<span id="hs_cos_wrapper_module_14689916337559" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_google_search" style="" data-hs-cos-general-type="widget" data-hs-cos-type="google_search">        <div id="hs-search-module" class="hs-search-module b99c963c-32e5-499e-9906-9a397b815c76">
            <form class="hs-form">
                <div class="field" role="search">
                    <label for="hs-search-b99c963c-32e5-499e-9906-9a397b815c76">Search website</label>
                    <div class="input">
                        <input type="text" id="hs-search-b99c963c-32e5-499e-9906-9a397b815c76" class="hs-input" value="">
                        <a class="hs-button primary">Search Google</a>
                    </div>
                </div>
            </form>
        </div>
        <script>
  jQuery(function(){
      var $             = jQuery;
      var url           = 'http://google.com/search?q=site:' + location.protocol + '//' + location.hostname + ' ';
      var $searchModule = $('.hs-search-module.b99c963c-32e5-499e-9906-9a397b815c76');
      var $input        = $searchModule.find('input');
      var $button       = $searchModule.find('.hs-button.primary');
      if (false) {
          $input.val(decodeURIComponent(location.pathname.split('/').join(' ').split('.').join(' ').split('-').join(' ').split('_').join('')));
      }
      $button.click(function(){
          var newUrl = url + $input.val();
          var win = window.open(newUrl, '_blank');
          if (win) {
              //Browser has allowed it to be opened
              win.focus();
          } else {
              //Browser has blocked it
              location.href = newUrl;
          }
      });
      $input.keypress(function(e){
          if (e.keyCode !== 13) return;
          e.preventDefault();
          $button.click();
      });
  });
</script>

