<script type="text/javascript"> 
   
   var next_slide_id = 1;
   var myTimer = setInterval(function(){myTimerAction()},4000);
   
   $(document).ready(function() 
   {
	  	
		
		
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_001.png');
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_002.png');
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_003.png');
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_001_thumb.png');
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_002_thumb.png');
		var image1 = $('<img />').attr('src', '/images/main_page/animated_ads/ad_003_thumb.png');
   });
   

   function myTimerAction()
   {
		showAdSlide(next_slide_id);
		if (next_slide_id == 4)
		{
			showAdSlide(1);
			clearInterval(myTimer);
		}
		next_slide_id++;
   }
   
   function showAdSlideUserClick(slide_id)
   {
		clearInterval(myTimer);
		showAdSlide(slide_id); 
   }
   
   
   function showAdSlide(slide_id)
   {
	   
	   if (slide_id == 1)
	   {
			$("#myImage").attr("src", "/images/main_page/animated_ads/ad_001.png");
			/*ID=1*/
			$("#myImageURL").attr("href", "http://www.pemicro.com/products/product_comparisons/cyclone_vs_fx.cfm");
			/*$("#myImageURL").attr("href", "url.cfm?id=1");*/
			
			$("#myThumbImage1").attr("src", "/images/main_page/animated_ads/ad_001_thumb.png");
			$("#myThumbImage2").attr("src", "/images/main_page/animated_ads/ad_002_thumb_off.png");
			$("#myThumbImage3").attr("src", "/images/main_page/animated_ads/ad_003_thumb_off.png");
	   }
	   else if (slide_id == 2)
	   {
			$("#myImage").attr("src", "/images/main_page/animated_ads/ad_002.png");
			/*ID=2*/
			$("#myImageURL").attr("href", "http://www.pemicro.com/products/product_view.cfm?product_ID=15320151");
			
			$("#myThumbImage1").attr("src", "/images/main_page/animated_ads/ad_001_thumb_off.png");
			$("#myThumbImage2").attr("src", "/images/main_page/animated_ads/ad_002_thumb.png");
			$("#myThumbImage3").attr("src", "/images/main_page/animated_ads/ad_003_thumb_off.png");
	   }
	   else if (slide_id == 3)
	   {
			$("#myImage").attr("src", "/images/main_page/animated_ads/ad_003.png");
			/*ID=3*/
			$("#myImageURL").attr("href", "http://www.pemicro.com/multilink/index.cfm");
			
			$("#myThumbImage1").attr("src", "/images/main_page/animated_ads/ad_001_thumb_off.png");
			$("#myThumbImage2").attr("src", "/images/main_page/animated_ads/ad_002_thumb_off.png");
			$("#myThumbImage3").attr("src", "/images/main_page/animated_ads/ad_003_thumb.png");
	   }	
   }
   
