var search_showing = false;

function getObject(id,d)
{
	var i, x;  
	if(!d) d=document; 
	if(!(x=d[id])&&d.all) 
		x=d.all[id]; 
	for (i=0;!x&&i<d.forms.length;i++) 
		x=d.forms[i][id];
	for(i=0;!x&&d.layers&&i<d.layers.length;i++) 
		x=getObject(id,d.layers[i].document);
	if(!x && document.getElementById) 
		x=document.getElementById(id); 
	return x;
};

function show_search()
{
	if (search_showing == false)
	{
		getObject("searchDIV").style.display = "block";
		search_showing = true;
	}
	else
	{
		getObject("searchDIV").style.display = "none";
		search_showing = false;
	}
}

