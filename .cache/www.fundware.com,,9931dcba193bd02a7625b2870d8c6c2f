  function fixAnchor(hash) {
    if (hash && hash.length > 1) {
      var anchor = $('[name="' + hash.substr(1) + '"]');
      if (anchor.length) {
        setTimeout(function() {
          window.scrollTo(0, anchor.offset().top - 60);
        }, 100);
      }
    }
  }

  // Move the Omnibar to the body and out of the footer to avoid inheriting footer styling.
  $("body").prepend($("#omnibar"));
  
