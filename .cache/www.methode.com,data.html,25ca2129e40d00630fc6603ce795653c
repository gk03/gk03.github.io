$('#linkEngineering').click(function() {
	if ($('#Div-Engineering').is(":hidden")) {
					$('#Div-Manufacturing').hide();
					$('#Div-Sales').hide();
				   
					
					$('#Div-Engineering').slideDown('fast');
	} else {
					$('#Div-Engineering').slideUp('fast');
	};
return false;
});

$('#linkManufacturing').click(function() {
	if ($('#Div-Manufacturing').is(":hidden")) {
					$('#Div-Engineering').hide();
					$('#Div-Sales').hide();
			
					
					$('#Div-Manufacturing').slideDown('fast');
	} else {
					$('#Div-Manufacturing').slideUp('fast');
	};
return false;
});

$('#linkSales').click(function() {
	if ($('#Div-Sales').is(":hidden")) {
					$('#Div-Engineering').hide();
					$('#Div-Manufacturing').hide();
				 
					
					$('#Div-Sales').slideDown('fast');
	} else {
