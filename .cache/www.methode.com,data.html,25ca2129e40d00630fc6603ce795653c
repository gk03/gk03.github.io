 <script><!--collapse menu begin-->

var imagePath = "";

$(document).ready(function(){

$('#mycontainer').show(600, function(){

//---new code

$('#hideDiv').css({'background':'url("/style/template/close.png") no-repeat top left', 'height':'16px', 'width':'72px' , 'border':'0px solid #000' , 'position':'relative' , 'top':'-17px'}); //adjust sizing
//---end of new code

$('#hideDiv').attr('title', 'Hide');

$('#hideDiv').html(' ');
})

var hide = false;

$('#hideDiv').click(function(){

if(hide){

hide = false;

//---new code

$('#hideDiv').css({'background':'url("/style/template/close.png") no-repeat top left' , 'height':'16px', 'width':'72px', 'position':'relative' , 'top':'-17px'}); //adjust sizing
//---end of new code
$('#hideDiv').attr('title', 'Hide');

$('#hideDiv').html(" ");

$('#mycontainer').show(600);

} else {

hide = true;

//---new code

$('#hideDiv').css({'background':'url("/style/template/open.png") no-repeat top left' ,' border-bottom':'1px solid #000' ,'height':'16px', 'width':'72px', 'position':'relative' , 'top':'0px'}) ; //adjust sizing



//---end of new code
$('#hideDiv').attr('title', 'Show');



$('#hideDiv').html(" ");



$('#mycontainer').hide(600);

}
})
});
</script>








    
