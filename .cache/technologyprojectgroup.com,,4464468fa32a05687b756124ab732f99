	jQuery('#menu > li').each( function(){
		if( jQuery(this).find('ul.sub-menu').length > 0 && !jQuery(this).hasClass('current-menu-item') && !jQuery(this).hasClass('current-menu-parent') ){
			jQuery(this).find('ul').css('display', 'block').slideUp(0);
			jQuery(this).hover( function(){				
					jQuery(this).css('height', 'auto');
					jQuery(this).find('ul').slideDown('fast');
				},
				function(){
					jQuery(this).find('ul').slideUp(0);
					jQuery(this).css('height', '47px');
				});
		};
		
		if( jQuery(this).find('ul.sub-menu').length > 0 ){
			jQuery(this).addClass('menu-parent');
			jQuery(this).find('ul li a').append(' <img src="http://technologyprojectgroup.com/wp-content/themes/tpg/images/arrow-right.png" alt="arrow" />');
		}
		
		if( jQuery(this).hasClass('current-menu-parent') || jQuery(this).hasClass('current-menu-item') ){
			jQuery(this).next().css('top', '-1px');
		}
	})
</script>
		
