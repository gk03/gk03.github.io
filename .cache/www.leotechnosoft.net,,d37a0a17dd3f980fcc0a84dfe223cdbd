<script type="text/javascript">
    $(".innerspace").click(function () {
        //window.location = $(this).find("a").attr("href");
        if ($(this).find("a").attr("target") === '_blank') {
            var win = window.open($(this).find("a").attr("href"), '_blank');
            win.focus();
        } else {
            window.location = $(this).find("a").attr("href");
        }
        return false;
    });

    function DispVideo(Show) {
        //alert(Show);
        if (Show == 1) {
            $('.divCall').show();
            document.getElementById('ifrVid').src = 'https://www.youtube.com/embed/YtQFrjpO7l0?autoplay=0;&amp;modestbranding=0;autohide=0&amp;showinfo=0&amp;controls=1;&amp;rel=0';
        } else {
            $('.divCall').hide();
            document.getElementById('ifrVid').src = '';
        }
        return false;
    }

    $('#myModal').on('hidden.bs.modal', function () {
        document.getElementById('ifrVid').src = 'https://www.youtube.com/embed/YtQFrjpO7l0?autoplay=0;&amp;modestbranding=0;autohide=0&amp;showinfo=0&amp;controls=1;&amp;rel=0';
        this.currentTime = 0;
        //this.pause();     
    });
</script>

