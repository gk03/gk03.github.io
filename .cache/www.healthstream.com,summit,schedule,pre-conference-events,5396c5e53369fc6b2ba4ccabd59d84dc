			$('.accordion-navigation').click(function() {
					if ($(this).children('.panelcontent').is(':visible')) {
						$(this).children('.panelcontent').slideUp();
						$(this).removeClass('active');
					} else {
						$('.panelcontent').slideUp();
						$('.accordion-navigation').removeClass('active');
						$(this).children('.panelcontent').slideDown();
						$(this).addClass('active');
					}
				});

