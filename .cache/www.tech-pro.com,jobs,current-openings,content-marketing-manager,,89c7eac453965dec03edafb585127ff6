function validation()
{
	if (document.getElementById("your_name").value == '')
	{
	document.getElementById("name_err").style.display = "block";
	return false;
	}
	else
	{
	document.getElementById("name_err").style.display = "none";
	}

	if (document.getElementById("your_email").value == '')
	{
	document.getElementById("email_err").style.display = "block";
	return false;
	}
	else
	{
	document.getElementById("email_err").style.display = "none";
	}

	if (document.getElementById("your_message").value == '')
	{
	document.getElementById("msg_err").style.display = "block";
	return false;
	}
	else
	{
	document.getElementById("msg_err").style.display = "none";
	}

	if (document.getElementById("your_cv").value == '')
	{
	document.getElementById("resume_err").style.display = "block";
	return false;
	}
	else
	{
	document.getElementById("resume_err").style.display = "none";
	}

	if (document.getElementById("antispam_answer").value == '')
	{
	document.getElementById("answer_err").style.display = "block";
	return false;
	}
	else
	{
	document.getElementById("answer_err").style.display = "none";
	}
}
