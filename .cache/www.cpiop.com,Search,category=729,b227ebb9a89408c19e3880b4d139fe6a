<script>
    // increment Quantity up by 1
    $(document).on('click', '.itemQuantityButton', function (e) {
        e.preventDefault();
        var currentVal = parseInt($(this).prev(".quantityInput").val());
        if (!currentVal || currentVal == "" || currentVal == "NaN") currentVal = 0;
        $(this).prev(".quantityInput").val(currentVal + 1);
    });

    $(function() {
        // custom select box
        $("#sortResultsView input");

        var sb = new SelectBox({
            selectbox: $("#sortResultsView select"),
            width: 150,
            height: 155,
            scrollOptions: {
                mouseWheelSpeed: 30
            },
            changeCallback: function(val) {
                var urlParams = {};
                var match,
                    pl = /\+/g, // Regex for replacing addition symbol with a space
                    search = /([^&=]+)=?([^&]*)/g,
                    decode = function(s) { return decodeURIComponent(s.replace(pl, " ")); },
                    query = window.location.search.substring(1);

                while (match = search.exec(decode(query))) {
                    var key = decode(match[1]);
                    if (!(key in urlParams)) {
                        urlParams[decode(match[1])] = [];
                    }
                    urlParams[decode(match[1])].push(decode(match[2]));
                }
                   

                urlParams.order = val;

                window.location = window.location.pathname + "?" + $.param(urlParams, true);
            }
        });
    });
</script>



