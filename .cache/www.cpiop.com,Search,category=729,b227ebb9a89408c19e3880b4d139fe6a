    <script type="text/javascript" src="/Scripts/homegrown-plugins/TopBottomButtons.min.js"></script>
<script type="text/javascript" src="/Scripts/jquery-plugins/jquery.cookie.js"></script>
<script type="text/javascript" src="/Scripts/homegrown-plugins/gridlistmodeswitcher.js"></script>
    <script>
    $(function () {
        var offsetf = ($(document).height() - $(window).height() - 320); // offset for the down to bottom arrows - affected by lazy loading
        var duration = 500;

        TopBottomButtons.top(duration);
        TopBottomButtons.bottom(offsetf, duration);

        var loading = false;
        var itemsLoaded = 4;
        var totalItems = 4;
        var lazyLoadFunc = function() {
            var scrolled = $(window).scrollTop() + $(window).height();
            var bottomOfLoaded = $('#products').position().top + $('#products').outerHeight();
            var diff = bottomOfLoaded - scrolled;
            var rows = diff / 464;
            if (rows < 3 && !loading) {
                loading = true;
                $.ajax({
                    type: 'GET',
                    url: '/ContinueSearch?start=' + itemsLoaded + '&category=729',
                    error: function(xhr, status, err) {
                        // todo error handling?
                    },
                    success: function (data) {
                        if (data.resultData != undefined) {
                            itemsLoaded = data.resultData.Values[1];
                            $('#products').append(data.resultData.Values[0]);
                            $("#ajax_responses").html(data.resultData.Values[2]);
                            $("#ajax_responses").find("script").each(function(){
                                eval($(this).text());
                            });
                            
                        }
                        loading = false;

                        offsetf = ($(document).height() - $(window).height() - 320); // resets the offset for down to bottom arrows
                        TopBottomButtons.resetBottom(offsetf, duration);
                        if ($(window).width() > 1000) {
                            $('.down-to-bottom').stop(true, true).show();
                        }
                        if (itemsLoaded >= totalItems) {
                            // all items loaded, disable spinner and lazy load
                            $('#searchSpinner').hide();
                            $(window).off("scroll.lazyload", lazyLoadFunc);
                        }
                    }
                });
            }
        };

        $(window).on("scroll.lazyload", lazyLoadFunc);

        GridListModeSwitcher();

        
        $("#products").off('click.GAdetailclick').on('click.GAdetailclick', 'div.itemSummary a', function (event) {
            var href = $(this).attr('href');
            if ($(this).data('ga-type') == "detail") {
                event.preventDefault();
                var id = $(this).closest('div.itemSummary').data('ga-id');
                var name = $(this).closest('div.itemSummary').data('ga-name');

                ga('ec:addProduct', {
                    'id': id,
                    'name': name
                });

                // Send the promo_click action with an event.
                ga('ec:setAction', 'click', { list: 'Search - Job Forms' });
                ga('send', 'event', 'UX', 'click', 'Search - Job Forms', {
                    hitCallback: createFunctionWithTimeout(function() {
                        window.location = href;
                    })
                });
            }
        });
    });
</script>



