    <script type="text/javascript">
    function validateSearchForm(){
      var x=document.forms["searchform"]["searchinput"].value;
      if (x=="Site Search" || x=="" || x==" " || x==null) {
        alert("Please enter a valid search term");
        return false;
      }
    }
    </script>
    
