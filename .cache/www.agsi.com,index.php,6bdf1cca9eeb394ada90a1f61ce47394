    <script type="text/javascript">
      $(function() {
        $('#searchinput').example('Site Search');
    
        $('input').each(function() { $.data(this, 'default', this.value); }).css("color","#999")
        $('input').each(function() { $.data(this, 'default', this.value); }).css("font-style","italic")
    
        .focus(function() {
            if (!$.data(this, 'edited')) {
                this.value = "";
                $(this).css("color","black");
                $(this).css("font-style","normal");
            }
        }).change(function() {
            $.data(this, 'edited', this.value != "");
        }).blur(function() {
            if (!$.data(this, 'edited')) {
                this.value = $.data(this, 'default');
                $(this).css("color","#999");
                $(this).css("font-style","italic");
            }
        }); 
      });
    </script>

    <script>
      $( function() {
        // Fix for Bootstrap 2.3.2 sub-menu clicks on mobile devices
        $('.dropdown-toggle').click(function(e) {
          e.preventDefault();
          setTimeout($.proxy(function() {
            if ('ontouchstart' in document.documentElement) {
              $(this).siblings('.dropdown-backdrop').off().remove();
            }
          }, this), 0);
        });

        $(document).on('touchstart.dropdown.data-api', '.dropdown-submenu > a', function (event) {
          event.preventDefault();
        });
        $('.dropdown-submenu ul.dropdown-menu li a').on('touchstart', function(e) {
          e.preventDefault();
          window.location.href = $(this).attr('href');
        })

      });
    </script>

