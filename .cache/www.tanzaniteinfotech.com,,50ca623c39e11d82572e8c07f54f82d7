<script>
	$(document).ready(function(e) {
			$('.team_container').hide();
			$('.team_container.active').show();
        $('.slide').click(function(e) {
            $('.slide').removeClass('active');
			$(this).addClass('active');
			$('.team_container').removeClass('active');
			$('.team_container'). hide();
			var a = $(this).attr('rel');
				$('#'+a).addClass('active');
				$('#'+a).show();
        });
    });
</script> 

