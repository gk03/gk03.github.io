$(document).ready(function() {
	
	$('#contactpage').show();	
		var footer_height=$('#footer_top_wrap').height();
	footer_height=(footer_height*-1)+20;
	$('#footer_top_wrap').css('margin-bottom',footer_height+'px');
	$('#footer_top_wrap').css('display','table');
	
	$('#hide_footer').hide();
	$('#footer_top_wrap').mouseover(function()
	{
		$('#hide_footer').show();
		$('#footer_top_wrap').fadeIn("slow");
		$('#footer_top_wrap').css('margin-bottom','0px');
		$('#footer_wrap').css('background-image', 'url(http://www.grasptech.com/wp-content/themes/grasp/images/bgFooter.png)');
		$('#footer_wrap').css('background-repeat','repeat-x');

	});
	
	$('#hide_footer').mouseover(function()
	{
		$('#footer_wrap').css('background-image', '');
		$('#hide_footer').hide();
		$('#footer_top_wrap').css('margin-bottom',footer_height+'px');
	});
	
  	$('#sales_question').click(function()
	{ 	 
		$('#divContactForm').show();
		$('#contactpage').hide();
		$('#contactSubject').val('SalesQuestion');
    });  
	
	$('#investor_question').click(function()
	{ 	 
		$('#divContactForm').show();
		$('#contactpage').hide();
		$('#contactSubject').val("MediaQuestion");
    }); 
	
  
	  
	//FancyBox  
	$(".video").click(function() {
	   $.fancybox({
		'padding' : 0, // optional
		'title' : this.title,
		'content': '<embed src="http://www.grasptech.com/wp-content/themes/grasp/jwplayer.swf?file='+this.href+'&amp;autostart=true&amp;fs=1" type="application/x-shockwave-flash" width="600" height="400" wmode="opaque" allowfullscreen="true" allowscriptaccess="always"></embed>'               
	   }); // fancybox
	   return false;
	  }); // click
	  

});//close docready
</script>

