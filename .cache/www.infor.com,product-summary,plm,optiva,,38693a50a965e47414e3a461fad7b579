    function newwindow()
    {
        dismissChatInvitation();
        var windowHeight=800;
        var windowWidth=740;
        var deviceWidth = (window.innerWidth > 0) ? window.innerWidth : screen.width;
        if(deviceWidth <= 320)
        {
            windowHeight=320;
            windowWidth=320;
        }
        $("#chatRightNowInvite").css("dispaly","none");
        var referer = window.location.href;
        var openChat = "no";
        if(rightNowChat)
        {
            openChat = "yes";
        }
        if (typeof dcsMultiTrack != 'undefined')
        {
            var wtEs = getWebtrendsWtEsString();
            //dcsMultiTrack('WT.es',wtEs);
            Webtrends.multiTrack({
                args: {
                    'WT.es': wtEs
                },
                callback: function (a) {
                    window.open(mailFormUrl + "?referer=" + referer + "&openChat=" + openChat, 'jav', 'width=' + windowWidth + ',height=' + windowHeight + ',resizable=yes,scrollbars=yes');
                }
            });
        } else {
            window.open(mailFormUrl + "?referer=" + referer + "&openChat=" + openChat, 'jav', 'width=' + windowWidth + ',height=' + windowHeight + ',resizable=yes,scrollbars=yes');
        }

    }
    function applyEmailLink()
    {
        var emailLinkText = 'Email';
        var emailUsLinkText = 'Email Us';
        $("#top-nav-chat-link-legacy").text(emailLinkText);
        $("a[href='javascript:newwindow()']").text(emailLinkText);
        $("li.chat").find("a").attr("href","javascript:newwindow()");
        $("span.standalone-chat-form").text(emailUsLinkText);
        $("input[name='openChat']").val("no");
    }

