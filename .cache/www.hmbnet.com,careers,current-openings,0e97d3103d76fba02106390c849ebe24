            animation: false,
            openOnClick: whetherToOpenOnClick,
            open: function (e) {
                if (window.DataIntelligenceSubmitScript) {
                    var item = $(e.item);

                    DataIntelligenceSubmitScript._client.sentenceClient.writeSentence({
                        predicate: "Toggle navigation",
                        object: item.find("a:first").text(),
                        objectMetadata: [
                                                    {
