<script type="text/javascript">
var player = '';
var APIModules = '';
var prevItem = 1;
function onTemplateLoad(experienceID)
{
player = brightcove.api.getExperience(experienceID);
// APIModules = brightcove.api.modules.APIModules;
APIModules = player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
}
function onTemplateReady(evt)
{
w = $('#videoplayer').width();
$('#videoplayer').height(w*.8);	// set height to be same as width
// alert('onTemplateReady');
//console.log( 'onTemplateReady' );
videoPlayer = player.getModule(APIModules.VIDEO_PLAYER);
// get Video ID
vID = $('#videoID').val();
// Play Video
APIModules.cueVideoByID(vID);
}
function loadVideo(vID, item)
{
// Switch display of highlighted links
$('#link_' + prevItem).show();
$('#nolink_' + prevItem).hide();
$('#link_' + item).hide();
$('#nolink_' + item).show();
// Set Video ID
$('#videoID').val(vID);
APIModules.loadVideoByID(vID);
prevItem = item;
}
</script>
