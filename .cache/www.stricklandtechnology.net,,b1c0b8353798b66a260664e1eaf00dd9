<!-- Validate Login -->
function validatelogin() {
var errorcount=0;
if (document.login.username.value == "") {
document.login.username.value = "Invalid Username";
errorcount++;
}
if (document.login.password.value == "") {
document.getElementById("passalert").style.visibility="visible";
errorcount++;
}
if (errorcount != 0){
return false;
}
else{
document.login.submit();
}
}
// end validate login
// Greeting script
		var d = new Date();
		var HourOfDay = d.getHours();
		if (HourOfDay < 12) {var greeting = 'Good Morning, '} 
		if (HourOfDay >= 12 && HourOfDay <=18) {var greeting = 'Good Afternoon, '}
		if (HourOfDay >18) {var greeting = 'Good Evening, '}
// End Greeting script
</script>

