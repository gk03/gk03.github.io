$(document).ready(function(){
$('#Say-slides') 
	.after('<div class="pagination2">') 
	.cycle({ 
	    fx:     'scrollLeft', 
    timeout: 5000, 
	    pager:  '.pagination2' 
	});	
	$('.pagination2 a').click(function() { 
		$('#Say-slides').cycle('pause'); 
	});
	//header
    if($(window).width() > 641)
    {
        jQuery("#header-menu li").hover(
            function(){ if ( jQuery(this).find('ul:first').css('display') == 'none' ) { jQuery(this).find('ul:first').slideDown(268); } 
},
            function(){ if ( jQuery(this).find('ul:first').css('display') == 'block' ) { jQuery(this).find('ul:first').slideUp(268); } }
        );
    }
    jQuery("#header-menu ul").css({display: "none"}); // Opera Fix
	//end header 
});
$(document).ready(function(){
    if($(window).width() < 641)
    {
        $("#header-menu li a").click(function(e){
            if( $(this).parent().children('ul').length > 0 ){
                e.preventDefault(); //prevent the link from being followed
                $(this).removeAttribute('href'); //remove the a element
                //alert('adi');
            }
        });
        $('#header-menu li').click(function(){
       var checkElement = $(this).children('ul');
        if((checkElement.is('#header-menu  li ul')) && (checkElement.is(':visible'))) {
            $(checkElement).slideUp();
        }
        if((checkElement.is('#header-menu  li ul')) && (!checkElement.is(':visible'))) {
            $('#header-menu  li ul').slideUp('normal');
            checkElement.slideToggle('normal');
            return false;
        }
        });
    }
});
</script>
