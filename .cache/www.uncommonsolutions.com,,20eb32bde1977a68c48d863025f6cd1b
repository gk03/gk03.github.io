<script type="text/javascript">
    var scounter = 0;
    jQuery(document).ready(function () {
        set_slider_height();
        swap_img();
    });

    function set_slider_height()
    {
        jQuery('#sizer').height(jQuery(window).height()/2);
    }

    function swap_img()
    {
        var obj = jQuery('#parallax');
        obj.data('img-url',simg[scounter]);
        obj.data('surl',surl[scounter]);
        jQuery('#parallax .parallax-img').css('background-image', 'url(' + simg[scounter] + ')');
        jQuery('#messageD').html(decodeURIComponent(scontent[scounter].replace(/PRC/,"'")).replace(/\"/g,''));
        var url = '';
        url=surl[scounter];
        var hi = jQuery('#messageD').height()+40;
        jQuery('#sbuttons i').css('color', 'white');
        jQuery('#sbuttons #i' + scounter).css('color', '#bfd73e');
        jQuery('#messageLink').height(hi);
        if(url=='')
        {
            jQuery('#messageLink').hide();
        }
        else {
            jQuery('#messageLink').show();
            if(url.match(/http/))
            {
                jQuery('#messageLink').wrap('<a target=_blank style="color: white;" href="' + url + '"></a>');

            } else {
                jQuery('#messageLink').wrap('<a style="color: white;" href="' + url + '"></a>');
            }
            jQuery('#messageLink').html('<div id=learn>Learn More</div>');
        }
        scounter++;
        if (scounter > mscounter) scounter = 0;
        setTimeout(swap_img, 8000);
    }
</script>

