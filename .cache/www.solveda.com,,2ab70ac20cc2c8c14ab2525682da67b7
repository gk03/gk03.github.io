	jQuery(document).ready(function(){
		jQuery(".portfolioItem").hover(function(){
				//stuff to do on mouse enter
				colorImage = jQuery(this).children(".colorImage").find('img');
				colorImgSrc = jQuery(colorImage).attr("src");
				
				bwImage = jQuery(this).children(".bwImage").find('img');
				bwImgSrc = jQuery(bwImage).attr("src");
				
				colorImage.attr('src', bwImgSrc);
				bwImage.attr('src', colorImgSrc);
			}, function(){
			
				//stuff to do on mouse leave
			
				colorImage = jQuery(this).children(".colorImage").find('img');
				colorImgSrc = jQuery(colorImage).attr("src");
				
				bwImage = jQuery(this).children(".bwImage").find('img');
				bwImgSrc = jQuery(bwImage).attr("src");
				
				colorImage.attr('src', bwImgSrc);
				bwImage.attr('src', colorImgSrc);
		});
	});
</script>
