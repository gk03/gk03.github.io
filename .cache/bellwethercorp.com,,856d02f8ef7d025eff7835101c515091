	<script>
			
		jQuery(document).ready(function(){

			jQuery('.feat-prod-footer a').on('click',function(e){

	  			e.preventDefault();

				jQuery('form input[type="text"]').val("");
				jQuery('form input[type="email"]').val("");
				jQuery('form input[type="tel"]').val("");
				jQuery('form input[type="number"]').val("");
				jQuery('form textarea').val("");

	
				if( jQuery(this).attr( "title" ) == "PMX" ) {				  			

	  				jQuery('#PMX-modal').modal('toggle');
	  				
	  			}
  			
				if( jQuery(this).attr( "title" ) == "BPM" ) {				  			
	  				
	  				jQuery('#BPM-modal').modal('toggle');
	  				
	  			}
  			
				if( jQuery(this).attr( "title" ) == "EPMX" ) {				  			
	  				
	  				jQuery('#EPMX-modal').modal('toggle');
	  				
	  			}
  			
				if( jQuery(this).attr( "title" ) == "PDF-dwnld" ) {				  			
	  				
	  				jQuery('#PDF-dwnld-modal').modal('toggle');
	  				
	  			}
  			
			});
			
			jQuery('form').on('submit', function(event){
				var requireSubmit = 0;
				var emailAddress = jQuery('.email input', this).val();
				
				if ( emailAddress.indexOf("gmail.com") >= 0 || emailAddress.indexOf("hotmail.com") >= 0 || emailAddress.indexOf("yahoo.com") >= 0 || emailAddress.indexOf("aol.com") >= 0 ) {
					event.preventDefault();
					jQuery('.comment-input-error-valid', this).show();
				}else{	
					jQuery('.required', this).each(function(){
						if(jQuery(this).val() == "" || !jQuery(this).val().trim()){
							requireSubmit = requireSubmit+1;
							jQuery(this).next('.comment-input-error').show();
							event.preventDefault();
	
						}else{
							jQuery(this).next('.comment-input-error').hide();					
						}
					})
					if(requireSubmit < 1 ){
						jQuery('.modal:visible').modal('toggle');
						jQuery(this).next('.comment-input-error').hide();
					}
				}	
			});
		});

	</script>


