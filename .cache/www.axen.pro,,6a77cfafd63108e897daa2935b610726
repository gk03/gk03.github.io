                var pais = geoplugin_countryCode(); 
    var sitio= "";
                  
                               switch(pais)
                {
                case "MX" : case  "MEX" :
                
                  sitio = "/mex/es/";
                  
                  break;
                  
                case "USA":       case "US":
                    sitio = "/usa/";
                  break;
                
                case  "ANT": case "AN": case "BHS": case "BS": case "BRB": case "BB": case "CUB": case "CU": case "DMA": case "DM": case "GRD": case "GD": case "HTI": case "HT": case "JAM": case "JM": case "DOM": case "DO": case "KNA": case "KN": case "VCT": case "VC": case "LCA": case "LC": case "TTO": case "TT": case "PRI": case "PR":
                    sitio = "/car/";
                  break;
                  
                  
                default:
                    sitio = "/usa/";
                }
                
                
                window.location.replace(sitio);
                
  </script>
