    <script type="text/javascript">
        var MNI = MNI || {};
        MNI.CurrentCulture = 'en-US';
        MNI.CultureDateFormat = 'M/d/yyyy';
        MNI.BaseUrl = 'http://www.kctechcouncil.com';
        MNI.jQuery = jQuery.noConflict(true);
