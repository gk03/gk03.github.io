               <script src="http://code.jquery.com/jquery-1.8.2.js" type="text/javascript"></script>
<script type="text/javascript">
    $(function () {
        var currentPosition = 0;
        var slideWidth = 900;
        var slides = $('.slide');
        var numberOfSlides = slides.length;
        setInterval(changePosition, 5000);

        slides.wrapAll('<div id="slidesHolder"></div>');
        slides.css({ 'float': 'left' });
        $('#slidesHolder').css('width', slideWidth * numberOfSlides);

        var string1 = currentPosition + 1
        $("#stopControl").text(string1 + " of 7");

        $('.control').bind('click', function () {
            currentPosition = ($(this).attr('id') == 'rightControl') ? currentPosition + 1 : currentPosition - 1;
            //alert(currentPosition);
            doChange('fast');


        });



        function changePosition() {
            currentPosition++;
            doChange();
        };

        function doChange(v) {
            if (currentPosition >= numberOfSlides) {
                currentPosition = 0;
                moveZero();
            }
            else if (currentPosition < 0) {
                currentPosition = 6;
                if ('fast' == v) {
                    fastMoveSlide();
                }
                else {
                    moveSlide();
                }
            }
            else {
                if ('fast' == v) {
                    fastMoveSlide();
                } else {
                    moveSlide();
                }
            }


            var string1 = currentPosition + 1;
            $("#stopControl").text(string1 + " of 7");
        };

        function fastMoveSlide() {

            //            $('#slidesHolder').animate({ 'marginLeft': slideWidth * (-currentPosition) });
            $('#slidesHolder').animate({ opacity: '0.4' }, 0, 'linear');
            $('#slidesHolder').animate({ 'marginLeft': slideWidth * (-currentPosition), opacity: '0.8' }, 0, 'linear');


        };

        function moveSlide() {

            //            $('#slidesHolder').animate({ 'marginLeft': slideWidth * (-currentPosition) });
            $('#slidesHolder').animate({ opacity: '0.4' }, 1000, 'linear');
            $('#slidesHolder').animate({ 'marginLeft': slideWidth * (-currentPosition), opacity: '0.8' }, 1000, 'linear');


        };

        function moveZero() {

            //            $('#slidesHolder').animate({ 'marginLeft': slideWidth * (-currentPosition) });
            $('#slidesHolder').animate({ opacity: '0.4' }, 1000, 'linear');
            $('#slidesHolder').animate({ 'marginLeft': 0, opacity: '0.8' }, 0, 'linear');


        };


    });
</script>
