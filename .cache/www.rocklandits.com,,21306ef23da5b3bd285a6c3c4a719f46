	function validateForm() {
		var email = document.forms["form1"]["email"].value;
		if (email == null || email == "" || email == "Email" || validateEmail(email) ==  false) {
			alert("Email must be filled out with valid email");
			return false;
		}
		var phone = document.forms["form1"]["phone"].value;
		if (phone == null || phone == "" || phone == "Phone") {
			alert("Phone must be filled out");
			return false;
		}
		var first = document.forms["form1"]["fname"].value;
		if (first == null || first == "" || first == "First Name") {
			alert("First Name must be filled out");
			return false;
		}
	}
	
	function validateEmail(email) {
    var re = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
    return re.test(email);
	}
	

</script>

