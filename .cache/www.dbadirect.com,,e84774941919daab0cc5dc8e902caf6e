<script type="text/javascript"> 
   jQuery(document).ready(function () { 
       jQuery(".togglecontent").slideUp(0); 
       jQuery(".trigger").click(function() {
			var item_clicked = jQuery(this);
			if(jQuery(item_clicked).next('.togglecontent').css('display') == 'block'){
				jQuery(item_clicked).removeClass('active');
				jQuery(item_clicked).next('.togglecontent').slideUp("slow");
			}else{
				jQuery(".togglecontent").each(function(){
					if(jQuery(this).css('display')=='block'){
						jQuery(this).slideUp();
						jQuery(this).prev(".trigger").removeClass('active')
					}
				});
				jQuery(item_clicked).addClass('active');
				var timeout = setTimeout( function(){
									jQuery(item_clicked).next(".togglecontent").slideToggle("slow");								
									clearTimeout(timeout);
								},100 ); 				
			}
							
        }); 
		
}); </script>

