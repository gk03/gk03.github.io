function blank()



{

	if(document.getElementById('captcha-text').value == 'CODE'){



	document.getElementById('captcha-text').value='';



	}

}



function fill()



{

	if(document.getElementById('captcha-text').value == ''){



	document.getElementById('captcha-text').value='CODE';



	}	

}

</script>

