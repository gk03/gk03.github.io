var popUpWin=0;

function popUpWindow(URLStr, left, top, width, height)

{

  if(popUpWin)

  {

    if(!popUpWin.closed) popUpWin.close();

  }

	leftVal = (screen.width - width) / 2;
	topVal = (screen.height - height) / 2;


  popUpWin = open(URLStr, 'popUpWin', 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,copyhistory=yes,width='+width+',height='+height+',left='+leftVal+', top='+topVal+',screenX='+left+',screenY='+top+'');

}
