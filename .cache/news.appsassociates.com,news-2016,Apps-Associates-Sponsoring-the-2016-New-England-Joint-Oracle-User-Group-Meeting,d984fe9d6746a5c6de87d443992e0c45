<!-------------- side button ------------->
<script>
$(document).ready(function()
{
  $('#pollSlider-button').click(function() {
    if($(this).css("margin-right") == "225px")
    {
        $('.pollSlider').animate({"margin-right": '-=225'});
        $('#pollSlider-button').animate({"margin-right": '-=225'});
    }
    else
    {
        $('.pollSlider').animate({"margin-right": '+=225'});
        $('#pollSlider-button').animate({"margin-right": '+=225'});
    }


  });
 });         
 </script>
 
<!-------------- side button ------------->



