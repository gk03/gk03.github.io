<!----------------- management team ---------------->
<script type="text/javascript">
function slideonlyone(thechosenone) {
     $('.newboxes2').each(function(index) {
          if ($(this).attr("id") == thechosenone) {
               $(this).slideToggle(200);
          }
          else {
               $(this).slideUp(600);
          }
     });
}
    </script>
<!----------------- management team ---------------->

<!-------------- form input space --------------->
<script>
$('input, textarea').on('keydown', function (e) {
if (e.keyCode == 32) {
e.stopPropagation();
}
});
</script>
