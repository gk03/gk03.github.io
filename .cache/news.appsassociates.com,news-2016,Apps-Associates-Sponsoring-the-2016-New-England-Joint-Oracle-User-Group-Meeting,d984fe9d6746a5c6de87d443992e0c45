<script type="text/javascript">
function slideonlyonemenu(thechosenone) {
     $('.menumob').each(function(index) {
          if ($(this).attr("id") == thechosenone) {
               $(this).slideToggle(200);
          }
          else {
               $(this).slideUp(600);
          }
     });
}
    </script>


