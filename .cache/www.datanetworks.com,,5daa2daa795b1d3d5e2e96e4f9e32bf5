<script>
(function ($) {
  $(document).ready(function() {
  
		
	var aboutTimeout = null;
	$( ".menu-item-about:not('.current-menu-item, .current-menu-ancestor'), .dropdown-about" ).hover(function() {
		//alert('1');
		if (aboutTimeout !== null) {
			clearInterval(aboutTimeout);
			aboutTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-about)" ).stop().hide();
		$( ".dropdown-about" ).stop().slideDown(500);
	}, function() {
		if (aboutTimeout !== null) {
			clearInterval(aboutTimeout);
			aboutTimeout = null;
		}
		aboutTimeout = setTimeout(function() {
			$( ".dropdown-about" ).stop().slideUp(500);
		}, 350);
	});
	
		
	var servicesTimeout = null;
	$( ".menu-item-services:not('.current-menu-item, .current-menu-ancestor'), .dropdown-services" ).hover(function() {
		//alert('1');
		if (servicesTimeout !== null) {
			clearInterval(servicesTimeout);
			servicesTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-services)" ).stop().hide();
		$( ".dropdown-services" ).stop().slideDown(500);
	}, function() {
		if (servicesTimeout !== null) {
			clearInterval(servicesTimeout);
			servicesTimeout = null;
		}
		servicesTimeout = setTimeout(function() {
			$( ".dropdown-services" ).stop().slideUp(500);
		}, 350);
	});
	
		
	var solutionsTimeout = null;
	$( ".menu-item-solutions:not('.current-menu-item, .current-menu-ancestor'), .dropdown-solutions" ).hover(function() {
		//alert('1');
		if (solutionsTimeout !== null) {
			clearInterval(solutionsTimeout);
			solutionsTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-solutions)" ).stop().hide();
		$( ".dropdown-solutions" ).stop().slideDown(500);
	}, function() {
		if (solutionsTimeout !== null) {
			clearInterval(solutionsTimeout);
			solutionsTimeout = null;
		}
		solutionsTimeout = setTimeout(function() {
			$( ".dropdown-solutions" ).stop().slideUp(500);
		}, 350);
	});
	
		
	var marketsTimeout = null;
	$( ".menu-item-markets:not('.current-menu-item, .current-menu-ancestor'), .dropdown-markets" ).hover(function() {
		//alert('1');
		if (marketsTimeout !== null) {
			clearInterval(marketsTimeout);
			marketsTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-markets)" ).stop().hide();
		$( ".dropdown-markets" ).stop().slideDown(500);
	}, function() {
		if (marketsTimeout !== null) {
			clearInterval(marketsTimeout);
			marketsTimeout = null;
		}
		marketsTimeout = setTimeout(function() {
			$( ".dropdown-markets" ).stop().slideUp(500);
		}, 350);
	});
	
		
	var partnersTimeout = null;
	$( ".menu-item-partners:not('.current-menu-item, .current-menu-ancestor'), .dropdown-partners" ).hover(function() {
		//alert('1');
		if (partnersTimeout !== null) {
			clearInterval(partnersTimeout);
			partnersTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-partners)" ).stop().hide();
		$( ".dropdown-partners" ).stop().slideDown(500);
	}, function() {
		if (partnersTimeout !== null) {
			clearInterval(partnersTimeout);
			partnersTimeout = null;
		}
		partnersTimeout = setTimeout(function() {
			$( ".dropdown-partners" ).stop().slideUp(500);
		}, 350);
	});
	
		
	var resourcesTimeout = null;
	$( ".menu-item-resources:not('.current-menu-item, .current-menu-ancestor'), .dropdown-resources" ).hover(function() {
		//alert('1');
		if (resourcesTimeout !== null) {
			clearInterval(resourcesTimeout);
			resourcesTimeout = null;
		}
		$( "#menu-dropdowns aside:not(.dropdown-resources)" ).stop().hide();
		$( ".dropdown-resources" ).stop().slideDown(500);
	}, function() {
		if (resourcesTimeout !== null) {
			clearInterval(resourcesTimeout);
			resourcesTimeout = null;
		}
		resourcesTimeout = setTimeout(function() {
			$( ".dropdown-resources" ).stop().slideUp(500);
		}, 350);
	});
	
	
  });
