<h2><span id="dnn_ctr1733_dnnTITLE_titleLabel" class="Head">Categories</span>
</h2>
<div id="dnn_ctr1733_ContentPane" class="ContainerPane"> <div id="dnn_ctr1733_ModuleContent" class="DNNModuleContent ModBlogC">
<div id="blogServiceErrorBox1733"></div>
<div id="categoryList1733" class="categoryList_list">
</div>
<script type="text/javascript">
 var catList = [
 {id: 1971, text: ".Net Development", nrposts: 18, link: "http://capturedtech.com/Home/Term/1971/Net-Development", parentId: 0},{id: 1972, text: "Conferences", nrposts: 8, link: "http://capturedtech.com/Home/Term/1972/Conferences", parentId: 0},{id: 1973, text: "General Business", nrposts: 64, link: "http://capturedtech.com/Home/Term/1973/General-Business", parentId: 0},{id: 1974, text: "Online Marketing", nrposts: 112, link: "http://capturedtech.com/Home/Term/1974/Online-Marketing", parentId: 0},{id: 1975, text: "Social Media", nrposts: 34, link: "http://capturedtech.com/Home/Term/1975/Social-Media", parentId: 0},{id: 1976, text: "Web Development", nrposts: 71, link: "http://capturedtech.com/Home/Term/1976/Web-Development", parentId: 0},{id: 1977, text: "Wordpress", nrposts: 20, link: "http://capturedtech.com/Home/Term/1977/Wordpress", parentId: 0},
 {}
 ]

  function addCategories(listItem, id) {
   var first = true;
   var ul = null;
   $.each($.grep(catList, function(c, i) {
   return (c.parentId==id);}), function(i, v){
    if (first) {
     ul = $('<ul></ul>');
     listItem.append(ul);
     first=false;
    }
    addCategories(ul.append('<li><a href="' + v.link + '">' + v.text + ' (' + v.nrposts + ')</a></li>'), v.id);   
   });
  }

 $(document).ready(function () {
  addCategories($('#categoryList1733'), 0)
 });
</script>
