   ];
   $('#blogCal1550').datepicker({
   onSelect: function(dateText, inst) {
    var url = 'http://capturedtech.com/?end=';
    url += dateText;
    window.location.href = encodeURI(url);
   },
   dateFormat: "yy-mm-dd",
   beforeShowDay: function(date) {
     var sDate = date.getFullYear() + '-' + (date.getMonth()+1) + '-' + date.getDate();
     if ($.inArray(sDate, selDates) >=0 ) {
      return [true,'selectedDate'];
     } else {
      return [true,''];
     }
    }
   });
   $('#blogCal1550').datepicker('setDate', '2016-09-21');
  });
 } (jQuery, window.Sys));
