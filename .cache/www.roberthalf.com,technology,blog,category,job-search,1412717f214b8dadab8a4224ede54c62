function getGSParams(url) {
    if(typeof url == 'undefined') {
    url = window.location;
    }
    var lobId = '';
    var custId = '';
    var country = '';
    var locale = '';
    var source = '';
    var lobFolder = '';


    country = Drupal.settings.rh_global.country;

    var path = url.pathname.split('/');
    locale = 'en_' + country.toUpperCase();
    lobFolder = path[1];

    switch(lobFolder) {
    case 'accountemps':
    lobId = 'AT';
    break;
    case 'finance':
    lobId = 'RHFA';
    break;
    case 'creativegroup':
    lobId = 'TCG';
    break;
    case 'legal':
    lobId = 'RHL';
    break;
    case 'management-resources':
    lobId = 'RHMR';
    break;
    case 'technology':
    lobId = 'RHT';
    break;
    case 'officeteam':
    lobId = 'OT';
    break;
    }

    var data = '{"z":"RH","a":"' + lobId + '","b":"' + custId + '","c":"' + country + '","d":"' + locale + '","e":"' + source + '"}';
    return data;
    }



