// Intentionally before doc ready
if(typeof Drupal.settings.rh_job_search !== 'undefined' && typeof Drupal.settings.rh_job_search.query_string !== 'undefined') {
if(Drupal.settings.rh_job_search.query_string.substring(1) == '') {
