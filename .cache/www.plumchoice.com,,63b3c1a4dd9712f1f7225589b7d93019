});
</script><div class="ls-wp-fullwidth-container" style="height:500px;"><div class="ls-wp-fullwidth-helper"><div id="layerslider_21" class="ls-wp-container" style="width:100%;height:500px;margin:0 auto;margin-bottom: 0px;"><div class="ls-slide" id="banner-crowded-cloud" data-ls="slidedelay:14000;transition2d:5;"><img src="http://www.plumchoice.com/wp-content/uploads/2014/10/banner-CAS-bkgd.png" class="ls-bg" alt="banner-CAS-bkgd" /><img class="ls-l" style="top:250px;left:172px;opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */;white-space: nowrap;" data-ls="offsetxin:0;offsetyin:400;durationin:400;easingin:easeInOutQuad;offsetxout:-800;durationout:5000;showuntil:1;easingout:linear;" src="http://www.plumchoice.com/wp-content/uploads/2015/05/image-building-5.png" alt=""><img class="ls-l" style="top:-518px;left:264px;opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */;white-space: nowrap;" data-ls="offsetxin:0;offsetyin:700;durationin:500;delayin:900;offsetxout:-800;durationout:5000;showuntil:1;easingout:linear;" src="http://www.plumchoice.com/wp-content/uploads/2015/05/image-building-2.png" alt=""><img class="ls-l" style="top:71px;left:430px;opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */;white-space: nowrap;" data-ls="offsetxin:0;offsetyin:500;durationin:500;delayin:1920;offsetxout:-800;durationout:5000;showuntil:1;easingout:linear;" src="http://www.plumchoice.com/wp-content/uploads/2015/05/image-building-1.png" alt=""><img class="ls-l" style="top:46px;left:666px;opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */;white-space: nowrap;" data-ls="offsetxin:0;offsetyin:600;durationin:500;delayin:2400;offsetxout:-800;durationout:5000;showuntil:1;easingout:linear;" src="http://www.plumchoice.com/wp-content/uploads/2015/05/image-building-3.png" alt=""><img class="ls-l" style="top:246px;left:927px;opacity: 0.4; filter: alpha(opacity=40); /* For IE8 and earlier */;white-space: nowrap;" data-ls="offsetxin:0;offset