			jQuery('.grid-layout .post').css('width', gridwidth);
			jQuery('.grid-layout').isotope({
				layoutMode: 'masonry',
				itemSelector: '.post',
				transformsEnabled: false,
				masonry: {
					columnWidth: gridwidth,
					gutterWidth: 40
