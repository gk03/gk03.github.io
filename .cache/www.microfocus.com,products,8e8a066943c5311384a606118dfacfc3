    <script type='text/javascript'>
        $(function() {

            function paramReplace(key, value) {
                var query = window.location.search;
                var re = new RegExp("[\\?&]" + key + "=([^&#]*)");
                var matches = re.exec(query);
                var new_query;

                if (matches === null) {
                    // if there are no params, append the parameter
                    var delimeter = '?';
                    if (query.indexOf('?') !== -1) {
                        delimeter = '&';
                    }
                    new_query = query + delimeter + key + '=' + value;
                } else {
                    var delimeter = matches[0].charAt(0);
                    new_query = query.replace(re, delimeter + key + "=" + value);
                }
                new_query = new_query.replace(/page=(\d+)/i, 'page=1');
                return new_query;
            }

            var url = window.location.protocol + '//' + window.location.host + window.location.pathname;

            $("select").on("change", function() {
                var self = $(this);
                var value = self.val();
                var filter = self.data('id');

                // build url
                if(value !== '') {
                    var new_query = paramReplace(filter, value);
                    window.location = url + new_query;
                }
            });

            jQuery('.filterable a').click(function(e) {
                e.preventDefault();
                var self = $(this);
                var value = self.data('value');
                var filter = 'letter';

                // build url
                if(value !== '') {
                    var new_query = paramReplace(filter, value);
                    window.location = url + new_query;
                }
            });

        });
    </script>

