<script>
function Set_Cookie(name,value, path,expires,domain,secure) 
{
	document.cookie = name + "=" +escape(value) +
		( (expires) ? ";expires=" + expires.toGMTString() : "") +
		 ";path=/" +
		//( (path) ? ";path=" + "/music" : "") + 
		( (domain) ? ";domain=" + domain : "") +
		( (secure) ? ";secure" : "");
}

function loadPrintable()
{
	if (String(window.location).indexOf("?") < 0)
		window.open(remove(String(window.location), "#") + '?PrintableVersion=yes', 'PrintableVersion' , 'width=800,height=600,scrollbars=no,noresize'); 
	else
		window.open(window.location + '&PrintableVersion=yes', 'PrintableVersion' , 'width=800,height=600,scrollbars=no,noresize');
}

function remove(s, t) 
{
	i = s.indexOf(t);
	r = "";
	if (i == -1) return s;
	r += s.substring(0,i) + remove(s.substring(i + t.length), t);
	return r;
}

function getCookie(name)
{
	var dc = document.cookie;
	var prefix = name + "=";
	var begin = dc.indexOf("; " + prefix);
	if (begin == -1)
	{
		begin = dc.indexOf(prefix);
	if (begin != 0) return null;
	}
	else
	{
		begin += 2;
	}
	var end = document.cookie.indexOf(";", begin);
	if (end == -1)
	{
		end = dc.length;
	}
	return unescape(dc.substring(begin + prefix.length, end));
}

function trackLink(link)
{
	Set_Cookie("linkSource", link);
	//alert(getCookie("linkSource"));
	return true;
}

Set_Cookie('DiscMakersScreenRes', screen.width+'x'+screen.height);
Set_Cookie("linkSource", "");

function preFillForms()
{
	var x = document.forms.length;
	while(x >= 0)
	{
		x--;
		var frm = document.forms[x];		


if (typeof (InitFormLabels) == typeof (Function)) { InitFormLabels($(frm)); }
	}	
}

function setInForm(frm, elemName, elemValue)
{
	if (frm == null)
		return;
	
	if (frm.elements == null)
		return;

	for (i=0;i<frm.elements.length;i++)
	{
		if(frm.elements[i].name > "")
		{
			if (elemName.toUpperCase() == frm.elements[i].name.toUpperCase())
			{
				frm.elements[i].value = elemValue;
			}
		}//if undefined
	}
}	
