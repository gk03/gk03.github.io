<script type="text/javascript" language="javascript">


$(document).ready(function () {

	$('li.login-dropdown').on('mouseover', function(e) {
		e.stopPropagation();
		$('.account-dropdown').show();
	});
	$('li.login-dropdown').mouseleave(function(){
		$('.account-dropdown').hide();
	});

    $('div.mid-nav-item p').on('mouseover', function(e) {
        e.stopPropagation();
        $(this).next('.mid-nav-cont-dropdown').show();
    });

    $('.mid-nav-item').mouseleave(function(){
      $('.mid-nav-cont-dropdown').hide();
    });

    $('div.mid-nav-item.shop-all p').on('mouseover', function(e) {
        e.stopPropagation();
        $('.mid-nav-cont-dropdown').hide();
        $(this).next('.mid-nav-cont-shopall').show();
    });

    $('.mid-nav-item.shop-all').mouseleave(function(){
      $('.mid-nav-cont-shopall').hide();
    });

	if ($("#new-page-wrap").hasClass("no-border-needed")) {
		$("#new-design-wrapper").addClass('border-none');
	}
	if ($("#new-page-wrap").hasClass("new-home-page")) {
		$("#new-design-wrapper").removeClass('macsales-wrap');
		$("#new-design-wrapper").addClass('header-margin');
	}

});

$(window).scroll(function() {
	var scroll = $(window).scrollTop();
	if (scroll >= 150) {
		$("#header-navigation").addClass("invisible");
		$("#mini-site-header").removeClass("invisible");
		$("#return-old-look.desktop").addClass("invisible");
	}
	else {
		$("#header-navigation").removeClass("invisible");
		$("#mini-site-header").addClass("invisible");
		$("#return-old-look.desktop").removeClass("invisible");
	}
});

</script>



