					<script type="text/javascript">



						function validateForm(name)



						{



							if('' == name.FirstName.value || 'First Name' == name.FirstName.value) {



								name.FirstName.style.border = "1px solid #800000";



								name.FirstName.style.color = "#800000";



							}



							if('' == name.Email.value || 'Email' == name.Email.value) {



								name.Email.style.border = "1px solid #800000";



								name.Email.style.color = "#800000";



							}



							if('' == name.FirstName.value || '' == name.Email.value  || 'First Name' == name.FirstName.value|| 'Email' == name.FirstName.value) {



								return false;



							}



							else



							{



								return true;



							}



						}



					</script>



