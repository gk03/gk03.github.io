    <script>
        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
            results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }
        function opensidebar() {
            jQuery("#contactBox").slideDown("slow", function () { });
            jQuery("#movingFormBox").animate({ marginBottom: 295 }, 600);            
        }
        jQuery(function () {
            var hasCompletedForm = getParameterByName('Thanks').toLowerCase();
            var numItems = jQuery('.mainform').length;
            if (numItems < 1 && hasCompletedForm != "yes") {
                setTimeout(function () {
                    jQuery("#contactBox").slideDown("slow", function () { });
                    jQuery("#movingFormBox").animate({ marginBottom: 295 }, 600);
                    jQuery('.numbersOnly').keyup(function () {
                        this.value = this.value.replace(/[^0-9\.]/g, '');
                    });
                }, 2500);
            }
        });
    </script>
