            }
            
            var circleHeight = $('.circle-top-text').height();
            $('.circle-top-text').css({
                left: (ww / 2) - (circleHeight / 2)
            });
            
            $('.circle-top-text-inner').each(function(){
                var elHeight = $(this).height(),
                    elHalfHeight = elHeight / 2;
                    
                $(this).css({
                    'top': (circleHeight / 2) - (elHalfHeight - 10)
                });
            });
            
            setTimeout(function(){
                $('.circle-top-text-inner').addClass('active');
            }, 1200);
        });
        
        $(window).resize(function(){
           var ww = $(window).width();
            
            $('.top-large-text').each(function(){
                var elWidth = $(this).width();
                
                $(this).height(elWidth / 4);
            });
            
            $('.top-small-text').each(function(){
                var elWidth = $(this).width();
                
                $(this).height(elWidth / 20);
            });
            
            if (ww > 767) {
                var topHeight = $('.home-top-section-inner').height();
                $('.circle-top-text').height(topHeight * 1.2).width(topHeight * 1.2);
            } else {
                var topHeight = $('.home-top-section-inner').height();
                $('.circle-top-text').height(topHeight * 1.1).width(topHeight * 1.1);
            }
            
            var circleHeight = $('.circle-top-text').height();
            $('.circle-top-text').css({
                left: (ww / 2) - (circleHeight / 2)
            });
            
            $('.circle-top-text-inner').each(function(){
                var elHeight = $(this).height(),
                    elHalfHeight = elHeight / 2;
                    
                $(this).css({
                    'top': (circleHeight / 2) - (elHalfHeight - 10)
                });
            });
        });
    </script>

<!-- end coded_template: id:4365874251 path:custom/page/web_page_basic/home-09-16.html -->
</body></html>