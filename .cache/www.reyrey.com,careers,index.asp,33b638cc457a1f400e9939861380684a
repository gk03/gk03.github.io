<script src="/javascript/jwplayer.js"></script>
<script>
$(function(){
  $('.videoLink').click(function(){
    var location = $(this).attr('data-location');
	if(location == "dayton") {
		$('#currentVideo').html('');
      	$('<div id="videoPlayer">').appendTo('#currentVideo');
      	jwplayer('videoPlayer').setup({
	        flashplayer: '/media/player.swf',
	        width: '395',
	        height: '224',
	        file: '/media/DaytonFacilityTours_2016Update_051816.mp4',
	        autostart: true
	      });
	} else if(location == "houston") {
		$('#currentVideo').html('');
      	$('<div id="videoPlayer">').appendTo('#currentVideo');
      	jwplayer('videoPlayer').setup({
	        flashplayer: '/media/player.swf',
	        width: '395',
	        height: '224',
	        file: '/media/houstontour.mp4',
	        autostart: true
	      });
	} else if(location == "celina") {
		$('#currentVideo').html('');
      	$('<div id="videoPlayer">').appendTo('#currentVideo');
      	jwplayer('videoPlayer').setup({
	        flashplayer: '/media/player.swf',
	        width: '395',
	        height: '224',
	        file: '/media/CelinaFacilityTour2012.mp4',
	        autostart: true
	      });
	} else {
		$('.otherVideos').children('ul').children('li').removeClass('active');
		writeFlash('currentVideo', '395', '224', '"Reynolds and Reynolds"', '/media/'+ location +'.swf');
	    $(this).parent("li").addClass('active');
	}
    return false;
  });
});
</script>		
