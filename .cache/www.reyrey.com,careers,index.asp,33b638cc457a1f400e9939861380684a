<script>
$(function(){
	$("#emailCont").click(function() {
		if($("#slideDown").css("top") == '35px') {
			$("#emailButton").attr("src","/images/subscribeDown.jpg");
			$("#slideDown").animate({
				top: '-362px'
			},function() {$("#slideDown").hide()});
		} else {
			$("#slideDown").show();
			$("#emailButton").attr("src","/images/subscribeUp.jpg");
			$("#slideDown").animate({
				top: '35px'
			});
		}
	});
});
</script>
