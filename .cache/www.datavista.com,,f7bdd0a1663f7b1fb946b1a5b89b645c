  <script>
  
  

  
  $('.first-slide').fadeIn(0, function(){
	  $('.animate-logo').fadeIn(0);
	  $('.animate-BR').delay(1000).animate({'right': 0}, 1500);

  });
  
  
 // var timeID = setTimeout(function(){
	//  $('.first-slide').fadeOut(1500);
 //}, 8000);
  
$('.animate-BR').click(function(){
	
	//clearTimeout(timeID);
	//clearTimeout(loadSlider);
	$('.first-slide').fadeOut(3500, function(){
		
		 $('#da-slider').cslider({
 
  current     : 0,    
    // index of current slide
     
  bgincrement : 50,   
    // increment the background position 
    // (parallax effect) when sliding
     
   autoplay    : true,
    // slideshow on / off
     
   interval    : 6500  
    // time between transitions
     
});
		
	});
	
	
	
setTimeout(function(){
	hoverStates();
	}, 1600);
	
  });
  
  
var daDots =[];

function hoverStates(){
	
	
$('.da-dots a').each(function(index, element) {  
	daDots.push($(this));	
});

for(var i=0;i<daDots.length;i++)
{
daDots[i].bind('mouseenter mouseleave', function(){

	var thisIndex = $('.da-dots a').index(this);
	$('.hover-item-' + thisIndex).fadeToggle(200);

	
	});
}

}
$(window).load(function() {
       $('.animate-BR').click();
});
</script>



