		 <script type='text/javascript'>
		 function setCookie(cname,cvalue,exdays)
		 {
			 document.cookie = cname + "=" + cvalue ;
		 }

		 function getCookie(cname)
		 {
			 var name = cname + "=";
			 var ca = document.cookie.split(';');
			 
			   for(var i=0; i<ca.length; i++) 
			   { 
				   var c = ca[i].trim();
				   if (c.indexOf(name)==0){ return c.substring(name.length,c.length); }
			   }
			 return "";
		 }
		 
		 if(getCookie('jquery_popup') == ""){
			 setCookie("jquery_popup", 'sessionexists', 1); 
			
		 	$(document).ready(function(){
			    var overlay = $('<div id="overlay"></div>');
                overlay.show();
                overlay.appendTo(document.body);

                $(".popup").show();
                $(".close").click(function(){
                    $(".popup").hide();
                    overlay.appendTo(document.body).remove();
                    return false;
                });

                $(".x").click(function(){
                    $(".popup").hide();
                    overlay.appendTo(document.body).remove();
                    return false;
                });
            });

		 	 $(window).load(function(){
                $(".popup-loader").hide();
                $(".popup-inner").css("visibility", "visible");
            });

		 	$("#overlay").ready(function(){
				$("body").addClass("body-overflow");
				$(".x").click(function(){
                   $("body").removeClass("body-overflow");
                });
     		});
		 }
            
        </script>

<script type="text/javascript" language="javascript">
    $(window).load( function(){      
    	
        $('#homepage-carousel').show();
        /*request for quote*/
    	
        $(".request_quote_image").show();
        $("#request_quote").hide();
        $(".request_quote_image").click(function() { 
            var w = $(window).width(); 
            if(w<=600){
                window.location.href ='/requestofquote';
            } 
            else{

                $('#request_quote').show();
            }
        });
        //$(window).on('resize', function () {
        $(".request_quote_close,.btn_cancel").click(function() {
            $("#request_quote").hide();
            $("input[type=text],#pmessage,#address").val('');
        });
			
        //});
		
		          
        function getActiveThumb(){
            var $actval = 	$('#homepage-carousel').triggerHandler( 'currentVisible' );
			
            return $actval;
            //center = Math.floor($active.length / 2);
        }
        //	Responsive layout, resizing the items
        $('#homepage-carousel').carouFredSel({
            responsive: true,
            width: '100%',
            height:'auto',
            swipe:true,
            scroll: {
                fx: "crossfade",
                duration: 500,
                timeoutDuration: 5000
                
            },
            onCreate:function(){
                $('#homepage-carousel').parent().height($('#homepage-carousel').children().first().height());
                $('#homepage-carousel').height($('#homepage-carousel').children().first().height());
                $(window).on('resize', function () {
                    $('#homepage-carousel').parent().height($('#homepage-carousel').children().first().height());
                    $('#homepage-carousel').height($('#homepage-carousel').children().first().height());
                }).trigger('resize');
            }
        });
		
        $('#pager2').find( 'img' ).click(function() { 
            var pos = $(this).attr( 'rel' );
            //alert(pos);  
            //$('#homepage-carousel').trigger( 'slideTo', pos );
            $('#homepage-carousel').trigger( 'slideTo', [ 'div[id="'+ pos +'"]' ] );
        });
		
        $('#homepage-carousel').bind("DOMSubtreeModified", function() {
            $('#pager2 img').each(function(){
                $(this).removeClass('selected');
                if($('#homepage-carousel').eq(0).find('img').attr('src') == $(this).attr('src')){
                    $(this).addClass('selected');
                }
            });
        });
		
    });
			
    
</script>

<script type="text/javascript">
    $(function(){  

        //Latest news rss
		
        $("#latest_news").html('Loading...'); 
        $.post("/getlatestrssfeed/",{token:'1474182303'},function(data){					
            //alert(data);
            //1016547967
            $("#latest_news").html(data); 
            $('#carousel_news').carouFredSel({
                responsive: true, 
                auto:false,
                width: '100%',
                scroll: {
                    fx: "crossfade",
                    easing: "quadratic",
                    duration: 500,
                    timeoutDuration: 5000
                },
		                   
                prev: '#news-prev',
                next: '#news-next',
                items: {
                    width: 400,
                    //height: '30%',	//	optionally resize item-height
                    visible: {
                        min: 1,
                        max: 999
                    }
                }
		                    
            });
        });

        // Most active news rss
        $("#most_active_news").html('Loading...');  
        $.post("/getmostactiverssfeed/",{token:'1474182303'},function(data){					
            //alert(data); 
            $("#most_active_news").html(data); 
        });	 
        //$(".abt_domains").hide();
        $("#innerprefix,.dropdown-arrow").on('click',function(){
        	
            $(".abt_domains").toggle();
        	
        });
        $("#domain_res").hide();
        $("#cmdSubmit").on('click',function(){
            var dname = $("#dname").val();
            var selectedprefix = $("#selectedprefix").attr('rel');
            //alert(selectedprefix);
            var checkedprx = [];
            checkedprx.push(selectedprefix);
            $("input[name='prefix[]']:checked").each(function ()
            { 
                checkedprx.push($(this).val());
            });
            //console.log(checkedprx);
            $("#domain_res").html("");
            if(dname!=""){
                $(".abt_domains").hide();
                $("#domain_res").html('<div class="margin-loader"><img src="/ugimg/loader.gif" /></div>');  
                $("#domain_res").show();
                
                $.post("/checkdomain",{token:1474182303,dname:dname,checkedprx:checkedprx},function(data){					
                    //console.log(data); 
                    $("#domain_res").html(data); 
                    var checkeddomain = []; 
                    $("input[name='chkbookdomain[]']").on('change',function(){
                        if($(this).is(":checked")) {
                            checkeddomain.push($(this).val());
                        }
                        else {
                            checkeddomain.pop($(this).val());
                        }
                        var attachhref = 'registerdomain?dname='+checkeddomain;
                        $("#addhref a").attr('href',attachhref);
                    });
                    $("#addhref a").on('click',function(){
                        if(checkeddomain==""){
                            alert('Please select at least one domain for booking');
                            return false;
                        }
                        else
                            return true;
                    });
                    $("#domcancel").on('click',function(){
                        $("#domain_res").hide();
                        $("#dname").val('');
                    });
                });
            }
            else{
                alert("Please enter the domain name");
            }
        });  
    });	  
