<script type="text/javascript"> 
$(".numeric").numeric();
                        $(document).ready(function() {  
                            var form = $("#request_me_form");

                            var pname = $("#pname");
                            var pnameEmpty = $("#pname_not_empty");
                            var pemail = $("#pemail");
                            var pemailEmpty = $("#pemail_not_empty");
                            var hidcaptcha = $("#generatedcaptcha"); 
                            var capchta = $("#enteredcaptcha");
                            var captchaInvalid  = $("#captcha_not_empty");
                            $("#cmdSubmitFeedback").click(function() {  
		
                                var flag = 1;
		
                                /* empty validation */
                                if(!validateEmpty(pname, pnameEmpty, "the name")) { 			
                                    flag = 0;
                                }
                                if(!validateEmpty(pemail, pemailEmpty, "the email")) { 			
                                    flag = 0;
                                }
		
                                if(pemail.val() != ""){
                                    if(!validateEmail(pemail, pemailEmpty)) { 			
                                        flag = 0;
                                    }	
                                }
		
                                /*if(!CheckCombo(pcomments, pcommentsEmpty, "your interest")) { 			
                                            flag = 0;
                                  }*/

                                if(!validateEmpty(capchta, captchaInvalid, "the captcha")) { 			
                                    flag = 0;
                                }
								
                                if(capchta.val()!=""){  
                                    if(!ValidateCaptcha(capchta, hidcaptcha, captchaInvalid,"valid captcha")) { 			
                                        flag = 0;
                                    }	
                                } 
		
                                if(flag){
                                    var param= {};
                                    param['pname']= $("#pname").val();
                                    param['pemail']= $("#pemail").val();
                                    param['pcontact']= $("#pcontact").val();
                                    param['pcity']= $("#pcity").val();
                                    param['pstate']= $("#pstate").val();
                                    param['pcountry']= $("#pcountry").val();
                                    param['pcode']= $("#pcode").val();
                                    param['prequest']= $("#prequest").val();
                                    param['pmessage']= $("#pmessage").val(); 
                                    param['paddress']= $("#paddress").val();
                                    param['generatedcaptcha']= $("#generatedcaptcha").val();
                                    param['enteredcaptcha']= $("#enteredcaptcha").val();
			  
                                    $.ajax ({
                                        url:"/sendfeedback",
                                        type: 'POST',
                                        data: { result : param},
                                        async: true,
                                        beforeSend:function() { 
                                            $("#request_me_form").show(); 
                                        },
                                        success: function (result) {
			        	
                                            if(result == "true") {
                                                $("#request_me_form").hide(); 
                                                $(".suc_message").html("Thank you for the quote request! we will revert to you soon.");
                                                $("#pname").val("");
                                                $("#pemail").val("");
                                                $("#pcontact").val("");
                                                $("#pcity").val("");
                                                $("#pstate").val("");
                                                $("#pcountry").val("");
                                                $("#pcode").val("");
                                                $("#prequest").val("");
                                                $("#pmessage").val("");
                                            } else if(result == "false") { 
                                                $("#request_me_form").show(); 
                                                $(".suc_message").html("Email cannot be sent at this time. Please try again later.");
                                            } else{
                                            	 $("#request_me_form").show(); 
                                                 $(".suc_message").html("The CAPTCHA entered is invalid.");
                                            }
                                        },
                                        error: function (xhr, ajaxOptions, thrownError) {
                                            $(".request_me_form").show();  
                                            $(".suc_message").html("Network error please check the internet connection");
                                        }
                                    }); 
                                    //return true;
 	 		
                                }	 
                                else
                                    return false;
                            });
 	
                        });


                    </script>
</div> 

