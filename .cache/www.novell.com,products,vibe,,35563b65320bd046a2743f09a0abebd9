				case "KW" :
				case "LV" :
				case "LB" :
				case "LR" :
				case "LT" :
				case "MK" :
				case "MG" :
				case "MW" :
				case "MV" :
				case "ML" :
				case "MU" :
				case "YT" :
				case "MD" :
				case "MA" :
				case "NA" :
				case "NE" :
				case "OM" :
				case "PL" :
				case "QA" :
				case "RE" :
				case "RO" :
				case "RU" :
				case "RW" :
				case "SH" :
				case "SA" :
				case "CS" :
				case "SC" :
				case "SK" :
				case "SI" :
				case "TN" :
				case "TR" :
				case "UG" :
				case "UA" :
				case "AE" :
				case "UZ" :
				case "EH" :
				case "YE" :
				case "ZM" :
				case "ZW" :

				//custom
				case "AT" :
				case "DE" :
				case "CH" :
					jQuery(address_field_id).show();
					break;
				default:
					jQuery(address_field_id).hide();
			}
			return true;
		}
	}
	
	jQuery(document).ready(function() {
		var temp_select = jQuery("<SELECT class='state_temp' style='display: none;'></SELECT>");
		jQuery('#form_builder_form_57d7f55450f38').append(temp_select);


		apply_country_change_57d7f55450f38();
		if (versionCompare(jQuery.fn.jquery, "1.10") > 0) {
			jQuery(document).on('change', '#form_builder_form_57d7f55450f38 .form_builder_country_list', function() {
				var new_value = jQuery(this).val();
				apply_country_change_57d7f55450f38(new_value);
			});
		} else {
			jQuery('#form_builder_form_57d7f55450f38 .form_builder_country_list').change(function() {
				var new_value = jQuery(this).val();
				apply_country_change_57d7f55450f38(new_value);
			});
		}
		
		var zip_field_id = '#form_builder_form_57d7f55450f38 #form_builder_postalCode';
		jQuery(zip_field_id + " input").keyup(function() {
			apply_zip_change_57d7f55450f38();
		});
	});
</script>


