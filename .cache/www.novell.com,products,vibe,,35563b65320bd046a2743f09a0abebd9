<script type="text/javascript">
	versionCompare = function(left, right) {
		if (typeof left + typeof right != 'stringstring') {
			return false;
		}

		var a = left.split('.')
		,   b = right.split('.')
		,   i = 0, len = Math.max(a.length, b.length);

		for (; i < len; i++) {
			if ((a[i] && !b[i] && parseInt(a[i]) > 0) || (parseInt(a[i]) > parseInt(b[i]))) {
				return 1;
			} else if ((b[i] && !a[i] && parseInt(b[i]) > 0) || (parseInt(a[i]) < parseInt(b[i]))) {
				return -1;
			}
		}

		return 0;
	};
	
	function apply_zip_change_57d7f55450f38() {
		var zip_field_id = '#form_builder_form_57d7f55450f38 #form_builder_postalCode';
			var country = jQuery("#form_builder_form_57d7f55450f38 .form_builder_country_list").val();
		if(country === 'DE') {
			var eventNameValue = jQuery('#form_builder_form_57d7f55450f38 input[name=eventName]').val();
			var C_Subject_Line_SCR1_field = jQuery('#form_builder_form_57d7f55450f38 input[name=C_Subject_Line_SCR1]');
			var C_Subject_Line_SCR1 = '' + eventNameValue + " (" + country + ")";
			
			var postal_code_value = jQuery(zip_field_id + " input").val();
			if (postal_code_value !== '') {
				C_Subject_Line_SCR1 = '' + eventNameValue + " (" + country + " " + postal_code_value + ")";
			}
			C_Subject_Line_SCR1_field.val(C_Subject_Line_SCR1);
		}
	}
	
	function apply_country_change_57d7f55450f38(country) {
		var address_field_id = '#form_builder_form_57d7f55450f38 #form_builder_address1';
		var zip_field_id = '#form_builder_form_57d7f55450f38 #form_builder_postalCode';
		var state_field_id = '#form_builder_form_57d7f55450f38 #form_builder_stateProvince';
		
		
		if (typeof country == 'undefined') {
			country = jQuery("#form_builder_form_57d7f55450f38 .form_builder_country_list").val();
		}
		
		if(country == null || country == 'undefined') {
			country = 'undefined';
		} else {
			var postal_code_value;
			var eventNameValue = jQuery('#form_builder_form_57d7f55450f38 input[name=eventName]').val();
			var C_Subject_Line_SCR1_field = jQuery('#form_builder_form_57d7f55450f38 input[name=C_Subject_Line_SCR1]');
			var C_Subject_Line_SCR1 = '' + eventNameValue + " (" + country + ")";
			
			
			
			switch(country) {
				case "US":
				case "CA":
				case "UK":
					jQuery(zip_field_id).show();
					jQuery(state_field_id).show();
					var all_options = jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list option:not(:first)');
					jQuery('#form_builder_form_57d7f55450f38 .state_temp').append(all_options);
					jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list option:not(:first)').remove();

					var correct_states_selector = '#form_builder_form_57d7f55450f38 .state_temp option[data-country='+country+']';
					var correct_states = jQuery(correct_states_selector);
					jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list').append(correct_states);
					jQuery(correct_states_selector).remove();
					break;
				case "AU":
				case "IN":
					jQuery(zip_field_id).hide();
					jQuery(state_field_id).show();
					var all_options = jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list option:not(:first)');
					jQuery('#form_builder_form_57d7f55450f38 .state_temp').append(all_options);
					jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list option:not(:first)').remove();

					var correct_states_selector = '#form_builder_form_57d7f55450f38 .state_temp option[data-country='+country+']';
					var correct_states = jQuery(correct_states_selector);
					jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list').append(correct_states);
					jQuery(correct_states_selector).remove();
					
					postal_code_value = jQuery(zip_field_id + " input").val();
					if (postal_code_value != '') {
						C_Subject_Line_SCR1 = '' + eventNameValue + " (" + country + " " + postal_code_value + ")";
					}
					
					break;
				case "DE":
				case "ES":
				case "FR":
				case "CH":
				case "ZA":
					jQuery(state_field_id).hide();
					jQuery(zip_field_id).show();
					
					postal_code_value = jQuery(zip_field_id + " input").val();
					if (postal_code_value != '') {
						C_Subject_Line_SCR1 = '' + eventNameValue + " (" + country + " " + postal_code_value + ")";
					}
					break;
				default:
					jQuery('#form_builder_form_57d7f55450f38 .form_builder_state_list option:first').attr('selected', 'selected');
					jQuery(zip_field_id + ", " + state_field_id).hide();
			}
			C_Subject_Line_SCR1_field.val(C_Subject_Line_SCR1);

			switch(country) {
				//Asia Pacific
				case "AF" :
				case "AS" :
				case "BD" :
				case "BT" :
				case "BN" :
				case "KH" :
				case "CX" :
				case "PF" :
				case "ID" :
				case "MN" :
				case "MM" :
				case "NP" :
				case "NC" :
				case "PK" :
				case "PW" :
				case "PG" :
				case "PH" :
				case "WS" :
				case "SB" :
				case "LK" :
				case "TH" :
				case "TO" :
				case "VN" :

				//Latin America
				case "AI" :
				case "AG" :
				case "AR" :
				case "AW" :
				case "BS" :
				case "BB" :
				case "BZ" :
				case "BM" :
				case "BO" :
				case "BR" :
				case "KY" :
				case "CL" :
				case "CO" :
				case "CR" :
				case "DM" :
				case "DO" :
				case "EC" :
				case "SV" :
				case "GF" :
				case "GD" :
