<script type="text/javascript">
	function getQueryVariable(variable) {
		var query = window.location.search.substring(1);
		var vars = query.split('&');
		for (var i = 0; i < vars.length; i++) {
			var pair = vars[i].split('=');
			if (decodeURIComponent(pair[0]) == variable) {
				return decodeURIComponent(pair[1]);
			}
		}
		return false;
	}
	
	/* Cookie Functions
	---------------------------------------------------- */
	if (typeof readCookie != 'function') {
		window.readCookie = function(name) {
			var cks = document.cookie.split(';');
			for(var i=cks.length-1; i >= 0; i--) {
				var c = cks[i];
				while(c.charAt(0)===' ') {
					c = c.substring(1,c.length);
				}
				if(c.indexOf(name+"=")===0) {
					return c.substring(name.length+1,c.length);
				}
			}
			return null;
		};
	}

	if (typeof deleteCookie != 'function') {
		window.deleteCookie = function(name,path,domain) {
			if(!domain) {
				domain = ".attachmate.com"; 
			}
			if(readCookie(name)) { document.cookie = name + "=" +
				((path) ? ";path=" + path : ";path=/") +
				";domain=" + domain + ";expires=Thu, 01-Jan-1970 00:00:01 GMT";
			}
		};
	}

	if (typeof createCookie != 'function') {
		window.createCookie = function(name,value,days,path,domain) {
			var expires = "";
			if(!domain) {
				domain = ".attachmate.com"; 
			}
			if(days) {
				var date = new Date();
				date.setTime(date.getTime()+(days*24*60*60*1000));
				expires = "; expires="+date.toGMTString();
			}
			document.cookie = name+"="+value+expires +
				((path) ? "; path=" + path : "; path=/") +
				"; domain=" + domain;
	
		};
	}
	
	function submit_formbuilder_form_57d7f55450f38() {
		jQuery('#form_builder_submit_status_57d7f55450f38').show();

		

		var submit_data = jQuery('#form_builder_form_57d7f55450f38').serialize();
					var validate_url = '//www.novell.com/common/inc/form_builder_transport.php?hide_demandbase=true&url=/forms/validate_form/first_last_name|email|company_phone|city_country|address|postal|stateProvince|comment|optin/novell_rac?callback=?';
			var submit_url = '//www.novell.com/common/inc/form_builder_transport.php?hide_demandbase=true&url=/forms/form_submit/novell_rac/?callback=?';
				jQuery.ajax({
			type: 'post',
			url: validate_url,
			data: submit_data,
			success: function(data) {
				if (data.success == true) {
					var field_results = data.validation_results.results;

					// check if everything that didn't validate is shown (for address, zip, state)
					var fully_validated = true;
					for (x in field_results) {
						if (field_results[x].validated == false) {
							var field_container = jQuery('#form_builder_form_57d7f55450f38 #form_builder_'+x);
							if (field_container.is(':visible')) {
								fully_validated = false;
							}
						}
					}


					// now check if everything validated
					if (data.validation_results.validated == false && fully_validated == false) {
						// remove all existing errors
						jQuery('#form_builder_form_57d7f55450f38 .form_field_container').removeClass('error');
						var count = 0;
						for (x in field_results) {
							if (field_results[x].validated == false) {
								var field_container = jQuery('#form_builder_form_57d7f55450f38 #form_builder_'+x);
								if (field_container.is(':visible')) {
									if (count === 0) {
										jQuery('input, select', field_container).focus();
									}
									jQuery('.validation_error_notice', field_container).html(field_results[x].messages[0]);
									field_container.addClass('error');
									count++;
								}
							}
						}

						jQuery('#form_builder_submit_status_57d7f55450f38').hide();
					} else {
//							jQuery('#form_builder_form_57d7f55450f38').attr('action', data.validation_success_form_action);
//							console.log (jQuery('#form_builder_form_57d7f55450f38').serialize());
//							jQuery('#form_builder_form_57d7f55450f38').submit();

							
							  window.dataLayer = window.dataLayer || [];
							  window.dataLayer.push({
								'event' : 'formBuilderSubmitSuccess',
								'formId' : 'RAC'
							  });
						//////////////////////////////////////////////////////////////////
						// VALIDATION SUCCESS - SO NOW WE ACTUALLY SUBMIT THE FORM
						//--------------------------------------------------------------------
							if (typeof data.after_validate_js_function != 'undefined') {
								window[data.after_validate_js_function]('#form_builder_form_57d7f55450f38');
							}
							jQuery.ajax({
								type: 'post',
								url: submit_url,
								data: submit_data,
								success: function(data) {
//										createCookie("submitted_eloqua_email", encodeURIComponent(data.eloqua_id), 30);
									if (typeof data.eloqua_id != "undefined") {
										var domain = window.location.hostname;
										createCookie("elid", encodeURIComponent(data.eloqua_id), 30, '/', domain);
									}
									if (data.success == true) {
										// if successful we need to forward to the thankyou page
										if (data.inline_thankyou_html !== false) {
											// add form submit success event to SUSE RAC form
											if (window._gat && window._gat._getTracker) {
												ga('send', 'event', { 
													eventCategory: 'Form Submit', 
													eventAction: 'Submit', 
													eventLabel: 'RAC'
												});
											}


											var replace_el = jQuery("#form_builder_form_57d7f55450f38").closest("#"+data.inline_thankyou_replace_id);
											replace_el.html(data.inline_thankyou_html);
											var el_offset_top = replace_el.offset().top;
											jQuery(window).scrollTop(el_offset_top - 10);

										} else {
											if (data.thankyouPage != undefined) {
												// set our customer email address in local storage now
												if ( $('#c_email').length ) {
													sessionStorage.customerEmail = $('#c_email').val();
												}
												if (window._gat && window._gat._getTracker) {
													ga('send', 'event', { 
														eventCategory: 'Form Submit', 
														eventAction: 'Submit', 
														eventLabel: 'RAC'
													});
												}
												
												setTimeout(function() {
													window.location.href = data.thankyouPage;
												}, 400);
											}
										}
									} else {
										// DREW TODO - take care of this error case
									}
								},
								complete: function() {
//										jQuery('#form_builder_submit_status_57d7f55450f38').hide();
//										console.log ("came into form submit complete");
								},
								error: function() {
//										console.log ("came into form submit error");
//										 DREW TODO - take care of this error case
								},
								dataType: 'jsonp'
							});
						//---------------------------------------------------------------------
					}
				} else {
					// DREW TODO - deal with failed ajax call
					jQuery('#form_builder_submit_status_57d7f55450f38').hide();
				}
			},
			complete: function() {
				// DREW TODO - finish this
//					console.log ("came into complete");
			},
			error: function() {
				jQuery('#form_builder_submit_status_57d7f55450f38').hide();
				// DREW TODO - finish this
//					console.log ("came into error");
			},
			dataType: 'jsonp'
		});
	}
	
	var came_into_doc_ready_57d7f55450f38 = false;
	jQuery(document).ready(function() {
		if (came_into_doc_ready_57d7f55450f38 == true) {
			return;
		}
		came_into_doc_ready_57d7f55450f38 = true;
		
		// put in the user prefered country if there is a cookie
		var preferred_country = null;
		if(readCookie('novell_country') !== null) { 
			preferred_country = readCookie('novell_country');
		}
		if(readCookie('user_country') !== null) { 
			preferred_country = readCookie('user_country');
		}
		if (preferred_country !== null) {
			jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='preferred_country' value='"+preferred_country+"' />");
		}
		
		
		// check to see if the language is set - if so then us it to add value to eloqua forms
		var first_hidden_form = jQuery('#form_builder_form_57d7f55450f38 input:hidden:first');
		var curr_lang = 'en-us';
		if(readCookie('novell_language') !== null) { 
			curr_lang = readCookie('novell_language');
		}
		if(readCookie('user_language') !== null) { 
			curr_lang = readCookie('user_language');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='language' value='"+curr_lang+"' />").after("<input type='hidden' name='Language' value='"+curr_lang+"' />");
		
		
		// read the a7 cookie (for w_search_type)
		var novell_a7 = '';
		if(readCookie('novell_a7') !== null) {
			novell_a7 = readCookie('novell_a7');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_search_type' value='"+novell_a7+"' />");
		
		// read the w_search_campaign cookie (for w_search_campaign)
		var novell_search_campaign = '';
		if(readCookie('novell_search_campaign') !== null) {
			novell_search_campaign = readCookie('novell_search_campaign');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_search_campaign' value='"+novell_search_campaign+"' />");
		
		// read the novell_search_paid_keyword cookie (for w_search_campaign)
		var novell_search_paid_keyword = '';
		if(readCookie('novell_search_paid_keyword') !== null) {
			novell_search_paid_keyword = readCookie('novell_search_paid_keyword');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_search_terms' value='"+novell_search_paid_keyword+"' />");
		
		
		// set the w_form_submit_url
		var w_form_submit_url = (window.location != window.parent.location) ? document.referrer: document.location;
		if (w_form_submit_url == 'undefined') {
			w_form_submit_url = window.location.href
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_form_submit_url' value='"+w_form_submit_url+"' />");
		
		// set the w_landing_page_url based on novell_search_landing cookie
		var w_landing_page_url = window.location.href;
		if(readCookie('novell_search_landing') !== null) {
			w_landing_page_url = readCookie('novell_search_landing');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_landing_page_url' value='"+w_landing_page_url+"' />");
		
		
		// set utm_campaign based on novell_utm_campaign cookie
		var novell_utm_campaign = '';
		if(readCookie('novell_utm_campaign') !== null) {
			novell_utm_campaign = readCookie('novell_utm_campaign');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='utm_campaign' value='"+novell_utm_campaign+"' />");
		
		// set utm_medium based on novell_utm_medium cookie
		var novell_utm_medium = '';
		if(readCookie('novell_utm_medium') !== null) {
			novell_utm_medium = readCookie('novell_utm_medium');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='utm_medium' value='"+novell_utm_medium+"' />");
		
		// w_icid based on icid cookie
		var w_icid = '';
		if (readCookie('icid') !== null) {
			w_icid = readCookie('icid');
		}
		if (document.getElementById('w_icid')) {
			jQuery('#form_builder_form_57d7f55450f38 input[id=w_icid]').remove();
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_icid' value='"+w_icid+"' />");

		// set utm_source based on novell_utm_medium cookie
		var novell_utm_source = '';
		if(readCookie('novell_utm_source') !== null) {
			novell_utm_source = readCookie('novell_utm_source');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='utm_source' value='"+novell_utm_source+"' />");
		
		// set w_download_url based on w_download_url cookie
		var w_download_url = '';
		if(readCookie('w_download_url') !== null) {
			w_download_url = readCookie('w_download_url');
		}
		if (w_download_url != '') {
			jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_download_url' value='"+w_download_url+"' />");
		}
		
		// set elqCustomerGUID based on eloqua_id cookie
		var eloqua_id = '';
		if(readCookie('eloqua_id') !== null) {
			eloqua_id = readCookie('eloqua_id');
		}
		jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='elqCustomerGUID' value='"+eloqua_id+"' />");
		
		
		// set the eventName, eventId, contactGroup based on if a query param was passed in (for netiq)
		var overwrite_eventName = getQueryVariable('eventName');
		var overwrite_eventId = getQueryVariable('eventId');
		var overwrite_contactGroup = getQueryVariable('contactGroup');
		if (overwrite_eventName !== false) {
			jQuery("#form_builder_form_57d7f55450f38 input[name='eventName']").val(overwrite_eventName);
		}
		jQuery("#form_builder_form_57d7f55450f38 input[name='eventID']").attr('name','eventId');
		if (overwrite_eventId !== false) {
			jQuery("#form_builder_form_57d7f55450f38 input[name='eventId']").val(overwrite_eventId);
		}
		
		/////////////////////////////////////////////////////////////////////////
		// set CampaignSourceCode based on get params
		var overwrite_campaign_source = getQueryVariable('CampaignSourceCode');
		if (overwrite_campaign_source) {
			if (jQuery("input[name='CampaignSourceCode']").length > 0) {
				jQuery("input[name='CampaignSourceCode']").val(overwrite_campaign_source);
			}
		}
		
		/////////////////////////////////////////////////////////////////////////
		// set campaign_description and campaign name based on get params
		var overwrite_campaign_description = getQueryVariable('campaign_description');
		var overwrite_campaign_id = getQueryVariable('campaign_id');
		if (overwrite_campaign_description !== false) {
			if (jQuery("#form_builder_form_57d7f55450f38 input[name='campaign_description']").length > 0) {
				jQuery("#form_builder_form_57d7f55450f38 input[name='campaign_description']").val(overwrite_campaign_description);
			} else {
				jQuery("#form_builder_form_57d7f55450f38").prepend("<input type='hidden' name='campaign_description' value='" + overwrite_campaign_description + "' />");
			}
		}
		if (overwrite_campaign_id !== false) {
			if (jQuery("#form_builder_form_57d7f55450f38 input[name='campaign_id']").length > 0) {
				jQuery("#form_builder_form_57d7f55450f38 input[name='campaign_id']").val(overwrite_campaign_id);
			} else {
				jQuery("#form_builder_form_57d7f55450f38").prepend("<input type='hidden' name='campaign_id' value='" + overwrite_campaign_id + "' />");
			}
		}
		// end get params
		
		
		if (typeof elq_product_name == 'string'     ) {				
			jQuery('#form_builder_form_57d7f55450f38 input:hidden:first').after("<input type='hidden' name='w_product' value='" + elq_product_name + "' />");
		}
		
		
		jQuery('#form_builder_submit_57d7f55450f38').click(function(e) {
			e.preventDefault();
			submit_formbuilder_form_57d7f55450f38();
		});
		
		jQuery('#form_builder_form_57d7f55450f38').submit(function(e) {
			e.preventDefault();
			submit_formbuilder_form_57d7f55450f38();
		});
	});
</script>









