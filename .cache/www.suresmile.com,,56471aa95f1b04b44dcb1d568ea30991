                        // Tooltop show hide
                        $(document).ready(onDocumentReady);
                        function onDocumentReady() {
                            $('div.study .chart').hover(onStudyOver, onStudyOut);
                             $('div.tooltip').click(onTooltopOut);
                        }
                        function onStudyOver() {
                        $(this).children('div.tooltip').show();
                        }

                        function onStudyOut() {
                            $(this).children('div.tooltip').hide();
                        }
                        function onTooltopOut() {
                            $('div.tooltip').hide();
                        }
                        //Hero title on center
                        var capHig = $('.leaderboard div.caption').outerHeight();
                        var caph2 = capHig / 2;
                        $('.leaderboard div.caption').css('margin-top',-caph2)
