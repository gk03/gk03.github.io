<script type="text/javascript">

var icpForm2380 = document.getElementById('icpsignup2380');

if (document.location.protocol === "https:")

	icpForm2380.action = "https://app.icontact.com/icp/signup.php";
function verifyRequired2380() {
  if (icpForm2380["fields_email"].value == "") {
    icpForm2380["fields_email"].focus();
    alert("The Email field is required.");
    return false;
  }
  if (icpForm2380["fields_fname"].value == "") {
    icpForm2380["fields_fname"].focus();
    alert("The First Name field is required.");
    return false;
  }
  if (icpForm2380["fields_lname"].value == "") {
    icpForm2380["fields_lname"].focus();
    alert("The Last Name field is required.");
    return false;
  }


return true;
}
</script>


