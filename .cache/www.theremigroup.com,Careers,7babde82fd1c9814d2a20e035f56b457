 <script type="text/javascript">
     jQuery(function (q) {
         q(".form_div_1215").validationEngine({
             promptPosition: 'topRight',
             ajaxFormValidationURL: '/DesktopModules/DNNGo_PowerForms/ajaxValidate.aspx?ModuleID=1215'
         });

         q(".form_div_1215 input[lang='Submit']").click(function () {

             if (!$('.form_div_1215').validationEngine('validate')) {
                 return false;
             }

             if($("#g-recaptcha-1215").is("div") )
             {
                 if ( !$("#g-recaptcha-1215").data("GRecaptcha"))
                 {
                     $("#g-recaptcha-msg-1215").show("fast",function(){
                         $(this).fadeOut(5000);
                     });
                     return false;
                 }
             }
             
             

         });

         q(".form_div_1215 input[lang='reset']").click(function () {
             $('.form_div_1215').validationEngine('hideAll');
         });

         q(".form_div_1215 .jquery-datepick").datepick({ dateFormat: 'mm/dd/yyyy' });
   






     });
