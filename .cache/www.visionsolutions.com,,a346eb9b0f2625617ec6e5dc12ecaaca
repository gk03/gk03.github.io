    $(document).ready(function() {
        var $carousel = $('.carousel');

        function carouselInit(carousel) {
            if (carousel.length) {
                carousel.carouFredSel({
                    auto: 6000,
                    responsive: true,
                    circular: true,
                    items: {
                        width: true
                    },
                    prev: {
                        button: '.prev'
                    },
                    next: {
                        button: '.next'
                    },
                    pagination: {
                        container: '.pager'
                    },
                    scroll: {
                        duration: 800
                    }
                });

                
            }
        }

        carouselInit($carousel);

        $(window).on('resize', function() {
            carouselInit($carousel);
        });


        $(window).load(function() {
            $(window).resize();
        }) ;


    });
