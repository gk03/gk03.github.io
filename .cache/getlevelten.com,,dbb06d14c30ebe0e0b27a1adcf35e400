<script>
function L10iMonitor() {
    this.vardata = {};
    this.logdata = [];
    this.logkeys = {};
    //this.vardata['init'] = +new Date();
    this.setVar = function (key, value) {
        _l10im.vardata[key] = value;
    };
    this.log = function (message, key) {
        var time = +new Date;
        //_l10im.logdata[time] = message;
        var entry = {};
        entry['time'] = time;
        entry['msg'] = message;
        _l10im.logdata.push(entry);
        if (key != undefined) {
            _l10im.logkeys[key] = _l10im.logdata.length - 1;
        }
    };
    this.markTime = function (key) {
        _l10im.log(key, key);
        //_l10im.timedata[key] = +new Date();
    };
    this.logExists = function (message) {
        var time = +new Date;
        //_l10im.logdata[time] = message;
        var entry = {};
        entry['time'] = time;
        entry['msg'] = message;
        _l10im.logdata.push(entry);
    };
    this.saveReport = function (id, uid, type) {
        id = (id !== undefined) ? id : '';
        uid = (uid !== undefined) ? uid : '';
        type = (type !== undefined) ? type : '';
        var url = '/stacktracejs/log_js?id=' + id + '&uid=' + uid + '&type=' + type;
        var entry = {};
        if ((typeof JSON != 'undefined') && (typeof JSON.stringify != 'undefined')) {
            url += '&logjson=' + encodeURIComponent(JSON.stringify(_l10im.logdata));
            url += '&varjson=' + encodeURIComponent(JSON.stringify(_l10im.vardata));
        }
        script = document.createElement('script');
        script.src = url;
        document.getElementsByTagName('head')[0].appendChild(script);
    };
    this._encodeUrlQueryParams = function (params, prefix) {
        var str = [], k;
        prefix = (prefix != undefined) ? prefix + '_' : '';
        for (k in params) {
            if (params.hasOwnProperty(k)) {
                str.push(prefix + encodeURIComponent(k) + "=" + encodeURIComponent(params[k]));
            }
        }
        return str.join("&");
    };
}
var _l10im = new L10iMonitor();
_l10im.markTime('init');

