            msg += ' _ioq does not exist.';
        }
        else if (typeof _ioq.getCookie !== 'function') {
            msg += ' _ioq.getCookie does not exist.';
        }
        else {
            //pstr += ', vtk=' + _ioq.getCookie('l10ivtk');
            var vtk = _ioq.getCookie('l10ivtk');
            if (vtk) {
                uid = vtk;
                // qbaka.user = vtk;
            }
        }
