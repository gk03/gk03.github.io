    function newWindow(url)
    {
        window.open(url);
    }

    function searchSpecEntered()
    {
        return ((document.frmSearch.sch_spec.value != '') &&
                (document.frmSearch.sch_spec.value != '(enter search)'));
    }

    function enterSearch()
    {
        if (!searchSpecEntered())
        {
            document.frmSearch.sch_spec.value='';
        }
    }

    function leaveSearch()
    {
        if (!searchSpecEntered())
        {
            document.frmSearch.sch_spec.value='(enter search)';
        }
    }
    function startSearch()
    {
        if (searchSpecEntered())
        {
            
            document.frmSearch.action='/cgi-bin/search.cgi';
            
            document.frmSearch.submit();
        }
        else
        {
            return false;
        }

    }
