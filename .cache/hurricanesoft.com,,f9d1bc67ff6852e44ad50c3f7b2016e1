        (function ()
        {
            function buildHref (showNotice)
            {
                var href = [];
                
                href.push(protocol);
                href.push("//");
                href.push(host);
                
                if (showNotice)
                    href.push("/changenotice.html");
                else
                    href.push(pathName);
                
                href.push(hash);
                
                return href.join("");
            }
            
             var location = window.location;
             var hash = location.hash;
             var hostName = location.hostname;
             var protocol = location.protocol;
             var pathName = location.pathname;
             var href = location.href;
             var host = location.host;
             
             if (host.indexOf("hurricanesoft") != -1)
                 window.location.href = buildHref(true);
        })();
        
        <!--Hide        
        
        function validateForm()
        {
            if (document.frmSurvey.surveyOther.value != "")
            {
                document.frmSurvey.survey[4].checked = true;
            }

            var isChecked = false;
            for (var i = 0; i < document.frmSurvey.survey.length; i++)
            {
                if (document.frmSurvey.survey[i].checked)
                {
                    isChecked = true;
                    break;
                }
            }

            if (!isChecked)
            {
                alert("Please make a selection and re-submit.");
                return false;
            }
            else if (document.frmSurvey.survey[4].checked && (document.frmSurvey.surveyOther.value == ""))
            {
                alert("Please enter a value for 'Other' and re-submit.");
                return false;
            }

            return true;
        }

        // end Hide-->
