<section class="leaders page-part" id="board">
<div class="row" style="text-align:center;padding-top:1em;max-width:700px;">
<h2 class="header">Advisory Board</h2>
		                                    	
                  	                  	<article class="leader" onClick="jQuery('div', this).slideToggle()">
