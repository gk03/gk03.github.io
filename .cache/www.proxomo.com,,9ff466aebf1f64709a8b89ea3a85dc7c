    <div id="signupformoutcome" style="display: none">
        <!-- Box Start-->



    </div>
    <script type="text/javascript">
        function showlogin() {
            closeforms();
            $("#loginform").show();
        };

        function showsignup() {
            closeforms();
            $("#signupform").show();
        };

        function showsignupoutcome() {
            closeforms();
            $("#signupformoutcome").show();
        };

        function closeforms() {
            $("#signupform").hide();
            $("#signupformoutcome").hide();
            $("#loginform").hide();
        };

        function performsignup(theEmail, thePwd) {
            closeforms();


            // alert("href: " + window.location.href + "\nhash: " + window.location.hash + "\nhost: " + window.location.host + "\nhostname: " + window.location.hostname + "\npath: " + window.location.path + "\nport: " + window.location.port + "\nprotocol: " + window.location.protocol);

            //var registrationPath = "wrong.txt";
            var registrationPath = "https://127.0.0.2:447/Register/RegisterNewUser/?email=" + theEmail + "&" + "pwd=" + thePwd;
            // var registrationPath = "http://127.0.0.1:81/Home/RegisterNewUser/?email=" + theEmail + "&" + "pwd=" + thePwd;

            // START of GET operation -- notice we define an ERROR handler to display errors
            var jqxhr = $.getJSON(registrationPath, function (data) {
                var mainMessage;
                var detailsMessage;
                //alert("Data Loaded: " + data.error);
                switch (data.error) {
                    case "1": // registration already completed EARLIER. Ask them to log in
                        mainMessage = "Howdy! We see that your registration was completed earlier. Email address: " + theEmail + ".";
                        detailsMessage = "You're already registered, please login.";
                        break;
                    case "2": // initial registration successful!
                        mainMessage = "Hurray! You have signed up successfully with email: " + theEmail + ".";
                        detailsMessage = "Please check your email and follow the link within to complete your signup.";
                        break;
                    case "3":
                        mainMessage = "Howdy! You have already registered with us but have not finalized the process. Email address: " + theEmail + ".";
                        detailsMessage = "We sent you another confirmation email. Please check your email and follow the link within.";
                        break;
                    default:
                        mainMessage = "Yikes! We are unable to complete your sign up request for this email. Email address: " + theEmail + ".";
                        detailsMessage = "We would love to figure out what happened. Just send us an email with the following code:" + data.error;
                } // end switch

                document.getElementById("signupoutcomemessage-main").innerHTML = "<b>" + mainMessage + "</b>";
                document.getElementById("signupoutcomemessage-details").innerHTML = detailsMessage;
                $("#signupformoutcome").show();
            })

            //            .success(function (data) {
            //                //alert("inside callback: SUCCESS");
            //                })

            .error(function (XHR, textStatus, exception) {
                var responseText = jQuery.parseJSON(XHR.responseText);
                alert("JQuery Get failed - ERROR INFO:\n" + " TextStatus: " + textStatus + ", Error: " + responseText + ", Status: " + XHR.status);
            })

            .complete(function () {
                //alert("inside callback: COMPLETE");
            }); // END of GET operation

        }; // END of function: performsignup

    </script>
    
    <script>
        $(function () {
            var zIndexNumber = 1000;
            $('div').each(function () {
                $(this).css('zIndex', zIndexNumber);
                zIndexNumber -= 10;
            });
        }); </script>
    

