<script type="text/javascript">
    $(function () {
        $('.LoginLink').each(function (index) {
            if ($(this).text() == "Login") {
                $(this).hide();
            }
            else {
                $(this).show();
            }
        });
    });
</script>
<script>
    $(document).foundation();
    function smoothScrollToId(id) {
        $('html, body').animate({
            'scrollTop': ($(id).offset().top - 118)
        }, 1500);
    }

    function smoothScrollToTop() {
        $('html, body').animate({
            'scrollTop': 0
        }, 1500);
    }

    $(".goToTop").click(function () {
        smoothScrollToTop();
    });

    $(function () {
        if ($('#ControlBar').length) {
            $('.fixed').css("margin-top", "54px");
            $('.scrollingContent').css("margin-top", "172px");
        }
    });

    $(function () {
        //Get the page part of the url
        var pathArray = window.location.pathname.split("/");
        //if FR or DE set correct text
        if (pathArray[1] == 'fr') {
            $(".dnnInputFileWrapper").each(function () {
                $(this).text('Sélectionner fichier');
            });
            $("input.eyc_button[type=file]").data('text', 'Sélectionner fichier');
        }
        else if (pathArray[1] == 'de') {
            $(".dnnInputFileWrapper").each(function () {
                $(this).text('Datei auswählen');
            });
            $("input.eyc_button[type=file]").data('text', 'Datei auswählen');
        }
    });

</script>
 
<script type="text/javascript">
    $(document).ready(function () {
        try {
            var theVideoElement = window.location.hash.split("#")[1];
            var theQuery = "[data-reveal-id='" + theVideoElement + "']";
            $(theQuery).trigger("click");
        }
        catch(err) {
            //In case it is not a video.
        }
    })
</script>
