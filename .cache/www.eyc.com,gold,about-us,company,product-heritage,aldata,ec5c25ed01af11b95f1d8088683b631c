    if (isAldataRedirect()) {
        //dnnModal.show($('#popupURL').val(), false, 700, 750, false);
        var $dialog = $('<div id="aldataPopup"></div>')
               .html('<iframe style="border: 0px; padding:10px;" src="' + $('#popupURL').val() + '" width="100%" height="100%"></iframe>')
               .dialog({
                   autoOpen: false,
                   modal: true,
                   height: 310,
                   width: 850
               });
        $dialog.dialog('open');
    }

    $('.ui-widget-overlay').click(function () { $("#aldataPopup").dialog("close"); });

    function isAldataRedirect() {
        var index = window.location.href.indexOf('#aldata');
        return index >= 0 ? true : false;
    }
</script>
