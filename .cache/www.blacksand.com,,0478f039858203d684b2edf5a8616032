<!-- style1- single position, rotate & static -->
<div id="topinc_imageBanner_pnlStyle1">
	
	<!-- rotating image -->
	<div id="topinc_imageBanner_style1_rotate">
		
		<!-- rotate script -->
		<script type="text/javascript">
			var ImageLoad = [['/images/banners/4.jpg',''],['/images/banners/5.jpg',''],['/images/banners/6.jpg',''],['/images/banners/7.jpg','']];
			var ImageCount		= ImageLoad.length;
			var ImageDelay		= 4000;
			var FirstLoad		= 0;
			var QuickStartID	= 0;
			var htmlString		= "";
			function ImageChange()
			{
				var index = Math.floor(Math.random() * ImageCount);
				htmlString = '<img border="0" src="';
				htmlString += ImageLoad[index][0];
				htmlString += '" width="273" height="181" />';

				document.getElementById('randomImage').innerHTML = htmlString;

				if(FirstLoad == 0)
				{
					SlowFinish();
				}
			}

			function  QuickStart()
			{
				QuickStartID=setInterval("ImageChange()", 1000);
			}
			function SlowFinish()
			{
				clearInterval(QuickStartID);
				FirstLoad = 1;
				setInterval("ImageChange()", ImageDelay);
			}
			QuickStart()
		 </script>
		<!-- link -->
		
		<!-- no link -->
		<div id="topinc_imageBanner_style1_rotate_nolink">
			<div id="randomImage"></div>
		</div>
	
	</div>
	
	<!-- static image -->
	

</div>

