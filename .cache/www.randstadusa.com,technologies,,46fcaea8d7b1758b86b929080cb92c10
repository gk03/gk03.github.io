<!-- User Identity -->
 <script type="text/javascript">
     $(document).ready(function () {
         var identity = 'False';
         if (identity=="False" && navigator.geolocation) {
             navigator.geolocation.getCurrentPosition(function (position) {
                 var retZip = "";
                 var lat = position.coords.latitude;
                 var lng = position.coords.longitude;
                 var ip = "";
                 
                 var pos = new google.maps.LatLng(position.coords.latitude, position.coords.longitude);
                 new google.maps.Geocoder().geocode({ 'latLng': pos }, function (res, status) {
                     var zip = res[0].formatted_address.match(/,\s\w{2}\s(\d{5})/);
                     retZip = zip[1];

                     if (retZip != null) {

                         $.ajax({
                             type: "POST",
                             url: "/Custom/Services/JobSearchHelper.asmx/SetUserIdentity ",
                             cache: false,
                             contentType: "application/json; charset=utf-8",
                             data: "{'ip':'" + ip + "', 'lat':'" + lat + "', 'lng':'" + lng + "', 'zip':'" + retZip + "','session':''}",
                             dataType: "json",
                             success: function(msg) {
                             },
                             error: function(xhr, msg) {
                                 //alert(msg + '\n' + xhr.responseText);
                             }
                         });
                         //Job search page
                         if ($find("JobSearchLocation") != null) {
                             $find("JobSearchLocation").set_Text(retZip);
                         }
                         //Job search mobile
                         if ($find("JobSearchLocationMobile") != null) {
                             $find("JobSearchLocationMobile").set_Text(retZip);
                         }
                         //Office Search page
                         if ($find("SearchLocation") != null) {
                             $find("SearchLocation").set_Text(retZip);
                         }
                         //Top Office Search bar
                         if ($find("TopJobSearchLocation") != null) {
                             $find("TopJobSearchLocation").set_Text(retZip);
                         }
                         
                     }
                 });
                 
                 
                 
             });
         };
     });
</script>
      
