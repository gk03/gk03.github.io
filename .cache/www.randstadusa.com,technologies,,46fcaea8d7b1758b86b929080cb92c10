            var thumbnailSlides = [];
            var generalRandomStart = sliderHelper.getSliderSetting("GeneralRandomStart") || false;
            var generalPause = parseInt(sliderHelper.getSliderSetting("GeneralPause")) || 5000;
            generalPause = generalPause > 500 ? generalPause : 500;
            var controlsControls = sliderHelper.getSliderSetting("ControlsControls") || false;
            var PagerBuildPagerThumbnail = sliderHelper.getSliderSetting("PagerBuildPagerThumbnail") || false;

            $(".bxslider li:first .sld-img:first").one('load', function () {
                $('.bxslider li:first').css("background-url", this.src);
            }).each(function () {
                if (this.complete) $(this).load();
            });

            $(document).ready(function () {
                $('.bxslider li').show();
                SliderDefaults = {
                    mode: 'horizontal',
                    infiniteLoop: false,
                    autoControls: false,
                    pager: true,
                    startSlide: 0,
                    auto: false,
                    autoHover: true,
                    responsive: true,
                    speed: 600,
                    video: false,
                    useCSS: false,
                    controls: controlsControls,
                    pause: generalPause,
                    randomStart: generalRandomStart,
                    onSlideAfter: function (slide, oldIndex, newIndex) {
                        //sliderHelper.handleCurrentSliderItem(slider, oldIndex, newIndex);
                        //sliderHelper.lockPause = false;
                    },
                    onSliderLoad: function (index) {
                        //slider.stopAuto(false);
                        //if (slider.getSlideCount() > 1) {
                        //    sliderHelper.handleCurrentSliderItem(slider, index, index);
                        //}
                    }
                };

                if (PagerBuildPagerThumbnail) {
                    SliderDefaults.buildPager = function (slideIndex) {
                        if (thumbnailSlides[slideIndex]) {
                            return thumbnailSlides[slideIndex];
                        }

                        var title = sliderHelper.getSliderItemSetting(slideIndex, "title");
                        var imageUrl = sliderHelper.getSliderItemSetting(slideIndex, "imageUrl");

                        var htmlString = ""
                        if (imageUrl != undefined && imageUrl != "") {
                            htmlString = '<img src="' + imageUrl + '" />';
                        }
                        if (title != undefined && title != "") {
                            htmlString = htmlString + '<span class="bx-pager-link-label">' + title + '</span>';
                        }
                        thumbnailSlides[slideIndex] = htmlString;
                        return htmlString;
                    }
                }
                var slider = $('#' + sliderElementId).bxSlider(SliderDefaults);
                var sliderContainer = slider.parent().parent().parent();

                if (SliderDefaults.autoHover) {
                    sliderContainer.hover(
                        function () {
                            slider.stopAuto();
                            sliderHelper.pauseSlide = true;
                        }, function () {
                            if (!sliderHelper.lockPause) {
                                slider.startAuto();
                                sliderHelper.pauseSlide = false;
                            }
                        });

                    sliderContainer.mousemove(function () {
                        slider.stopAuto();
                        sliderHelper.pauseSlide = true;
                    });
                };

                $(document).on("onVideoHover",
                    function () {
                        slider.stopAuto();
                        sliderHelper.pauseSlide = true;
                    });
      
            });
                
           
        </script>
    </div>
