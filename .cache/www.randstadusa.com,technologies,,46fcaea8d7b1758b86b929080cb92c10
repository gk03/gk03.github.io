<script type="text/javascript">
    //<![CDATA[
    function GetStockResults() {
        $.ajax(
            {
                type: "GET",
                url: '/StockQuote.ashx',
                async: true,
                cache: false,
                contentType: "application/xml; charset=utf-8",
            })
            .done(function (data) {
                var xml = $(data);
                $(".investor-price").text(xml.find("Currency").text() + ' ' + xml.find("CurrentPrice").text());
                $(".investor-date-date").append(xml.find("Date").text() + " " + xml.find("Time").text() + " CET");
                $(".investor-symbol").append(xml.find("Symbol").text());

                var change = xml.find("Change").text();
                if (change.charAt(0) == "-") {
                    $(".investor-price").addClass("investor-price-down");
                }
                else {
                    $(".investor-price").addClass("investor-price-up");
                }
            });
    };
    $(document).ready(function () {
        GetStockResults();
    });
    //]]>>
</script>

