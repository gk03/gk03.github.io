<script>
	jQuery(document).ready(function(){
		if($.cookie("css")) {
			$("#changeme").attr("href",$.cookie("css"));
		}
		$("#toggle_theme a").click(function() { 
			$("#changeme").attr("href",$(this).attr('rel'));
			$.cookie("css",$(this).attr('rel'), {expires: 365, path: '/'});
			return false;
		});	
	});
</script>

