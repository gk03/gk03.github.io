<script language="javascript" type="text/javascript">
    
    function setText(controlID, msg)
    {
        var temp = document.all[controlID];
        if(temp.value == "")
        {
            temp.value = msg;
        }        
    }
    
    function clearText( controlID, msg)
    {
        var temp = document.all[controlID];
        if(temp.value == msg)
        {
            temp.value = "";
        }           
    }
    

    function emailPassword()
    {
        var strEmail = document.all['txtemail'].value;
	
	if (strEmail.length == 0 || strEmail.indexOf("@") < 0)
		alert("Please enter your email address.");
	else
		self.location = "http://connect.onsiteaustin.com/v4_6_release/services/system_io/Portal/EmailPassword.aspx?e=" + strEmail;
    }
</script>



