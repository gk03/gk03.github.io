(function( $ ){

	$(window).load(function(){
		windowHeight = $(document).height() + 20;
		$('#footer_button_text').click(function(){
			
			if( !$(this).hasClass('open') ){
				$('#footer_content').slideToggle('slow', 'swing');
				$('#footer_button_text').addClass('open');
				$('body, html').animate({scrollTop: windowHeight});
			}else{
				$('#footer_content').slideToggle('slow', 'swing');
				$('#footer_button_text').removeClass('open');
			}		
			
		});
	});
		
})(jQuery);	
</script>


