<script language="javascript">
    clickMenu2 = function(quicklinksmenu) {
    if (document.getElementById(quicklinksmenu)) {
	var getEls2 = document.getElementById(quicklinksmenu).getElementsByTagName("LI");
	var getAgn2 = getEls2;

	for (var i=0; i<getEls2.length; i++) {
			getEls2[i].onclick=function() {
				for (var x=0; x<getAgn2.length; x++) {
				getAgn2[x].className=getAgn2[x].className.replace("unclick", "");
				getAgn2[x].className=getAgn2[x].className.replace("click2", "unclick");
				}
			if ((this.className.indexOf('unclick'))!=-1) {
				this.className=this.className.replace("unclick", "");;
				}
				else {
				this.className+=" click2";
				}
			}
			getEls2[i].onmouseover=function() {
				this.className+=" hover";
			}
			getEls2[i].onmouseout=function() {
				this.className=this.className.replace("hover", "");
			}
		}
	}
	}
window.onload = function(){clickMenu2('quicklinksmenu')}; 
</script> 

