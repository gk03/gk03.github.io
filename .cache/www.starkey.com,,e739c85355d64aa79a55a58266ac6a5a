                d.src = a;
                d.type = 'text/java' + c;
                d.async = true;
                a = b.getElementsByTagName(c)[0];
                a.parentNode.insertBefore(d, a);
            }
        })();
    </script>
    <script>
        if (document.documentElement.lang == "it-IT") {
        	document.write('<script src="/Sites/Starkey/scripts/js/international/italy-cookie.js"><\/script>');
        }
    </script>
