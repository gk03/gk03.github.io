<script type="text/javascript">
function resetNavBanner(){
	var ua = window.navigator.userAgent;
	var msie = ua.indexOf("MSIE ");
	if(msie!=-1){
		$('#nav').css('margin-top','0px');
	}
};
$(document).ready(function() {
	resetNavBanner();
});
$(document).ready(function() {
// hides the slickbox as soon as the DOM is ready
// (a little sooner than page load)
	$('#sitesearch').show();
	$('#contractsearch').hide();
	$('#jobsearch').hide();
// shows the slickbox on clicking the noted link  
	$('a#searchsite').change(function() {
	$('#sitesearch').show();
	$('#contractsearch').hide();
	$('#jobsearch').hide();
	return false;
	});
// hides the slickbox on clicking the noted link  
	$('a#searchcontracts').click(function() {
	$('#sitesearch').hide();
	$('#contractsearch').show();
	$('#jobsearch').hide();
	return false;
	});
// toggles the slickbox on clicking the noted link  
	$('a#searchjobs').click(function() {
	$('#sitesearch').hide();
	$('#contractsearch').hide();
	$('#jobsearch').show();
	return false;
	});
});

</script>
