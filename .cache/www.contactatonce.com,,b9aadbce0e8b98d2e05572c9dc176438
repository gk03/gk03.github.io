<script type="text/javascript">
    $(document).ready(function() {
        var count = 2;
        $("#blog-show-more").click(function(){
            if($("#automotive").hasClass('active')){
                    var total='12';
                }
                if($("#property-management").hasClass('active')){
                    var total='7';
                }
                if($("#real-estate").hasClass('active')){
                     var total='3';
                }
                if($("#corporate").hasClass('active')){
                     var total='3';
                }
                  if(count > total){
                      return false;
                  }else{
                   loadArticle(count);
                  }
                   count++;
        });
        
        /*  $(window).scroll(function(){
                if($("#automotive").hasClass('active')){
                    var total='12';
                }
                if($("#property-management").hasClass('active')){
                    var total='7';
                }
                if($("#real-estate").hasClass('active')){
                     var total='3';
                }
                if($("#corporate").hasClass('active')){
                     var total='3';
                }
                if($(window).scrollTop() == $(document).height() - $(window).height()){
                  if(count > total){
                      return false;
                  }else{
                   loadArticle(count);
                  }
                   count++;
                }
        }); */

        function loadArticle(pageNumber){
                $('a#inifiniteLoader').show('fast');
                if($("#automotive").hasClass('active')){
                    var vertical="automotive";
                }
                if($("#property-management").hasClass('active')){
                    var vertical="property-management";
                }
                if($("#real-estate").hasClass('active')){
                    var vertical="real-estate";
                }
                if($("#corporate").hasClass('active')){
                    var vertical="corporate";
                }
                $.ajax({
                    url: "http://www.contactatonce.com/wp-admin/admin-ajax.php",
                    type:'POST',
                    data: "action=infinite_scroll&page_no="+ pageNumber + '&loop_file=loop_ajax&verticalvar='+vertical, 
                    success: function(html){
                        $('a#inifiniteLoader').hide('1000');
                        /*This will be the div where our content will be loaded*/
                        if(vertical=="automotive"){
                            $("#automotive").append(html); 
                        }
                        if(vertical=="property-management"){
                            $("#property-management").append(html); 
                        }
                        if(vertical=="real-estate"){
                            $("#real-estate").append(html); 
                        }
                        if(vertical=="corporate"){
                            $("#corporate").append(html); 
                        }
                    }
                });
            return false;
        }

    });
</script>

