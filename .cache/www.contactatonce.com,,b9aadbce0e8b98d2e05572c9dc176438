        <!-- Live demo popup after 10 seconds -->
        <script type="text/javascript">
        function openFancybox() {
                setTimeout( function() {$('#various1').trigger('click'); },60000);
            }
        $(document).ready(function() {
            if(window.location.hash){
            $('html,body').animate({
                    scrollTop: $(window.location.hash).offset().top -100
            });
        }   
        
            openFancybox();
            $('#various1').fancybox({padding:0, closeBtn: false});
            });
        </script>
        <!-- /Live demo popup after 10 seconds -->

                
                
        
