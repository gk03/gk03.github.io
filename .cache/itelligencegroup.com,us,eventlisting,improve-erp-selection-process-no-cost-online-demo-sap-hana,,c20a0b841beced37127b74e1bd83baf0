				dataString += '&has-title-de=' + contactFrmObj.find("input[name*='has-title-de']").val();
				dataString += '&has-function-de=' + contactFrmObj.find("input[name*='has-function-de']").val();

				dataString += '&has-firstname=' + contactFrmObj.find("input[name*='has-firstname']").val();
				dataString += '&has-lastname=' + contactFrmObj.find("input[name*='has-lastname']").val();
				dataString += '&has-company=' + contactFrmObj.find("input[name*='has-company']").val();
				dataString += '&has-email=' + contactFrmObj.find("input[name*='has-email']").val();
				dataString += '&has-phonenumber=' + contactFrmObj.find("input[name*='has-phonenumber']").val();
				dataString += '&has-participation=' + contactFrmObj.find("input[name*='has-participation']").val();
				dataString += '&has-street-n-number=' + contactFrmObj.find("input[name*='has-street-n-number']").val();
				dataString += '&has-zip=' + contactFrmObj.find("input[name*='has-zip']").val();
				dataString += '&has-city=' + contactFrmObj.find("input[name*='has-city']").val();
				dataString += '&has-enquirytype=' + contactFrmObj.find("input[name*='has-enquirytype']").val();
				dataString += '&has-yourmessage=' + contactFrmObj.find("input[name*='has-yourmessage']").val();
				dataString += '&has-job-title=' + contactFrmObj.find("input[name*='has-job-title']").val();
				dataString += '&has-street=' + contactFrmObj.find("input[name*='has-street']").val();
				dataString += '&has-position=' + contactFrmObj.find("input[name*='has-position']").val();
				dataString += '&has-city-zip-code=' + contactFrmObj.find("input[name*='has-city-zip-code']").val();
				dataString += '&has-country=' + contactFrmObj.find("input[name*='has-country']").val();
				dataString += '&has-fax=' + contactFrmObj.find("input[name*='has-fax']").val();
				dataString += '&has-what-interest=' + contactFrmObj.find("input[name*='has-what-interest']").val();
				dataString += '&has-attend-drinks=' + contactFrmObj.find("input[name*='has-attend-drinks']").val();
				dataString += '&has-attend-dinner=' + contactFrmObj.find("input[name*='has-attend-dinner']").val();
				dataString += '&has-accommodation=' + contactFrmObj.find("input[name*='has-accommodation']").val();

				dataString += '&road=' + contactFrmObj.find("input[name*='road']").val();
				dataString += '&zip=' + contactFrmObj.find("input[name*='zip']").val();
				dataString += '&city=' + contactFrmObj.find("input[name*='city']").val();
				dataString += '&confirmation=' + contactFrmObj.find("input[name*='confirmation']").val();
				dataString += '&invoice=' + contactFrmObj.find("input[name*='invoice']").val();
				dataString += '&customernumber=' + contactFrmObj.find("input[name*='customernumber']").val();
				dataString += '&course_code=' + contactFrmObj.find("input[name*='course_code']").val();
				dataString += '&course_name=' + contactFrmObj.find("input[name*='course_name']").val();
				dataString += '&course_date=' + contactFrmObj.find("input[name*='course_date']").val();
				dataString += '&course_price=' + contactFrmObj.find("input[name*='course_price']").val();
				dataString += '&course_loc=' + contactFrmObj.find("input[name*='course_loc']").val();
				dataString += '&salutation-1=' + contactFrmObj.find("select[name*='salutation-1']").val();
				dataString += '&participant-firstname-1=' + contactFrmObj.find("input[name*='participant-firstname-1']").val();
				dataString += '&participant-lastname-1=' + contactFrmObj.find("input[name*='participant-lastname-1']").val();
				dataString += '&participant-title-1=' + contactFrmObj.find("input[name*='participant-title-1']").val();
				dataString += '&participant-phone-1=' + contactFrmObj.find("input[name*='participant-phone-1']").val();
				dataString += '&salutation-2=' + contactFrmObj.find("select[name*='salutation-2']").val();
				dataString += '&participant-firstname-2=' + contactFrmObj.find("input[name*='participant-firstname-2']").val();
				dataString += '&participant-lastname-2=' + contactFrmObj.find("input[name*='participant-lastname-2']").val();
				dataString += '&participant-title-2=' + contactFrmObj.find("input[name*='participant-title-2']").val();
				dataString += '&participant-phone-2=' + contactFrmObj.find("input[name*='participant-phone-2']").val();
				dataString += '&salutation-3=' + contactFrmObj.find("select[name*='salutation-3']").val();
				dataString += '&participant-firstname-3=' + contactFrmObj.find("input[name*='participant-firstname-3']").val();
				dataString += '&participant-lastname-3=' + contactFrmObj.find("input[name*='participant-lastname-3']").val();
				dataString += '&participant-title-3=' + contactFrmObj.find("input[name*='participant-title-3']").val();
				dataString += '&participant-phone-3=' + contactFrmObj.find("input[name*='participant-phone-3']").val();

				dataString += '&rand=' + contactFrmObj.find("input[name*='random-identifier']").val();
				dataString += '&isajax=1';

				dataString += '&captcha=' + contactFrmObj.find("input[name*='captcha']").val();

				// console.log(dataString);

				return this;
			};

			var show_search_icon = function () {
				jQuery('#general-frm-wrap input:submit').css({
					backgroundImage: 'url(/wp-content/themes/itelligence/img/components/form-submit-arrow.jpg)',
					backgroundPosition: 'right 6px'
				}).prop('disabled', false);
				return this;
			};

			var show_loading_icon = function () {
				var icon_width = jQuery('#general-frm-wrap input:submit').width() - 23;
				jQuery('#general-frm-wrap input:submit').css({
					backgroundImage: 'url(/wp-content/themes/itelligence/img/components/loading-icon.gif)',
					backgroundPosition: icon_width + 'px 7px'
				}).prop('disabled', true);
				return this;
			};

			var submit_form = function () {
								helper.send_json_request('/wp-content/themes/itelligence/includes/', contact_form.handle_validation_or_success, dataString, 'POST');
				contact_form.show_loading_icon();
				return this;
			};

			var handle_validation_or_success = function (data) {
				console.log(data);
				var validationObject = JSON.parse(data);
				jQuery('span.frm-error').hide();
				if (validationObject.passed_ajax_validation == 1) {
					jQuery("#general-frm-wrap form").submit();
				}
				else if (validationObject.success !== 1) {
					var contactFrmObj = jQuery('#general-frm-wrap form');
					contact_form.show_search_icon();
					// loop through the validation objects
					for (var key in validationObject) {
						var error = validationObject[key];
						jQuery('#' + error.field + '-err-message').fadeIn(500);
					}


					//captcha
					contactFrmObj.find("input[name*='captcha']").val('');
					var captchaSrc = contactFrmObj.find("#captcha").attr('src');
					contactFrmObj.find("#captcha").attr('src', captchaSrc + Math.random());

				} else {
					contact_form.show_loading_icon();
					// success!!
										window.location.href = "http://itelligencegroup.com/us/";
				}

				return this;
			};

			var return_is_not_ajax_form = function () {
				var contactFrmObj = jQuery('#general-frm-wrap form');
				return contactFrmObj.find("input[name*='is-not-ajax-form']").val();
			};

			// API Interface for contact form
			return {
				colate_form_data: colate_form_data,
				submit_form: submit_form,
				handle_validation_or_success: handle_validation_or_success,
				show_search_icon: show_search_icon,
				show_loading_icon: show_loading_icon,
				return_is_not_ajax_form: return_is_not_ajax_form
			};

		})();

		jQuery('#general-frm-wrap input:submit').click(function (e) {

			e.preventDefault();
			jQuery(this).prop('disabled', true);
			contact_form.colate_form_data().submit_form();

		});

		// Newsletter form - formerly known as newsletter-form.php

		var newsletter_form = (function () {

			var dataString = '';

			var colate_form_data = function () {
				dataString = '';
				var contactFrmObj = jQuery('#newsletter-frm-wrap form');
				dataString += 'email=' + contactFrmObj.find("input[name*='email']").val();
				dataString += '&email-landing-page=http://itelligencegroup.com/us/newsletter-email-thank-you';

				// hidden fields
				dataString += '&site-region=' + contactFrmObj.find("input[name*='site-region']").val();
				dataString += '&from-name=' + contactFrmObj.find("input[name*='from-name']").val();
				dataString += '&from-email=' + contactFrmObj.find("input[name*='from-email']").val();

				return this;
			};

			var show_search_icon = function () {
				jQuery('#newsletter-frm-wrap input:submit').css({
					backgroundImage: 'url(/wp-content/themes/itelligence/img/components/cta-arrow-large.jpg)'
				}).prop('disabled', false);
				return this;
			};

			var show_loading_icon = function () {
				var icon_width = jQuery('#newsletter-frm-wrap input:submit').width() - 23;
				jQuery('#newsletter-frm-wrap input:submit').css({
					backgroundImage: 'url(/wp-content/themes/itelligence/img/components/large-loading-icon.gif)'
				}).prop('disabled', true);
				return this;
			};

			var submit_form = function () {
				helper.send_json_request('/wp-content/themes/itelligence/includes/submit_newsletter_form.php', newsletter_form.handle_validation_or_success, dataString, 'POST');
				newsletter_form.show_loading_icon();
				return this;
			};

			var handle_validation_or_success = function (data) {
				var validationObject = JSON.parse(data);
				jQuery('span.frm-error').hide();
				if (validationObject.success !== 1) {
					newsletter_form.show_search_icon();
					// loop through the validation objects
					for (var key in validationObject) {
						var error = validationObject[key];
						jQuery('#newsletter-' + error.field + '-err-message').fadeIn(500);
					}
				} else {
					newsletter_form.show_loading_icon();
					// success!!
					var randomString = validationObject.rand;
					window.location.href = "http://itelligencegroup.com/us/newsletter-more-information?rand=" + randomString;
				}

				return this;
			};

			// API Interface for contact form
			return {
				colate_form_data: colate_form_data,
				submit_form: submit_form,
				handle_validation_or_success: handle_validation_or_success,
				show_search_icon: show_search_icon,
				show_loading_icon: show_loading_icon
			};

		})();

		var hash = encodeURIComponent(window.location.hash.replace('#', '')).replace('%2F', '/');
		hash && jQuery('ul.nav a[href="#' + hash + '"]').tab('show');

		jQuery('.nav-tabs a').click(function (e) {
			jQuery(this).tab('show');
			var scrollmem = jQuery('body').scrollTop();
			window.location.hash = this.hash;
			jQuery('html,body').scrollTop(scrollmem);
		});

		jQuery('.newsletter-signup #newsletter-frm-wrap input:submit').click(function (e) {
			e.preventDefault();
			newsletter_form.colate_form_data().submit_form();
		});
	});
</script>
</body>
</html>
