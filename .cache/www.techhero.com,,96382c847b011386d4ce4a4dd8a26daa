<script type="text/javascript" src="//bypronto.com/wp-content/uploads/sites/980/scripts/viewportchecker.js"></script>
<script>

    jQuery(document).ready(function(){	 
        
        $('#run-customer').viewportChecker({
            offset: 100,
            callbackFunction: function(){runNumber()}
        });
        
    });
    
    function runNumber(){
        $('#run-customer').html(1);
        $('#run-server').html(1);
        $('#run-device').html(1);

        $('#run-customer').html(447);
        $('#run-server').html(1502);
        $('#run-device').html(11562);
    }

</script>
