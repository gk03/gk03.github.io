	window.delayedJavascriptArray.push(function(){
		
	$(document).ready(function() {
        var activeChannel = document.getElementsByName('channelMarker')[0];
        // don't break if channelMarker is not set
        if (!(typeof activeChannel === "undefined")) {
            var activeChannelguid=document.getElementsByName('channelMarker')[0].value;
            var topChannelArray =  $('ul.topnav').first().children('li');
            for (var i = 0, len = topChannelArray.length; i < len; i++ ) {
                var li_tag = topChannelArray[i];
                var currentClass = li_tag.getAttribute('class');
                if(currentClass.indexOf(activeChannelguid) > -1){
                    if(currentClass.indexOf('currentLocation') > -1){
                    } else{
                        var updatedClass = currentClass + ' currentLocation' ;
                        li_tag.className = updatedClass;
                    }
                } else {
                    if(currentClass.indexOf('currentLocation') > -1){
                        currentClass.replace('currentLocation', '');
                        li_tag.className = currentClass;
                    }

                }
            }
        }
	})

	});
