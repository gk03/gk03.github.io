<script>
    function tagLinkEvent(eventname, varname, varvalue)
        {
            if(eventname != "")
            {
                dataLayer.push({'event': eventname});
            }
            if(varname != "")
            {
                dataLayer = [{varname: varvalue}];

            }
        }
</script>
