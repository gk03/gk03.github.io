<script>
	if (window.delayedJavascriptArray == undefined || window.delayedJavascriptArray == null) {
		window.delayedJavascriptArray = new Array();
	}
	window.delayedJavascriptArray.push(function(){
		
	$(document).ready(function() {
		var domainU = document.domain;
		//alert("domainU = " + domainU);
		changeDomainName(domainU);
	})
	
	function changeDomainName(domainURL) {
		var domainDotIndex = domainURL.indexOf('.');
		var domainSubStr = domainURL.substring(domainDotIndex + 1);
		var elems = document.querySelectorAll('.locale');
		var domainName = "" ;
		for(var i = 0; i < elems.length; i++) {
		    var elem = elems[i];
		    var compURL = elem.firstChild.getAttribute('href');
		    var compURLDotIndex = compURL.indexOf('.');
		    var compURLSubStr = compURL.substring(compURLDotIndex + 1);
		    if(domainSubStr.toLowerCase() === compURLSubStr.toLowerCase()){
			    //alert("domainSubStr = " +domainSubStr);
			    //alert("compURLSubStr = " +compURLSubStr);
		   	 	domainName = elem.firstChild.childNodes[0].nodeValue
			}else{
				
			}
		}
		
		//alert($("#displaydomain")
	    //.clone()    //clone the element
	    // .children() //select all the children
	    //.remove()   //remove all the children
	    // .end()  //again go back to selected element
	    //.text());

		var domain_anchor = document.getElementById("displaydomain");
		var domain_to_change = domain_anchor.childNodes[1];
		
		if(domainName !== null && domainName !== "") {
			domain_to_change.nodeValue = domainName;
		}
		
		
	}

	});
</script>
