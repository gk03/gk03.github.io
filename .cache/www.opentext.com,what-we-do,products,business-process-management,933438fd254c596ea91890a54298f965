function cc_isEuDomain() {
    return typeof cc.initobj.settings.onlyshowwithineu === 'undefined'
}

function cc_ready() {
    var cc_cookie = getCookie('cc_necessary');
    var cc_ineu = getCookie('cc_ineu');
    return (cc_isEuDomain() || cc_cookie != null || cc_ineu != null || cc.allasked || cc.checkedremote);
}

analytics_consent = false;

