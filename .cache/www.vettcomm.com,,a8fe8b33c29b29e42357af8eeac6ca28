		<script type="text/javascript">

			console.log("Flipping Boxes - Version 1.0 - MuseThemes.com");
			
			$(function() {
					
					$(".s3").detach().appendTo('#u17662 .panel');
					$(".s4").detach().appendTo('#u17662 .panel');
					
					var width = $('.s3').width();
					var height = $('.s3').outerHeight();
					
					//alert("w:"+width+" "+"h:"+height);
					//$('#u17662').css({"width":width,"height":height});
					
					$('#u17662 .panel').css({"width":width,"height":height});
					 
			});
		</script>
		
		<script>
			$(document).ready(function(){
		
				// set up hover panels
				// although this can be done without JavaScript, we've attached these events
				// because it causes the hover to be triggered when the element is tapped on a touch device
				$('.panel').hover(function(){
					$(this).addClass('flip');
				},function(){
					$(this).removeClass('flip');
				});
		
				
			});
		</script>
	

	
	
