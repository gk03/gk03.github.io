<script type="text/javascript">
    function showDetails(type) {
        var variable = '';
        if (type == 'D')
            variable = document.getElementById('searchInput').value;
        else
            variable = document.getElementById('global_search_search').value;
        var url = '/search.aspx?q=' + variable;
        document.location.href = url;
    }  
	
    </script>
    <script type="text/javascript">
        if (document.documentElement.clientWidth < 600) {
            $(document).ready(function () {
                $('.mySlideToggler').click(function () {
                    $('.mySlideContent').slideUp();
                    $('.mySlideToggler').removeClass("active");
                    if (!$(this).next().is(":visible")) {
                        $(this).addClass("active").next('.mySlideContent').slideDown();

                    }
                });
            });
        }
    </script>
