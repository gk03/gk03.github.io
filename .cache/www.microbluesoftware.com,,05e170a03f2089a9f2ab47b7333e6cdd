XSP.attachEvent("view:_id1:_id2:_id4:_id11:_id17:_id45", "view:_id1:_id2:_id4:_id11:_id17:lnkLogin", "onclick", view__id1__id2__id4__id11__id17__id45_clientSide_onclick, false, 2);
