function view__id1__id2__id4__id11__id17__id45_clientSide_onclick(thisEvent) {
dijit.byId('dialogLogin').show();
if ('false' == 'true') {
	dojo.byId('view:_id1:_id2:_id4:_id11:_id17:username').setAttribute("value", returnCookieVal('username'));
}else{
	dojo.cookie('username', null, { expires: -1, domain: 'homebrew.microbluesoftware.com' });
}

}

function view__id1__id2__id4__id11__id17__id46_clientSide_onClientLoad(thisEvent) {
// from version 1.5.1
// the use of a url parameter to invoke the login dialog is not required, you may leave the parameter empty

if ('Anonymous' == 'Anonymous')
{
	var result = "";  
	
	if (result == "true") {
		dijit.byId('dialogLogin').show();
		dojo.byId('view:_id1:_id2:_id4:_id11:_id17:username').setAttribute("value", returnCookieVal('username'));
	}
}

}

XSP.addOnLoad(function() {
