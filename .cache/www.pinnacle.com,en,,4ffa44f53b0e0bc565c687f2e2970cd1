        oddSelector.dropdownButton({
            onItemSelected: function() {
                $scope.oddSelected($(this).data('value'));
                window.pinnacle.setUserPrefsCookie("priceStyle", $(this).data('value'));
                fnPlaceItemFirst($scope, $(this), true);
            }
