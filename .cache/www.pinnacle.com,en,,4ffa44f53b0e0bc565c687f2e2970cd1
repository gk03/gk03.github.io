<script type="text/javascript">
    $(document).ready(function() {
        var redirectToRtnPage = false;

        if ($.cookie('HomePageVisitedTime') != null) {
            var stayOnHomeTime = 43200000;
            var currentTime = new Date().valueOf();
            var creationTime = new Date($.cookie('HomePageVisitedTime')).valueOf();

            redirectToRtnPage = (currentTime - creationTime) > stayOnHomeTime;
        }

        if (redirectToRtnPage) {
            $('a[href="/en"]').each(function() {
                $(this).attr('href', "/en/rtn");
            });
            $('a[href="/en/"]').each(function() {
                $(this).attr('href', "/en/rtn");
            });
        }
    });
</script>



    
