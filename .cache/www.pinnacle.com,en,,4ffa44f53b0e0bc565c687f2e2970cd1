<script type="text/javascript">
    $(function () {
        var loginButton = $("#loginButton");
        if (location.protocol === 'https:') {
            loginButton.on('click',showLoginPopup);
			setTimeout(loadLoginContent);          
        } else {
            loginButton.attr("href", "https://www.pinnacle.com/en/login");
        }
    });
    function loadLoginContent() {
        var loginPopup = $("#loginPopup");
        $.get("/en/login-popup", function(data) {
            try {
                loginPopup.html(data);
            }
            finally {
                
				window.pinnacle.AssignForgotHandlers();
            }
        });
    }
    function showLoginPopup(e) {
        e.preventDefault();
        var loginPopup = $("#loginPopup");
        if (!!!loginPopup.data('loaded')) {
            loginPopup.data('loaded', true);
            loadLoginContent(loginPopup);
            loginPopup.dialog({
                dialogClass: "login-close",
                draggable: false,
                modal: true,
                resizable: false,
                width: 750,
                height: 500,
                open: function() {
                    $('.ui-widget-overlay').addClass('login-overlay');
                },
                close: function() {
                    $('.ui-widget-overlay').removeClass('login-overlay');
                }
            });
        } else {
            loginPopup.dialog('open');
        }
        loginPopup.click(function (e) {
        	var wrapper = loginPopup.find(".login__wrapper");
        	var holder = loginPopup.find(".login__wrapper__holder");
            if (e.target == loginPopup[0] || e.target == wrapper[0] || e.target == holder[0]) {
                loginPopup.dialog('close');
            }
        });
        $(".login-overlay").click(function () { loginPopup.dialog('close'); });  
    }
</script>

<ul class="quicklinks">
  <li><a class="button blue-bg login" id="loginButton" href="#">Login</a></li>  
  <li><a class="button green-bg signup-link" href="/signup/desktop/en-GB?ico=menuopen" target="_blank">Open Account</a></li>    
</ul>

