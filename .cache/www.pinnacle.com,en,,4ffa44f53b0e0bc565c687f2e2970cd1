<script type="text/javascript">
    var homePageExpiration = 315360000000;
    var stayOnHomeTime = 43200000;
    var redirect = true;
    var currentTime = new Date().valueOf();
    if ($.cookie('HomePageVisitedTime') != null) {
        var creationTime = new Date($.cookie('HomePageVisitedTime')).valueOf();
        redirect =  (currentTime - creationTime) > stayOnHomeTime;
    }
    else {
        $.cookie('HomePageVisitedTime', new Date().toUTCString(),  { path: "/", domain: ".pinnacle.com", expires : new Date(currentTime + homePageExpiration), raw: true });
        redirect = false;
    }
    if (redirect) {
        window.location.replace("/en/rtn");
    }
</script>
 

    

