    <script type="text/javascript">
        $(document).ready(function () {
            if (!Modernizr.input.placeholder) {
                $("input").each(function () {
                    if ($(this).val() == "" && $(this).attr("placeholder") != "") {
                        $(this).val($(this).attr("placeholder"));
                        $(this).focus(function () {
                            if ($(this).val() == $(this).attr("placeholder")) $(this).val("");
                        });
                        $(this).blur(function () {
                            if ($(this).val() == "") $(this).val($(this).attr("placeholder"));
                        });
                    }
                });
            }

            $("#old-browser-modal").center();
            $("nav.second").sticky({ topSpacing: 74 });
        });

        //Modernizr modern feature tests
        $(function () {
            if (document.cookie != null) {
                var cookie = document.cookie.split(";");
                var doNotShowAgain = false;
                for (var i = 0; i < cookie.length; i++) {
                    if (cookie[i].indexOf("doNotShowAgain") >= 0) {
                        doNotShowAgain = true;
                    }
                }
                if (!doNotShowAgain) {
                    if (Modernizr.fontface &&
                !Modernizr.borderradius &&
                !Modernizr.boxshadow &&
                !Modernizr.opacity &&
                !Modernizr.cssanimations &&
                !Modernizr.csstransforms &&
                !Modernizr.svg &&
                !Modernizr.video &&
                !Modernizr.inputtypes.tel &&
                !Modernizr.inputtypes.date) {

                        $("#old-browser-modal").center();
                        $("#old-browser-modal").reveal();
                    }
                }
            }

            $('.old-browser-close').click(function () {
                $('#old-browser-modal').trigger('reveal:close');
                document.cookie = "doNotShowAgain=true";
            });

            //document.domain = 'sqlsentry.com'
            var cartCount = $.cookie("CartItemCount");
            if (cartCount > 0) {
                $(".cartlabel").html(cartCount);
                $(".cartlink").attr("title", cartCount + " items");
                $(".cartlabel").css("left", 30 - ($(".cartimage").width() / 2));
                $(".cartimage").prop("src", "/images/icons/cart-red.png");
            }


            //hide zen brand
            $('.branding').css('display', 'none');
            var helpButtonLoading = setInterval(function () { HelpButtonLoading() }, 500);
            var ticketSubmissionFormLoading = setInterval(function () { TicketSubmissionFormLoading() }, 500);
            function HelpButtonLoading() {
                if ($("#launcher").length) {
                    $("#launcher").css("right", "50px");
                    clearInterval(helpButtonLoading);

                    //Track help button click on Kissmetrics
                    var helpButton = $('#launcher').contents().find(".Button--launcher");
                    //alert(helpButton.html());
                    helpButton.click(function () {
                        _kmq.push(['trackClick', 'btm_rt_help', 'click_help']);
                        console.log('clicked help');
                    })
                }
            }
            function TicketSubmissionFormLoading() {
                if ($("#ticketSubmissionForm").length) {
                    $("#ticketSubmissionForm").css("right", "35px");
                    clearInterval(ticketSubmissionFormLoading);
                }
            }
        });

        function reveal_lock() {
            //Lock parent scroll when revealed
            $("body").addClass("modal-open");
            return true;
        };

        function reveal_unlock() {
            //Unlock parent scroll when revealed
            $("body").removeClass("modal-open");
            return true;
        };

        function moveTo(contentArea) {
            if (contentArea.indexOf("#") == 0) {
                var goPosition = $(contentArea).offset().top;
                $('html,body').animate({ scrollTop: goPosition }, 'slow');
            }
            else {
                window.location.href = contentArea;
            }
        }

        function moveToDownload() {
            ga('send', 'pageview', '/vp/downloads-tab/click-dl-full-trial/');
            if ($(".EvaluationRegistrationForm").length > 0) {
                var goPosition = $(".EvaluationRegistrationForm").offset().top - 50;
                $('html,body').animate({ scrollTop: goPosition }, 'slow');

                if ($(".DLpanel").is(":visible")) {
                    $("#trigger").trigger("click");
                }

                return false;
            }
            else {
                document.location.href = "/download-trial/trial";
            }
        }

        $(".DownloadPlanExplorerLink").click(function () {
            ga('send', 'pageview', '/vp/downloads-tab/click-dl-plan-explorer/');
            document.location.href = '/plan-explorer#download';

            if ($(".DLpanel").is(":visible")) {
                $("#trigger").trigger("click");
            }

            return false;
        });

        if (typeof (Zenbox) !== "undefined") {
            Zenbox.init({
                dropboxID: "20158840",
                url: "https://sqlsentry.zendesk.com",
                tabTooltip: "Support",
                tabImageURL: "https://assets.zendesk.com/external/zenbox/images/tab_support.png",
                tabColor: "black",
                tabPosition: "Left"
            });
        }
    </script>
    

    <style type="text/css" media="screen, projection">
