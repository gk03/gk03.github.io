<script type="text/javascript">
    function callDocumentByPaginate(info) {
        var str = info.split('~');
        var checkUrl = '/control/categoryAjaxFired;jsessionid=7EBA122BA419FAD26450348B6218A5B7.jvm1';
        if(checkUrl.search("http"))
            var ajaxUrl = '/control/categoryAjaxFired;jsessionid=7EBA122BA419FAD26450348B6218A5B7.jvm1';
        else
            var ajaxUrl = 'https://www.plyscale.com/control/categoryAjaxFiredSecure;jsessionid=7EBA122BA419FAD26450348B6218A5B7.jvm1';
            
        //jQuerry Ajax Request
        jQuery.ajax({
            url: ajaxUrl,
            type: 'POST',
            data: {"category_id" : str[0], "VIEW_SIZE" : str[1], "VIEW_INDEX" : str[2]},
            error: function(msg) {
                alert("An error occured loading content! : " + msg);
            },
            success: function(msg) {
                jQuery('#div3').html(msg);
            }
        });
     }
</script>


