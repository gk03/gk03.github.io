    <script type="text/javascript" src="js/json2.js"></script>
    <script type="text/javascript" src="/js/funciones-carousel.js"></script>
    <script type="text/javascript" src="/js/jcarousel.min.js"></script>

    <script>
        $().ready(function () {
            $("#RefineSearch_category").change(function () {
                // armo el objeto que servira de parametro, para ello utilizo una libreria de JSON
                //este parametro mapeara con el definido en el web service
                var params = new Object();
                params.category_id = $("#RefineSearch_category").val();
                params = JSON.stringify(params);
                $.ajax({
                    type: "POST",
                    url: "/Functions.asmx/getSpecialitiesByCategory",
                    data: params,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: true,
                    success: LoadEspecialidades,
                    error: ShowError
                });
            });
            $("#formSearch").submit(function (event) {
                if ($("#RefineSearch_category").val() > 0) {
                    this.action = "/offers/" + $("#RefineSearch_category option:selected").text().replace(/\s+/g, '-').toLowerCase();
                }
                return true;
            });
        });
    </script>

    <script>
        $(document).ready(function () {
            $('#customizedservices_form').submit(function (event) {
                event.preventDefault();
                var params = new Object();
                params.email = $('#customizedservices_email').val();
                params = JSON.stringify(params);
                $.ajax({
                    type: "POST",
                    url: '/functions.asmx/setCustomizedService',
                    data: params,
                    success: function (data) {
                        $("#customizedservices_message").empty();
                        if (data == 'true') {
                            $('#fieldset_customizedservices').remove();
                            $("#customizedservices_message").append('<h3 class="font-22 junge white">THANK YOU</h3>');
                        }
                        else {
                            $("#customizedservices_message").append('<h3 class="font-22 junge white">This email address is already registered.</h3>');
                        }
                    }
                });
            });
        });
    </script>

