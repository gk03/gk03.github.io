<script type="text/javascript">
var utag_data = {
    "alm_site": "propc",
    "alm_site_group": "insurance",
    "site_name": "PropertyCasualty360",
    "content_type": "home",
    "server": "propertycasualty360",
    "page_name": "home",
    "delivered_topic": "\/home"
};
var site_env = "PROD";
if( typeof Summit !== 'undefined' ){ 
  if(site_env === 'DEV' || site_env === 'QA'){
	  utag_data['oas_site'] = Summit.OAS.manager.site.replace('www.','test.www.');
  }else{
	  utag_data['oas_site'] = Summit.OAS.manager.site;
  }
	utag_data['oas_page'] = Summit.OAS.manager.pagetag;
	utag_data['oas_listpos'] = Summit.OAS.manager.listpos;
}
(function(a,b,c,d){ 
	a='//tags.tiqcdn.com/utag/alm/main/prod/utag.js'; 
	b=document;c='script';d=b.createElement(c);d.src=a;d.type='text/java'+c;d.async=true; 
	a=b.getElementsByTagName(c)[0];a.parentNode.insertBefore(d,a); 
})(); 

$(document).ready(function(){
	var article_share = $("#article-share");
	if( article_share ){
		$("#article-share").bind('click', function(event){
			if( event.target.src ){
				var src = event.target.src;
				utag_share_event( src );
			}
		});
		$("#header_gigya_share_bar").bind('click', function(event){
			if( event.target.src ){
				var src = event.target.src;
				utag_share_event( src );
			}
		});
	}
	$(".article-share li").each(function(){
		$(this).find("a[class^=ir]").on('click', function(event){
			if( $(this).attr('class')  ){
				var src = $(this).attr('class') ;
				utag_share_event( src );
			}
		});
	});
	function shareEventHandler(evt) { 
	    if (evt.type == 'addthis.menu.share') { 
	        utag_share_event( evt.data.service );
	    }
	}
	if( typeof addthis !== 'undefined' ) {
		addthis.addEventListener('addthis.menu.share', shareEventHandler);
	}
	function utag_share_event( src ){
		if( src.match(/email/g) ){
			utag.link({ 'server' : 'propertycasualty360', 'event_name': 'social share','share_type' : 'share-mail'});
		}else if( src.match(/facebook/g) ){
			utag.link({ 'server' : 'propertycasualty360', 'event_name': 'social share','share_type' : 'share-facebook'});
		}else if( src.match(/twitter/g) ){
			utag.link({ 'server' : 'propertycasualty360', 'event_name': 'social share','share_type' : 'share-twitter'});
		}else if( src.match(/google/g)){
			utag.link({ 'server' : 'propertycasualty360', 'event_name': 'social share','share_type' : 'share-googleplus'});
		}else if( src.match(/linkedin/g) ){
			utag.link({ 'server' : 'propertycasualty360', 'event_name': 'social share','share_type' : 'share-linkedin'});
		}
	}
	});
</script>
<!-- END Tealium -->
  
</body>
</html>
