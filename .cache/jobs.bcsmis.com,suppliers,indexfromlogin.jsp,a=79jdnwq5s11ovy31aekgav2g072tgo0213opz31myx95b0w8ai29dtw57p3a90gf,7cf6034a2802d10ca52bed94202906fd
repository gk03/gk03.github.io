function checkonly1(){
	if(document.form3.checkbox1.checked){
		document.form3.checkbox2.checked=false;
		document.Form1.checked1.value="1";
		document.Form1.checked2.value="0";
	}
	else document.Form1.checked1.value="0";

}
function checkonly2(){
	if(document.form3.checkbox2.checked){
		document.form3.checkbox1.checked=false;
		document.Form1.checked1.value="0";
		document.Form1.checked2.value="1";
	}
	else document.Form1.checked2.value="0";
}
function quickSearch(obj)  { 
   if (trim(obj).length != 0) { //If no errors have occured then submit the form
	document.form2.action = "/jobseekers/myjobs/searchjobsdone_outside.jsp";
	document.form2.submit();
   } else {
	alert("Please enter a search keyword.");
   }

}


<!--
// name - name of the desired cookie
// * return string containing value of specified cookie or null if cookie does not exist

/**********USE THESE ONLY IF FOLLOW IS INVOKED***************/
function getCookie(name) {
  var dc = document.cookie;
  var prefix = name + "=";
  var begin = dc.indexOf("; " + prefix);
  if (begin == -1) {
    begin = dc.indexOf(prefix);
    if (begin != 0) return null;
  } else
    begin += 2;
  var end = document.cookie.indexOf(";", begin);
  if (end == -1)
    end = dc.length;
  return unescape(dc.substring(begin + prefix.length, end));
}

function encryptIt(input) {
  // the following letters and numbers and other characters are going
  // to be encrypted.
  letters = "abcdefghijklmnopqrstuvwxyz";
  encrypt = new Array("%61","%62","c","%64","e","%66","%67","%68","%69",
			"%6a","%6b","%6c","m","n","o","%70","%71","%72","%73","t",
			"%75","%76","w","%78","%79","%7a");
  var output = "";
  var foundit;
  for(var count = 0; count < input.length; count++) {
    daChar = input.charAt(count);
    foundit=0;
    for (i = 0; i < letters.length; i++) {
      if (daChar == letters.charAt(i)) {
        foundit=1;
        output += encrypt[i];
        break;
      }
    }
    if(foundit==0) {
		output += daChar;
    }
  }
  return output;
}
