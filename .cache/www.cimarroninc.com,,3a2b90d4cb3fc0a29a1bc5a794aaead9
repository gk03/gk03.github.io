<script type="text/javascript">
$(window).scroll(function(){

	var wScroll = $(this).scrollTop();

	if(wScroll > ($('#mobile_core_comp').offset().top - $(window).height()/1.9) && !$('#core_comps_tiles').hasClass('shown')){
    $('#core_comps_tiles').addClass('shown')
		var i = 0;

		$('.core_comp_tile').each(function(i){
			setTimeout(function(){
  			$('.core_comp_tile').eq(i).addClass('is-showing');
      }, 150*(i+1));
		});

	}

});
</script>
