<!-- added by Swati on 11Aug2016 to remove query string from contactus.aspx page -->
<script type="text/javascript">

$(document).ready(function(){
	$("a").each(function (){
	var hrefval = $(this).attr("href");
		if (hrefval === undefined || hrefval === null) 
	{
	    //alert('undefined value');
	}
	else
	{
		
		var riskindx = hrefval.indexOf("/risk/");	
		
		
		if((riskindx==0) && (hrefval.indexOf("contactus.aspx?")!= -1))
		{
		
          var newhref=hrefval.split('?')[0];
			$(this).attr("href",newhref);
		
		}
		
	}
	});
});
</script>
