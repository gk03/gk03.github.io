     <script>
         document.onload = LoadAceScript();
         function LoadAceScript() {
             var FindIE = /*@cc_on!@*/false || !!document.documentMode;
             if (FindIE == false) {
                 document.write("<script src='/ace131/assets/js/ace.min.js'>" + "<" + "/script>");
           }           
       }
   </script>
    <script>
        $(function () {
            var $tbls = $('table.table');
            $tbls.each(function (i, item) {
                var table = item;
                if (table != null) {
                    var head = document.createElement("THEAD");
                    head.style.display = "table-header-group";
                    head.appendChild(table.rows[0]);
                    table.insertBefore(head, table.childNodes[0]);
                }
            });
        });
    </script>

    <!-- This is to clear datepicker textbox. instead of postback(dont delete) -->
    <script>
        function ClearText(sender) {
            var ff = sender;
            if (ff.indexOf("imgBtnClear") >= 0) {
                var res = ff.replace("imgBtnClear", "txtDate");
                document.getElementById(res).value = "";
            }
            return false;
        }
    </script>
    
     <!-- Enter event Script to fire relative button(Begin) -->
    <script>        
        $(document).ready(function () {
            var button = $(document).find('input[type=submit]');
            var ValidButtons = $(document).find("[onclick~='return']");
            var TotLength = ValidButtons.length;
            var len = 0;
            for (var clr = 0; clr < TotLength; clr++) {
                var btnid = '#' + ValidButtons[clr].id;
                var getDiv = $(btnid).attr("onclick")
                if ((typeof getDiv != "undefined")) {
                    if (getDiv.indexOf("ValidateTab") >= 0 || getDiv.indexOf("ValidateOnClick") >= 0) {
                        var str = getDiv.substring(getDiv.indexOf("(") + 1, getDiv.indexOf(")"))
                        var TotStr = "javascript:return WebForm_FireDefaultButton(event,'" + ValidButtons[clr].id + "')";
                        $('#' + str).attr("onkeypress", TotStr);
                    }
                }
               
            }
        });

        var prm = Sys.WebForms.PageRequestManager.getInstance();

        prm.add_endRequest(function () {
            var button = $(document).find('input[type=submit]');
            var ValidButtons = $(document).find("[onclick~='return']");
            var TotLength = ValidButtons.length;
            var len = 0;
            for (var clr = 0; clr < TotLength; clr++) {
                var btnid = '#' + ValidButtons[clr].id;
                var getDiv = $(btnid).attr("onclick")
                if ((typeof getDiv != "undefined")) {
                    if (getDiv.indexOf("ValidateTab") >= 0 || getDiv.indexOf("ValidateOnClick") >= 0) {
                        var str = getDiv.substring(getDiv.indexOf("(") + 1, getDiv.indexOf(")"))
                        var TotStr = "javascript:return WebForm_FireDefaultButton(event,'" + ValidButtons[clr].id + "')";
                        $('#' + str).attr("onkeypress", TotStr);
                    }
                }
            }
        });
    </script>     
    <script>
        function WebForm_FireDefaultButton(event, target) {
            if (event.keyCode == 13) {
                // this variable has to be added
                var __nonMSDOMBrowser =
                    (window.navigator.appName.toLowerCase().indexOf('explorer') == -1);

                var src = event.srcElement || event.target;
                if (!src || (src.tagName.toLowerCase() != "textarea")) {
                    var defaultButton;
                    if (__nonMSDOMBrowser) {
                        defaultButton = document.getElementById(target);
                    }
                    else {
                        defaultButton = document.all[target];
                    }
                    if (defaultButton && typeof (defaultButton.click) != "undefined") {
                        defaultButton.click();
                        event.cancelBubble = true;
                        if (event.stopPropagation) event.stopPropagation();
                        return false;
                    }
                }
            }
            return true;
        }
    </script>   
    <!-- Enter event Script to fire relative button(End) -->
    


