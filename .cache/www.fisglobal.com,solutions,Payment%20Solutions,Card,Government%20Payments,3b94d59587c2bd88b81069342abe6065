    <script type="text/javascript">
        function DoSearch(e) {
            var txt = $("#srch-term").val();
            if (!txt) {
                if (e.preventDefault) {
                    e.preventDefault();
                }
                else if (e.returnValue) {
                    e.returnValue = false;
                }
                else {
                    return false;
                }
            }
            return true;
        }
