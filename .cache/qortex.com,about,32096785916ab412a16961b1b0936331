<script src="/assets/javascripts/plugins/jquery.flexslider-8e7b4d3e085b99a109318cf9daaf9323.js"></script>
<script>

  $(function(){

    $('.testimonials-flexslider').flexslider({
      slideshow: false,
      start: function(slider){

        var listHeights = [];

        _.each($(slider).find('.slides li'), function(item){

          listHeights.push( $(item).height() );

        });

        $(slider).find('.slides .column-r,.slides .column-r .inner').height(  _.max(listHeights) - 30 );
      }
    });
  })
</script>



		
			
