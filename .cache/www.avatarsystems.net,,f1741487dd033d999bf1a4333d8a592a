    var today = new Date();
    var rand = today.getSeconds();
    if (rand % 2 == 0) {
        var integra = document.getElementById("integraWrapper").innerHTML;
        var petroware = document.getElementById("petrowareWrapper").innerHTML;
        document.getElementById("integraWrapper").innerHTML = petroware;
        document.getElementById("petrowareWrapper").innerHTML = integra;

        document.getElementById("integraBox").className = "box-1 box-petroware";
        document.getElementById("petrowareBox").className = "box-1 box-integra";
    }
    </script> 
