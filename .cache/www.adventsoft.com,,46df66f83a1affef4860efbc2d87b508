  fw_menu_1.addMenuItem("Web Site Development","location='/solutions/webprocess.htm'");
  fw_menu_1.addMenuItem("eInfrastructure Network Solutions","location='/solutions/einfraintro.htm'");
  fw_menu_1.addMenuItem("Enterprise Security Management","location='/solutions/networksecurity.htm'");
  fw_menu_1.addMenuItem("System Integration","location='/solutions/integration.htm'");
  fw_menu_1.addMenuItem("Professional Staffing","location='/solutions/staffing.htm'");
  fw_menu_1.hideOnMouseOut=true;
 

  fw_menu_1.writeMenus();
} // fwLoadMenus()

