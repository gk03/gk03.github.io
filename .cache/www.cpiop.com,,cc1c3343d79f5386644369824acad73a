    <script src="/bundles/entireSitePluginsCombined?v=eTHOWRonCZeLbwegEfexnSxXmWkDs5EeiNT69i1wlGo1"></script>

    <script src="/bundles/tabSlideOut?v=gadfQDzFZWE0ufKWiCAwMP8-z0eEfvHUJBhcN3AEXV01"></script>

    <script src="/bundles/bxslider?v=vAMTiQxb7d8wNwmLb8MueXqdwC8BFdpnc4NkK7ulMW81"></script>

    <script src="/bundles/jscrollpane?v=nHjHR9H1MulU4KeSynsUPMct6i2Opwo_JNu_uYJroPU1"></script>

    <script src="/bundles/tableFunctions?v=4VwSMSLrFxQ2Zt_RoqsvyPF6yaBUG2Rsu0R4SPlcj1s1"></script>


    <script>
        // "official" ready function. All the ready functions below this should be moved to here
        $(document).ready(function () {
            $('#whole-header').scrollToFixed({
                zIndex: 1001
            });

            $('#headBorder').scrollToFixed({
                marginTop: 101
            });

            $('#scrollableleft').scrollToFixed({
                marginTop: function () {
                    var marginTop = $(window).height() - $('#foot').outerHeight(true) - $('#scrollableleft').outerHeight(true) + 6;
                    if (marginTop >= ($('#whole-header').outerHeight() + 12)) return ($('#whole-header').outerHeight() + 12);
                    return marginTop;
                },
                zIndex: 999
            });

            // Display-Hide Header border
            function showDiv() {
                var scrollllls = $(window).scrollTop();
                if (scrollllls > 100) {
                    $('#headBorder').fadeIn('linear');
                } else {
                    $('#headBorder').fadeOut('linear');
                };
            }
            $(window).scroll(showDiv);

            $('#header_bottom_tab_list > li a.header-bottom-tooltip').each(function () {
                // tooltip for the header
                var tip1 = $(this);
                tip1.tooltipster({
                    content: tip1.find('.header-cat-name').html(),
                    trigger: 'hover',
                    position: 'bottom',
                    positionTracker: true,
                    theme: 'tooltipster-cpi',
                    delay: 100,
                    contentAsHTML: true
                });
            });

            $('.cpi-tooltip-base').each(function () {
                // tooltip for the whole site
                var tip2 = $(this);
                tip2.tooltipster({
                    content: tip2.attr('tooltip'),
                    trigger: 'hover',
                    position: 'bottom',
                    positionTracker: true,
                    theme: 'tooltipster-default',
                    maxWidth: 215,
                    delay: 100
                });
            });



            // Left Nav - BLUE_HEADER becomes unclickable
            $("#left-nav a.BLUE_HEADER").removeAttr("href");

            // impliment jQuery SelectBox
            var sb = new SelectBox({
                selectbox: $("#switchDifferentPurchasingGroup"),
                width: 311,
                height: 155,
                scrollOptions: {
                    mouseWheelSpeed: 30
                },
                changeCallback: function (val) {
                    if (val !== "") {
                        $.ajax({
                            type: 'POST',
                            url: '/SetGroup',
                            data: {
                                groupId: val
                            },
                            success: function () {
                                location.reload();
                            },
                            error: function (xhr, status, err) {
                                $.jGrowl(err + " Please try again or contact CPI for further service.", { position: "center", header: "Error" });
                            }
                        });
                    }
                }
            });
            $("#contab-selectbox").each(function () {
                var $contabSelect = $(this);

                var sb = new SelectBox({
                    selectbox: $contabSelect,
                    width: 275,
                    height: 100,
                    scrollOptions: {
                        mouseWheelSpeed: 30
                    },
                    changeCallback: function (val) {
                        if (val !== "") {
                            $contabSelect.val(val);
                        }
                    }
                });
            });

            // use class="j-scroll-pane" to impliment jScrollPane for custom scrollbar
            $('.j-scroll-pane').jScrollPane(
            {
                mouseWheelSpeed: 30
            });
            $('.j-scroll-pane-arrows').jScrollPane(
            {
                showArrows: true,
                horizontalGutter: 10,
                mouseWheelSpeed: 30
            });

            // animates the slide open and closed
            $('#chat-tab-whole-piece').tabSlideOut({
                tabHandle: '#chat-tab-trigger',
                pathToTabImage: null,
                imageHeight: null,
                imageWidth: null,
                handlePos: 0,
                tabLocation: 'right',
                speed: 300,
                action: 'click',
                topPos: '112px',
                fixedPosition: true
            });
            $('#contact-tab-whole-piece').tabSlideOut({
                tabHandle: '#contact-us-tab-trigger',
                pathToTabImage: null,
                imageHeight: null,
                imageWidth: null,
                handlePos: 140,
                tabLocation: 'right',
                speed: 300,
                action: 'click',
                topPos: '112px',
                fixedPosition: true
            });



            $("#chattab-bottom-trigger").off("click").on("click", function () {
                var $contab = $('#contact-us-tab-trigger');
                $contab.click();
                var $chattab = $("#chat-tab-trigger");
                $chattab.click();
                return false;
            });

            $(document).ready(function () {
                var left = 1000;
                $('#contab-chars-remaining-count').text(left);
                $('textarea#contab-message').keyup(function () {
                    left = 1000 - $(this).val().length;
                    if (left < 0) {
                        $('#contab-chars-remaining-count').addClass("overlimit");
                    }
                    if (left >= 0) {
                        $('#contab-chars-remaining-count').removeClass("overlimit");
                    }
                    $('#contab-chars-remaining-count').text(left);
                });
            });


            

        });
    </script>
    
    
    <script src="/bundles/waypoints?v=wvkJnf24K0IO-Igv2ny_MR4XkXnU8nmKc0hVr4Klc1c1"></script>

    <script type="text/javascript" src="/Scripts/homegrown-plugins/TopBottomButtons.min.js"></script>
    <script type="text/javascript">
        $(function () {
            // Banner slider setup
            var sTransitionSpeed = 500;
            var sTimer = 5000;
            var sWidth = '100%';

            var bannerslider = $('.bxslider').bxSlider({
                auto: true,
                pause: sTimer,
                mode: 'fade',
                speed: sTransitionSpeed,
                responsive: false,
            });

            var numberOfImages = $('.bxslider li').length;

            if (numberOfImages > 1) {
                bannerslider.reloadSlider();
            } else {
                bannerslider.destroySlider();
            }


            if ('' == "False") {
                $.jGrowl("Login Failed!", { position: "center", header: "Error" });
            }

            // Swap Header phone/icons
            function showHeaderIcons() {
                var scrollllls = $(window).scrollTop();
                if (scrollllls < 240) {
                    $('#left_header_bottom_icons').slideUp();
                    $('#left_header_bottom_phone').slideDown();
                    $('#main_logo').removeClass('shift');
                } else {
                    $('#left_header_bottom_icons').slideDown();
                    $('#left_header_bottom_phone').slideUp();
                    $('#main_logo').addClass('shift');
                };
            }


            $('#left_header_bottom_phone').show();
            $('#left_header_bottom_icons').hide();
            $('#main_logo').removeClass('shift');

            $(window).scroll(showHeaderIcons);




            var $elements = $('.category_tier1_container');
            var $headerCategories = $("a.header-bottom-tooltip");

            $elements.each(function () {
                new Waypoint({
                    element: this,
                    handler: function (direction) {
                        var previousWaypoint = this.previous()
                        var id = 0;
                        var pieces;
                        var catId;
                        var header;
                        if (direction == 'down') {
                            previousWaypoint = this.previous()

                            $headerCategories.removeClass('active');

                            id = this.element.id;
                            pieces = id.split("-");
                            catId = pieces[1];
                            header = "headerclickto-" + catId;
                            $("a#" + header).addClass('active');


                            //if (previousWaypoint) {
                            //    $(previousWaypoint.element).css('background-color', 'green')
                            //}


                        } else {
                            previousWaypoint = this
                            var currentWaypoint = this.previous(); // trigger this section once viewable when scrolling up

                            if (currentWaypoint) {
                                $headerCategories.removeClass('active');
                                //$(previousWaypoint.element).css('background-color', 'green')


                                id = currentWaypoint.element.id;
                                pieces = id.split("-");
                                catId = pieces[1];
                                header = "headerclickto-" + catId;
                                $("a#" + header).addClass('active');

                                //$(currentWaypoint.element).css('background-color', 'purple')
                            };
                        };
                    },
                    offset: '192px'
                });
            });

            var duration = 500;
            var offsetf = ($(document).height() - $(window).height() - 320);

            TopBottomButtons.top(duration);
            TopBottomButtons.bottom(offsetf, duration);

            
            $(".category-ad-space a").off('click.GApromocatclick').on('click.GApromocatclick', function (event) {
                var href = $(this).attr('href');
                var target = $(this).attr('target');
                if (target != '_blank') {
                    event.preventDefault();
                }

                var id = $(this).parent().data('ga-id');
                var name = $(this).parent().data('ga-name');
                var position = $(this).parent().data('ga-position');

                ga('ec:addPromo', {               // Promo details provided in a promoFieldObject.
                    'id': id,             // Promotion ID. Required (string).
                    'name': name,          // Promotion name (string).
                    'position': position      // Position  (string).
                });

                // Send the promo_click action with an event.
                ga('ec:setAction', 'promo_click');
                ga('send', 'event', 'Internal Promotions', 'click', name, {
                    hitCallback: createFunctionWithTimeout(function () {
                        if (target != '_blank') {
                            window.location = href;
                        }
                    })
                });
            });
            $("#banner-container .bx-viewport a").off('click.GApromobannerclick').on('click.GApromobannerclick', function (event) {
                var href = $(this).attr('href');
                var target = $(this).attr('target');
                if (target != '_blank') {
                    event.preventDefault();
                }

            var id = $(this).parent().data('ga-id');
            var name = $(this).parent().data('ga-name');
            var position = $(this).parent().data('ga-position');

            ga('ec:addPromo', {               // Promo details provided in a promoFieldObject.
                'id': id,             // Promotion ID. Required (string).
                'name': name,          // Promotion name (string).
                'position': position      // Position  (string).
            });

            // Send the promo_click action with an event.
            ga('ec:setAction', 'promo_click');
            ga('send', 'event', 'Internal Promotions', 'click', name, {
                hitCallback: createFunctionWithTimeout(function () {
                    if (target != '_blank') {
                        window.location = href;
                    }
                })
            });
      
            });

            $("#banner-container .right a").off('click.GApromobannerclick').on('click.GApromobannerclick', function (event) {
                var href = $(this).attr('href');
                var target = $(this).attr('target');
                if (target != '_blank') {
                    event.preventDefault();
                }

                var id = $(this).parent().data('ga-id');
                var name = $(this).parent().data('ga-name');
                var position = $(this).parent().data('ga-position');

                ga('ec:addPromo', {               // Promo details provided in a promoFieldObject.
                    'id': id,             // Promotion ID. Required (string).
                    'name': name,          // Promotion name (string).
                    'position': position      // Position  (string).
                });

                // Send the promo_click action with an event.
                ga('ec:setAction', 'promo_click');
                ga('send', 'event', 'Internal Promotions', 'click', name, {
                    hitCallback: createFunctionWithTimeout(function () {
                        if (target != '_blank') {
                            window.location = href;
                        }
                    })
                });

            });
           
            $("#banner-container .farright a").off('click.GApromobannerclick').on('click.GApromobannerclick', function (event) {
                var href = $(this).attr('href');
                var target = $(this).attr('target');
                if (target != '_blank') {
                    event.preventDefault();
                }

                var id = $(this).parent().data('ga-id');
                var name = $(this).parent().data('ga-name');
                var position = $(this).parent().data('ga-position');

                ga('ec:addPromo', {               // Promo details provided in a promoFieldObject.
                    'id': id,             // Promotion ID. Required (string).
                    'name': name,          // Promotion name (string).
                    'position': position      // Position  (string).
                });

                // Send the promo_click action with an event.
                ga('ec:setAction', 'promo_click');
                ga('send', 'event', 'Internal Promotions', 'click', name, {
                    hitCallback: createFunctionWithTimeout(function () {
                        if (target != '_blank') {
                            window.location = href;
                        }
                    })
                });

            });
        });

    </script>



