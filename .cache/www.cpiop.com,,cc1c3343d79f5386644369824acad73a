    <script>
        // row of tabs opening/closing logic
        $('#header_top_tab_list > li').each(function () {
            var tab = $(this);
            
            var activeMouseHandler = function (e) {
                var dropdown = tab.find('.dropped-tab');
                if (e.target == dropdown.get(0)) {
                    return;
                }

                if (dropdown.has(e.target).length == 0) {
                    tab.removeClass('active');
                    $('body').off('click', activeMouseHandler);
                    tab.one('click', inactiveMouseHandler);
                }
            };

            var inactiveMouseHandler = function (e) {
                tab.addClass('active');
                window.setTimeout(function () {
                    $('body').on('click', activeMouseHandler);
                }, 0);
                if (tab.attr('id') == "cart-tab") {
                    window.setTimeout(function () {
                        if ($('#cart-scrollable').data('jsp')) {
                            $('#cart-scrollable').data('jsp').destroy();
                        }
                        $('#cart-scrollable').jScrollPane({ mouseWheelSpeed: 30 }).data('jsp');
                    }, 0);
                } else if (tab.attr('id') == "quickorder-tab") {
                    window.setTimeout(function () {
                        if ($('#quickorder-form').data('jsp')) {
                            $('#quickorder-form').data('jsp').destroy();
                        }
                        $('#quickorder-form').jScrollPane({ mouseWheelSpeed: 30 }).data('jsp');
                    }, 0);
                };
            };

            tab.one('click', inactiveMouseHandler);
        });
    </script>

