<form action="/Login" id="login-dropdown-form" method="post">    <fieldset id="body">
        
            <label for="login-dropdown-username">Username</label>
            <input data-val="true" data-val-required="The Username field is required." id="login-dropdown-username" name="Username" type="text" value="" />
        
            <label for="login-dropdown-password">Password</label>
            <input data-val="true" data-val-required="The Password field is required." id="login-dropdown-password" name="Password" type="password" />
        
        <label class="checkbox-label" for="login-dropdown-remember"><input type="checkbox" id="login-dropdown-remember" name="Remember" value="true" checked="checked" />Stay signed in.</label>
        <span class="red" id="login-dropdown-error-container"></span>
        <input class="button orange" type="submit" id="login-dropdown-submit" value="Log In" /><br />
        <input type="hidden" name="ReturnUrl" value="http://www.cpiop.com/" />

        <span><a href="/Request-Password-Reset">Can't access your account?</a></span><br />
        

    </fieldset>
</form>


<script>
    $(document).ready(function() {
        $('form#login-dropdown-form').on("submit", function (e) {
            e.preventDefault();
            e.returnValue = false;

            $('#login-dropdown-submit').addClass('disabled').attr('disabled', 'disabled');
            var $form = $(this);
            var errmsg = $('#login-dropdown-error-container');
            
                $.ajax({
                    type: 'POST',
                    url: '/LoginCheck',
                    data: JSON.stringify({
                        log: {
                            Username: $('#login-dropdown-username').val(),
                            Password: $('#login-dropdown-password').val()
                        }
                    }),
                    contentType: 'application/json',
                    dataType: 'json',
                    traditional: true,
                    success: function (data) {
                        if (data == 'deleted') {
                            errmsg.empty().text("This account has been deleted.");
                            $('#login-dropdown-submit').removeAttr('disabled').removeClass('disabled');
                        } else if (data == true) {
                            errmsg.empty();
                            $form.off('submit');
                            $form.submit();
                        } else {
                            errmsg.empty().text("Invalid Username or Password.");
                            $('#login-dropdown-submit').removeAttr('disabled').removeClass('disabled');
                        }
                    },
                    error: function () { 
                        errmsg.empty().text("Error. Please try again.");

                        $('#login-dropdown-submit').removeAttr('disabled').removeClass('disabled');
                    }
                });
            
        });

    });
</script>
