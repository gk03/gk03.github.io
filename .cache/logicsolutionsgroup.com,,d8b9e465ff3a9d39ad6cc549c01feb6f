<script type="text/javascript">
$( document ).ready(function() {
    function Divs() {
        var divs= $('#ThisIsTheParent section'),
            now = divs.filter(':visible'),
            next = now.next().length ? now.next() : divs.first(),
            speed = 3000;
    
        now.fadeOut(speed);
        next.fadeIn(speed);
    }
    
    $(function () {
        setInterval(Divs, 8000);
    });

});


</script>

