<script type="text/javascript">
$(document).ready(function() {

$('#footer .nav > ul.navmenu > li > a').click(
	function(){
		if( $(this).hasClass('active') ){
			return true;
		}
		else{
			$(this).parent().siblings().find('.sub-menu').stop().slideUp(150);
			$(this).addClass('active').stop().siblings('.sub-menu').slideDown(150);
			return false;
		}
	}
);

$('.nav ul.solution').find('.current_page_parent.current_page_ancestor').first().addClass('test').siblings('.current-menu-ancestor').removeClass('current_page_ancestor current_page_parent');

$('#header #nav ul.main li.menu-item-has-children').each(
	function(){
		$(this).hover(
			function(){
				$(this).prev().addClass('hovering-sib');
			},
			function(){
				$(this).prev().removeClass('hovering-sib');
			}
		);
	}
);

$('.blogmenu > li.blog-menu > a').each(
	function(){
		
		$(this).click(function(e){
			e.preventDefault();
			$(this).parent().toggleClass('current_page_item');
		})
	}
);


}
);

</script>


