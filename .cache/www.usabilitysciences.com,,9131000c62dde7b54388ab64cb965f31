<script type="text/javascript">
$(document).ready(function() {
	if ("ontouchstart" in window || "ontouch" in window) { $('body').addClass('touch'); }
		
	window.onload = function () {
	  $('#loader').fadeOut(500, function(){ $('#loader').remove(); } );
    }
});
</script>


<script type="text/javascript" src="http://www.usabilitysciences.com/wp-content/themes/dd-usability-science/js/responsiveslides.min.js"></script>
<script type="text/javascript" src="http://www.usabilitysciences.com/wp-content/themes/dd-usability-science/js/slideshow-resize.js"></script>
<script type="text/javascript">
$(document).ready(function() {

  $(window).load(function() {
	$("#slides").responsiveSlides({
		auto: true,
		speed: 1000,
		timeout: 5500,
		pager: false,
		nav: true
	});
  });
  /////////////////

$('.accordion-action').each(
	function(){
		$(this).click
		(			function(e){
				e.preventDefault();
				$(this).parent().parent().parent().siblings('li').removeClass('active');
				$(this).parent().parent().parent().siblings('li').find('.accordion-content').stop().slideUp(150);

				$(this).parent().parent().parent().addClass('active');
				$(this).parent().parent().parent().find('.accordion-content').stop().slideDown(150);
			}
		);
	}
);

//////////
$(".wpnp-tabs a").click(function(event) {
    event.preventDefault();
    $(this).parent().addClass("current");
    $(this).parent().siblings().removeClass("current");
    var tab = $(this).attr("href");
    $(".wpnp-dd-tabs").not(tab).removeClass('current');
    $(tab).addClass('current');
});

//////////
$(".tab-title").click(function(event) {
    event.preventDefault();
    $(".tab-title").removeClass("active").parent().find('.wpnp-dd-accordion').slideUp(150);
    $(this).addClass("active");
    var tab = $(this).attr("href");
    $(".wpnp-dd-tabs").not(tab).removeClass('current');
    $(tab).addClass('current').find('.wpnp-dd-accordion').slideDown(150);
});


//////////
$(".nav.tabbed a").click(function(event) {
    event.preventDefault();
    $(this).parent().addClass("current_page_item");
    $(this).parent().siblings().removeClass("current_page_item");
    var tab = $(this).attr("href");
    $(".tab-content-section").not(tab).removeClass('active');
    $(tab).addClass('active');
});


$('#client-testimonials').find('.testimonials:last').addClass('last');

});
</script>
