    function ValidatePhoneNumberV2(txtId, spnID) {
        if ($("." + txtId).length > 0) {
            var txtval = $("." + txtId).text().replace(/^\s+|\s+$/g, "");

            if (txtval != null && txtval != "") {
                var newValue = txtval.replace('-', '');
                var newvalue11 = newValue.replace('-', '');
                var newValue22 = newvalue11.replace('.', '');
                var newValue33 = newValue22.replace('.', '');
                var newValue1 = newValue33.replace('(', '');
                var newValue2 = newValue1.replace(')', '');
                var newValue3 = newValue2.replace(' ', '');
                var len = newValue3.replace(/^\s+|\s+$/g, "").trim().length;

                if (len != 10) {
                    $("." + txtId).html('');
                    $('.fa-phone').hide();
                    $("." + txtId).hide();
                    return false;
                }
                else {
                    if (isNumber(newValue3) && Number(newValue3) > 0) {
                        var appnd = " (" + newValue3.slice(0, -7) + ") " + newValue3.slice(3, -4) + "-" + newValue3.substr(6);
                        $('.fa-phone').show();
                        $("." + txtId).show();
                        $("." + txtId).html(appnd);
                        return true;
                    }
                    else {
                        $("." + txtId).html('');
                        $('.fa-phone').hide();
                        $("." + txtId).hide();
                        return false;
                    }
                }
            }
            else {
                $("." + txtId).html('');
                $('.fa-phone').hide();
                $("." + txtId).hide();
                return false;
            }
            return true;
        }
        else {
            return false;
        }
    }

    $(function () {
        ValidatePhoneNumberV2('contactworkphone', '');
        // ValidatePhoneNumberV2('contactworkphonestrip', '');
        ValidatePhoneNumberV2('contactworkphoneweb', '');
        ValidatePhoneNumberV2('contactworkphone1', '');
        ValidatePhoneNumberV2('contactworkphone2', '');
    });
