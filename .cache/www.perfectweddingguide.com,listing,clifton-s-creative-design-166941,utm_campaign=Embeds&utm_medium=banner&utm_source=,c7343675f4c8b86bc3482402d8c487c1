    var ShowCategories
    $(function () {
        $('#SSelectedCategory').text('Invitations & Stationery');
        $('#SSelectedCategory').attr('webPath', 'wedding-invitations-stationery');
        $('#SSelectedCategory_Event').text('Category');
        $('#SSelectedCategory_Event').attr('webPath', '');
        $('#SSelectedCategoryMobile').text('Invitations & Stationery');
        $('#SSelectedCategoryMobile').attr('webPath', 'wedding-invitations-stationery');
        $('#SSelectedCategoryMobile_Event').text('Category');
        $('#SSelectedCategoryMobile_Event').attr('webPath', '');
        //Set Radius by cookies
        SetRadius('Radius');
    });
    function ManageVendorCategortRadius(catName) {
        if ('reception venues,ceremony venues,honeymoon venues,guest accommodations' != 'all') {
            if ('reception venues,ceremony venues,honeymoon venues,guest accommodations'.indexOf(catName.toLowerCase()) == -1) {
                $('#SearchContainer_Web').addClass('vendor-remove-radius');
                $('.search4-menu-mob-txt').addClass('vendor-remove-radius-mobile');
                $('#SearchRadiusWeb').hide();
                $('#SSelectedRadius').text('All');
                $('#SearchRadiusMobile').hide();
            }
            else {
                $('#SearchContainer_Web').removeClass('vendor-remove-radius');
                $('.search4-menu-mob-txt').removeClass('vendor-remove-radius-mobile');
                $('#SearchRadiusWeb').show();
                $('#SearchRadiusMobile').show();
            }
        }
    }

    function ShowSearchTab(type) {
        $("#search_inner_div").show();
        $('#search_openclose_img').attr('src', 'https://s3.amazonaws.com/9b98664d-254d-46eb-8780-8a3788d1fe35/pwg2_0/v15/images/home-images.png');
        ActivateSearchTab(type)
    }
</script>
