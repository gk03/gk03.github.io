        }



        /**
         * When scrolled all the way to the bottom, add more tiles
         */
        function onScroll() {
            // Check if we're within 100 pixels of the bottom edge of the broser window.
            var winHeight = window.innerHeight ? window.innerHeight : $window.height(), // iphone fix
                closeToBottom = ($window.scrollTop() + winHeight > $document.height() - 100);

            if (closeToBottom) {
                // Get the first then items from the grid, clone them, and add them to the bottom of the grid
                var $items = $('.video-image-div', $tiles),
                    $firstTen = $items.slice(0, 30);
                $tiles.append($firstTen.clone());

                //applyLayout();
            }
        };

        // Call the layout function for the first time
        applyLayout();
        IntializeStarIcon();

    })(jQuery);

    function IntializeStarIcon() {
        $(".vendor_stars_inner_span").raty({
            readOnly: true,
            path: '/images/Rating',
            score: function () {
                return $(this).attr('data-rating');
            }
        });

        $(".vendor_profile_stars_inner").raty({
            readOnly: true,
            path: '/images/Rating',
            score: function () {
                return $(this).attr('data-rating');
            }
        });
        $(".profile_star_rating_star5").raty({
            readOnly: true,
            path: '/images/Rating',
            score: function () {
                return $(this).attr('data-rating');
            }
        });

        $(".vendor_profile_stars_inner1 ").raty({
            readOnly: true,
            path: '/images/Rating',
            score: function () {
                return $(this).attr('data-rating');
            }
        });

        $(".vendor_stars_inner_span_v").raty({
            readOnly: true,
            path: '/images/Rating',
            score: function () {
                return $(this).attr('data-rating');
            }
        });
    }

    function resetReviewControl() {
