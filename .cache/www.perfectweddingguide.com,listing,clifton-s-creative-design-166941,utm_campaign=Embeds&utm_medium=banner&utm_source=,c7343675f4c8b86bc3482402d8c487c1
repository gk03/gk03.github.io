        position: fixed;
        top: 0;
        z-index: 10000;
    }
</style>


<script type="text/javascript">
    var tempcount = 0;
    var bksubType="";
    function OpenDescPopup(id, divId) {
        $("#popupFavImage").attr("src", "").attr("src", $("#imgGallery_" + id).attr("data-src"));
        $("#popupFavImage1").attr("src", "").attr("src", $("#imgGallery_" + id).attr("data-src"));
        OpenPopup(divId, 834, false);
        $("#" + divId).css("width", "inherit");
        $("#" + divId).attr("attrrId", id);
        $("#" + divId).attr("bType", 'vendor');
        if(''=="Mob" && '0' =="0"){
            $("#" + divId).fadeIn().prepend('<div style="float: right; margin-right: 14px;"><a href="#" class="close"><img class="btn_close" src="/images/close-popup.png" style="margin-left:0px;margin-right:10px;" class="embedlbtn_close" title="Close Window" alt="Close" /></a></div>');
        }
        else{
            if(''=="Web"){
                $("#" + divId).fadeIn().prepend('<div style="float: right; margin-right: 14px;"><a href="#" class="close"><img class="btn_close" src="/images/close-popup.png" style="margin-left:0px;margin-right:10px;" class="embedlbtn_close" title="Close Window" alt="Close" /></a></div>');
            }
        }
        ClearControlLogin();

        ForMobileDisplay();
    }

    function ValidateUserLogin(id) {
        LoadLoaderConentPage('outer-main-width', false);
        var action = '/ajax-call/Others/ValidateUserLogin/'
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: {},
            success: function (data) {
                if (data.AcknowledgeType == 1) {
                    if (data.UserId != 0) {
                        ToAddFavouratePopUp(id);

                        $('.outer-main-width').find(".ajax_overlay").remove();
                    }
                    else {
                        $('#divbeforelogin').show();
                        $('#divafterlogin').hide();
                        OpenDescPopup(id, "Wedpop");
                        RemoveLoaderConentPage('outer-main-width');
                    }
                }
            }
        });
    }
    function ToAddFavouratePopUp(id) {
        $('#divbeforelogin').hide();
        $('#divafterlogin').show();
        OpenDescPopup(id, "BookMark");

    }
    function ValidateUserName() {
        var username = $("#txtloginEmail").val().replace(/^\s+|\s+$/g, "");
        if (!ValidateTextBox(username)) {
            $("#spnUserName").text('Please Enter Your Email.');
            $("#spnUserName").show();
            return false;
        }
        else {
            $("#spnUserName").hide();
            if (!ValidateEmail(username)) {
                $("#spnUserName").text("A valid email address is required.");
                $("#spnUserName").show();
                return false;
            }
            else {
                $("#spnUserName").hide();
            }
        }
        return true;
    }

    function ValidateLoginPassword() {
        var password = $("#txtPassword").val();
        if (!ValidateTextBox(password)) {
            $("#spnPassword").text('Please enter your password.');
            $("#spnPassword").show();
            return false;
        }
        else {
            $("#spnPassword").hide();
        }
        return true;
    }

    function SignInClick() {

        var username = $("#txtloginEmail").val().replace(/^\s+|\s+$/g, "").toLowerCase();
        var password = $("#txtPassword").val();

        if (!ValidateUserName())
            return false;

        if (!ValidateLoginPassword())
            return false;


        LoadLoaderConentPage('BookmarkpopLogin', false);
        var action = "/tool/others/UserSignIn/";
        var bookMarkTypeId = $("#Wedpop").attr("attrrId");

        var redirectPth = $("#Wedpop").attr("redirectPth");
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: { UserName: username, Password: password, bookMarkTypeId: bookMarkTypeId, redirectPth: redirectPth },
            success: function (data) {
                if (data.AcknowledgeType == 1) {

                    window.location.href = data.RedirectURL;
                }
                else {
                    RemoveLoaderConentPage('BookmarkpopLogin');
                    if (data.Message == "No User exists") {
                        $("#spnUserName").text(data.Message).show();
                    }
                    else
                        AlertMessageCloseExistingPopup(data.Message, "");
                }
            }
        });
    }

    function ClearControlLogin() {
        $("#txtloginEmail").val('');
        $("#txtPassword").val('');
        $("#spnPassword").text('').hide();
        $("#spnUserName").text('').hide();
        $('#txtfolderName').val('');
        $("#spnfoldername").text("");
        $("#spnfoldername").hide();
    }
    function RedirectToSignUp() {
        $('#form').attr('action', "/tool/others/signupuser/");
        document.forms['form'].submit();
    }

</script>
