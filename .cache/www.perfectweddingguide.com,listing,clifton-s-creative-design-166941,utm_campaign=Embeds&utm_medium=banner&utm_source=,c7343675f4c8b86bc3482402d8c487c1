<script>

    $(function () {
        var Currentreview = '0';
        if ('' != "") {
            ShowOriginalPhoto('', '0', '0');
        }
        ResetDragPhotoDiv(true);
        if ('detail' != "detail" && Currentreview == '0') {
            ScrollToDiv($('#mainmenus'));
        }

        $('.home-blog-img-container').mouseover(function () { $(this).find('.social_share_links_img').css({ "opacity": "1" }) });
        $('.home-blog-img-container').mouseout(function () { $(this).find('.social_share_links_img').css({ "opacity": "0" }) });
    });

    function CallBackAfterDrop() {
        var mediaIds = [];
        $('#tiles_photo li').each(function () {
            mediaIds.push($(this).attr("mediaid"));
        });
    }

    function ShowOriginalPhoto(url, width, height) {
        var galleryType = 1;
        if (galleryType == 1) {
            $('#dragdrop_Gallery_img').show();
            $('#dragdrop_Gallery_img').one("load", function () {

                var dimension;
                var browseH;

                var imgH = $('#dragdrop_Gallery_img').height();

                
                dimension = GetResizeDimension(width, imgH, width, imgH);
                browseH = 75;
                //}

                $('#dragdrop_Gallery_img').attr("width", dimension.Width);
                $('#dragdrop_Gallery_img').attr("height", dimension.Height);
                $('#dragdrop_Gallery_img').css('bottom', dimension.MarginBottom).css('right', dimension.MarginRight);

                $('.inner-dragdropbox').css('height', dimension.Height + "px")
                    .css('width', dimension.Width + "px")
                    .css('margin-top', '-' + dimension.Height + "px");

                $('#drop_dragdrop_Gallery').addClass('transparentDiv');

                $('#gallerybtn').addClass('pink-bttn-gallery-active');
                $('#bannerbtn').removeClass('pink-bttn-gallery-active');

                var top = parseInt((parseFloat(dimension.Height) - browseH) / 2);

                $('.inner-dragdropbox-browse').css('top', top + "px")
                    .css('position', 'relative');

            }).attr("src", url);
        }
    }

    function ResetDragPhotoDiv(onload) {
        var photoCt = 0;
        photoCt = $('.scrollEl').length;

        if (photoCt == 0) {
            $('.ind-Crow-box1txt').css('position', 'relative');
            $('#drop_dragdrop_Gallery').addClass('drop_dragdrop_Gallery_important');

            var dimension;
            var browseH;
            var defltH = 375;
            var defltW = 500;

            
            dimension = GetResizeDimension(defltW, defltH, defltW, defltH);
            browseH = 75;
            //}

            $('.inner-dragdropbox').css('height', dimension.Height + "px")
            .css('width', dimension.Width + "px")
            .css('margin-top', "0px");

            var top = parseInt((parseFloat(dimension.Height) - browseH) / 2);

            $('.inner-dragdropbox-browse').css('top', top + "px")
                .css('position', 'relative');

            $('#galleryDiv').hide();
            $('#drop_dragdrop_Gallery').removeClass('transparentDiv');
            $('#dragdrop_Gallery_imgDiv').hide();
        }
        else {
            $('#galleryDiv').show();
            $('#drop_dragdrop_Gallery').addClass('drop_dragdrop_Gallery_other');
            $('.ind-Crow-box1txt').css('position', 'absolute');
            $('#dragdrop_Gallery_imgDiv').show();

            if (jQuery_2_1_3('.Rtop-menu-outer').is(':visible')) {
                jQuery_2_1_3('#tiles_photo').shapeshift({ minColumns: 2 });
            }
            else {
                jQuery_2_1_3('#tiles_photo').shapeshift({
                    minHeight: 300
                });
            }

            if (!onload) {
                $("#tiles_photo").css('height', (parseInt($("#tiles_photo").height()) + 150) + 'px');
            }
        }
    }

    $(document).ready(function () {
        if ('1' == '0') {
            $('#awardarea').addClass('award_area');
        }

        VendorLikeUs('http://www.perfectweddingguide.com/listing/clifton-s-creative-design-166941/');

        var quality = 5;
        var responsiveness = 5;
        var professionalism = 5;
        var flexibility = 5;
        var costtovalue = 5;

        $(".rating").raty({
            path: '/images/Rating',
            click: function (score, evt) {
                if ($(this).attr('id') == "quality") {
                    quality = score;
                }
                else if ($(this).attr('id') == "responsiveness") {
                    responsiveness = score;
                }
                else if ($(this).attr('id') == "professionalism") {
                    professionalism = score;
                }
                else if ($(this).attr('id') == "flexibility") {
                    flexibility = score;
                }
                else if ($(this).attr('id') == "costtovalue") {
                    costtovalue = score;
                }
                var avgrating = parseInt(quality) + parseInt(responsiveness) + parseInt(professionalism) + parseInt(flexibility) + parseInt(costtovalue);
                $('#spnAvgRatingvlaue').html('');
                $('#spnAvgRatingvlaue').html(parseFloat(avgrating / 5).toFixed(1) + '/5.0');
            }
        });

        resetReviewControl();

        $("#btnSubmit").click(function () {
            var status = true;
            if (!ValidateFirstName('txtFirstName', 'spnFirstName'))
                status = false;

            if (!ValidateLastName('txtLastname', 'spnLastName'))
                status = false;

            if (!ValidateWeddingdate('txtWeddingDate', 'spnWeddingdate')) {
                status = false;
            }

            if (!ValidateNewEmail('txtemailaddress', 'spnemailaddress')) {
                status = false;
            }

            if (!ValidateTextBoxRequired('txtReview', 'spnreviewtitle', 'Please enter review Title.'))
                status = false;

            if (!ValidateTextBoxRequired('txtWrite', 'spnreviewtext', 'Please enter your Review.'))
                status = false;

            if (!status) {
                return status;
            }

            var reviewTitle = $("#txtReview").val();
            var write = $("#txtWrite").val();
            var firstname = $("#txtFirstName").val().replace(/^\s+|\s+$/g, "");
            var lastname = $("#txtLastname").val().replace(/^\s+|\s+$/g, "");
            var weddingdate = $("#txtWeddingDate").val().replace(/^\s+|\s+$/g, "");
            var email = $("#txtemailaddress").val().replace(/^\s+|\s+$/g, "");

            quality = $("#quality").raty('score');
            responsiveness = $("#responsiveness").raty('score');
            professionalism = $("#professionalism").raty('score');
            flexibility = $("#flexibility").raty('score');
            costtovalue = $("#costtovalue").raty('score');
            LoadLoaderConentPage('content-list-txt-space', false);
            var action = '/ajax-call/WeddingVendors/SaveReviewV2/'
            $.ajax({
                type: 'POST',
                url: action.toLowerCase(),
                data: { Title: reviewTitle, Comment: write, Quality: quality, Responsiveness: responsiveness, Professionalism: professionalism, Flexibility: flexibility, CostValue: costtovalue, FirstName: firstname, LastName: lastname, Email: email, WeddingDate: weddingdate, ListingID: '166941' },
                success: function (data) {
                    if (data.data.AcknowledgeType == 1) {
                        AlertMessage("Review posted successfully.")
                        resetReviewControl();
                        //window.location.href = "http://www.perfectweddingguide.com/listing/clifton-s-creative-design-166941/reviews/";
                        GetVendorReviewPath('http://www.perfectweddingguide.com/listing/clifton-s-creative-design-166941/reviews/');
                    }
                    else {
                        AlertMessage("Some error occured while posting review");
                    }
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                }
            });
        });
    });

    function resetContactControl() {
        $("#txtFirstName1").val('');
        $("#txtLastname1").val('');
        $("#txtweddingdate1").val('');
        $("#txtemailaddress1").val('')
        $("#txtphonenumber").val('');
        $("#txtContent").val('');
        $(".rad1").text('');
        $(".rad1").hide();
    }

    function SendInfo(pagetype) {
        var status = true;
        if (!ValidateFirstName('txtFirstName1', 'spnFirstName1'))
            status = false;

        if (!ValidateLastName('txtLastname1', 'spnLastName1'))
            status = false;

        if (!ValidateWeddingdate('txtweddingdate1', 'spnWeddingdate1'))
            status = false;

        if (!ValidateNewEmail('txtemailaddress1', 'spnemailaddress1'))
            status = false;

        if (!ValidatePhoneNumber('txtphonenumber', 'spnphonenumber'))
            status = false;

        //if (!ValidateTextBoxLimit('txtContent', 'spncontent', 'Please enter comment', 'Please enter a comment at least 25 characters', 25))
        //    status = false;

        if (!status) {
            return status;
        }
        var firstname = $("#txtFirstName1").val().replace(/^\s+|\s+$/g, "");
        var lastname = $("#txtLastname1").val().replace(/^\s+|\s+$/g, "");
        var weddingdate = $("#txtweddingdate1").val();
        var Email = $("#txtemailaddress1").val().replace(/^\s+|\s+$/g, "").toLowerCase();
        var phonenumber = $("#txtphonenumber").val().replace(/^\s+|\s+$/g, "");
        var Contents = $("#txtContent").val().replace(/^\s+|\s+$/g, "");
        var listingIds = '166941';

        var items = new Array();
        items = listingIds.split(",");

        var requestData = {
            ListingIds: items,
            VendorEmail: { FirstName: firstname, LastName: lastname, Email: Email, Phone: phonenumber, WeddingDate: weddingdate, Questions: Contents }
        };
        if (pagetype == "web") {
            LoadLoaderConentPage('content-list-txt-space', false);
            // OnContactClosePopup();
        }
        else {
            LoadLoaderConentPage('con-from', false);
        }
        var action = '/ajax-call/WeddingVendors/SendEmailRequestV2/';
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: JSON.stringify(requestData),
            contentType: 'application/json',
            success: function (data) {
                if (data.data.AcknowledgeType == 1) {
                    //AlertMessage(data.data.Message, "");
                    AlertMessage('Email sent successfully.');
                    if (pagetype == "mob") {
                        $('.con-from').find(".ajax_overlay").remove();
                        $('.close_inner').trigger('click');
                    }
                    else {
                        $('.content-list-txt-space').find(".ajax_overlay").remove();
                    }
                    resetContactControl();
                }
                else if (data.data.AcknowledgeType == 2) {
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                    //OnContactClosePopup();
                    //AlertMessageCloseExistingPopup(data.data.Message, "");
                }
                else {
                    // OnContactClosePopup();
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                    if (data.data.AcknowledgeType == undefined) {

                    }
                }
            }
        });
    }

    function OpenVendorPopup() {
        $('#Contactpopup').addClass('popup_block');
        $('#Contactpopup').css('display', 'none');
        OpenPopup('Contactpopup', 600, true);
        $("#Contactpopup").css("margin-top", "-310");
    }

    (function ($) {
        var $tiles = $('#js-cover-photo-carousel'),
            $handler = $('.video-image-div', $tiles),
            $main = $('#main-div'),
            $window = $(window),
            $document = $(document),
            options = {
                autoResize: true, // This will auto-update the layout when the browser window is resized.
                container: $main, // Optional, used for some extra CSS styling
                offset: 0, // Optional, the distance between grid items
                itemWidth: 241 // Optional, the width of a grid item
            };

        // Init JQuery Unveil
        $("img").unveil();

        $('.fancybox').fancybox({
            afterLoad: function (current, previous) {
                // use this call the Google Analytics code
                var currentMediaid = current.id;
                var url = current.href;
                var caption = $('#vendorsDivText').text();
                //EventAnalytic('gallery-photo', 'view', '');
                EventAnalytic('image', caption, url);
                SaveTrackingDetails('wedding-vendors', 'vendorphotos', '166941');

                this.skin.append($('#fancy_custom_detail').clone().css('display', 'block'));
                this.skin.append("<div class='clear'></div>");
            },
            helpers: {
                title: {
                    type: 'inside',
                    position: 'bottom'
                }
            },
            tpl: {
                next: '<a title="Next" class="fancybox-nav fancybox-next" ><span></span></a>',
                prev: '<a title="Previous" class="fancybox-nav fancybox-prev"><span></span></a>'
            }
        });

        function applyLayout() {
            //$tiles.imagesLoaded(function () {
            //    // Destroy the old handler
            //    if ($handler.wookmarkInstance) {
            //        $handler.wookmarkInstance.clear();
            //    }

            //    // Create a new layout handler.
