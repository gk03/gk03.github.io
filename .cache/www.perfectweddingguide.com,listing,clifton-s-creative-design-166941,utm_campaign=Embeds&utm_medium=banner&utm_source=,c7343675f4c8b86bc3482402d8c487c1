    function AddBookMark(resId, folderName) {
        var action = '/ajax-call/Others/AddToBookMark/';
        var bookMarkType = $("#BookMark").attr("bType");
        LoadLoaderConentPage('FolderPopupLogin', false);
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: { bookMarkId: resId ,folderName:folderName,bookMarkType:bookMarkType,bookMarkTypeSubType:bksubType},
            success: function (data) {
                if (data.AcknowledgeType == 1) {

                    $('.divFavourate_' + resId).next().css('color','#C54A5C');
                    $('.divFavourate_' + resId).attr('isremove','1');
                    $('.bookmared-white').css('color', '#C54A5C');


                    $('.addtobookvendor').html("");
                    $('.addtobookvendor').html("Remove Bookmark");

                    $('.screen-lock-mob').removeClass('screen-lock-mob');

                    $('#photoDetail_' + resId).attr('IsBookmark','1');


                    $(".divFavourate_" + resId).each(function(e){
                        $("#addbookmark").removeClass("fa fa-heart-o fa-3 testing-white-heart");
                        $("#addbookmark").addClass("fa fa-heart fa-3 testing-white-heart");
                        $($('.divFavourate_' + resId)[e]).attr('isremove', '1');

                        if ($(this).siblings('.icon-add-remove-bookmark:visible').length) {
                            $(this).siblings('.icon-add-remove-bookmark').children('span').removeClass('icon-plus').addClass('icon-minus');
                        }
                    });

                    BindFolder('');
                    RemoveLoaderConentPage('FolderPopupLogin');
                    AlertMessageCloseExistingPopup(data.Message, "");
                }
                else  if (data.AcknowledgeType == 0)
                {
                    RemoveLoaderConentPage('FolderPopupLogin');
                    AlertMessageCloseExistingPopup(data.Message, "");

                }
            }
        });
    }

    function RemoveBookMark(resId) {
        LoadLoaderConentPage('outer-main-width', false);
        var action = '/ajax-call/Others/RemoveBookMark/'
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: { resId: resId },
            success: function (data) {
                if (data.AcknowledgeType == 1) {

                    $('.divFavourate_' + resId).next().css('color','#fff');
                    $('.divFavourate_' + resId).attr('isremove','0');

                    $('.bookmared-white').css('color', '#fff');

                    $('#photoDetail_' + resId).attr('IsBookmark','0');

                    $('.addtobookvendor').html('');
                    $('.addtobookvendor').html("Add to Bookmark");
                    $('.screen-lock-mob').removeClass('screen-lock-mob');

                    AlertMessage(data.Message, "");
                    $(".divFavourate_" + resId).each(function(e){
                        $("#addbookmark").removeClass("fa fa-heart fa-3 testing-white-heart");
                        $("#addbookmark").addClass("fa fa-heart-o fa-3 testing-white-heart");
                        $($('.divFavourate_' + resId)[e]).attr('isremove', '0');

                        if ($(this).siblings('.icon-add-remove-bookmark:visible').length) {
                            $(this).siblings('.icon-add-remove-bookmark').children('span').removeClass('icon-minus').addClass('icon-plus');
                        }
                    });
                    BindFolder('');
                    RemoveLoaderConentPage('outer-main-width');
                }
            }
        });
    }

    function ForMobileDisplay() {
        if ($('.Rtop-menu-outer').is(':visible')) {
            $('#Wedpop').css("width", "276px");
            $('#Wedpop').css("margin-left", "-148px");
            $('#Wedpop').css("margin-top", " -186px");
            $('#BookMark').css("width", "276px");
            $('#BookMark').css("margin-left", "-148px");
            $('#BookMark').css("margin-top", " -210px");

        }
        else{
            $("#Wedpop").css("margin-top","-260px");
            $("#BookMark").css("margin-top","-260px");
        }
    }

    function ClosePopupV2() {
        $('.screen-lock-mob').removeClass('screen-lock-mob');
        $('#fadePopup , .popup_block').fadeOut(function () {
            $('#fadePopup, a.close').remove();
        });
    }
</script>

