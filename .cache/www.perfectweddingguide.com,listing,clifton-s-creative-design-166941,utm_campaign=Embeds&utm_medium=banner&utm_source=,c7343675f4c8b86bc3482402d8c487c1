    function GetVendorReviewPath(Vurl) {
        $('#formDetailReview').attr('action', Vurl);
        //$('#type').val('details');
        document.forms['formDetailReview'].submit();
    }

    $('a.close').live('click', function () {
        if ($('#videoIframe').length > 0) {
            var url = $('#videoIframe').find('.video').attr('src');
            $('#videoIframe').find('.video').attr('src', '');
            return false;
        }
    });
</script>

<script>
    $(document).ready(function () {
        var collectionLinks = $(".vendorlink");
        for (var i = 0; i < collectionLinks.length; i++) {
            $(collectionLinks[i]).click(function (event) {
                event.preventDefault();
                var href = $(this).attr("href");
                $('#formDetailReview').attr('action', href);
                $('#type').val("details");
                document.forms['formDetailReview'].submit();
            });
        }
    });

    $(document).ready(function () {
        var collectionLinks = $(".vendorlinkslickslide");
        for (var i = 0; i < collectionLinks.length; i++) {
            $(collectionLinks[i]).click(function (event) {
                event.preventDefault();
                var href = $(this).attr("href");
                var photoid = $(this).attr("data-photoid");
                $('#formDetailReview').attr('action', href);
                $('#type').val("details");
                $('#slickslidephotoid').val(photoid);
                document.forms['formDetailReview'].submit();
            });
        }
    });
</script>

