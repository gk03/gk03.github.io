<script type="text/javascript">
    $('.videopop').click(function () {
        var mediatype = $(this).find('.clvideodetail').attr('mediatype');
        var pName = $(this).find('.clvideodetail').attr('videocaption');
        var pDescription = $(this).find('.clvideodetail').attr('videodescription');
        var imgUrl = $(this).find('.clvideodetail').attr('videourl');
        var pCredit = $(this).find('.clvideodetail').attr('videocredit');
        var mediaId = $(this).find('.clvideodetail').attr('mediaId');
        var thumburl = $(this).find('.clvideodetail').attr('image-url');
        var popupWidthDefault = 1000;
        var videoIframeDefaultW = 620;
        var videoIframeDefaultH = 500;
        var popupWidth = 0;

        $('#videoIframe').css('height', videoIframeDefaultH + 'px');
        $('#videoIframe').css('width', videoIframeDefaultW + 'px');
        popupWidth = popupWidthDefault;

        if (mediatype == "3") {
            //var myId = getYoutubeId(imgUrl);
            //imgUrl = '//www.youtube.com/embed/' + myId + '';
            $('#videoIframe').empty().append("<iframe src='" + imgUrl + "' height='500px' width='620px' class='video'></iframe>");
        }
        if (mediatype == "4") {
            var myId = getvimeoid(imgUrl);
            imgUrl = '//player.vimeo.com/video/' + myId + '?portrait=0&color=333';
            $('#videoIframe').empty().append("<iframe src='" + imgUrl + "' height='500px' width='620px' class='video'></iframe>");
        }
        if (mediatype == "6") {
            var myId = getwistiaid(imgUrl);

            var text = $('#wistia_EmbedCode_' + mediaId).text();
            $('#videoIframe').empty().html(text);

            var wistiaW = $('#videoIframe').find('#wistia_' + myId).width();
            var wistiaH = $('#videoIframe').find('#wistia_' + myId).height();

            if (wistiaW > videoIframeDefaultW) {
                wistiaH = GetResizeHeight(wistiaW, wistiaH, videoIframeDefaultW);
                wistiaW = videoIframeDefaultW;

                $('#videoIframe').find('#wistia_' + myId).css('width', wistiaW + 'px');
                $('#videoIframe').find('#wistia_' + myId).css('height', wistiaH + 'px');

                $('#videoIframe').css('width', wistiaW + 'px');
                $('#videoIframe').css('height', wistiaH + 'px');
                popupWidth = popupWidthDefault;

                if ($('#videoIframe').find('#wistia_' + myId + '_seo').length > 0) {
                    $('#videoIframe').find('#wistia_' + myId + '_seo').css('width', wistiaW + 'px');
                    $('#videoIframe').find('#wistia_' + myId + '_seo').css('height', wistiaH + 'px');
                }
            }
            if (wistiaH > videoIframeDefaultH) {
                wistiaW = GetResizeWidth(wistiaW, wistiaH, videoIframeDefaultH);
                wistiaH = videoIframeDefaultH;

                $('#videoIframe').find('#wistia_' + myId).css('width', wistiaW + 'px');
                $('#videoIframe').find('#wistia_' + myId).css('height', wistiaH + 'px');

                popupWidth = parseFloat(popupWidthDefault) - parseFloat((parseFloat(videoIframeDefaultW) - wistiaW));

                $('#videoIframe').css('width', wistiaW + 'px');
                $('#videoIframe').css('height', wistiaH + 'px');

                if ($('#videoIframe').find('#wistia_' + myId + '_seo').length > 0) {
                    $('#videoIframe').find('#wistia_' + myId + '_seo').css('width', wistiaW + 'px');
                    $('#videoIframe').find('#wistia_' + myId + '_seo').css('height', wistiaH + 'px');
                }
            }
        }

        if (pName == undefined) {
            pName = '';
        }

        while (pDescription.indexOf("%27") > -1) {
            pDescription = pDescription.replace("%27", "'");
        }

        if (pCredit == '' || pCredit == null || pCredit == undefined) {
            $('#video_creditmain').hide();
        }
        else { $('#video_creditmain').show(); }

        $('#video_photocredit').text(pCredit);
        $('#video_photoName').text(pName);
        $('#video_photoDescription').text(pDescription);

        $('#video_SSControl').show();
        //var shareUrl = 'http://www.perfectweddingguide.com/listing/clifton-s-creative-design-166941/?utm_campaign=Embeds&amp;utm_medium=banner&amp;utm_source=';


        //var pshareUrl = shareUrl + mediaId + '/';

        var shareUrl = "http://www.perfectweddingguide.com/" + "listing/" + 'clifton-s-creative-design-166941' + "/videos/" + mediaId;


        $('#video_fbbtn').attr('onclick', 'PhotoFBShare("' + shareUrl + '")');
        $('#video_twiterbtn').attr('onclick', 'TweetByGetBitly("' + shareUrl + '","' + escape(pName) + '")');
        $('#video_gpbtn').attr('onclick', 'PhotoShareGoogle("' + shareUrl + '")');
        $('#video_pinit').attr('onclick', 'VendorPinInterest("' + shareUrl + '","' + thumburl + '","' + pDescription + '")');
        VendorLikeUsFancyBox(shareUrl);
        //LikeUsVideo(shareUrl);
        OpenPopup('videopopup', popupWidth, true);
        SaveTrackingDetails('wedding-vendors', 'vendorvideo', '166941');
    });

    function getYoutubeId(url) {
        var regExp = /^.*(youtu.be\/|v\/|u\/\w\/|embed\/|watch\?v=|\&v=)([^#\&\?]*).*/;
        var match = url.match(regExp);
        if (match && match[2].length == 11) {
            return match[2];
        } else {
            return 'error';
        }
    }

    function getvimeoid(url) {
        vimeo_Reg = /https?:\/\/(?:www\.|player\.)?vimeo.com\/(?:channels\/(?:\w+\/)?|groups\/([^\/]*)\/videos\/|album\/(\d+)\/video\/|video\/|)(\d+)(?:$|\/|\?)/;
        var match = url.match(vimeo_Reg);
        if (match) {
            return match[3];
        } else {
            return "<span class='error'>error</span>";
        }
    }

    function getwistiaid(url) {
        wistia_Reg = /https?:\/\/(.+)?(wistia.com|wi.st)\/(medias|embed)\/([^#\&\?]*).*/;
        var match = url.match(wistia_Reg);
        if (match) {
            return match[4];
        } else {
            return "<span class='error'>error</span>";
        }
    }

    function SetInnerDataofImage() {
        $('.inner-img-div .coverThumb').each(function (e) {
            var img = $($('.inner-img-div .coverThumb')[e]);
            var h = img.height();

            if (h != 0 || h != undefined) {
                var top = (h - 57) / 2;
                var id = img.parent().attr('id');

                //$('#innerdata_' + id).css('margin-top', top + 'px');
                var videoImg = $('#videoImg_' + id);

                if (videoImg.length != 0) {
                    videoImg.css('margin-top', top + 'px');
                }
            }
        });
    }

    function GetMapDirection() {
        if (!ValidateInputText('txtDirections', 'spndirections'))
            return false;
        else {
            $('#mapDirectionsPanel').show();
            getMapDirections($('#txtDirections').val());
        }
    }

    function sticky_relocate() {
        var window_top = $(window).scrollTop();
        var div_top = $('#sticky-anchor').offset().top;
        if (window_top > div_top) {
            $('.topscrollbar').removeClass('topscrollbarhide');
            $('.topscrollbar').addClass('topscrollbarshow');
            $('.sidebar-contact-vendor-wrapper').addClass('pos_fix');
            $('.sidebar-contact-vendor-wrapper').removeClass('pos_stc');
            $('.sidebar-contact-vendor-wrapper .topscrollbar').addClass('stick');
            $('#sticky-anchor').height($('sidebar-contact-vendor-wrapper topscrollbar').outerHeight());

        } else {
            $('.sidebar-contact-vendor-wrapper .topscrollbar').removeClass('stick');
            $('.sidebar-contact-vendor-wrapper').addClass('pos_stc');
            $('.sidebar-contact-vendor-wrapper').removeClass('pos_fix');
            $('#sticky-anchor').height(0);
            $('.topscrollbar').removeClass('topscrollbarshow');
            $('.topscrollbar').addClass('topscrollbarhide');
        }
    }

    $(function () {
        $(window).scroll(sticky_relocate);
        sticky_relocate();
    });

    
</script>
