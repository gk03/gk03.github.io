<script type="text/javascript">

    function BindFolder(BookMarkValue)
    {
        //LoadLoaderConentPage('outer-main-width', false);
        var action = '/ajax-call/Others/BindFolderByUserId/'
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: {UserId:0,FolderType:'vendor'},
            success: function (data) {
                if (data.AcknowledgeType == 1) {
                    PopulateBookmarkFolder(data.list.BookmarkFolders);
                    //RemoveLoaderConentPage('outer-main-width');
                    if(BookMarkValue!="")
                    {
                        var myVar = setInterval(function () { ToAddFavouratePopUp(Number(BookMarkValue)); clearInterval(myVar); }, 600);
                    }

                }
            }
        });
    }

    $(window).bind("load", function () {
        var BookMarkValue = '';
        if (BookMarkValue != "" &&  0!=0) {
            BindFolder(BookMarkValue);

            // AlertMessage("Selected item has been bookmarked successfully.", '');
        }
        else if(0!=0)
            BindFolder('');
    });

    function PopulateBookmarkFolder(data)
    {
        var script = "";
        var bookmarktype='vendor';
        if(data.length>0)
        {
            for (var i = 0; i < data.length; i++) {
                script = script + "<li style='padding: 5px 5%;' id='listFolder_"+data[i].Id+"'>"+
                    '<input type="checkbox" class="tvcheckbox" checked="checked" id="' + data[i].Id + '" value="' + data[i].FolderName +'" >' +
                                "<label id='lblId' style='margin-left:10px;' >" + data[i].FolderName + "</label> </li>";
            }
            $("#folder_list").html('').append(script);
        }

    }

    function AddFoldertoDiv() {
        if ($('#txtfolderName').val().replace(/^\s+|\s+$/g, "") != "" && $('#txtfolderName').val().replace(/^\s+|\s+$/g, "") != null) {

            if (ValidateSpecialCharacter('txtfolderName', 'spnfoldername')) {
                $("#spnfoldername").hide();


                tempcount = tempcount + 1;

                var FolderName = $('#txtfolderName').val().replace(/^\s+|\s+$/g, "");

                var  script = "<li style='padding: 5px 5%;' id='listFolder_"+tempcount+"'>"+
                    '<input type="checkbox" class="tvcheckbox" checked="checked" id="' + tempcount + '" value="' + FolderName +'" >' +
                                   "<label id='lblId' style='margin-left:10px;' >" + FolderName + "</label> </li>";

                $("#folder_list").append(script);

                $('#txtfolderName').val('');
                $("#spnfoldername").text("");
                $("#spnfoldername").hide();
            }
        }
        else {
            $("#spnfoldername").text("Add Folder Name.");
            $("#spnfoldername").show();
        }
    }
    function ValidateSpecialCharacter(txtid, spnid) {
        var txtct = $('#' + txtid).val();
        txtct = txtct.replace(/^\s+|\s+$/g, "");
        if (txtct != "") {
            if (txtct.indexOf('"') != -1) {
                $("#" + spnid).text('Double Quotes are Not Allow.');
                $("#" + spnid).show();
                return false;
            }
            else {
                $("#" + spnid).text('');
                $("#" + spnid).hide();
            }
            if (txtct.indexOf('\\') != -1) {
                $("#" + spnid).text('Back Slash Is Not Allowed.');
                $("#" + spnid).show();
                return false;
            }
            else {
                $("#" + spnid).text('');
                $("#" + spnid).hide();
                return true;
            }
        }
        else {
            $("#" + spnid).text('');
            $("#" + spnid).hide();
            return true;
        }
        return true;
    }

    function SaveFolderandBookMark()
    {
        if(ValidateFolder())
        {
            var elem="";
            $("#folder_list input:checkbox:checked").each(function() {
                elem =  $(this).val()+"," + elem;
            });
            elem = elem.replace(/,$/, "");

            AddBookMark($("#BookMark").attr("attrrId"),elem);
        }
    }
    function ValidateFolder()
    {
        if($("#folder_list input:checkbox").length==0)
        {
            $("#spnfoldername").text("Add Folder Name.");
            $("#spnfoldername").show();
            return false;
        }
        else if($("#folder_list input:checkbox:checked").length==0)
        {
            $("#spnfoldername").text("Please Select Folder Name.");
            $("#spnfoldername").show();
            return false;
        }
        else{
            $("#spnfoldername").text("");
            $("#spnfoldername").hide();
            return true;
        }

        return true;
    }

    function BookmarkItem(resId)
    {
        if( $('.divFavourate_'+resId).attr("isremove")=='0')
        {
            ValidateUserLogin(resId);
        }
        else{
            RemoveBookMark(resId);
        }
    }

    function BookmarkItemV2(resId, resType)
    {
        if( $('.divFavourate_'+resId).attr("isremove")=='0')
        {
            bksubType=resType;
            //alert(resType);
            ValidateUserLogin(resId);
        }
        else{
            RemoveBookMark(resId);
        }
    }

    function FancypopBookmarkItem(resId, resType)
    {
        FancyPopupTracking();
        if( $('.divFavourate_'+resId).attr("isremove")=='0')
        {
            bksubType=resType;
            ValidateUserLogin(resId);
        }
        else{
            RemoveBookMark(resId);
        }
    }

