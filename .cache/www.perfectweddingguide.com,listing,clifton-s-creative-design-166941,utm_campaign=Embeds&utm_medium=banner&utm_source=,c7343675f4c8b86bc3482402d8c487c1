                <script>
                    $(document).ready(function () {

                        // Configure/customize these variables.
                        var showChar = 400;  // How many characters are shown by default
                        var ReviewsshowChar = 200;  // How many characters are shown by default
                        var ellipsestext = "...";
                        var moretext = "Read more";
                        var lesstext = "Read less";


                        $('.more').each(function () {
                            var content = $(this).html();
                            content = content.trim();
                            if (content.length > showChar) {

                                var c = content.substr(0, showChar);
                                var h = content.substr(showChar, content.length - showChar);

                                var html = c + '<span class="moreellipses">' + ellipsestext + '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink  fa fa-arrow-circle-down" id="description">' + moretext + '</a></span>';

                                $(this).html(html);
                            }

                        });

                        $("#description").click(function () {
                            if ($(this).hasClass("less")) {
                                $(this).removeClass("morelink fa fa-arrow-circle-up");
                                $(this).addClass("morelink fa fa-arrow-circle-down");
                                $(this).removeClass("less");
                                $(this).html(moretext);
                            } else {
                                $(this).removeClass("morelink fa fa-arrow-circle-down");
                                $(this).addClass("morelink fa fa-arrow-circle-up");
                                $(this).addClass("less");
                                $(this).html(lesstext);
                            }
                            $(this).parent().prev().toggle();
                            $(this).prev().toggle();
                            return false;
                        });

                        $('.reviewmore').each(function () {
                            var content = $(this).html();
                            content = content.trim();
                            if (content.length > ReviewsshowChar) {
                                var c = content.substr(0, ReviewsshowChar);
                                var h = content.substr(ReviewsshowChar, content.length - ReviewsshowChar);
                                var html = c + '<span class="moreellipses">' + ellipsestext + '&nbsp;</span><span class="morecontent"><span>' + h + '</span>&nbsp;&nbsp;<a href="" class="morelink  fa fa-arrow-circle-down reviewdescription">' + moretext + '</a></span>';
                                $(this).html(html);
                            }
                        });

                        $(".reviewdescription").click(function () {
                            if ($(this).hasClass("less")) {
                                $(this).removeClass("morelink fa fa-arrow-circle-up");
                                $(this).addClass("morelink fa fa-arrow-circle-down");
                                $(this).removeClass("less");
                                $(this).html(moretext);
                            } else {
                                $(this).removeClass("morelink fa fa-arrow-circle-down");
                                $(this).addClass("morelink fa fa-arrow-circle-up");
                                $(this).addClass("less");
                                $(this).html(lesstext);
                            }
                            $(this).parent().prev().toggle();
                            $(this).prev().toggle();
                            return false;
                        });
                    });

                    function ValidatePhoneNumberV1(txtId, spnID) {
                        var txtval = $("#" + txtId).text().replace(/^\s+|\s+$/g, "");

                        if (txtval != null && txtval != "") {
                            var newValue = txtval.replace('-', '');
                            var newvalue11 = newValue.replace('-', '');
                            var newValue22 = newvalue11.replace('.', '');
                            var newValue33 = newValue22.replace('.', '');
                            var newValue1 = newValue33.replace('(', '');
                            var newValue2 = newValue1.replace(')', '');
                            var newValue3 = newValue2.replace(' ', '');
                            var len = newValue3.replace(/^\s+|\s+$/g, "").trim().length;

                            if (len != 10) {
                                $("#" + txtId).html('');
                                $('.fa-phone').hide();
                                $("#" + txtId).hide();
                                return false;
                            }
                            else {
                                if (isNumber(newValue3) && Number(newValue3) > 0) {
                                    var appnd = " (" + newValue3.slice(0, -7) + ") " + newValue3.slice(3, -4) + "-" + newValue3.substr(6);
                                    $("#" + txtId).html(appnd);
                                    return true;
                                }
                                else {
                                    $("#" + txtId).html('');
                                    $('.fa-phone').hide();
                                    $("#" + txtId).hide();
                                    return false;
                                }
                            }
                        }
                        else {
                            $("#" + txtId).html('');
                            $('.fa-phone').hide();
                            $("#" + txtId).hide();
                            return false;
                        }

                        return true;
                    }

                    $(function () {
                        ValidatePhoneNumberV1('mobworkphone', '');
                    });

                </script>       



                                    

