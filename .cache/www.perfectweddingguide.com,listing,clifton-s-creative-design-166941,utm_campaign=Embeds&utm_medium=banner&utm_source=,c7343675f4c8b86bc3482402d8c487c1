<script>
    function ContactToVendor(name) {
        var vendorname = unescape(name);
        var vendor_name = vendorname.split('+').join(' ');
        VendorContactsPopup(vendor_name);
        EventAnalytic('Button', 'Contact Vendor', '');
    }

    function VendorContactsPopup(Name) {
        $("#uiTo").html('');
        for (var i = 0; i < Name.split(',').length; i++) {
            var script = "";
            if (i == 0)
                script = script + "<li class='livendor' id='liVendor_" + Name.split(',')[i].split('_')[1] + "'><div><div style='float:left;'>" + Name.split(',')[i].split('_')[0] + "</div><div class='clear'></div></div></li>";
            else {
                script = script + "<li class='livendor' id='liVendor_" + Name.split(',')[i].split('_')[1] + "'><div style='margin-top:5px;'><div style='float:left;'>" + Name.split(',')[i].split('_')[0] +
               "</div><div style='float: left; margin: -5px 6px;'><img onclick='RemoveVendorName(\"" + Name.split(',')[i].split('_')[1] + "\")'  class='compare-cross-img-ico' src='/images/compare-cross-ico.png'></div><div class='clear'></div></div></li>";
            }

            $("#uiTo").append(script);
        }

        OpenVendorPopup();
    }

    function OpenVendorPopup() {
        resetContactControlPopup();

        $('#Contactpopup').addClass('popup_block');
        $('#Contactpopup').css('display', 'none');
        OpenPopup('Contactpopup', 600, true);
        ForMobileDisplayContactUs();
    }

    function ForMobileDisplayContactUs() {
        if ($('.Rtop-menu-outer').is(':visible')) {
            $('#Contactpopup').css("width", "276px");
            $('#Contactpopup').css("margin-left", "-148px");
            $('#Contactpopup').css("margin-top", " -190.5px");
            $('.btn_close').css("margin-left", "255px");
        }
    }

    function SendContactPopupInfo() {
        var status = true;
        if (!ValidateFirstName('txtFirstName2', 'spnFirstName2'))
            status = false;

        if (!ValidateLastName('txtLastname2', 'spnLastName2'))
            status = false;

        if (!ValidateWeddingdate('txtWeddingdate2', 'spnWeddingdate2'))
            status = false;

        if (!ValidateNewEmail('txtemailaddress2', 'spnemailaddress2'))
            status = false;

        if (!ValidatePhoneNumber('txtphonenumber2', 'spnphonenumber2'))
            status = false;

        //if (!ValidateTextBoxLimit('txtContent2', 'spncontent2', 'Please enter comment', 'Please enter a comment at least 25 characters', 25))
        //    status = false;

        if (!status) {
            return status;
        }
        var firstname = $("#txtFirstName2").val().replace(/^\s+|\s+$/g, "");
        var lastname = $("#txtLastname2").val().replace(/^\s+|\s+$/g, "");
        var weddingdate = $("#txtWeddingdate2").val();
        var Email = $("#txtemailaddress2").val().replace(/^\s+|\s+$/g, "").toLowerCase();
        var phonenumber = $("#txtphonenumber2").val().replace(/^\s+|\s+$/g, "");
        var Contents = $("#txtContent2").val().replace(/^\s+|\s+$/g, "");
        var listingIds = '166941';

        var items = new Array();
        items = listingIds.split(",");

        var requestData = {
            ListingIds: items,
            VendorEmail: { FirstName: firstname, LastName: lastname, Email: Email, Phone: phonenumber, WeddingDate: weddingdate, Questions: Contents }
        };

        LoadLoaderConentPage('content-list-txt-space', false);
        OnContactClosePopup();

        var action = '/ajax-call/WeddingVendors/SendEmailRequestV2/';
        $.ajax({
            type: 'POST',
            url: action.toLowerCase(),
            data: JSON.stringify(requestData),
            contentType: 'application/json',
            success: function (data) {
                if (data.data.AcknowledgeType == 1) {
                    AlertMessage('Email sent successfully.');
                    //AlertMessage(data.data.Message, "");
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                    resetContactControlPopup();
                }
                else if (data.data.AcknowledgeType == 2) {
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                    OnContactClosePopup();
                    //AlertMessageCloseExistingPopup(data.data.Message, "");
                }
                else {
                    OnContactClosePopup();
                    $('.content-list-txt-space').find(".ajax_overlay").remove();
                    if (data.data.AcknowledgeType == undefined) {

                    }
                }
            }
        });
    }

    function OnContactClosePopup() {
        ClosePopup();
        resetContactControlPopup();
    }

