        quality = 5; responsiveness = 5; professionalism = 5; flexibility = 5; costtovalue = 5;

        $("#quality").raty('score', quality);
        $("#responsiveness").raty('score', responsiveness);
        $("#professionalism").raty('score', professionalism);
        $("#flexibility").raty('score', flexibility);
        $("#costtovalue").raty('score', costtovalue);
        var avgrating = parseInt(quality) + parseInt(responsiveness) + parseInt(professionalism) + parseInt(flexibility) + parseInt(costtovalue);
        $('#spnAvgRatingvlaue').html('');
        $('#spnAvgRatingvlaue').html(parseFloat(avgrating / 5).toFixed(1) + '/5.0');
    }
    function GetFBLikeButtonV3(url) {
        var script = '<iframe src="http://www.facebook.com/plugins/like.php?href=' + url +
            '&amp;layout=standard&amp;show_faces=false&amp;action=like&amp;colorscheme=light&amp' +
            'scrolling="no" frameborder="0" style="overflow: hidden;width: auto;" allowtransparency="true"></iframe>';
        return script;
    }

    function VendorLikeUs(href) {
        if (href != null && href != "") {
            href = href.replace(/^\s+|\s+$/g, "");
            var script = "";
            script = GetFBLikeButtonV3(href);
            $("#divFBlike").html('');
            $("#divFBlike").html(script);
        }
    }

    function FancyBindPhotoDetails(id) {
        var pCredit = $('#photoDetail_' + id).attr('photoCredit');
        var pName = $('#photoDetail_' + id).attr('photoName');

        

        
        if (pName == undefined) {
            pName = '';
        }

        var pDescription = $('#photoDetail_' + id).attr('photoDescription');

        while (pDescription.indexOf("%27") > -1) {
            pDescription = pDescription.replace("%27", "'");
        }

        var imgUrl = $('#photoDetail_' + id).attr('imgUrl');
        var rootPath = "http://www.perfectweddingguide.com/";

        if (pCredit == '' || pCredit == null || pCredit == undefined) {
            $('#fancy_creditmain').hide();
        }
        else { $('#fancy_creditmain').show(); }

        $('#fancy_photocredit').text(pCredit);
        $('#vendorsDivText').text(pName);
        $('#fancy_photoDescription').text(pDescription);

        $('#SSControl').show();

        
        var shareUrl = "http://www.perfectweddingguide.com/" + "listing/" + 'clifton-s-creative-design-166941' + "/photos/" + id;
        $('.WIA-social-twittebg').attr('onclick', 'TweetByGetBitly("' + shareUrl + '","' + pName + '")');
        $('.WIA-social-fbbg').attr('onclick', 'PhotoFBShare("' + shareUrl + '")');
        $('.WIA-social-pinbg').attr('onclick', 'VendorPinInterest("' + shareUrl + '","' + imgUrl + '","' + pDescription + '")');
        $('.WIA-social-gpbg').attr('onclick', 'PhotoShareGoogle("' + shareUrl + '")');
        VendorLikeUsFancyBox(shareUrl);
        EventAnalytic('gallery-photo', 'view', '');
    }

    function PhotoFBShare(url) {
        return OpenFBShareWindow(url);
    }

    function VendorLikeUsFancyBox(href) {
        if (href != null && href != "") {
            href = href.replace(/^\s+|\s+$/g, "");
            var script = "";
            script = GetFBLikeButtonV3(href);
            $("#divFBlikeFancy").html('');
            $("#divFBlikeFancy").html(script);
            if ('detail' == "videos") {
                $("#video_divFBlike").html('');
                $("#video_divFBlike").html(script);
            }
        }
    }

