<div class="mobSwitch" onclick="location.href='/ajax-call/home/SetCookieForCssSwitch/'" id="divSwitchCSS"><span id="SwitchDiv">View Desktop Site</span></div>
<script type="text/javascript">
    function GetMobileBrowser() {
        if (jQuery.browser.mobile) {
            return 1;  // 1 for Mobile.
        }
        else if (isiPhone() || isAndroidTablet()) {
            return 2;   // 2 for Ipad Tab
        }
        else {
            return 0;  // 0 for Web.
        }
    }
    function SwitchCss(currentScope) {
        if (GetMobileBrowser() == 1) {
            if (currentScope.replace(/^\s+|\s+$/g, "") == "Mob") {
                $("#SwitchDiv").text("View Desktop Site");
            }
            else if (currentScope.replace(/^\s+|\s+$/g, "") == "Web") {
                $("#SwitchDiv").text("View Mobile Site");
            }
        }
        //else if (GetMobileBrowser() == 2) {
        //    if (currentScope.replace(/^\s+|\s+$/g, "") == "Tab") {
        //        $("#SwitchDiv").text("View Desktop Site");
        //    }
        //    else if (currentScope.replace(/^\s+|\s+$/g, "") == "Web") {
        //        $("#SwitchDiv").text("View Tab Site");
        //    }
        //}
    }
    var currentScope = 'Web';
    SwitchCss(currentScope);
</script>



</div>
