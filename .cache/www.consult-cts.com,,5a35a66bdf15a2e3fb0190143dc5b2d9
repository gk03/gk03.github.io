<script type="text/javascript">
	$(function() {

		// Make the images cycle
		$('#cycler').cycle({
			delay: 0, // additional delay for first transition
			timeout: 9000, // milliseconds between transitions
			speed: 1000, // duration of transition
			pause: 0 // pause on hover (set to 1 to enable)
		});
		
		// Make our tabs be tabby
		$('#cts_values div.tab_content').hide(); // Hide all divs
		$('#cts_values div.tab_content:first').show(); // Show the first div
		$('#cts_values ul li:first').addClass('selected'); // Set the class of the first link to active
		$('#cts_values ul li a').click(function() { //When any link is clicked
			$('#cts_values ul li').removeClass('selected'); // Remove active class from all links
			$(this).parent().addClass('selected'); //Set clicked link class to active
			$(this).blur(); // Don't show stupid outline in IE
			var currentTab = $(this).attr('href'); // Set variable currentTab to value of href attribute of clicked link
			$('#cts_values div.tab_content').hide(); // Hide all divs
			//$('#cts_values div.tab_content').fadeOut(); // Hide all divs
			$(currentTab).show(); // Show div with id equal to variable currentTab
			return false;
		});
	});
</script><meta name="google-site-verification" content="bg-APs1yCzSXsAziruo8vxBmErr9GPe_sILRp59Htho" />
