  $('a[href="' + directory + '"]').css('color', 'black');
  $('a[href="' + currentPage + '"]').css('color', 'black');
  $('#mainnav > li > a').each(function() {    
    var link = $(this).attr('href');
    if (!link) link = $(this).text().toLowerCase()
    if (link.indexOf('#') != -1) 
      link = link.substr(0, link.indexOf('#'));
    if (currentPage.indexOf(link) != -1 && link != '') {
      $(this).parent('li').addClass('currentTab');
      $(this).css('color', 'white');
      return;
    }
  });
