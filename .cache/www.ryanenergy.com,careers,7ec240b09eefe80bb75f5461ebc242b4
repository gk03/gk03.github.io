    $(document).on('click', '.hideAll', function () {
        $('.targetDiv').hide();
        $('.button').removeClass('selected');
    });

    $(document).on('click', '.showSingle', function () {
        $('.targetDiv').hide();
        $('#div' + $(this).attr('target')).show();

        $('.button').removeClass('selected');
        $('#btn' + $(this).attr('target')).addClass('selected');
    });

    $(document).on('click', '.showPace', function () {
        $('.targetDiv').hide();
        $('.button').removeClass('selected');

        $('.buttonH').removeClass('selected');
        $('#btnH' + $(this).attr('target')).addClass('selected');

        $('.targetHorz').hide();
        $('#horz' + $(this).attr('target')).show();
    });
