<script type="text/javascript">
(function ($) {

   <!-- notifications close -->
   $(".box-close-btn").on("click", function() {
      var me = $(this);
      var noti = me.closest(".scode_notification");
      noti.fadeOut("slow", function() {
         noti.remove();
      });
      return false;
   });

   <!-- anchors -->
   $(".anchor").on("click", function() {
      var headerHeight = $(".header").outerHeight() + 50;
      var me = $(this);
      var id = me.attr("href");
      $('html,body').animate({scrollTop: $(id).offset().top - headerHeight},'slow');
      return false;
   });


   <!-- modal tabs -->
   $(".modal-menu-tabs a").on("click", function() {
      var me = $(this);
      var modalnav = me.closest(".modal-menu-tabs");
      modalnav.find("a").removeClass("active");
      me.addClass("active");
   });
   
   var $lightbox = $('#lightbox');

   $('[data-target="#lightbox"]').on('click', function(event) {
     var $img = $(this).find('img'), 
         src = $img.attr('src'),
         alt = $img.attr('alt');
         
     $lightbox.find('img').attr('src', src);
     $lightbox.find('img').attr('alt', alt);
   });

   $lightbox.on('shown.bs.modal', function (e) {
     var $img = $lightbox.find('img');
     var $imgWidth = $img.width();
     
     $lightbox.find('.modal-dialog').css({'width': $imgWidth});
   });

}(jQuery));
</script>


