<script type="text/javascript">
$(document).ready(function(){
    $(window).scroll(function(){ //onscroll
        var scrolleddown = false;//used to keep the state
        if($(window).scrollTop() > 1){ //if they've scrolled down
            if(scrolleddown == false){
                scrolleddown = true;
                
				$('#fixedElement').stop(true, false).animate({top:'400'},500)
				$('#blog').stop(true, false).animate({top:'400'},500)
				
            }
        }else{
            //below resets everything back to original state when user scrolls back up
            scrolleddown = false;
                
				  $('#fixedElement').css("position","fixed").stop(true, false).animate({top:'200'},500)
				   $('#blog').css("position","fixed").stop(true, false).animate({top:'200'},500)
				
				  
        }
    });
});
</script>
 <script type="text/javascript">

   //Created / Generates the captcha function    
    function draw()
    {
        var a = Math.ceil(Math.random() * 10)+ '';
        var b = Math.ceil(Math.random() * 10)+ '';       
        var c = Math.ceil(Math.random() * 10)+ '';  
        var d = Math.ceil(Math.random() * 10)+ '';  
        
        var code = a + b+ c+d;
		
       document.getElementById('test1').value = code;
    }

  
    
 
    </script>
