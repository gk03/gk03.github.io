<script type="text/javascript">
    $().ready(function () {
        $('#loginModal').dialog({autoOpen: false, resizable: false, draggable: false, modal: true, height: 300, width: 450, open: function () { $(this).parent().appendTo('form'); SetKeyPress(); } });
        function SetKeyPress() {
            $("form input").keypress(function (e) {
                if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                    $('input[type=image] .defaultSubmit').click();
                    return false;
                } else {
                    return true;
                }
            });
        }
    });
    
    $(document).ready(function () {
        $("input").keypress(function (e) {
            if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
                $('.defaultSubmit').click();
                return false;
            } else {
                return true;
            }
        });
    });
</script>
