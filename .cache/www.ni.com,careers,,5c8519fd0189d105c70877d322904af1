<script language="JavaScript1.2" src="/javascript/language/_goto_page_by_locale_2012.js" type="text/javascript"></script>
	<script language="javascript">
        $(function() {
            var bcObj = jQuery.parseJSON($('#breadcrumbsJSON').val()),
            crumbsList = '';
            if (typeof(bcObj)!== 'undefined' && bcObj!==null)
            {
                bcLength = bcObj.breadcrumbs.length;
                // Collect the Breadcrumbs and construct markup
                $(bcObj.breadcrumbs).each(function(i) {
                    var $this = $(this),
                        label = $this[0].label,
                        link = $this[0].link;
                        if((i+1) == bcLength){
                            crumbsList += label;
                        }else{
                            crumbsList += '<a href="'+link+'">'+label+'</a> > ';
                        }
                });
            }
            // Write out the breadcrumbs to container
            $('.global-header-breadcrumb').html(crumbsList);
        });
</script>
