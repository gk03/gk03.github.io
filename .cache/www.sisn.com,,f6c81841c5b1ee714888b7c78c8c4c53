	(function( $ ) {
			$('.hub-tabs ul.tabs li').click(function(){
				var tab_id = $(this).attr('data-tab');

				$('.hub-tabs ul.tabs li').removeClass('active');
				$('.hub-tabs .tabContent').hide().removeClass('active')

				$(this).addClass('active');
				$("#"+tab_id).slideDown().addClass('active');
			})

