<script>


	$(document).ready(function(){
	    $("#mainNav_button").click(function(){
	    	var height = $(window).height() + "px";

	        $("#sideNavigation").toggle();
	        $("#sideNavigation").css("overflow", "auto");
	        $("#sideNavigation").css("height", height);
	    });
	});

	$(document).mouseup(function (e)
	{
	    var container = $("#sideNavigation");

	    if (!container.is(e.target) // if the target of the click isn't the container...
	        && container.has(e.target).length === 0) // ... nor a descendant of the container
	    {
	        container.hide();
			$("body").css("position", "relative");
			$("body").css("overflow", "auto");	        
	    }
	});

	/*$(document).ready(function(){
		if ($("#sideNavigation").css('display') == 'block') {
			var value = false;
			$("#sideNavigation").click(function() {
			 	value = false;
			 });
			$("#body").click(function() {
				if (value) {
				    $("#sideNavigation").hide();
				   	$("body").css("position", "relative");
				 	$("body").css("overflow", "auto");
			 	}
			 });
		}
	});*/

	$(document).ready(function(){
		if ($(window).size() < 768) {
			$("body").click(function() {
		        if ($("#sideNavigation").css('display') == 'block') {
		        	$("body").css("position", "fixed");	
		        } else {
		    		$("body").css("position", "relative");
		        	$("body").css("overflow", "auto");
		        }
			});
		}
	});

	$(document).ready(function(){
		$("#exit_nav").click(function() {
	        $("#sideNavigation").hide();
		});
	});

	$(document).ready(function(){
		$(".menu-item").click(function() {
			if ($(window).width() < 768) {
				$(this).find(".sub-menu").slideToggle("slow");
			}
		});
	});

	$(window).resize( function() {
		if ($(window).width() > 767) {
			$(".sect").css("display", "inline-block");
		} else if ($(window).width() < 768 && $(window).width() > 400) {
			$(".sect").hide();
		}
	});

	$(document).ready(function() {
		$(".accorHead").click(function() {
			if ($(window).width() < 768) {

				var item = $(this).find(".sect");

				item.slideToggle('fast', function() {
					if (item.is(':visible')) {
        				item.css('display','inline-block');
					}
				})
			}
		})
	});
		

	$(window).resize(function() {
		if ($(window).width() > 767) {
			$("#sideNavigation").hide();
		}
	});

	$(window).resize(function() {
	    var height = $(window).height() + "px";
	    $("#sideNavigation").css("height", height);
	});

</script>

<script type="text/javascript">
  $(document).ready(function($) {
    $('#accordion').find('.accordion-toggle').click(function(){

      //Expand or collapse this panel
      $(this).next().slideToggle('fast');

      //Hide the other panels
      $(".accordion-content").not($(this).next()).slideUp('fast');

    });
  });
</script>

