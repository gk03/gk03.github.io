<script>

// Adjust on Page Load
$(document).ready(function(){
	sliderResponsiveAdjust();
});

// Adjust on Page Resize (and mobile turns)
$(window).on('resize',function(){
	sliderResponsiveAdjust();
});

// Function Size Map
function sliderResponsiveAdjust() {
	
	// get slider element dimentions
	sliderWidth = $("#slider").width();
	sliderHeight = $("#slider").height();
	//alert(sliderWidth);
	
	/********************************************************/
	
	maxWidth = 669;
	if (maxWidth > window.innerWidth) {
		maxWidth = window.innerWidth;
	}
	//alert(maxWidth);
	
	maxHeight = 302;
	
	// get adjustment ratio (padding) for menu menu
	adjustment = 40; 
	ratio = maxWidth / (sliderWidth + adjustment);
	// round ratio to 3 decimals
	ratio = Math.round(ratio * 1000) / 1000;
	//alert(ratio);
	
	if (ratio < 0.945) {
	
		// resize slider
		$("#slider").css({
			"-webkit-transform" 				: "scale(" + ratio + ")",
			"-moz-transform" 						: "scale(" + ratio + ")",
			"-ms-transform" 						: "scale(" + ratio + ")",
			"-o-transform" 							: "scale(" + ratio + ")",
			"transform" 								: "scale(" + ratio + ")"
		});
		
		// adjust position and margin
		marginAdjustment = 5; 
		marginAdjust = (Math.round(maxHeight - (maxHeight * ratio))) - marginAdjustment;
		//alert(marginBottom);
		
		$("#slider").css({
			"margin-bottom"			 				: "-" + marginAdjust + "px",
			"top"			 									: "-20px"
		});		
		
	} else {
		
		// resize slider
		$("#slider").css({
			"-webkit-transform" 				: "scale(1)",
			"-moz-transform" 						: "scale(1)",
			"-ms-transform" 						: "scale(1)",
			"-o-transform" 							: "scale(1)",
			"transform" 								: "scale(1)"
		});
		
		// adjust bottom margin
		$("#slider").css({
			"margin-bottom"			 				: "35px",
			"top"			 									: "0px"
		});		
		
	}
	
	/********************************************************/
	
}
</script>

