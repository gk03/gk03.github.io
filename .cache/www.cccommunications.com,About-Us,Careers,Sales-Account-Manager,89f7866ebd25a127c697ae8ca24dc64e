<script type="text/javascript">
    jQuery().ready(function ($) {

        function HoverOver7528ca61ec() {

            $(this).addClass('rmhover').find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOut7528ca61ec() {
            $(this).removeClass('rmhover').find(".subMenu:first").animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        function HoverOversub7528ca61ec() {
            $(this).find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutsub7528ca61ec() {
            $(this).find(".subMenu:first").stop().animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        var config7528ca61ec = {
            sensitivity: 1,
            interval: 100,
            over: HoverOver7528ca61ec,
            timeout: 1000,
            out: HoverOut7528ca61ec
        };
        var configsub7528ca61ec = {
            sensitivity: 1,
            interval: 100,
            over: HoverOversub7528ca61ec,
            timeout: 1000,
            out: HoverOutsub7528ca61ec
        };
        $("#gomenu7528ca61ec li.dir").hoverIntent(config7528ca61ec);
        $("#gomenu7528ca61ec .subMenu li.dir").hoverIntent(configsub7528ca61ec);
        $("#gomenu7528ca61ec").find("li.current").each(function (i) {
            $(this).find(".subMenu:first").show();
        });
    });
</script>
 
 


