  $(document).ready(function() {

    $('.task').click(function() {
      var sectionId = $(this).data('id');
      var selectedSection = $('#' + sectionId);

      selectedSection.show();
      $('.js-task').each(function() {
        if($(this).attr('id') != sectionId) {
          $(this).hide();
        }
      });
      window.location.href = '#' + sectionId;
    });

    $('.js-task').first().show();

    $('#short-lead-form').validate();

    var flashMessage = 'null';

    if(flashMessage) {
      var flashMessageObj = JSON.parse(flashMessage);
      toastr.success(flashMessageObj.text, '', { positionClass: 'toast-top-center', closeButton: true });
    }

  });
  </script>
