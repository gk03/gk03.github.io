    <script>
        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + "Domain=.fecon.com;  Path=/; " + expires;
            document.cookie = cname + "=" + cvalue + "; " + expires; //set it on both domains
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }
        function delete_cookie(name) {
            //console.log("delete");
            document.cookie = name + '=; ' + "Domain=fecon.com;  Path=/; " + 'expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }
        function sync() {
            window.setTimeout(synchronizeDomainWithCookie, 600);
        }
        function synchronizeDomainWithCookie() {

            var parts = location.hostname.split('.');
            var subdomain = parts.shift();
            var upperleveldomain = parts.join('.');
            var domain = subdomain;
            var needsRedirect = false;
            var transCookie = getCookie("googtrans");
            switch (transCookie) {
                case "/en/ru":

                case "/en/de":

                case "/en/pt":

                case "/en/fr":

                case "/en/es":

                    delete_cookie("googtrans");
                    redirect = window.location.href;
                    var domainIndex = redirect.indexOf(domain);
                    var newDomain = transCookie.replace("/en/", "");
                    if (newDomain != domain) {
                        if (domain == "fecon" || domain == "")
                            newDomain = newDomain + ".fecon";

                        redirect = redirect.replace(redirect.substring(domainIndex, domain.length + domainIndex), newDomain);
                        needsRedirect = true;
                    }
                    break;
                default:

                    //else{
                    //       redirect = Request.Url.ToString();
                    //     domainIndex = redirect.IndexOf(domain);
                    //    redirect = redirect.Remove(domainIndex, domain.Length);

                    //    Response.Redirect(redirect);
                    // }
                    break;
            }
            switch (domain) {
                case "ru":
                case "de":
                case "pt":
                case "fr":
                case "es":
                    setCookie("googtrans", "/en/" + domain, 1);
                    break;
                case "fecon":
                case "":
                case "Www":
                    //setCookie("googtrans", "", 1);
                    break;
            }
            if (needsRedirect) {
                delete_cookie("googtrans");
                window.location.replace(redirect);
            }
            //console.log(domain);
        }



    </script>
