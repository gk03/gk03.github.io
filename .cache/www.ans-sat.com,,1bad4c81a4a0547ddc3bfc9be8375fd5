<script language="javascript" type="text/javascript">
	var popUpWin=0;
	function popUpWindow(URLStr, left, top, width, height)
	{
	  if(popUpWin)
	  {
		if(!popUpWin.closed) popUpWin.close();
	  }
	  popUpWin = open(URLStr, 'popUpWin', 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,copyhistory=yes,width='+width+',height='+height+',left='+left+', top='+top+',screenX='+left+',screenY='+top+'');
	}
	
	function popUpWindowCenter(URLStr, width, height)
	{
//	  if(popUpWin)
//	  {
//		if(!popUpWin.closed) popUpWin.close();
//	  }
	  var top = (screen.availHeight / 2) - (height / 2);
	  var left = (screen.availWidth / 2) - (width / 2);
	  window.open(URLStr, '', 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,copyhistory=yes,width='+width+',height='+height+',left='+left+', top='+top+',screenX='+left+',screenY='+top+'');
	}
</script>

<script type="text/javascript">
function showhide2(id, show) {
this.src="/icons/up_arrow.jpg";
if (el = document.getElementById(id)) {
if (null==show) show = el.style.display=='none';
el.style.display = (show ? '' : 'none');
}

if (el = document.getElementById(id+'icon')) {
if (null==show) show = el.src.display=='/icons/down_arrow.jpg';
el.src = (show ? '/icons/up_arrow.jpg' : '/icons/down_arrow.jpg'); 
} 

}
</script>




