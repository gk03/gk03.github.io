	<script type="text/javascript">
	timerval = setInterval("slideSwitch()", 14000 );
	
	$(document).ready(function(){
		
		$('.panelAreaLeft h3:first').addClass("active");
		$('div.panelAreaRight:eq(0)> div:not(:first)').hide(); 
	
		$('.panelAreaLeft h3').click(function(){
			$('div[id$="PanelBlock"]').hide();
			$("div[id="+$(this).attr("id")+"PanelBlock]").show();
			$(this).addClass("active");
			$(this).siblings("h3").removeClass("active");
			//reset timer so they get 5 seconds on current slide.
			clearInterval(timerval);
			timerval = setInterval("slideSwitch()", 14000 );
		});						
	});
	
	function slideSwitch() {
		$('div[id$="PanelBlock"]').hide();
		
		if($("h3.active").attr("id")=="Services") {
			$('div.panelAreaRight:eq(0)> div:first').show();
			$(".panelAreaLeft h3:first").addClass("active");
			$("h3.active:last").removeClass("active");
		} else {
			$("div[id="+$("h3.active").next("h3").attr("id")+"PanelBlock]").show();
			$("h3.active").next("h3").addClass("active");
			$("h3.active:first").removeClass("active");
		}
	}

	</script>
