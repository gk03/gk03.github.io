<script>
$(function(){
   // Add click-event listener
   $(".menu li a").click(function(){
      // Remove the current class from all a tags
      $(".menu li a").removeClass("current");
      // Add the current class to the clicked a
      $(this).addClass("current");
   });
});
</script>

