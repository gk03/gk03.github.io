<script type="text/javascript">
$ = jQuery;
			$(document).ready(function() {
			var currentTime = new Date()
			var year = currentTime.getFullYear()
			$(".year-container").html(year);


				function GetUrlValue(VarSearch) {
					var SearchString = window.location.search.substring(1);
					var VariableArray = SearchString.split('&');
					for(var i = 0; i < VariableArray.length; i++) {
						var KeyValuePair = VariableArray[i].split('=');
						if(KeyValuePair[0] == VarSearch) {
							return KeyValuePair[1];
						}
					}
				}

				var utm_source = GetUrlValue('utm_source');
				var utm_medium = GetUrlValue('utm_medium');
				var utm_campaign = GetUrlValue('utm_campaign');
				var utm_term = GetUrlValue('utm_term');
				var utm_content = GetUrlValue('utm_content');
				var gclid = GetUrlValue('gclid');


if(utm_source == 'facebook')
{
//console.log(utm_source);

 $.cookie('teleNumber', '1', { expires: 7 });

}
if(utm_source == 'source_inline')
{
//console.log(utm_source);

$.cookie('teleNumber', '2', { expires: 7 });
}
if(utm_source == 'google')
{
//console.log(utm_source);

$.cookie('teleNumber', '3', { expires: 7 });

}

//console.log( $.cookie('teleNumber') );
//$('#firstNumber').html($.cookie('teleNumber'));

				if(utm_source != null || utm_medium != null || utm_campaign != null || utm_term != null || utm_content != null || gclid != null) {
					var formData = 'utm_source=' + utm_source + '&utm_medium=' + utm_medium + '&utm_campaign=' + utm_campaign + '&utm_term=' + utm_term + '&utm_content=' + utm_content + '&gclid=' + gclid;

					$.ajax({
						url : "custom_scripts/setSessionForAds.php",
						type : "POST",
						data : formData,
						success : function(data) {
							 //alert(data);
							//data - response from server
						}
					});

				}

			});


		</script>
