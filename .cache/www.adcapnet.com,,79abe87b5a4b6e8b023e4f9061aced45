				<script type="text/javascript">

			        jQuery(function($) {

			        var doMeAfterLoad = function() {
			            /*
			            * Jumbotron Videos List
			            */

			                        var jumboVideos = [ [{ type: "video/mp4",  src: "https://s3-us-west-2.amazonaws.com/extrateam-videos/roboto.mp4" },{ type: "video/webm", src: "https://s3-us-west-2.amazonaws.com/extrateam-videos/roboto.webm" },{ type: "video/ogg",  src: "https://s3-us-west-2.amazonaws.com/extrateam-videos/roboto.ogv" }] ]



			            var jumboCurrentSlide = 0;

			            var getNextSlide = function(){
			                if(jumboCurrentSlide + 1 >= jumboVideos.length){
			                    jumboCurrentSlide = 0;
			                } else
			                {
			                    jumboCurrentSlide++;
			                }
			                return jumboCurrentSlide;
			            }
			            var getPreviousSlide = function(){
			                if(jumboCurrentSlide <= 0){
			                    jumboCurrentSlide = jumboVideos.length-1;
			                } else
			                {
			                    jumboCurrentSlide--;
			                }
			                return jumboCurrentSlide;
			            }

			            var updateJumboGUI = function() {
			                for(var i=0; i<jumboVideos.length; i++) {
			                    if(i != jumboCurrentSlide) {
			                        $('#jumbo'+ i).fadeOut();
			                    }
			                    else
			                    {
			                       $('#jumbo'+i).fadeIn('slow');
			                    }
			                }
			            }

			            var videoCompleteFunc = function(){
			                BV.show(jumboVideos[getNextSlide()],{ ambient:true });
			                updateJumboGUI();
			            };

			            var resizeHomePage = function() {
			                var box1Height = $(window).height();
			                var mastHeadHeight = $('.site-header').height();
			                $('#box2').css('top', box1Height + 'px');
			                $('.scroll-more-anchor').css('top', ( ( box1Height - mastHeadHeight) + 'px' )  );
			            }
			            $( window ).resize(resizeHomePage);

			/*
			            $('#myCarousel').carousel({
			                interval: 10000
			            });

			            $('#myCarousel').on('slid.bs.carousel', function() {
			                    //alert("slid");
			            });
			*/


			            var BV = new $.BigVideo({useFlashForFirefox:false});
			            BV.container = $('#box1');
			            BV.init();


			                    var isWin7, isWinXP = false;
			        var isIE8, isIE10 = false;
			        var isFF29, isFF30 = false;
			        var ffversion = '';
			        var ua = navigator.userAgent.toLowerCase();
			        isWinXP = ua.indexOf('windows nt') > 0;
			        isWin7 = ua.indexOf('windows 6.1') > 0;
			        //detect IE8
			        function IsIE8Browser() {
			    			var rv = -1;
			    			var ua = navigator.userAgent;
			    			var re = new RegExp("Trident\/([0-9]{1,}[\.0-9]{0,})");
			    			if (re.exec(ua) != null) {
			        			rv = parseFloat(RegExp.$1);
			    			}
			    			return (rv == 4);
						}

					isIE8 = IsIE8Browser();

			        //detect ie10
			        /*@cc_on
			            if (/^10/.test(@_jscript_version)) {
			                isIE10 = true;
			        	}
			    	@*/

			        //detect ff
			        ffversion = '29';
			        isFF29 = navigator.userAgent.toLowerCase().indexOf('firefox/'+ffversion) > -1;

			        ffversion = '30';
			        var isFF30 = navigator.userAgent.toLowerCase().indexOf('firefox/'+ffversion) > -1;

			        if (Modernizr.touch || isIE10 || isIE8 || (isWin7 && isFF30) || (isWin7 && isFF29) || (isWinXP && isFF30) || (isWinXP && isFF29)   ) {
			                resizeHomePage();
			                $('#box2').show();
			                $('#big-video-wrap').fadeIn('slow');
			                updateJumboGUI();
			                //add screen size check and replace image
			                BV.show('video-poster.jpg');
			                resizeHomePage();
			            }

				    else {
			                BV.show(jumboVideos[0],{ ambient:true });

			                BV.getPlayer().on("ended", videoCompleteFunc);

			                BV.getPlayer().on('durationchange',function(){
			                    $('#box2').show();
			                    $('#big-video-wrap').fadeIn('slow');
			                    updateJumboGUI();
			                });

			                $('.jumbo_next_btn').click(function() {
			                    BV.show(jumboVideos[getNextSlide()],{ ambient:true });
			                    updateJumboGUI();
			                });

			                $('.jumbo_prev_btn').click(function() {
			                    BV.show(jumboVideos[getPreviousSlide()],{ ambient:true });
			                    updateJumboGUI();
			                });

			                resizeHomePage();
			            }



			        }

			       // Pace.on('done',  doMeAfterLoad);
			            doMeAfterLoad();



			    });

			</script>


			</div>


		</div>

