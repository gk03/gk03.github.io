<script type="text/javascript">

function togglerfix(){
	var tgg1 = document.getElementById("toggler1").style;
	tgg1.display="none";
	var tgg2 = document.getElementById("toggler2").style;
	tgg2.display="none";
	var tgg3 = document.getElementById("toggler3").style;
	tgg3.display="none";
	
	var acc1 = document.getElementById("acco1").style;
	acc1.display="block";
	var acc2 = document.getElementById("acco2").style;
	acc2.display="block";
	var acc3 = document.getElementById("acco3").style;
	acc3.display="block";
}

