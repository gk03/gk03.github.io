     <script type="text/javascript">
jQuery(document).ready(function($) {
		$("input:radio[name=cup_customer]").click(function() {
		var customer = $(this).val();
		if( $(".cup_toggle").hasClass('cup_red') )
			$(".cup_toggle").removeClass('cup_red');
		if( customer == 'Y' ){
			$('#cup_lead,.cup-forms .ninja-forms-form-wrap,#cup_alt').hide();
			$('#cup_cond_1,#cup_submit').show();
		}
		else{
			$('.cup_cond,#cup_submit').hide();
			$('#cup_lead,.cup-forms .ninja-forms-form-wrap,#cup_alt').show();
		}
	});
	
	$("input:radio[name=crm_customer]").click(function() {
		var customer = $(this).val();
		if( $(".crm_toggle").hasClass('crm_red') )
			$(".crm_toggle").removeClass('crm_red');
		if( customer == 'Y' ){
			$('#crm_lead,.crm-forms .ninja-forms-form-wrap,#crm_alt').hide();
			$('#crm_cond_1,#crm_submit').show();
		}
		else{
			$('.crm_cond,#crm_submit').hide();
			$('#crm_lead,.crm-forms .ninja-forms-form-wrap,#crm_alt').show();
		}
	});
	
	$('<input>').attr({
		type: 'hidden',
		id: 'cup_other',
		name: 'cup_other'
	}).appendTo('.cup-forms .ninja-forms-form-wrap form');
	
	$('<input>').attr({
		type: 'hidden',
		id: 'crm_other',
		name: 'crm_other'
	}).appendTo('.crm-forms .ninja-forms-form-wrap form');
	
	$("#cup_lookup_form").submit(function(){
		// diff steps
		//1 - lookup
		//2 - actions
		var ssn = $("input#cup_serialNumber").val();
		var zip = $("input#cup_postalCode").val();
		
		var step = $("input#cup_step_1").length;
		var queryurl = "http://www.cccis.com/wp-admin/admin-ajax.php?action=ajax_handler&cup_step=1&security=6255f513e4&cup_serialNumber="+ssn+"&cup_postalCode="+zip;
		if( !$('input:radio[name=cup_customer]').is(":checked")){
			$(".cup_toggle").addClass('cup_red');
			return false;	
		}
		else if( $('#lrm-2').length > 0 ){
			//repair methods page
			//if( ssn 	
			
		}
		else if( step > 0 ){
			$.ajax( {
				async: false,
				url: queryurl,
				dataType:'json',
				success: function( data ){
					//console( data );
					if( data.error){
						if( data.error == 'invalid' )
							$('#cup_message').text('Please enter a valid software security number and postal code.');	
						else if( data.error == 'confirmed' )
							$('#cup_message').text('You have already confirmed your details.');
						else
							$('#cup_message').html('<span>Sorry, we can&rsquo;t reach your account at this time. Please fill out <a href="http://www.cccis.com/collision-repairers/get-ccc-one-for-your-shop/">this form</a> and a CCC ONE representative will contact you in 24-48 hours.</span>');
					}
					else{
						/*if( data.mso == true ){ //true
							$('#cup_lookup_form #cup_message').text('');
							$('#cup_lookup_form #cup_message_2').html('<span>At this time you are not eligible to order UpdatePlus on the website. Please go <a href="http://www.cccis.com/collision-repairers/get-ccc-one-for-your-shop/">here</a> to contact your sales representative.</span>');
							$('#cup_lookup_form #cup_radio input').attr('disabled',true);
							$('#cup_lookup_form #cup_step_1').remove();
							$('#cup_lookup_form #cup_cond_1').hide();
							$('#cup_lookup_form #cup_cond_2').show();
							$('#cup_lookup_form input#cup_companyName').val( data.companyName );
							$('#cup_lookup_form input#cup_email').val( data.email );
							$('#cup_lookup_form input#cup_city').val( data.city );
							$('#cup_lookup_form input#cup_state').val( data.state );
							$('#cup_lookup_form input#cup_address').val( data.address );
							$('#cup_lookup_form input#cup_contactPhone').val( data.contactPhone );
							$('#cup_lookup_form input').attr('disabled','true');
							
						}
						else{*/
							$('#cup_lookup_form #cup_message').text('');
							$('#cup_lookup_form #cup_radio input').attr('disabled',true);
							$('#cup_lookup_form #cup_step_1').remove();
							$('#cup_lookup_form #cup_cond_1').hide();
							$('#cup_lookup_form #cup_cond_2').show();
							$('#cup_lookup_form input#cup_ID').val( data.id );
							$('#cup_lookup_form input#cup_companyName').val( data.companyName );
							$('#cup_lookup_form input#cup_email').val( data.email );
							$('#cup_lookup_form input#cup_city').val( data.city );
							$('#cup_lookup_form input#cup_state').val( data.state );
							$('#cup_lookup_form input#cup_address').val( data.address );
							$('#cup_lookup_form input#cup_contactPhone').val( data.contactPhone );
						//}
					}
				}
			});

			return false;
		}
		else{
			var filled = true;
			$('#cup_lookup_form .cup_req').each(function(i){
				if( $(this).val() == '' ){
					filled = false;
					$(this).addClass('cup_missing');
				}
				else{
				 	if( $(this).hasClass('cup_missing') )
						$(this).removeClass('cup_missing');	
				}
			});
			if( !filled ){
				$('#cup_lookup_form  #cup_message').text('All fields are required.');
				return false;
			}
			
			var email = $('input#cup_email').val();
			var queryeurl = "http://www.cccis.com/wp-admin/admin-ajax.php?action=ajax_handler&email_check=1&email="+email;
			
			var valid = true;
			$.ajax({
				async: false,
				url: queryeurl,
				dataType:'json',
				success: function( data ){
					if( data.valid == false ){
						$('input#cup_email').addClass('cup_missing');
						$('#cup_message').text('Invalid Email Address');
						valid = false;	
					}
				}			
			});
			return valid;
			
			/*
			$.ajax({
    async: false,
    url: "url",
    success: function() {
        b();
    }
});
*/
		}
	});
	
});
	</script>
