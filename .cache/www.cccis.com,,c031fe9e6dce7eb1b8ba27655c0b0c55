    <script>
    
    	$(".entry img, div.textcol img").addClass("img-responsive");

    	$(".entry img").addClass("lazy");    	
    	$(".lazy").lazyload({ threshold : 25, effect : "fadeIn" });
    	
    	/*	javascript media queries - it's not enough 
    		to hide the iframe, we want to prevent the video scripts from loading */
    	if (document.documentElement.clientWidth >= 768) {
			//alert(document.documentElement.clientWidth);
			$('.videocontainer iframe').each(function(){
				var iframevideosrc = $(this).data('src');
				$(this).attr('src',iframevideosrc);
			});
		}
		
		$('form.ninja-forms-form input:submit').not('form#ninja_forms_form_15 input:submit').click(function(){
			var faction = $(this).parents('form.ninja-forms-form').attr('action');
			if( faction.indexOf("ninja_forms_ajax_submit")!= -1 ){
				console.log('found');
			}
			else{
				if( $(this).attr("disabled") == true ){
					//do nothing
				}
				else{
					$(this).attr("disabled", true);
					$(this).parents('form').submit();
				}
				return false;
			}
		});
		$('form.ninja-forms-form').not('form#ninja_forms_form_15').submit(function(){
			var faction = $(this).attr('action');
			if( faction.indexOf("ninja_forms_ajax_submit")!= -1 ){
				console.log('found');	
			}
			else{
				if( $(this).children('input:submit').attr("disabled") != true ){
					$(this).children('input:submit').attr("disabled", true);
				}
				else{
					return false;
				}
			}
		});
		
	
	</script>

	<script>
    	var pathname = window.location.pathname;
    	/* create regexp to match current url pathname 
    	and remove trailing slash if present as it could collide with the link in navigation in case trailing slash wasn't present there */
    	urlRegExp = new RegExp(pathname.replace(/\/$/,'') + "$"); 
		if (urlRegExp != "/$/") {
			$("div.navbar-wrapper a, div.level2navcontainer a, ul.nav a, ul.contact-nav a").each(function(){
				if(urlRegExp.test(this.href.replace(/\/$/,''))){
					$(this).addClass('active');
				}
			});
		}
	</script>

