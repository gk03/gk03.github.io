                <!-- ### Begin Contact jQuery Scripts ### -->
                <script>

                    $("#-xx-contact_form #contact_phone").click(function () {
                        $("#contact_form #contact_phone").mask("(999) 999-9999");
                    });

                    $("#contact_form #contact_send").click(function () {

                        $.get("/__webservice/sendmail/index.php",
                            {
                                contact_name: $("#contact_form #contact_name").val(),
                                contact_email: $("#contact_form #contact_email").val(),
                                contact_phone: $("#contact_form #contact_phone").val(),
                                contact_company: $("#contact_form #contact_company").val(),
                                contact_message: $("#contact_form #contact_message").val()
                            },

                            function (data) {
                                //console.log(data);

                                $("#site_message").fadeIn("slow");
                                $("#site_message .message").html(data.substring(1));

                                // * Clear fields if Success
                                if (data.substring(0, 1) == "1") {
                                    $("#contact_form #contact_name").val("Name");
                                    $("#contact_form #contact_email").val("Email");
                                    $("#contact_form #contact_phone").val("Telephone");
                                    $("#contact_form #contact_company").val("Company Or Organization");
                                    $("#contact_form #contact_message").val("What do you want to say?");
                                }

                            });

                    });
                
                </script>
                <!-- ### End Contact jQuery Scripts ### -->

