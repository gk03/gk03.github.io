<!-- ### Begin script for hover overs ### -->
    <script>
        // Hides all content areas
        function hideall() {
            $("#cap1").hide();
            $("#cap2").hide();
            $("#cap3").hide();
            $("#cap4").hide();
            $("#cap5").hide();
        };

        // Changes all icons back to original color
        function changecolorall() {
            $("#icon_link1 .front img").attr('src', '/images/icons/tools.png');
            $("#icon_link2 .front img").attr('src', '/images/icons/knowledge.png');
            $("#icon_link3 .front img").attr('src', '/images/icons/social.png');
            $("#icon_link4 .front img").attr('src', '/images/icons/results.png');
        };

        $("#icon_link1").click(function () {
            hideall();
            changecolorall();
            $("#icon_link1 .front img").attr('src', '/images/icons/tools-re.png');
            $("#cap2").fadeIn(500);
            //$("#icon_info1").fadeIn(200);
        });

        $("#icon_link2").click(function () {
            hideall();
            changecolorall();
            $("#icon_link2 .front img").attr('src', '/images/icons/knowledge-re.png');
            $("#cap3").fadeIn(500);
            //$("#icon_info2").fadeIn(200);
        });

        $("#icon_link3").click(function () {
            hideall();
            changecolorall();
            $("#icon_link3 .front img").attr('src', '/images/icons/social-re.png');
            $("#cap4").fadeIn(500);
            //$("#icon_info3").fadeIn(200);
        });

        $("#icon_link4").click(function () {
            hideall();
            changecolorall();
            $("#icon_link4 .front img").attr('src', '/images/icons/results-re.png');
            $("#cap5").fadeIn(500);
            //$("#icon_info4").fadeIn(200);
        });
        

    </script>
<!-- ### End script for hover overs ### -->
