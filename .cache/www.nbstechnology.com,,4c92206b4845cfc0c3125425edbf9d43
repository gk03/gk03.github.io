<!--[if lte IE 7]><script type="text/javascript" src="http://www.nbstechnology.com/wp-content/plugins/multi-level-navigation-plugin/scripts/suckerfish_ie.js"></script><![endif]--><script>jQuery(document).ready(function() {
	jQuery("ul.sf-menu").superfish({
		animation:     {opacity:"show",height:"show"},  // fade-in and slide-down animation
		delay:        0,                            // delay on mouseout
		speed:        "fast",  // animation speed
		autoArrows:   "",  // enable generation of arrow mark-up
