function setwidth(){
  
   jQuery.noConflict();
   
   myHeight = jQuery(window).height();
   
   if(myHeight < 800){
      myHeight = 800;
   }
   
   mheight = myHeight - 213;
   
   document.getElementById('mainbody').style.height = mheight+'px';
   document.getElementById('mainbody').style.overflowY = 'scroll';
}

</script>

