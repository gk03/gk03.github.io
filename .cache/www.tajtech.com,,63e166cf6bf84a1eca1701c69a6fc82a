 <script type="text/javascript">
 jQuery.noConflict();
(function($) { 
  $(function() {
 $(document).ready(function(){
 $('#mainbodyhomepage').mouseenter(function(){
		$('.contentactive .moduleItemIntrotext-custom').stop();
 		$('.contentactive .moduleItemIntrotext-custom').css('bottom', '-268px');
		$('.contentactive').hide();
		
 });
  		
  
  $('#searchlink1').mouseenter(function(){
		$('.contentactive .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('.contentactive').hide();
		});
		$('#subcontenta1').addClass('contentactive');
		$('#subcontenta1').show(function(){
			$('#subcontenta1 .moduleItemIntrotext-custom').stop().animate({bottom: 0}, 300);
		});

  });
  
   $('#subcontenta1').mouseleave(function(){

  		$('#subcontenta1 .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('#subcontenta1').hide();
		});
  });
			//dropdowncontent.init("searchlink1", "left-top", 1000, "mouseover")
  		
  
  $('#searchlink2').mouseenter(function(){
		$('.contentactive .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('.contentactive').hide();
		});
		$('#subcontenta2').addClass('contentactive');
		$('#subcontenta2').show(function(){
			$('#subcontenta2 .moduleItemIntrotext-custom').stop().animate({bottom: 0}, 300);
		});

  });
  
   $('#subcontenta2').mouseleave(function(){

  		$('#subcontenta2 .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('#subcontenta2').hide();
		});
  });
			//dropdowncontent.init("searchlink2", "left-top", 1000, "mouseover")
  		
  
  $('#searchlink3').mouseenter(function(){
		$('.contentactive .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('.contentactive').hide();
		});
		$('#subcontenta3').addClass('contentactive');
		$('#subcontenta3').show(function(){
			$('#subcontenta3 .moduleItemIntrotext-custom').stop().animate({bottom: 0}, 300);
		});

  });
  
   $('#subcontenta3').mouseleave(function(){

  		$('#subcontenta3 .moduleItemIntrotext-custom').stop().animate({bottom: -268}, 300, function(){
			$('#subcontenta3').hide();
		});
  });
			//dropdowncontent.init("searchlink3", "left-top", 1000, "mouseover")
    });
    });
})(jQuery);
