								<script type="text/javascript">
								function ShowHideState() {
									if ($("#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_Country").val() == 'US') {
										$('#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_StateCountyContainer').removeClass('invisible');
										$('#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_StateCounty').attr('required', true);
									}
									else {
										$('#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_StateCountyContainer').addClass('invisible');
										$('#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_StateCounty').removeAttr('required');
									}
								}
				
								function PostPopUpForm_422DA1B0A01B526869EA8041B6DBC7FB(PopupID) {
									if ($('#pp_full_res #PopUpForm422DA1B0A01B526869EA8041B6DBC7FB').valid()) {
										var formData = $('#pp_full_res #PopUpForm422DA1B0A01B526869EA8041B6DBC7FB').serialize();
										$.ajax({
											type: 'post',
											url: '/cfcs/popups.cfc?method=processPopupForm&returnFormat=json&id=422DA1B0-A01B-5268-69EA8041B6DBC7FB',
											data: formData,
											dataType: "json",
											success: function(response) {
												//console.log(response);
												if (response.SUBMISSIONOK) {
													
													$.prettyPhoto.close();
												}
												else {
													var text = '<div style="text-align:center;">Sorry!<br />';
													for (i = 0; i < response.MESSAGES.length; i++) {
														text += response.MESSAGES[i] + "<br />";
													}
													text += "</div>";
													$('#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_inner').html(text);
												}
											},
											error: function(ErrorMsg) {
												console.log(ErrorMsg);
											}
										})
									}
								};
				
								$(document).ready(function() {
									var ShowNotification = true;
									
									var structStorage = {};
									structStorage.DisplayCount = 0;
									structStorage.VisitCount = 0;
									
											if (SessionStorageGet('PopUp_422DA1B0A01B526869EA8041B6DBC7FB')) {
												var structStorage = JSON.parse(SessionStorageGet('PopUp_422DA1B0A01B526869EA8041B6DBC7FB'));
											}
										

									structStorage.VisitCount = structStorage.VisitCount + 1;

									
										var listDisplayCount = [2,5,10];
										if (listDisplayCount.indexOf(structStorage.VisitCount) == -1) {
											ShowNotification = false;
										}
									

									if (ShowNotification) {
										setTimeout(function() {
											$.fn.prettyPhoto({
												theme: 'light_rounded',
												allow_resize: true,
												
												callback: function() {/*alert('Prettyphoto closed');*/}
												,
												markup: GetPrettyPhotoMarkup()
											});
											$.prettyPhoto.open('#422DA1B0A01B526869EA8041B6DBC7FB_container');
										}, 5000);

										
											$(document).on('keyup change',"#pp_full_res #422DA1B0A01B526869EA8041B6DBC7FB_Country",function() {
												ShowHideState();
											})
											
											$('#pp_full_res #PopUpForm422DA1B0A01B526869EA8041B6DBC7FB').validate();
										

									}

									var myDate = new Date();
									structStorage.LastDisplayed = myDate.toISOString();
									if (ShowNotification) {
										structStorage.DisplayCount = structStorage.DisplayCount + 1;
									}
									
											SessionStorageSet('PopUp_422DA1B0A01B526869EA8041B6DBC7FB', JSON.stringify(structStorage));
										

									
						
								});
