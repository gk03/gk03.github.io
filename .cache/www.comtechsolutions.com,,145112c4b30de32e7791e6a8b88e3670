		function Active(Name) {
			if (document.images) {
				document[Name].src = eval("act_" + Name + ".src")
			}
		}
		
		function Passive(Name) {
			if (document.images) {
				document[Name].src = eval("psv_" + Name + ".src")
			}
		}
					function Reset() 
{
        if (document.images) { fisloaded = true; }
        Passive('home');
		Passive('products');
		Passive('services');
		Passive('partners');
		Passive('support');
		Passive('company');
