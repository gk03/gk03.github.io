<span id="hs_cos_wrapper_module_14685171824452112" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_raw_html" style="" data-hs-cos-general-type="widget" data-hs-cos-type="raw_html"><script>
var win = $(window);
var banner = $('.custom-header>.row-fluid-wrapper:nth-child(1)');
var bannerHeight = banner.outerHeight();
var menu = $('.custom-header>.row-fluid-wrapper:nth-child(2)');
var origPosition = banner.css("position");
var origWidth = banner.css("width");
win.scroll(function(e){
var scrollPos = win.scrollTop();
console.log(scrollPos + " - " + bannerHeight);
if(scrollPos <= bannerHeight){
console.log("Setting to original");
menu.css("position",origPosition);
menu.css("top",null);
menu.css("width",origWidth);
}else{
console.log("Setting new position");
menu.css("position","fixed");
menu.css("top","0");
menu.css("width","100%");
}
});
</script></span>
