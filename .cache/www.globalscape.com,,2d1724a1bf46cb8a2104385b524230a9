  <script type="text/javascript">
      function startOlark() {
          olark('api.box.expand');
      }
      olark('api.chat.onOperatorsAvailable', function() {

          var availableButton = document.getElementById('available-button');
          var awayButton = document.getElementById('away-button');

          if (availableButton) {
              availableButton.style.visibility = 'visible';
          }
          if (awayButton) {
              awayButton.style.visibility = '';
          }
      });
      olark('api.chat.onOperatorsAway', function() {
          var availableButton = document.getElementById('available-button');
          var awayButton = document.getElementById('away-button');

          if (availableButton) {
              availableButton.style.visibility = '';
          }
          if (awayButton) {
              awayButton.style.visibility = 'visible';
          }
      });
  </script>

