<script>

function validate(frm)
{

if(frm.name1.value=='' || frm.name1.value=='Name')
  {
	alert("Please enter your name");
	frm.name1.focus();
    return false;
  }
 
 

if(frm.email.value=='' || frm.name1.value=='Email')
  {
	alert("Please enter e-mail address");
	frm.email.focus();
    return false;
  }
  
  var emailstr = frm.email.value;

  if(frm.email.value.length >= 1)
	{
	 var emailPat = /^[_a-zA-Z0-9-]+(\.[_a-zA-Z0-9-]+)*@[a-zA-Z0-9-]+(\.[a-zA-Z0-9-]+)*(\.[a-zA-Z]{2,3})$/;
	 var matchArray = emailstr.match(emailPat);
	 if (matchArray == null)
	    {
	    alert("Your e-mail address is Invalid");
	    frm.email.focus();
	    return false;
	    }
	}
}

function clr_email(ele)
{
	
	if(ele.value=='Email')
	{
	ele.value = '';
	}
}

 function fill_email(ele)
{
	
	if(ele.value=='')
	{
	ele.value = 'Email';
	}
}



function clr_name(ele)
{
	
	if(ele.value=='Name')
	{
	ele.value = '';
	}
}

 function fill_name(ele)
{
	
	if(ele.value=='')
	{
	ele.value = 'Name';
	}
}

function clr_comment(ele)
{
	
	if(ele.value=='Leave your comment here')
	{
	ele.value = '';
	}
}

function fill_comment(ele)
{
	if(ele.value=='')
	{
		ele.value = 'Leave your comment here';
	}
}
 </script>
 
