          var url='demandplanning.cfm';
          popUpWin1 =
          window.open(url,'NewWindow1','toolbar=0,location=no,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width=480,height=480');
     }

     else if(WhereTo == 'Supply Planning')
