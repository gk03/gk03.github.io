     {
          var url='financialplanning.cfm';
          popUpWin3 =
          window.open(url,'NewWindow3','toolbar=0,location=no,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width=480,height=480');
     }
     
     else if(WhereTo == 'Privacy Statement')
     {
          var url='privacystatement.cfm';
          popUpWin3 =
          window.open(url,'NewWindow3','toolbar=0,location=no,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width=480,height=480');
     }

     if (navigator.appName=='Netscape')
     {
          popUpWin.focus();
     }

}
