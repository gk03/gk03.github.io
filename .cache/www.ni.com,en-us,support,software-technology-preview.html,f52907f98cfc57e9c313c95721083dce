	        var matches = /(?:^|;)[ \t\r\n]*locale=([-a-zA-Z]+)(?:;|$)/.exec(document.cookie);
	    	var locale = matches && matches[1];
	    	if (locale) {
	    		var redirectTarget;
	    		if (redirectInfo.coveredLocales.indexOf(locale) == -1) {
	    			// User locale is not covered by this page.
	    			for (var i=0; i<redirectInfo.redirects.length; i++) {
	    				var redirect = redirectInfo.redirects[i];
	    				if (redirect.coveredLocales.indexOf(locale) >= 0) {
	    					redirectTarget = redirect.url;
	    					break;
	    				}
	    			}
	    			if (redirectTarget) {
	    				var location = document.location.href;
	    				document.location.replace(redirectTarget + document.location.search + document.location.hash);
	    				if (sessionStorage) {
	    					sessionStorage.setItem('ni-analytics.redirecting-page', location);
	    					sessionStorage.setItem('ni-analytics.original-referrer', document.referrer);
	    				}
	    			}
	    		}
	    	}
	    })();
