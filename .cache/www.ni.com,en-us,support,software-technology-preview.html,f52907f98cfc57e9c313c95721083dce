<script>
	(function() {
        window.fireStartOnce = false;
        window.fireEndOnce = false;
        window.addEventListener("message", function(event) {
            var data = event.data;
            var type = data.type;
            var mediaLength = data.mediaLength; 
            var mediaOffset = data.mediaOffset;
            var mediaID = data.mediaID;
            var mediaFriendly = data.mediaFriendly;
            var mediaName = data.mediaName;

            if (type === "start"){
                try {
                  if (!window.fireStartOnce) {
                      s.Media.open(mediaName,mediaLength,mediaPlayerName);
                      s.events = null;
                      s.contextData['complete'] = "";
                      s.contextData['start'] = "video";
                      s.Media.track(mediaName);
                      window.fireStartOnce = true;
                  }

                } catch (e) {
                  console.log (e);
                }
            } 
            
            if (type === "end"){
                try {
                    if (!window.fireEndOnce) {
                        s.Media.stop(mediaName,mediaOffset);
                        s.Media.close(mediaName);
                        s.events = "event16";
                        s.contextData['start'] = "";
                        s.contextData['complete'] = "video";
                        s.Media.track(mediaName);
                        window.fireEndOnce = true;
                    }

                } catch (e) {
                    console.log (e);
                }    
            }
        });
    }());
    
</script></div>
