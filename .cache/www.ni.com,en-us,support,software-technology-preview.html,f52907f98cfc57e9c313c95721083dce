<script>
   $(document).ready( function () {
     var imageSize = NI.utils.browserSize();
     if (imageSize === "xs") imageSize = "sm";
   
    $.each($(".highlight-image-card"), updateImageSource);
   
      $(".highlight-vertical-img").each(function(){
         var image_size = $(this).data(imageSize), element = $(this).children('img');
         if($(element).attr('src').indexOf(image_size) == -1){
          $(element).attr('src', $(element).attr('src').split('?')[0]);
          $(element).attr('src', $(element).attr('src') + image_size);
         }
     });
   
     /**
      * updates the padth needed for the release?
      * @param none
      * @returns true
      */
     function updateImageSource(){
       var source = $(this).data(imageSize);
       $("img", this).attr("src", source);
       return true;
     }
   
   }); 
   
</script>
