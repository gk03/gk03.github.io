<script src="/scripts/jquery.cookie.js"></script> 
<script src="/scripts/validation.js"></script> 
<script>


$(function() {
	
	
	



    if ($.cookie('GHX_optIn_firstName') != null) {
        $('#firstName').val($.cookie('GHX_optIn_firstName'));
    }
    if ($.cookie('GHX_optIn_lastName') != null) {
        $('#lastName').val($.cookie('GHX_optIn_lastName'));
    }

    if ($.cookie('GHX_optIn_emailAddress') != null) {
        $('#emailAddress').val($.cookie('GHX_optIn_emailAddress'));
    }

    if ($.cookie('GHX_optIn_businessPhone') != null) {
        $('#businessPhone').val($.cookie('GHX_optIn_businessPhone'));
    }


    if ($.cookie('GHX_typeofOrganization') != null) {
        $('#typeofOrganization').val($.cookie('GHX_typeofOrganization'));
    }



    if ($.cookie('GHX_optIn_company') != null) {
        $('#company').val($.cookie('GHX_optIn_company'));
    }

    if ($.cookie('GHX_greatestChallenge') != null) {
        $('#greatestChallenge').val($.cookie('GHX_greatestChallenge'));
    }




});

var GTITLE=document.forms["requestInfoForm"]["Gtitle"].value;


	

	
$('#requestInfoForm').validate({
	
	
   rules: {
	
		
        firstName: {
            required: true
        },
        lastName: {
            required: true
        },
        emailAddress: {
            required: true
        },
        company: {
            required: true
        },
        typeofOrganization: {
            required: true
        }




    },


    highlight: function(element, errorClass) {
        $(element).closest('.form-group-sm').addClass('has-error');

    },

    unhighlight: function(element, errorClass) {
        $(element).closest('.form-group-sm').removeClass('has-error');

    },

    submitHandler: function(form) {
		
		$('#requestButton').fadeOut();
        //alert('ajax being called..setting cookies now yo');

        $.cookie('GHX_optIn', '1', {
            path: '/'
        });
        $.cookie('GHX_optIn_firstName', $('input#firstName').val(), {
            path: '/'
        });
        $.cookie('GHX_optIn_lastName', $('input#lastName').val(), {
            path: '/'
        });
        $.cookie('GHX_optIn_emailAddress', $('input#emailAddress').val(), {
            path: '/'
        });
        $.cookie('GHX_optIn_businessPhone', $('input#businessPhone').val(), {
            path: '/'
        });
        $.cookie('GHX_typeofOrganization', $('select#typeofOrganization').val(), {
            path: '/'
        });
        $.cookie('GHX_optIn_company', $('input#company').val(), {
            path: '/'
        });
        $.cookie('GHX_greatestChallenge', $('textarea#greatestChallenge').val(), {
            path: '/'
        });

   
		
		
		if ($('input#Gtitle').val().length != 0) {
            return false;
        } 
		else{
		
			
			var dataString = 'elqSiteID=1716500231&elqFormName=SECRequestInformationForm&firstName=' + $('input#firstName').val() + '&lastName=' + $('input#lastName').val() + '&emailAddress=' + $('input#emailAddress').val() + '&businessPhone=' + $('input#businessPhone').val() + '&typeofOrganization=' + $('select#typeofOrganization').val() + '&company=' + $('input#company').val() + '&greatestChallenge=' + $('textarea#greatestChallenge').val() + '&pageTitle=' + $('input#pageTitle').val();

        $.ajax({

            type: "POST",
            url: "/PHP/SecEloquaProxy.php",
            data: dataString,
            success: function() {

                $('#formWell').fadeOut();
                $('#requestButton').fadeOut();
                $('#responseMessage').fadeIn();


            }

        });
			
			
		}
		
		
	}
});  


	


                    </script>
