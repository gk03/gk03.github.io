<script>


var end = new Date('05/23/2016 7:1 AM');

    var _second = 1000;
    var _minute = _second * 60;
    var _hour = _minute * 60;
    var _day = _hour * 24;
    var timer;

    function showRemaining() {
        var now = new Date();
        var distance = end - now;
        if (distance < 0) {

            clearInterval(timer);
            document.getElementById('SCScountdown').SCSinnerHTML = 'EXPIRED!';

            return;
        }
        var days = Math.ceil(distance / _day);
        var hours = Math.floor((distance % _day) / _hour);
        var minutes = Math.floor((distance % _hour) / _minute);
        var seconds = Math.floor((distance % _minute) / _second);

        document.getElementById('SCScountdown').innerHTML = days + ' days until ';
       
    }

    //timer = setInterval(showRemaining, 1000);


$(function() {


showRemaining()

    if ($.cookie('GHX_optIn_emailAddress') != null) {
        $('#emailAddressSubscribe').val($.cookie('GHX_optIn_emailAddress'));
    }



});




$('#subscribeForm').validate({


    rules: {

        emailAddress: {
            required: true
        }




    },


    highlight: function(element, errorClass) {
        $(element).closest('.form-group-sm').addClass('has-error');

    },

    unhighlight: function(element, errorClass) {
        $(element).closest('.form-group-sm').removeClass('has-error');

    },

    submitHandler: function(form) {

        $('#subscribeButton').fadeOut();
        //alert('ajax being called..setting cookies now yo');

      
        $.cookie('GHX_optIn_emailAddress', $('input#emailAddress').val(), {
            path: '/'
        });
       


        var dataString2 = 'elqSiteID=1716500231&elqFormName=copyOfDefaultWebDownloadForm-1445273735948&emailAddress=' + $('input#emailAddressSubscribe').val();

        $.ajax({

            type: "POST",
            url: "/PHP/SecEloquaProxy.php",
            data: dataString2,
            success: function() {

                $('#formWellSubscribe').fadeOut();
                $('#subscribeButton').fadeOut();
                $('#responseMessageSubscribe').fadeIn();


            }

        });




    }
});

</script>
