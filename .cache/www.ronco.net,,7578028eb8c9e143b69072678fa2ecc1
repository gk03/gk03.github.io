       <li class="AccordionPanel clearfix colelem100 temp_no_id" data-orig-id="u597239"><!-- vertical box --><!-- m_editable region-id="editable-static-tag-U597240-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link,clickable" --><div class="AccordionPanelTab museBGSize rgba-background clearfix colelem100 temp_no_id" data-muse-uid="U597240" data-muse-type="txt_frame" data-orig-id="u597240-8"><!-- content --><span class="placeholder" data-placeholder-for="u597240_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597240-2_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597240-3_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597240-6_content"><!-- placeholder node --></span></div><!-- /m_editable --><div class="AccordionPanelContent disn clearfix colelem100 temp_no_id" data-orig-id="u597241"><!-- column --><!-- m_editable region-id="editable-static-tag-U597242-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link" --><span class="clearfix colelem placeholder" data-placeholder-for="u597242-4_content"><!-- placeholder node --></span><!-- /m_editable --><!-- m_editable region-id="editable-static-tag-U597401-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link" --><span class="clearfix colelem placeholder" data-placeholder-for="u597401-13_content"><!-- placeholder node --></span><!-- /m_editable --></div></li>
       <li class="AccordionPanel clearfix colelem100 temp_no_id" data-orig-id="u597230"><!-- vertical box --><!-- m_editable region-id="editable-static-tag-U597231-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link,clickable" --><div class="AccordionPanelTab museBGSize rgba-background clearfix colelem100 temp_no_id" data-muse-uid="U597231" data-muse-type="txt_frame" data-orig-id="u597231-7"><!-- content --><span class="placeholder" data-placeholder-for="u597231-7_0_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597231-7_1_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597231-7_2_content"><!-- placeholder node --></span><span class="placeholder" data-placeholder-for="u597231-7_3_content"><!-- placeholder node --></span></div><!-- /m_editable --><div class="AccordionPanelContent disn clearfix colelem100 temp_no_id" data-orig-id="u597232"><!-- column --><!-- m_editable region-id="editable-static-tag-U597234-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link" --><span class="clearfix colelem placeholder" data-placeholder-for="u597234-4_content"><!-- placeholder node --></span><!-- /m_editable --><!-- m_editable region-id="editable-static-tag-U597233-BP_380" template="index.html" data-type="html" data-ice-options="disableImageResize,link" --><span class="clearfix colelem placeholder" data-placeholder-for="u597233-20_content"><!-- placeholder node --></span><!-- /m_editable --></div></li>
