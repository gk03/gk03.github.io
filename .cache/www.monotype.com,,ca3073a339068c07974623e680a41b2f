<script type="text/javascript">
    var AppSpace = AppSpace || {};
    if (AppSpace.MainPageTypeTesterDataOld == null) {
        AppSpace.MainPageTypeTesterDataOld = [];
    }
    AppSpace.pushTypeTesterDataOld = function (typeTesterData, guid) {
        AppSpace.MainPageTypeTesterDataOld.push({ 'FullData': typeTesterData, 'Id': guid });
