    <script type="text/javascript">
    function submit_form(){
        jQuery.each(jQuery("#WebToLeadForm input"), function() {
            if (jQuery(this).hasClass('nofill') && jQuery(this).val() != "") {
                jQuery(this).parent().addClass("has-error");
                jQuery(this).parent().removeClass("has-success");
            } else if ((!jQuery(this).hasClass('nofill') && jQuery(this).val() == "") || (jQuery(this).attr("type") == "email" && !jQuery(this).val().match(/^\w+(['\.\-\+]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,})+$/))) {
                jQuery(this).parent().addClass("has-error");
                jQuery(this).parent().removeClass("has-success");
            } else {
                jQuery(this).parent().addClass("has-success");
                jQuery(this).parent().removeClass("has-error");
            }
        });

        //Submit Form
        if (jQuery('#WebToLeadForm .has-error').length === 0) {
            console.log("Success");
            jQuery("#WebToLeadForm").submit();
        }
    }
    </script>

