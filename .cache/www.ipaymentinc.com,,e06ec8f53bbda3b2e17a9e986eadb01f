function getTopMenuLink() {
  var topMenuLink = $('li.dropdown > a.dropdown-toggle').filter(function(index) {  
    var currentPath = window.location.pathname;
  
    if (currentPath == "" || currentPath == "/") {
	currentPath = "/home"; //Default to /home
    }
  
    return (currentPath.toLowerCase().indexOf($(this).attr('href').toLowerCase()) == 0);
  });
  
  if (topMenuLink.length == 0) { //default to home
    topMenuLink = $('li.dropdown > a.dropdown-toggle[href="/home"]');
  }
  
  return topMenuLink;
}

function getTopMenuItem() {
  var topMenuItem = getTopMenuLink().closest('li.dropdown');
  return topMenuItem;
}

function getSubMenuLink() {
  var subMenuLink = getTopMenuItem().find('ul.dropdown-menu > li > a').filter(function(i) {
    return window.location.pathname.toLowerCase().indexOf($(this).attr('href').toLowerCase()) == 0;
  });
  
  return subMenuLink;
}

function getSubMenuItem() {
  var subMenuItem = getSubMenuLink().closest('li');
  return subMenuItem;
}

function getSubMenu() {
  var subMenu = getTopMenuItem().children('ul.dropdown-menu');
  return subMenu;
}

function getSubSubMenuLink() {
  var subSubMenuLink = getSubMenuItem().find('ul.dropdown-menu > li > a').filter(function(i) {
    return window.location.pathname.toLowerCase().indexOf($(this).attr('href').toLowerCase()) == 0;
  });
  
  return subSubMenuLink;
}

function getSubSubMenuItem() {
  var subSubMenuItem = getSubSubMenuLink().closest('li');  
  return subSubMenuItem;
}

function getSubSubMenu() {
  var subSubMenu = getSubMenuItem().children('ul.dropdown-menu');
  return subSubMenu;
}

function adjustSubMenu() {  
  var subMenu = getSubMenu();
  
  if (subMenu.length > 0 && subMenu.find('li').length > 0) {
    if (isDesktopView()) {
      $('div#Header').css('margin-bottom', '40px');
  
      centerSubMenuContents();
      centerMenuTriangleOnMenuItem();
    }
  
    else {
      $('div#Header').css('margin-bottom', '');
    
      leftAlignSubMenuContents();
    }
  }
}

function adjustSubSubMenu() {
  var subSubMenu = getSubSubMenu();
  
  if (subSubMenu.length > 0 && subSubMenu.find('li').length > 0) {
    if (isDesktopView()) {
      $('div#Header').css('margin-bottom', '80px');
      centerSubSubMenuContents();
    }
    else {
      $('div#Header').css('margin-bottom', '');
      leftAlignSubSubMenuContents();
    }
  }
}

function centerSubMenuContents() {
  var subMenu = $('.navbar .nav > li.dropdown.active > ul');
  var subMenuPaddingLeft = ($(window).width() - getSubMenuWidth())/2 + 'px';
  subMenu.css('padding-left', subMenuPaddingLeft);
}

function centerSubSubMenuContents() {
  var subSubMenu = $('.navbar .nav > li.dropdown.active > ul > li.dropdown.active > ul');
  
  var screenOrWindowWidth = $(window).width();
  /*if (deviceIsiOS()) {
    if (screen != null) {
      screenOrWindowWidth = screen.width; 
    }
  }*/
  
  if (!isBrowserSafari()) {
    var subSubMenuPaddingLeft = (screenOrWindowWidth - getSubSubMenuWidth())/2 + 'px';
    subSubMenu.css('margin-left', subSubMenuPaddingLeft);
  }
}

function leftAlignSubMenuContents() {
  var subMenu = $('.navbar .nav > li.dropdown.active > ul');
  subMenu.css('padding-left', '');
}

function leftAlignSubSubMenuContents() {
  var subSubMenu = $('.navbar .nav > li.dropdown.active > ul > li.dropdown.active > ul');
  subSubMenu.css('margin-left', '');
}

function getSubMenuWidth() {
  var subMenuWidth = 0;
  
  var subMenuItems = $('.navbar .nav > li.dropdown.active > .dropdown-menu > li');
  
  subMenuItems.each(function(i){
    subMenuWidth += $(this).width();
  });
  
  return subMenuWidth;
}

function getSubSubMenuWidth() {
  var subSubMenuWidth = 0;
  
  var subSubMenuItems = $('.navbar .nav > li.dropdown.active > .dropdown-menu > li > ul.dropdown-menu > li');
  
  subSubMenuItems.each(function(i){
    subSubMenuWidth += $(this).width();
  });
  
  return subSubMenuWidth;
}

function centerMenuTriangleOnMenuItem() {
  var topMenuItemLeft = $('.navbar .nav > li.dropdown.active').offset().left;
  var topMenuItemWidth = $('.navbar .nav > li.dropdown.active').width();
  
  var triangleLeft = topMenuItemLeft + (topMenuItemWidth / 2);
  
  $('.navbar .nav > li.dropdown.active > .dropdown-menu:after').css('left', triangleLeft);
}

//----------------------------------------

//----------------------------------------
//Code copied from custom.js to fix sticky nav bar
//when you reload the page, scrolled down

function stickyNavbar() {
  if (!isTouch()) {
      var $headerHeight = $("#header").height(), $navbarHeight = $("#stickyNavbar").height();
      
      var scrollX = $(window).scrollTop();
      if (scrollX > $headerHeight) {
          $("#stickyNavbar").removeClass("navbar-static-top").addClass("navbar-fixed-top");
          $(".large-screen #header").css({
              marginBottom: $navbarHeight
          });
      } else {
          $("#stickyNavbar").removeClass("navbar-fixed-top").addClass("navbar-static-top");
          $(".large-screen #header").css({
              marginBottom: 0
          });
      }
  }
}

function isTouch() {
  return $(window).width() < 980 ? true : false;
}
  
//----------------------------------------

function searchSite(searchText) {
  if (searchText != "") {
    var searchIndexName = "search-index";
    var searchUrl = "/search?indexCatalogue=" + 
      			searchIndexName + 
       			"&searchQuery=" +
      		 	searchText +
      			"&wordsMode=0";
    
    window.location = searchUrl;
  }
}

adjustFooter();

function adjustFooter() {  
  var footerY = 0;
  var footerHeight = 0;
  var windowHeight = 0;
  
  if ($('#Footer')[0]) {
    var footer = $('#Footer')[0];
    
    footerY = footer.getBoundingClientRect().top;
    footerHeight = $(footer).height();
    windowHeight = window.innerHeight;
    
    //The minimum Y value for the footer to not rise up
    var minFooterY = windowHeight - footerHeight;
    var footerTopMargin = parseInt($(footer).css('margin-top'));
    
    if (footerY < minFooterY) {
      footerTopMargin = minFooterY - footerY + footerTopMargin;
    }
                    
    $(footer).css('margin-top', footerTopMargin + 'px');
  }
}

function isMobileView() {
  return Modernizr.mq('(max-width: 979px)');
}

function isDesktopView() {
  return Modernizr.mq('(min-width: 980px)');
}

function deviceIsiOS() {
  var iOS = (navigator.userAgent.match(/(iPad|iPhone|iPod)/g) ? true : false);
  return iOS;
};

function isBrowserSafari() {
  var isSafari = false;
  
  //Chrome User Agent String contains "Chrome" and "Safari"
  //Safari User Agent only contains "Safari"
  isSafari = (navigator.userAgent.indexOf('Chrome') == -1) &&
    (navigator.userAgent.indexOf("Safari") > -1);
  
  return isSafari;
}

function showDisclaimerIfNecessary() {  
  var disclaimers = $('*').filter(function(){return $(this).data('disclaimer') !== undefined;});
  var disclaimerContainer = $('.foot-last.disclaimer');
  
  if (disclaimers.length > 0) {
    $('#MainWrapper').addClass('showDisclaimer');
    
    disclaimers.each(function(i) {
      var disclaimerHtml = "<div>" + $(this).data('disclaimer') + "</div>";
      
      disclaimerContainer.append(disclaimerHtml);
    });
  }
}
