		$('.animatedBanner').mouseover(function(){
			/**
			* Show the pause or play button
			* only when the mouse os over the banner
			*/
			if(animatedBanner.isPaused == true){
				// show play button
				$('#playButton').css('top', '10%');
			}else{
				// show pause button
				$('#pauseButton').css('top', '10%');
			}
			$('#playButton, #pauseButton').show();
		}).mouseleave(function(){
			// move buttons up and hide them
			$('#playButton').css('top', '-50%');
			$('#pauseButton').css('top', '-50%');
			$('#playButton, #pauseButton').hide();
		});

		</script>
