            data: {mpid: virpid},
            jsonp: 'callback',
            dataType: "jsonp",
            jsonpCallback: 'updateNumberTFN',
            success: function (returnValue) {
                console.log("ajax return value" + returnValue);
            },
            error: function (request, error) {
                console.log("error " + error);
            }
        });
    }

    function updateNumberTFN(affilateTFN) {
        var affliatesInclusionSeries = window.AFFLIATES_INCLUSION_SERIES;
        var listaffliateNo = window.AFFLIATES_NUMBERS.split(',');
        var chanNo = formatTFN(affilateTFN, '1');
        var temp = document.body.innerHTML;
        var phoneNumbers;

        if (affliatesInclusionSeries != '') {
            var test = '1?[-. ]?\\(?(' + affliatesInclusionSeries + ')\\)?[-. ]?\\d{3}[-. ]?\\\d{4}';
            phoneNumbers = temp.match(new RegExp(test, 'g'));
        } else {
            phoneNumbers = temp.match((/\b(\d{1}?[-. ]?\d{3}?[-. ]?\d{3}?[-. ]?\d{4})\b/g));
        }

        for (i = 0; i < phoneNumbers.length; i++) {
            for (j = 0; j < listaffliateNo.length; j++) {
                if (phoneNumbers[i] == listaffliateNo[j]) {
                    phoneNumbers[i] = '1-1X1-1X1-1XX1';
                }
                if (phoneNumbers[i].indexOf("(") > -1) {
                    phoneNumbers[i] = phoneNumbers[i].replace(/\(/gi, "").trim();
                }

            }
        }

        for (i = 0; i < phoneNumbers.length; i++) {
            $('body').textWalk(function () {
                this.data = this.data.replace(phoneNumbers[i], chanNo);
            });

            $("a").each(function () {
                var hrefAttribute = $(this).attr('href');
