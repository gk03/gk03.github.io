                  if (typeof hrefAttribute !== typeof undefined && hrefAttribute !== false) { 
					if (hrefAttribute.indexOf(phoneNumbers[j]) > -1) {
							$(this).removeClass( "tmo_tfn_number" );
					}
                  }   

               }); 

             	//invoca optimization
 			   var all = $('body *:not(:has("*")).tmo_tfn_number'); 

               all.each(function(){
                   if($(this).text()  && $(this).text().indexOf(phoneNumbers[j]) >-1)// &&  $(this).hasClass("tmo_tfn_number"))
    				{
                        var oldHTML = $(this).parent()[0].innerHTML;
                        if(oldHTML.indexOf("href")>-1){
                        $(this).removeClass( "tmo_tfn_number" );
                        }else{
                        var newText = oldHTML.replace("<span class=\"tmo_tfn_number\">"+phoneNumbers[j]+"</span>",phoneNumbers[j]);
