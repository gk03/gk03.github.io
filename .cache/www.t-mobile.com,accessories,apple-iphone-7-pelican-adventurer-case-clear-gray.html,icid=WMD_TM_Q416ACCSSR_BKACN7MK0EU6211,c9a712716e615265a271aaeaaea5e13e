<script type="text/JavaScript">
$(document).ready(function() {

           var accessoryServiceUrl=  "/Services/Activation/ActivationService.svc/AccessoryPrice";
            
           Clover.princingModule.getDevicePrices = function(){
            var accessoryIdValues = 'accessoryIdValue';
                
            $('.flexslider .product-card').each(function(){
                var deviceIdValue = $(this).attr('data-deviceid');
                var deviceType = $(this).attr('data-devicetype');
                if ((typeof deviceIdValue != "undefined")||(deviceIdValue != null)) {
                    if(deviceType == "accessory"){
                       accessoryIdValues =  deviceIdValue +','+accessoryIdValues;
                    }
                }
            });                           
            
            if ((accessoryIdValues !== '')&&(accessoryIdValues !== 'accessoryIdValue')) {
                    accessoryIdValues = accessoryIdValues.split(',accessoryIdValue');
                    Clover.princingModule.accessoryJsonJSquery = accessoryIdValues[0];
                Clover.accessory.accRecommendedAndRelatedPage = true ;
                setTimeout(function(){Clover.accessory.getAccessoryPrice(Clover.accessory.accessoryPageEnum.ACCRECOMMENDEDANDRELATED,Clover.princingModule.accessoryJsonJSquery);}, 100);
               }             
          };
         Clover.princingModule.getDevicePrices();
 });



</script> 
