    function removeClassInvocaTagging()
    { 
        var irpidCookie = 'invoca_session';
        var chk_irpid_URL = 'irpid';
        var isCookiexist = getCookie(irpidCookie);
        var irpidValue = decodeURIComponent((new RegExp('[?|&]' + chk_irpid_URL + '=' + '([^&;]+?)(&|#|;|$)').exec(document.URL) || [, ""])[1].replace(/\+/g, '%20')) || null;

        //invoca optimization
		var isNotLegalInvocaPage = function(){
            return ((window.location.href.indexOf('cell-phones.html') > -1 ) || (window.location.href.indexOf('internet-devices.html') > -1) || (window.location.href.indexOf('accessories.html') > -1) 
            || (window.location.href.indexOf('/cell-phones/') > -1) || (window.location.href.indexOf('/internet-devices/') > -1) || (window.location.href.indexOf('/accessories/') > -1));
        }

        if (isCookiexist || irpidValue != null) {
        var affliatesInclusionSeries = window.AFFLIATES_INCLUSION_SERIES_INVOCA;
        var phoneNumbers=window.AFFLIATES_EXCUSION_SERIES_INVOCA.split(',');
        var temp = document.body.innerHTML;
        var includedPhoneNos;

         if (affliatesInclusionSeries != '' && affliatesInclusionSeries != null) {
            var test = '1?[-. ]?\\(?(' + affliatesInclusionSeries + ')\\)?[-. ]?\\d{3}[-. ]?\\\d{4}';
            includedPhoneNos = temp.match(new RegExp(test, 'g'));
        } else {
            includedPhoneNos = temp.match((/\b(\d{1}?[-. ]?\d{3}?[-. ]?\d{3}?[-. ]?\d{4})\b/g));
        }

       if(includedPhoneNos != '' && includedPhoneNos != null){
        includedPhoneNos.push("1-800-T-MOBILE!");
        includedPhoneNos.push("1-800-T-MOBILE");
        includedPhoneNos.push("1-800-TMOBILE");
        }

     	//invoca optimization
        if (includedPhoneNos != '' && includedPhoneNos != null && !isNotLegalInvocaPage) {
             for (i = 0; i < includedPhoneNos.length; i++) {


 				$("a").each(function () {
                  var hrefAttribute = $(this).attr('href');
                  if (typeof hrefAttribute !== typeof undefined && hrefAttribute !== false) { 
					if (hrefAttribute.indexOf(includedPhoneNos[i]) > -1) {
                        if(!($(this).hasClass( "tmo_tfn_number" ))){
                           $(this).addClass( "tmo_tfn_number" );
                        }
					 }
                  }   

               });



                 var all = $('body *:not(:has("*"))'); 

                 all.each(function(){
                      var pText=$(this).text();
                      var pHTML = $(this)[0].innerHTML;


                      if(!$(this).hasClass("tmo_tfn_number") && pText.indexOf(includedPhoneNos[i]) > -1){
                          var newText = pHTML.replace(includedPhoneNos[i],"<span class=\"tmo_tfn_number\">"+includedPhoneNos[i]+"</span>");
                          $(this)[0].innerHTML=newText;
                       }
				});

             }
        }

