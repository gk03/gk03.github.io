                    if (hrefAttribute.indexOf(phoneNumbers[i]) > -1) {
                        $(this).attr('href', 'tel: ' + chanNo);
                        $(this).addClass( "tmo_tfn_number" );
                    }
                }
            });
        }
    }

    function formatTFN(nStr, txtadded) {
        nStr += '';
        x = nStr.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})(\d{4})/;

        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + '-' + '$2' + '-' + '$3');
        }
        return txtadded + '-' + x1 + x2;
    }
