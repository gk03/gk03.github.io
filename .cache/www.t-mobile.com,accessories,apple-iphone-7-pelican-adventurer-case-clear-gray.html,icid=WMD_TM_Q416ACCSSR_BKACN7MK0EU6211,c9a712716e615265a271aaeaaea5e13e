<script type="text/javascript">

$(document).ready(function() {


    Clover = Clover || {};
   Clover.accessory = Clover.accessory || {};
   Clover.accessory.onSaleLowMargin = "10";
   Clover.accessory.onSaleUpperMargin = "50";
   Clover.accessory.onSaleOverText = "over";
   Clover.accessory.onSalePercentage = "30";
   Clover.accessory.onSaleAmount = "50";
   Clover.accessory.onSaleText = "On Sale";
   Clover.accessory.onSaleOffText = "off";


    var jsonJSquery = "41b58e53-ad14-4ba3-a5ad-7db04a812382";
	var accessoryPricingUrl = "/Services/Activation/ActivationService.svc/AccessoryPrice";
	var promotionalPrice = 0;

    Clover.accessory.accPDPPage = true ;
    setTimeout(function(){Clover.accessory.getAccessoryPrice(Clover.accessory.accessoryPageEnum.ACCPDPPAGE,jsonJSquery);}, 100);

	$('.accessory-offer-text').click(function(){
	    var deviceId = $('.add-to-cart-link').attr("data-cartitemid");
	    var  envUrl = window.location.href ;
	    if(envUrl != null &&  envUrl != '')
	    {
        var  splitUrl= envUrl.split('accessories');
        var  concatUrl =splitUrl[0];
        popUp(concatUrl + 'Popup/Generic_Popup.aspx?action=additionalofferaccessory&accessory=' + deviceId , '400', '568', '45', '30', '0', '1', '1' );
	    }
	    });


	 function doPercentageRoundOff(percentageValue)
     {
         var finalPercentage = percentageValue;
         var percentBreak = finalPercentage % 10;


             if(percentBreak >= 5)
              {
                 finalPercentage = finalPercentage - (percentBreak-5);
              }
             else
              {
                 finalPercentage = finalPercentage - percentBreak;
              }

              return finalPercentage ;
     }
});

function popUp( url, height, width, top, left, toolbar, resize, scrollbars )
{
var pop = window.open( url, 'pop', 'height=' + height + ',width=' + width + ',top=' + top + ',left=' + left + ',toolbar=' + toolbar + ',resizable=' + resize + ',scrollbars=' + scrollbars );
    if( typeof window.focus != 'undefined' ) javascript: pop.focus();
}

function ExecuteEnsightenTagging(price)
{
    var productDetails = new Object();

    productDetails.productType = "accessory";
    productDetails.productID = "SUPM47661";
    productDetails.productName = $('#deviceDisplayName').html();
    productDetails.productPrice = price;

    taggingOperations.DoProductTagging(JSON.stringify(productDetails));

 }

</script>

  

