    function setClickIDCookie()
    {
        var clickID = "";

        if(location.href.indexOf('clickid') !== -1)
        {
			clickID = getParameterByName('clickid');
            setCookie("TMobileclickid", clickID, 14);

        }

    }

    function fireGlobalTagging()
    {
       var clientIP = "104.236.64.140, 198.143.38.41, 198.143.38.41, 10.75.89.239, 10.141.154.60";
       taggingOperations.globalPageTagging(clientIP);
    }
	
