    function setCookie(cookieName, cookieValue, exdays) {
        var currentDate = new Date();
        currentDate.setTime(currentDate.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + currentDate.toUTCString();
        document.cookie = cookieName + "=" + cookieValue + "; path=/;domain=t-mobile.com;" + expires;
    }

    function getCookie(cookieName) {
        var name = cookieName + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1);
            if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
    }

     /* Impact radius Code commented
    function UpdatePhoneNoBaseOnIrmpId(virpid) {
        $.ajax({
            type: "GET",
