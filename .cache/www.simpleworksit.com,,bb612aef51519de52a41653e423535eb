function handleTweets(tweets){
    var x = tweets.length;
    var n = 0;
    var element = document.getElementById('simplefeed');
    var html = '<ul>';
    while(n < x) {
      html += '<li>' + tweets[n] + '</li>';
      n++;
    }
    html += '</ul>';
    element.innerHTML = html;
}
