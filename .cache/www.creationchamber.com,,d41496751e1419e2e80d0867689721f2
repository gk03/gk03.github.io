<!-- CHANGE HOVER TO ON CLICK FOR MOBILE DEVICES
 ================================================== --> 

<script>
 var isiPad = /ipad/i.test(navigator.userAgent.toLowerCase());
 
 if (isiPad) { 
	 $("nav ul li").click(function() {
	 	event.stopPropagation();
	 	$("ul", this).toggle();
	 });
	
	$(document).click( function(){
		$('nav ul li > ul').hide();
	});
}
 </script> 

