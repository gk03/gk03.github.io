<script id="linkedin-entries-template" type="text/x-handlebars-template">
               {{#each values}}
                              <li>
                                             <a href="{{this.siteJobUrl}}">
                                               <div class="left"><img src="{{this.company.logoUrl}}" alt="{{this.company.name}}"></div>      

  
                                                            <div class="right">
                                                                           <h4 class="title">
                                                                                          {{this.position.title}}
                                                                           </h4>
                                                                           <p class="description">
                                                                                          {{this.descriptionSnippet}}
                                                                           </p>
                                                            </div>
                                             </a>
                              </li>
               {{/each}}
</script>
                              
<script type="text/javascript">

    function linkedInBlockEntries() {
               try {
                   globalLinkedInContext = {};
                   processing = 0;
                   linkedInChecker = null;
                   logoArray = {};

                   

                   Handlebars.registerHelper('logourl', function() {
                     return this.title + " " + this.description;
                   });

                   function createLinkedInEntries(context)
                   {
                       var source   = $("#linkedin-entries-template").html();
                       var template = Handlebars.compile(source);        
                       var html    = template(context);
                       $('#linkedin-entries').append(html);         
                   }

                   function runEntries()
                   {
                       if(LinkedInHelper.JOB_SEARCH_RESULTS!==null)
                       {
                           createLinkedInEntries(LinkedInHelper.JOB_SEARCH_RESULTS.jobs);    
                           clearInterval(linkedInChecker);
                       }    
                   }


                   LinkedInHelper = {
                         
                         JOB_SEARCH_RESULTS : null,

                         processJobSearch : function() {
                               LinkedInHelper.getJobSearch();
                         },
                         
                         getJobSearch: function() {
                              IN.API.Raw("/job-search:(jobs:(company:(id,name),site-job-url,description-snippet,position:(title)))?format=json&count=3&company-name=Emerson").result(function(value){
                                   LinkedInHelper.getCompanyLogoForJobsSearch(value);
                              }).error(function(error) {
                                                                                                         console.log(error);
                                                                              })
                                                                              ;
                         },
                         
                         getCompanyLogoForJobsSearch: function(jobSearchResults) {
                               var counter = jobSearchResults.jobs._count;
                               var processed = 0;
                               /*for (var i=0;i < jobSearchResults.jobs._count;i++) {
                                   var companyID = jobSearchResults.jobs.values[i].company.id;
                                   var searchLogo ='/companies/'+companyID+':(logo-url)';
                                   IN.API.Raw(searchLogo)
                                   .result(function(resValue){
                                         var logoURL=resValue.logoUrl;
                                         jobSearchResults.jobs.values[processed].company.logoUrl = logoURL;
                                         processed = processed + 1;                 
                                         if (processed == counter) {
                                               LinkedInHelper.JOB_SEARCH_RESULTS = jobSearchResults;
                                               //return  jobSearchResults;
                                         }
                                        
                                   });
                               }*/
                               for (var i=0;i < jobSearchResults.jobs._count;i++) {
                                   var companyID = jobSearchResults.jobs.values[i].company.id;
                                   var searchLogo ='/companies/'+companyID+':(id,logo-url,name)';
                                   IN.API.Raw(searchLogo)
                                   .result(function(resValue){
                                         var logoURL=resValue.logoUrl;
                                         var compID=resValue.id;
                                         for (var j=0;j < jobSearchResults.jobs._count;j++) {
                                               if(compID==jobSearchResults.jobs.values[j].company.id)
                                               {
                                                   jobSearchResults.jobs.values[j].company.logoUrl = logoURL;
                                                   console.log(resValue.name + " - " + compID + " - " + logoURL); 
                                               }
                                         }
                                         processed = processed + 1;                 
                                         if (processed == counter) {
                                               LinkedInHelper.JOB_SEARCH_RESULTS = jobSearchResults;
                                         }
                                         
                                   });
                               }
                         }
                   }

                       linkedInChecker = window.setInterval(runEntries,1000);
                       LinkedInHelper.processJobSearch();
                       
               } 
        catch(err){
                   console.log("Error: " + err.message);
               } 
        finally {
                   console.log("Ran Handlebars block LinkedIn entries.");
               }
    }

</script>                             

