function _RegisterWebPartPageCUI()
{
    var initInfo = {editable: false,isEditMode: false,allowWebPartAdder: false,listId: "{8753c99c-8a10-4abc-9a5b-a798afa70d0e}",itemId: 2,recycleBinEnabled: true,enableMinorVersioning: true,enableModeration: false,forceCheckout: true,rootFolderUrl: "\u002fen-us\u002fCareers\u002fPages",itemPermissions:{High:16,Low:196673}};
