			<script type="text/javascript">
				//jQuery('#ibm-footer').wrapAll("<footer role='contentinfo' aria-label='IBM' />");
						
				// transforming Show/More button gained from ECM fragments to v18 style
				jQuery( ".ibm-show-hide" ).removeClass( "ibm-container");
				jQuery( ".ibm-show-hide" ).attr( "data-widget", "showhide");
				jQuery( ".ibm-show-hide" ).attr( "data-type", "panel");
				jQuery( "a" ).each(function() {
					  if (jQuery(this).attr('href')==null){
						  jQuery(this).contents().unwrap();
					  }
				});
				
				// enabling show/hide for class="showmoreitems" in feature tab
				jQuery( ".showmoreitems" ).attr( "data-widget", "showhide");
				jQuery( ".showmoreitems" ).attr( "data-type", "panel");
				jQuery( ".showmoreitems" ).addClass( "ibm-show-hide");
				jQuery( ".showmoreitems" ).addClass( "ibm-alternate");
				jQuery( ".showmoreitems" ).contents().wrapAll(  "<div class='ibm-container-body' />");
				jQuery( ".showmoreitems" ).prepend( "<h2 id='showmoreitems'>More</h2>" );
				jQuery( ".showmoreitems" ).removeClass( "showmoreitems");
						
				// removing obsolete classes which does not work in v18 from entire page
				jQuery( ".ibm-bullet-list" ).removeClass( "ibm-bullet-list");
				jQuery( ".ibm-no-links" ).removeClass( "ibm-no-links");
				jQuery( ".ibm-feature-link" ).removeClass( "ibm-feature-link");
					
				//Adding this class=ibm-icon-nolink  as addition to show properly floating icons in v18 pages and not float around
				jQuery('.ibm-phone-link').addClass('ibm-icon-nolink');
				jQuery('.ibm-pdf-link').addClass('ibm-icon-nolink');
				jQuery('.ibm-youtube-link').addClass('ibm-icon-nolink');
					jQuery('.ibm-youtube-link').attr( "data-widget", "videoplayer");
					jQuery('.ibm-youtube-link').attr( "data-videodisplay", "overlay");
					jQuery('.ibm-youtube-link').attr( "data-videotype", "youtube");
					jQuery('.ibm-youtube-link').attr( "data-showvideoduration", "true");
					jQuery( "a.ibm-youtube-link" ).each(function() {
						var v_id = jQuery(this).attr('href');
						if(v_id != null) {
							v_id = v_id.substring( v_id.indexOf("v=")+2, v_id.length );
							if( v_id.indexOf("&") > 0 ) {
								v_id = v_id.substring( 0, v_id.indexOf("&") ); 
							}
							jQuery(this).attr( "data-videoid", v_id);	
						}						
					});
								
				// Adding this for fix floating forward icon on right column on family page sometimes
				setTimeout(function(){ 
					jQuery('.ibm-forward-link').addClass('ibm-icon-nolink');
					jQuery('.ibm-forward-link').addClass('ibm-inlinelink');
					
					// changing font of MORE button even must be <h2 class="ibm-h4"
					jQuery("#showmoreitems").removeClass("ibm-h2");
					jQuery("#showmoreitems").addClass("ibm-h4");
					
					
                }, 2000);
				
				// making customers hardcoded hero image from content looks better on v18. Works only for few examples.
				if( jQuery('#Subtitle').html() != null ) {
					if( jQuery('#Subtitle').html().replace(/\s| /g, '').trim().length == 0 ) {
						jQuery('#Subtitle').remove();
					}
				}
				
				//changing <h2> text </h2>     to    <span class="ibm-h2"> text </span>
				jQuery('h2').addClass('ibm-h2');
				jQuery('h3').addClass('ibm-h3');
				jQuery('h4').addClass('ibm-h4');
				jQuery('h5').addClass('ibm-h5');
				
				//tabs inside navigation tabs				
				jQuery( ".ibm-dyntabs" ).attr( "data-widget", "dyntabs");
				jQuery( ".ibm-dyntabs" ).removeClass( "ibm-container");
				
				// Overlay <a> identificator from v17 to v18 
				// detecting old > ibmweb.overlay.show('image3', this);return false;
				// replacing new > IBMCore.common.widget.overlay.show('image3');return false;
				jQuery( "a" ).each(function() {
					var el = jQuery(this).attr('onclick');
					if (el != null){
						var elVal = el;						
					
					    if (elVal.indexOf("ibmweb.overlay.show")>=0){
					    	elVal = elVal.trim();
					    	elVal = elVal.substring(elVal.indexOf("\'")+1, elVal.length);
					    	elVal = elVal.substring(0, elVal.indexOf("\'"));

							jQuery(this).attr("onclick", "IBMCore.common.widget.overlay.show(\'"+elVal+"\');return false;");						
						}
					}
				});
				// Overlay <div> content itself to work with v18
				jQuery( ".ibm-common-overlay" ).attr( "data-widget", "overlay");
				
				// Carousel v17 to v18
				jQuery("div").removeAttr("dojoattachpoint");
				 jQuery("div[widgetid]").removeAttr("tabindex");
				 jQuery("div[widgetid]").removeAttr("role");
				 jQuery("div[widgetid]").removeAttr("widgetid");
				jQuery("div.ibm-ribbon-nav").remove();
				jQuery("a.ibm-ribbon-next").remove();
				jQuery("a.ibm-ribbon-prev").remove();
				 jQuery(".ibm-ribbon-pane").unwrap();
				 jQuery(".ibm-ribbon-pane").removeAttr("style");
				 jQuery(".ibm-ribbon-pane").attr("data-widget","carousel");
				 jQuery(".ibm-ribbon-pane").removeAttr("class");
				jQuery(".ibm-ribbon").contents().unwrap();
				jQuery(".ibm-ribbon").removeClass("ibm-ribbon");
				 jQuery(".ibm-ribbon-section").contents().unwrap();
				 jQuery(".ibm-ribbon-section").removeClass("ibm-ribbon-section");
				
				//images downsize resize when making browser smaller
				jQuery("img").addClass("ibm-downsize");
				
				// hero image width fix 
				jQuery( "div[style]" ).each(function() {
					if( jQuery(this).width() == 620 && jQuery(this).css('position')=="relative") {
						jQuery(this).width("100%")
					}
					if( jQuery(this).height() == 140 && jQuery(this).css('position')=="absolute") {
						jQuery(this).css( { "background-repeat": "no-repeat" } );
					}
				});
				
				//changing width of choose Language go-button width for keeping it aligned in same row
				jQuery(".ibm-btn-go").css("min-width","25px");
				
				// livePerson unecessary tag removing from contact module feed
				jQuery("#lpbutton").remove();
				
				// replacing separate column icons and insert it into text to better alignment
				//jQuery("p.ibm-ind-link").removeClass("ibm-ind-link");
				//jQuery("a.ibm-popup-link").addClass("ibm-icon-nolink");
					
			</script>
			
