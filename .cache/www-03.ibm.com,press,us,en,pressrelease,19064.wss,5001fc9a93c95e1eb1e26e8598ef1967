			dojo.ready(function() {
				var iframes = dojo.query("iframe");
				var objects = dojo.query("object");
				var embeds = dojo.query("object embed");
				if(navigator.userAgent.indexOf("MSIE")>0) {
					objects.addContent('<param name="wmode" value="opaque">');
					for(var i=0; i<objects.length; i++) {										
						dojo.attr(embeds[i], "wmode", "transparent");
