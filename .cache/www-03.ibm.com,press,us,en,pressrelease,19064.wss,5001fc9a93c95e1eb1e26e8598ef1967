			// facebook popup window
			function fbPopup() {
				var fburl = dojo.query(".ibm-facebook-link")[0].href;
				var uidx = fburl.indexOf("?u");
				var tidx = fburl.indexOf("&t");
				var u = fburl.substring(uidx+3, tidx);
				var t = fburl.substring(tidx+3);
				window.open('http://www.facebook.com/sharer.php?u='+encodeURIComponent(u)+'&t='+encodeURIComponent(t),'sharer','toolbar=0,status=0,width=626,height=436');
				return false;
			}
			function sharePopup(obj) {
				window.open(obj.href,'sharer','toolbar=0,status=0,width=626,height=436');
			}
			
