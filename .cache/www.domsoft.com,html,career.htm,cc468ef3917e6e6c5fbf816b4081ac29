function imgOff(imgName) {

        if (document.images) {

                document[imgName].src = "../images/n_t.gif";
        }
}

function swapImage(theImage, theSource){

	var objectString,imageObject;
	if(document.images){
		if (typeof(theImage) == 'string') {
			objectString = 'document.' + theImage;
			imageObject = eval(objectString);
			imageObject.src = theSource;
		
		} else if ((typeof(theImage) == 'object') && theImage && theImage.src) {
			theImage.src = theSrc;
		}
	}
}
		
// -->
