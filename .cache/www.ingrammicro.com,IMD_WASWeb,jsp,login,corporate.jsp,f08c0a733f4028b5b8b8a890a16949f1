<script language="javascript">

        function imLogout()
        {
                // timeout cookies
              document.cookie = 'SMSESSION=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
              document.cookie = 'SMTRYNO=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
              document.cookie = 'SMCHALLENGE=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
              document.cookie = 'SMCREDS=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
              document.cookie = 'SMONDENIEDREDIR=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' +'; path=/; domain=.ingrammicro.com';
              document.cookie = 'HOSTDESTINATION=' + window.location.hostname + '; expires=' + 'Mon, 01-Jan-10 00:00:01 GMT' +'; path=/; domain=.ingrammicro.com';
                  document.cookie = 'IMTrail=';
              document.cookie = 'JSESSION=';

                parent.location.href = 'http://'+ window.location.hostname + '/IMD_WASWeb/jsp/login/corporate.jsp';

        }


        function submitcheck() {

                if(document.login_form.USER.value != "" && document.login_form.PASSWORD.value != "") {

                                document.cookie = 'SMSESSION=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
                        document.cookie = 'SMTRYNO=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
                        document.cookie = 'SMCHALLENGE=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
                        document.cookie = 'SMCREDS=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' + '; path=/; domain=.ingrammicro.com';
                                document.cookie = 'SMONDENIEDREDIR=; expires=' + 'Mon, 01-Jan-90 00:00:01 GMT' +'; path=/; domain=.ingrammicro.com';
                                return true;
                }
                else if (document.login_form.USER.value == "")
                {
                        alert("Please enter a UserName ");
                        window.location.reload();

                }
                else if(document.login_form.PASSWORD.value == "")
                {
                        alert("Please enter a Password ");
                        window.location.reload();
                }
        }



        function capitalize(value)
        {
          document.login_form.USER.value = value.toUpperCase();
        }

        if (window.addEventListener) {
        window.addEventListener("message", receiveMessage, false);
    } else if (window.attachEvent) {
        window.attachEvent("onmessage", receiveMessage);
    }

        var passedValue;
        function receiveMessage(evt) {
        //    if (evt.origin === 'http://imdomain.com') {
        passedValue = evt.data;
//     }
}

$(document).ready(function () {



    $('#frame').load(function () {
        if (passedValue!== undefined)
                window.top.location.href = passedValue;
    });
});

</script>
