</div>
</div>
</div>
</div>
</div>
</div>
</section>	
	
		
<script type="text/javascript">
$(document).ready(function(){
  $(".question").click(function() {
    $(this).parent().children(".answer").toggle();
    // track this as an event
    if ($(this).parent().children(".answer").is(":visible")) {  // only track when it opened, not on the close action
    	_gaq.push(['_trackEvent', 'FAQ', 'Expand', $(this).text() ]);
    }
  });
});
</script>

<script>

	$('#singleFAQ a').click(function(event) {
	
		event.preventDefault();

		var target = $(this).prop('hash');
				
		$('.holder:visible').not(target).fadeOut('slow', function() {
			// after we fade out old content, fade in new
			$(target).fadeIn('slow');
			});
			
	});

</script>

	


