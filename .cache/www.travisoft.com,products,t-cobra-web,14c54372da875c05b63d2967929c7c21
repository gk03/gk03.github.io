      <script type="text/javascript" charset="utf-8">
        $(document).ready(function(){jQuery('ul.sf-menu').stop().superfish();

          //Munchkin.init('611-AWL-635');
          $.get("/process/token.php",function(txt){
            $(".secure").append('<input type="hidden" name="ts" value="'+txt+'" />');
            });

          $('#contact__').ajaxForm({dataType:"json",beforeSubmit:showMessage,success:processJson});


        });

          function processJson(data) {

                if (data.error=="1"){
                    $("button").button('reset');
                    $('.alert-content').html(data.warning);
                    $('.alert').fadeIn();
                }else if (data.error=="0"){

                    $('.status').html(data.thankyou);
                    mktoMunchkin("611-AWL-635");
                    mktoMunchkinFunction(
                       'associateLead',
                       {
                          FirstName: data.FirstName,
                          LastName: data.LastName,
                          Company: data.Company,
                          Email: data.Email,
                          Phone: data.Phone,
                          LeadSource: data.LeadSource,
                          Campaign_Name__c: data.Campaign_Name__c,
                          Campaign_Source__c: data.Campaign_Source__c,
                          Campaign_Medium__c: data.Campaign_Medium__c,
                          Campaign_Content__c: data.Campaign_Content__c,
                          Campaign_Term__c: data.Campaign_Term__c,
                          last_contact: data.last_contact,

                       },
                       data.key
                    );
                    _gaq.push(['_trackPageview', '/thank-you/products/t-cobra-web']);

                }
            }

            function showMessage(){$("button").button('loading');}


      </script>

   
