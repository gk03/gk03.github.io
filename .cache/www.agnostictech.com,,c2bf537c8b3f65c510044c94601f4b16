<script>
function scroll_menu(){
	jQuery(window).bind('scroll', function(){
		if(jQuery(this).scrollTop() > 150) {
			jQuery(".fixedmenuwrap").slideDown(200);}
		else{
			jQuery(".fixedmenuwrap").slideUp(200);}
	});
}

scroll_menu();
</script>
</head>
