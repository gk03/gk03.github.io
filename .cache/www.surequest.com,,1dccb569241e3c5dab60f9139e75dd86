<script>
$(window).load(function(){
	matchCellHeight('#home-cells','.col-md-3');
	matchCellHeight('#home-cells','.small-heading');
	if($(window).width() > 975){
		matchCellHeight('#home-success','div[class^="col-"]');
		matchCellHeight('#home-news','div[class^="col-"]');
	}else{
		matchCellHeight('#home-quote','div[class^="col-"]');
	}
});

$(document).ready(function(){
	$('.animated').addClass('transparent').viewportChecker({
		classToAdd: 'slideInUp',
		classToRemove: 'transparent'
	});
});

$(window).resize(function(){
	matchCellHeight('#home-cells','.col-md-3');
	matchCellHeight('#home-cells','.small-heading');
	if($(window).width() > 975){
		matchCellHeight('#home-success','div[class^="col-"]');
		matchCellHeight('#home-news','.resize-cell');
	}else{
		matchCellHeight('#home-quote','div[class^="col-"]');
	}
})
</script>


