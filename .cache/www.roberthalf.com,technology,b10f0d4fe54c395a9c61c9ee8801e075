    function bannerClick(id){
		clickType = id;
//clearInterval(bannerTimer);
		if (clickType == 0) {
                if(jQuery(".current-image")[0] != jQuery("#first-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#first-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 1;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#one").addClass("selected");
                    jQuery("#first-image").addClass("current-image");
					
                }
		} else if (clickType == 1) {
                if(jQuery(".current-image")[0] != jQuery("#second-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#second-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 2;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#two").addClass("selected");
                    jQuery("#second-image").addClass("current-image");
                }
		} else if (clickType == 2) {
                if(jQuery(".current-image")[0] != jQuery("#third-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#third-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 3;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#three").addClass("selected");
                    jQuery("#third-image").addClass("current-image");
                }
		} else if (clickType == 3) {
                if(jQuery(".current-image")[0] != jQuery("#fourth-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#fourth-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 0;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#four").addClass("selected");
                    jQuery("#fourth-image").addClass("current-image");
                }
		}
		clearInterval(bannerTimer);
		bannerTimer = setInterval(function(){
        clickType++;
		//document.getElementById("test").innerHTML = clickType;
        if(clickType > 3){
            clickType = 0;
        }
		if (clickType == 0) {
                if(jQuery(".current-image")[0] != jQuery("#first-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#first-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 1;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#one").addClass("selected");
                    jQuery("#first-image").addClass("current-image");
                }
		} else if (clickType == 1) {
//alert("image 2");
                if(jQuery(".current-image")[0] != jQuery("#second-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#second-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 2;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#two").addClass("selected");
                    jQuery("#second-image").addClass("current-image");
                }
		} else if (clickType == 2) {
                if(jQuery(".current-image")[0] != jQuery("#third-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#third-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 3;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#three").addClass("selected");
                    jQuery("#third-image").addClass("current-image");
                }
		} else if (clickType == 3) {
                if(jQuery(".current-image")[0] != jQuery("#fourth-image")[0]){
                    jQuery(".current-image").hide("slide", {
                        direction: "right"
                    }, 900);
                    jQuery("#fourth-image").show("slide", {
                        direction: "left"
                    }, 900);
                    //clickType = 0;
                    jQuery(".current-image").removeClass("current-image");
                    jQuery(".selected").removeClass("selected");
                    jQuery("#four").addClass("selected");
                    jQuery("#fourth-image").addClass("current-image");
                }
		} 
		}, 6000);
	}
		

//--><!]]>
</script>    </div>
    </div>
  
  
  
  
  
  
</div>  </div>
</div>
  </div>
		</div><!-- .l-accolades-wrapper -->
	
