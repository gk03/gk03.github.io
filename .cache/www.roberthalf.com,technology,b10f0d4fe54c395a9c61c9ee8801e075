jQuery(document).ready(function() {

jQuery('a.advanced-search-toggle').remove();

jQuery('#nblog-categories option').each(function() {
  jQuery(this).val(jQuery(this).val().replace('en/', ''));
});
jQuery(".page-node .pane-menu .menu > li.active-trail").prevAll().css("display", "none");

jQuery('p.submitted-by').each(function() {
  jQuery(this).html(jQuery(this).html().replace('&amp;','&'));
});

