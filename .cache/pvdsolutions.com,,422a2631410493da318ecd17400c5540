<script type="text/javascript">
    $(document).ready(function() {

        $('.applicationRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.presRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.sessRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.transRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.netRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.dataRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);
        $('.physRoll').bind('mouseover', openSubMenu).bind('mouseout', closeSubMenu);


        function openSubMenu() {
            $(this).find('.applicationDiv').css('visibility', 'visible');
            $(this).find('.presDiv').css('visibility', 'visible');
            $(this).find('.sessDiv').css('visibility', 'visible');
            $(this).find('.transDiv').css('visibility', 'visible');
            $(this).find('.netDiv').css('visibility', 'visible');
            $(this).find('.dataDiv').css('visibility', 'visible');
            $(this).find('.physDiv').css('visibility', 'visible');


        }

        function closeSubMenu() {
            $(this).find('.applicationDiv').css('visibility', 'hidden');
            $(this).find('.presDiv').css('visibility', 'hidden');
            $(this).find('.sessDiv').css('visibility', 'hidden');
            $(this).find('.transDiv').css('visibility', 'hidden');
            $(this).find('.netDiv').css('visibility', 'hidden');
            $(this).find('.dataDiv').css('visibility', 'hidden');
            $(this).find('.physDiv').css('visibility', 'hidden');

        }

    });


</script>


