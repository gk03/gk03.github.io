<script>
function checkJoin(thisform){
	var missing = false;
	var filler = "      ";
	var str = "_______________________________________\n\n";
		str += "The information was not submitted because\nof the following error(s):\nPlease correct the error(s) and re-submit.\n";
		str += "_______________________________________\n\n";
		str += "-The following field(s) are required:\n";
	// Do Required Check
	
	if (!thisform.user_membership.value){
		str += filler + "Membership Type\n";
		missing = true;
	}
	
	if(missing){
		alert(str);
	}
	else{
		if (thisform.user_membership.value == 44 || isblank(thisform.user_promo.value) || thisform.user_promo.value == "Enter promotional code if applicable"){
			thisform.user_promo.value='';
			thisform.processFlagStep1.value=1;
			thisform.submit();
		}
		else{
			memPromoRate(thisform);
		}
	}
}
	
function checkRefresh(thisform)
	{
	var missing = false;
	var filler = "      ";
	var str = "_______________________________________\n\n";
		str += "The information was not submitted because\nof the following error(s):\nPlease correct the error(s) and re-submit.\n";
		str += "_______________________________________\n\n";
		str += "-The following field(s) are required:\n";
	// Do Required Check
	
	if(missing)
		{
			alert(str);
		}
	else
		{
			if (thisform.user_option == "undefined")
				{
				if (thisform.user_option[0].checked ==  false)
					{
						thisform.user_option[1].checked = true;
					}
				else
					{
						thisform.user_option[0].checked = true;
					}
				}
			//thisform.user_membership.value = thisform.user_membership.value;
			thisform.submit();
		}
	}

function checkJoinSearay(thisform,member_id){
	var missing = false;
	var filler = "      ";
	var str = "_______________________________________\n\n";
		str += "The information was not submitted because\nof the following error(s):\nPlease correct the error(s) and re-submit.\n";
		str += "_______________________________________\n\n";
		str += "-The following field(s) are required:\n";
	// Do Required Check
	
	if(missing){
		alert(str);
	}
	else{
		thisform.user_membership.value=member_id;
		
		if (thisform.user_membership.value == 44 || isblank(thisform.user_promo.value) || thisform.user_promo.value == "Enter promotional code if applicable"){
			thisform.user_promo.value='';
			thisform.processFlagStep1.value=1;
			thisform.submit();
		}
		else{
			memPromoRate(thisform);
		}
	}
}

function memPromoRate(f) {
	var v = f.user_promo.value + '|' + f.user_membership.value;
	u = new memUtilityCFC();
	u.setCallbackHandler(mprResult);
	u.getPromoRate(v);	
}
		
function mprResult(r) {
	if (r.ERROR == 'error'){
		var c = confirm("This Promotion is not available with the selected Membership Type.\n\nClick OK to continue at the regular rate or click Cancel");
		if(c){
			document.membershipForm.processFlagStep1.value=1;
			document.membershipForm.user_promo.value='';
			document.membershipForm.submit();
		}
	}
	else{
		document.membershipForm.processFlagStep1.value=1;
		document.membershipForm.submit();
	}
}
</script>



