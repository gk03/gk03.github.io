<script type="text/javascript">
    $(document).ready(function () {
        $("#newsevents").tabs();
        setEventHeight();
    });

    function setEventHeight() {

        var RecentNewsHeight = $('#tabs-News').height();
        var UpcomingEventsHeight = $('#tabs-Events').height();
        $('.panels').height((RecentNewsHeight > UpcomingEventsHeight) ? RecentNewsHeight + 10 : UpcomingEventsHeight + 10);
    }

</script>



