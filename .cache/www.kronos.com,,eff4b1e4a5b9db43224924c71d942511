    <script type="text/javascript">/* ====== ContentTitle_Industry Selector JS_ID_23622327810 ======= */
 jQuery(document).ready(function () {
        var isToggle = false;
        $('.industrySelector .selector_container').mouseenter(function () {
            $('.selector_popup').removeClass("inactive").addClass("active");
            isToggle = true;
        });

        $('.selector_popup').mouseleave(function () {
            if (isToggle) {
                $('.selector_popup').removeClass("active").addClass("inactive");
                isToggle = false;
            }
        });

        $('.industryList a').each(function () {
            $(this).hover(function () {

                $(".industryImage img").replaceWith($(this).next().children("img").clone().addClass("ImgShow"));
            }, function () {
                $(".industryImage img").replaceWith('<img src="/images/k-logo.jpg" alt="Kronos" class="ImgShow">');
            }
            );
        });
    });</script>
