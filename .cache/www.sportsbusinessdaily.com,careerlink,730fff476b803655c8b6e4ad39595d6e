<script type="text/javascript">
    var page = 1;
    function loadJobs() {
        var url = '/careerlink/Services/LoadJobsService.svc' + '/LoadJobs';
        var pageSize = 7;

        function UpdateJobs(res) {
            var data = res.LoadJobsResult;
            jQuery('#jobTmpl').tmpl(data).appendTo('.recent-jobs');

           if (data.length < pageSize)
            $('.load-more-content').hide();

                        page = page + 1;
        };
      
        $.ajax({
            type: "POST",
            url: url,
            contentType: "application/json",
            data: '{"userID":"-1", "page": "' + page + '", "pageSize": "' + pageSize + '"}',
            dataType: "json",
            success: function (result) {
                UpdateJobs(result);
            },
            error: function (textStatus) {
                alert(textStatus);
            }
        });
    }
</script>
