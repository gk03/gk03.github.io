    <script type="text/javascript">
        $(window).load(function () {
            $.getJSON('https://api.sportsbusinessdaily.com/api_careerlink/1/featuredarticles?APIKey=88777F91-A175-4EBD-9FC4-719BB2B97834', function (data) {
                var ExPitems = [];
                $.each(data, function (key, val) {
                    var thumb = (val.thumb != null && val.thumb != "") ? "<img src='" + val.thumb + "' />" : ""
                    if (key < 4) {
                        ExPitems.push('<li><a href="' + val.url + '" target="_blank">' + thumb + '<span>' + val.title + '</span></a></li>');
                    }
                });

                $("#featuredArticlesList").html(ExPitems.join(""));
            })
            .fail(function () {
                $("#featuredArticlesList").html("No items found");
            });
        })
    </script>
