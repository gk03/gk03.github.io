						<script type="text/javascript">
							$(document).ready(function () {
							// set search box value if browser doesn't support "placeholder" attribute on input boxes
								if (document.createElement("input").placeholder == undefined){
									$('#search_csquery, #search_csquery_mobile').val(GblSearch_helpText );
									$('#search_csquery, #search_csquery_mobile').on({
										focus: function() {
											if($(this).val() == GblSearch_helpText){
												$(this).val('');
											}										
										}, blur: function() {
											if($( this ).val == ''){
												$(this).val(GblSearch_helpText);
											}
										}	
									});
								}
								$('#search_csquery, #search_csquery_mobile').on({
									focus: function() {
									$(this).attr("placeholder", "");	
									}
								});
							});
							function up(){$(this).find('div').slideUp('fast');};
							function down(){$(this).find('div').slideDown('fast');};
