		$(document).ready(function() {
            //adds active class to home-community-slider navigation when swiping
          $(".home-community-slider .slick-slide[data-slick-index='0']").on("swipe",function(){
              event.preventDefault();
              $('.home-community-nav a').removeClass('active');
              $('.home-community-nav li:nth-child(2) a').addClass('active');
            });
            $(".home-community-slider .slick-slide[data-slick-index='1']").on("swipe",function(){
                event.preventDefault();
                  $('.home-community-nav a').removeClass('active');
                  $('.home-community-nav li:nth-child(3) a').addClass('active');
                });
            $(".home-community-slider .slick-slide[data-slick-index='2']").on("swipe",function(){
                event.preventDefault();
                  $('.home-community-nav a').removeClass('active');
                  $('.home-community-nav li:nth-child(1) a').addClass('active');
                });


            // handle pubs blog rollovers
            $('a.pubover').each(function() {
                $(this).hover(function() {
                    // hover in
                    $('a.pubover').removeClass('big');
                    $(this).addClass('big');
                });
            });

            // intial pubover
            $('div.pubs div.start a.pubover').addClass('big');

        });

		function closeModal(selector) {
		    $(".reveal-overlay").click();
		    return false;
		}

		function getCookie(cname) {
		    var cookie, cookiesArray;

		    cookie = cname + "=";
		    cookiesArray = document.cookie.split(';');
		    for (var i = 0; i < cookiesArray.length; i++) {
		        var c = cookiesArray[i];
		        while (c.charAt(0) == ' ') c = c.substring(1);
		        if (c.indexOf(cookie) == 0) return c.substring(cookie.length, c.length);
		    }
		    return "";
		}

		function checkCookie(cname) {
		    var cookie = getCookie(cname);
		    if (cookie != "") {
		        return true;
		    }
		    return false;
		}

    function clickTrack(label, category) {
       if (typeof ga == 'function') {
           ga('send', 'event', category, 'Click', label);
       } else {
           window.console && console.log("_trackEvent('" + category + "', 'click', '" + label + "')");
       }
     }
	</script>















