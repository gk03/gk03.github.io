<script>var GetVideoUrl = '/api/sitecore/Video/GetVideo';</script>
<script>
    var cardBGlobalhomepage5cards = { iterateCycle: 0, maxElementcnt: 12 }; //To store current data load cycle. maxElementcnt store max elements to be shown.
    var cardCounthomepage5cards = 5;
    function getResultsDesktophomepage5cards(iterCyc) {
        cardBGlobalhomepage5cards.iterateCycle = iterCyc;
        var max = 12 * iterCyc;
        var show = 12;
        var skpcnt = (max - cardBGlobalhomepage5cards.maxElementcnt);
        jQuery.ajax({
            url: '/api/sitecore/Cards/LoadCardB',
            type: 'POST',
            data: { 'datasourceid': 'bb72fc89-af05-42d2-9a1b-86d214aaf5dd', 'skipcnt': skpcnt, 'max': show, 'cardShare': 'no-share ', 'variation': 'no-share img-text', 'lang': 'en' },
            success: function (result) {
                jQuery("#homepage5cardsfive").append(jQuery(result))

            },
            error: function () {
            },
            complete: function () {  // call this method on "load more" button also.
                if(cardCounthomepage5cards <= max)
                {
                    jQuery('#loadclasshomepage5cards').hide();
                    jQuery('#ctaclasshomepage5cards').show();
                }

                var cardtype = 2;

                function cardVariations(cardtype, element) {

                    var start = 0;
                    var end = cardtype;

                    jQuery(element).each(function (index) {
                        if (index == start) {
                            jQuery(this).css('margin-left', '0px');
                            start = start + cardtype;
                        }

                        if (index == end - 1) {
                            jQuery(this).css('margin-right', '0px');
                            end = end + cardtype;
                        }
                    });
                }

                cardVariations(cardtype, ".homepage5cards.card-two .card-default");

                cardVariations(3, ".homepage5cards.card-three .card-default");
                cardVariations(4, ".homepage5cards.card-four .card-default");
                cardVariations(5, ".homepage5cards.card-five .card-default");
                jQuery(".img-wrapper").find('img').each(function () {
                    var original_img = jQuery(this).attr('src');
                    jQuery(this).parent().css('backgroundImage', 'url(' + original_img + ')').addClass("fit-to-width");
                });
                /*Ellipsis for card content overflow*/
                var element = document.querySelectorAll('.card .card-default');
                var element1 = document.querySelectorAll('.card .card-default .content');
                for (i = 0; i < element.length; ++i) {

                    if(!element[i].className.match("only-text btn-text"))
                    {
                        if ((element1[i].offsetHeight + 5 < element1[i].scrollHeight)) {
                            // your element have overflow
                            var node = document.createElement("div");
                            node.setAttribute("class", "ellipsis");
                            var textnode = document.createTextNode("...");
                            node.appendChild(textnode);
                            document.getElementsByClassName("content")[i].appendChild(node);
                        }
                        else {
                            //your element don't have overflow
                        }
                    }
                }
                //Div wrapping for Card Text Only Variation to handle text overflow issue in presence of Download button
                jQuery(".card .card-default.only-text.btn-text .content a.btn.btn-default").wrap("<div></div>");
            }
        });
    }
    function getResultsMobilehomepage5cards(iterCyc) {

        cardBGlobalhomepage5cards.iterateCycle = iterCyc;
        var max = 4*iterCyc;
        var show = 4;
        var skpcnt = (max - 4);
        jQuery.ajax({
            url: '/api/sitecore/Cards/LoadCardB',
            type: 'POST',
            data: { 'datasourceid': 'bb72fc89-af05-42d2-9a1b-86d214aaf5dd', 'skipcnt': skpcnt, 'max': show, 'cardShare': 'no-share ', 'variation': 'no-share img-text', 'lang': 'en' },
            success: function (result) {
                jQuery("#homepage5cardsfive").append(jQuery(result))

            },
            error: function () {
            },
            complete: function () {  // call this method on "load more" button also.
                if(cardCounthomepage5cards <= max)
                {
                    jQuery('#loadclasshomepage5cards').hide();
                    jQuery('#ctaclasshomepage5cards').show();
                }
                jQuery(".card .card-default.img-text .img-wrapper").find('img').each(function () {
                    var original_img = jQuery(this).attr('src');
                    jQuery(this).parent().css('backgroundImage', 'url(' + original_img + ')').addClass("fit-to-width");
                });
                //jQuery(".card .card-default.full-img .img-wrapper > img").each(function () {
                //    var original_img = jQuery(this).attr('src');
                //    jQuery(this).parent().parent().css('backgroundImage', 'url(' + original_img + ')').addClass("fit-to-width");
                //});
                jQuery(".card .card-default.full-img .img-wrapper > img").each(function () {
                    var original_img = jQuery(this).attr('src');
                    //jQuery(this).parent().parent().css('backgroundImage', 'url(' + original_img + ')').addClass("fit-to-width");
                    jQuery(this).closest('.card-default').css('backgroundImage', 'url(' + original_img + ')').addClass("fit-to-width");
                });
            }
        });
    }
    if (jQuery(window).width() > 751) {
        getResultsDesktophomepage5cards(++cardBGlobalhomepage5cards.iterateCycle); // incrementing iteratecycle on click of Load more.
        if(cardCounthomepage5cards > 12)
        {
            jQuery('#loadclasshomepage5cards').show();
            jQuery('#ctaclasshomepage5cards').hide();
        }
        jQuery("#loadbuttonhomepage5cards").click(function (event) { //event delegation in jquery.
            event.preventDefault();
            getResultsDesktophomepage5cards(++cardBGlobalhomepage5cards.iterateCycle); // incrementing iteratecycle on click of Load more.
        });
    }
    else{
        getResultsMobilehomepage5cards(++cardBGlobalhomepage5cards.iterateCycle); // incrementing iteratecycle on click of Load more.
        if(cardCounthomepage5cards > 4)
        {
            jQuery('#loadclasshomepage5cards').show();
            jQuery('#ctaclasshomepage5cards').hide();
        }
        jQuery("#loadbuttonhomepage5cards").click(function (event) { //event delegation in jquery.
            event.preventDefault();
            getResultsMobilehomepage5cards(++cardBGlobalhomepage5cards.iterateCycle); // incrementing iteratecycle on click of Load more.
        });
    }

</script>

