<script type="text/javascript">
    jQuery(document).ready(function (jQuery) {
        jQuery('li[class*=country] a').on('click', function (event) {
            event.preventDefault();
            var redirecturl = location.protocol + '//' + location.host + jQuery(this).attr('href');
            var redirectHome = location.protocol + '//' + location.host + '/' + jQuery(this).attr('data-lang');
            var nordicCountry = jQuery(this).attr('data-country');
            var itempath = jQuery(this).attr('data-itemPath');
            var lang = jQuery(this).attr('data-lang');
            jQuery.ajax({
                url: '/api/sitecore/LanguageSelector/HasItemVersion',
                data: { ItemPath: itempath, language: lang },
                success: function (data) {
                    if (data == "true" || data == true) {
                        jQuery.ajax({
                            cache: false,
                            url: '/api/sitecore/LanguageSelector/SetCookie',
                            data: { country: nordicCountry },
                            success: function (data) {
                                location.href = redirecturl;
                            }
                        });
                    }
                    else {
                        location.href = redirectHome;
                    }
                }
            });


        });
    });
</script>

