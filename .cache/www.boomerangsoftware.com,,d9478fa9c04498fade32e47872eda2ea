<script language="javascript" type="text/javascript">
    function Search(){
        if(document.getElementById('txtSearch').value != ''){
            window.location = 'Search.aspx?Search=' + document.getElementById('txtSearch').value;
        }
    }
    function SearchKeyPress(e){
        if(!e)
	    	e=window.event;
        if (e.keyCode == 13) 
        {
            Search();
            try
            {
                e.preventDefault();
            }
            catch ( oError )
            {
                e.returnValue = false;
            }
            return false;
        }
    }
</script>
