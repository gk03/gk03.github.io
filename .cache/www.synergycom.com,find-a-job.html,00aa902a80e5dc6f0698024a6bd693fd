				<script>
					(function() {
						
							var frameBaseSRC = document.getElementById("myIframe").src;
							
							var frameQueryString = document.location.href.split("jobcode=")[1];
							//location.hash="myIframe"
							
							 if (frameQueryString != undefined) {
								 
							  document.getElementById("myIframe").src=frameBaseSRC + "&reference=" + frameQueryString ;
							 }
						
						
						
					})();
				</script>

