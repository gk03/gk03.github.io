                msgs.push("Please enter your first name.");
                isValid = false;
            }

            if (lastName == "") {
                msgs.push("Please enter your last name.");
                isValid = false;
            }*/

            if (email == "") {
                msgs.push("Please enter your e-mail address.");
                isValid = false;
            }

            if (!isValid) {
                alert(msgs.join("\n"));
