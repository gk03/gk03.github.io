<script type="text/javascript">
    jQuery().ready(function ($) {

        function HoverOver8475178590() {

            $(this).addClass('rmhover').find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOut8475178590() {
            $(this).removeClass('rmhover').find(".subMenu:first").animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        function HoverOversub8475178590() {
            $(this).find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutsub8475178590() {
            $(this).find(".subMenu:first").stop().animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        var config8475178590 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOver8475178590,
            timeout: 1000,
            out: HoverOut8475178590
        };
        var configsub8475178590 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOversub8475178590,
            timeout: 1000,
            out: HoverOutsub8475178590
        };
        $("#gomenu8475178590 li.dir").hoverIntent(config8475178590);
        $("#gomenu8475178590 .subMenu li.dir").hoverIntent(configsub8475178590);
        $("#gomenu8475178590").find("li.current").each(function (i) {
            $(this).find(".subMenu:first").show();
        });
    });
</script>
 
 


