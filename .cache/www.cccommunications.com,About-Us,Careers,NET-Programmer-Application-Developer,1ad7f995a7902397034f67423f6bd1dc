        function checkFields() {
            var firstName = $.trim($("#dnn_ctr517_View_TextBoxFirstName").val());
            var lastName = $.trim($("#dnn_ctr517_View_TextBoxLastName").val());
            var email = $.trim($("#dnn_ctr517_View_TextBoxEmailAddress").val());

