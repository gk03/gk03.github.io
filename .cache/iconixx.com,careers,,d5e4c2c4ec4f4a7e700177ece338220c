<script>
(function($) {
   /* Every time the window is scrolled ... */
    $(window).scroll( function(){

		var _win = $(window);
	    var _left_el = $('body').find('img.move');
	    var _win_height = _win.height();
	    var _win_top = _win.scrollTop();
	    var gap = _win_height / 4;
	    var _pos = _win_height + _win_top - gap;
        /* Check the location of each desired element */
        $('.pc-screen').each( function(i){

            var bottom_of_object = $(this).offset().top + $(this).outerHeight();

            var bottom_of_window = $(window).scrollTop() + $(window).height();

            $.each(_left_el, function(index, el) {
              var _el = $(el);
              if(_el.length > 0) {
                var _top = _el.offset().top;
                if(bottom_of_window >=1800 ) {
                  _el.removeClass('move');
                } else {
                	jQuery('.client-logo').addClass('move');

                }
              }
            });
            /* If the object is completely visible in the window, fade it it */
            if( bottom_of_window >=1800 ){

                $(this).removeClass('move');

            }else if(bottom_of_window <1800){
            	$(this).addClass('move');
            	jQuery('.client-logo').addClass('move');
            }

        });

    });
})(jQuery);


</script>

