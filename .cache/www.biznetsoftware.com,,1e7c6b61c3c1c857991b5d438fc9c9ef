	<script>
		jQuery(document).ready(function($){
			if($('.home #hero #hero-video')){
				$('#hero-content a').click(function(e){
					e.preventDefault();
					$('#hero-video').slideDown(800,function(e){
						$('#hero-video-close').css('display','block');
					});
					$('#hero').slideUp(800);
				});
				$('#hero-video-close').click(function(e){
					e.preventDefault();
					hero_player.stopVideo();
					$('#hero-video').slideUp(800);
					$('#hero-video-close').css('display','none');
					$('#hero').slideDown(800);
				});
			}
		});
		//Youtube iFrame API
		var tag = document.createElement('script');
  		tag.id = 'iframe-demo';
  		tag.src = 'https://www.youtube.com/iframe_api';
  		var firstScriptTag = document.getElementsByTagName('script')[0];
  		firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
  		
  		//Target Player
  		var hero_player;
		function onYouTubeIframeAPIReady(){
			hero_player = new YT.Player('hero-video',{
				videoId: '7Tu4Nim7-dg',
				playerVars: {'controls':0, rel: 0, 'showinfo': 0, 'modestbranding': 1, 'enablejsapi': 1, 'origin': 'http://www.biznetsoftware.com' },
				events: {
					'onReady': onPlayerReady,
					'onStateChange': onPlayerStateChange
				}
			});
		}
  
  		function onPlayerReady(event){
    		
  		}
  		
  		function onPlayerStateChange(event){
  			switch(event.data){
  				case 0: jQuery('#hero-video-close').trigger('click');
  				break;
  			}
  		}
	</script>
