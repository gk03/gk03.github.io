    jQuery(document).ready(function () {
        jQuery("#reload_ecaptcha").click(function () {
            var captcha_title = 'Prove you are not a robot!';
            var captcha_textcolor = '000000';
            var captcha_width = '75';
            var captcha_height = '24';
