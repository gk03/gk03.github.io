    $(document).ready(function() {
        
        $('p:empty').remove();
        $('.selection-list p').contents().unwrap();
        
        if(window.location.href.indexOf("en-ANZ") > -1) {
            $('.countryCode ul li:nth-child(2)').addClass("current");
        }else if(window.location.href.indexOf("en-GB") > -1) {
            $('.countryCode ul li:nth-child(3)').addClass("current");
        }else{
            $('.countryCode ul li:nth-child(1)').addClass("current");
        }
    });
</script>




