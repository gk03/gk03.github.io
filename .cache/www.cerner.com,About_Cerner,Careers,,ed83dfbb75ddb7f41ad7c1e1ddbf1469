    $(document).ready(function () {
        $(".jcarousel a").each(function () {
            var anchor = $(this);
            var url = (anchor.attr('href').length == 0) ? "" : anchor.attr('href').trim();
            if (url == "" || url == "#") {
                anchor.removeAttr("onclick");
                anchor.removeAttr("href");
                anchor.css("cursor", "default");
            }
        });
    });
