function equalHeight(group) {
   tallest = 0;
   group.each(function() {
      thisHeight = jQuery(this).height();
      if(thisHeight > tallest) {
         tallest = thisHeight;
      }
   });
   group.height(tallest);
}
jQuery(document).ready(function() {
equalHeight(jQuery(".blue_block"));
    jQuery(window).resize(function(){
         jQuery(".blue_block").css('height','auto');
  equalHeight(jQuery(".blue_block"));
    });
   jQuery('.blog-list > div').hide();
    jQuery('.blog-list > div').eq(0).show();
    jQuery('.title-list .jspPane > div').eq(0).addClass('active');

jQuery('.title-list .jspPane > div').click(function(){
	jQuery('.blog-list > div').hide();
	jQuery('.title-list .jspPane > div').removeClass('active');
	var cl = jQuery(this).attr("class");
	console.log(cl);
	jQuery('.blog-list div.'+cl).show();
	jQuery(this).addClass('active');
	});
});

