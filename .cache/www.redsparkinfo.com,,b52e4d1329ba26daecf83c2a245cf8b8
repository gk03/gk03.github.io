<script>
    jQuery(document).ready(function(){
        jQuery('#menu-footer3 li').removeClass('menu-item-object-grid_products');

    });
</script>
<script type="text/javascript">
    jQuery(document).ready(function(){
        jQuery(".button_register").click(function(){
            var email_id = jQuery("#email_newletter").val();   

            var pattern = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            if(!pattern.test(email_id))
                {
                //alert('Please Enter valid E-mail Address');
                jQuery('#email_newletter').addClass('newsletter_email_alert');
                return false;
            }else{
                jQuery('#email_newletter').removeClass('newsletter_email_alert');         
                jQuery.ajax({
                    url: 'http://www.redsparkinfo.com/wp-admin/admin-ajax.php',
                    data: {
                        'action':'mailchimp_user_subscribe',
                        'email_id' : email_id 
                    },
                    success:function(data) {
                        if(jQuery.trim(data) == 'success'){

                            jQuery(".register").empty();
                            jQuery(".register").append('<h5>Thank You For Subscription</h5>');
                        }else{
                            jQuery(".register").empty();
                            jQuery(".register").html('<h5>Subscription Error</h5>')
                        }                    
                    },
                    error: function(errorThrown){
                        console.log(errorThrown);
                    }
                });
            }
        });
    });
</script>
