<script type="text/javascript">
$(window).load(function(){

	// Simple Accordion, for inside page sidemenu //
	$('.accordionTitle').click(function(){
	    var $target = $(this).closest('.simpleAccordion').find('.accordionReveal').slideToggle('fast', function() {
			var $target = $(this).closest('.simpleAccordion').find('div.view_all').toggle('fast');
			var $target = $(this).closest('.simpleAccordion').find('div.view_less').toggle('fast');
			var $target = $(this).closest('.simpleAccordion').find('.arrow').toggleClass('arrow_down');
	    });
	return false;
	});
	// For Tabmenu category pages
	$('.tab1').click(function(){
		$('.tab_wedge2').hide();
		$('.tab_wedge3').hide();
		$('.tab_wedge4').hide();
	});
	$('.tab2').click(function(){
		$('.tab_wedge2').show();
		$('.tab_wedge3').hide();
		$('.tab_wedge4').hide();
	});
	$('.tab3').click(function(){
		$('.tab_wedge2').hide();
		$('.tab_wedge3').show();
		$('.tab_wedge4').hide();
	});
	$('.tab4').click(function(){
		$('.tab_wedge2').hide();
		$('.tab_wedge3').hide();
		$('.tab_wedge4').show();
	});
	
});
</script>
<script type="text/javascript">

$(document).ready(function(){
	function setRandom(id){
	var s = "div." + id;
	var rndTotal = $(s).length;
	var rndNum = Math.floor((Math.random()*rndTotal)+1);
	var divRndId = "#" + id + rndNum;
	$(divRndId).removeClass();
	}
setRandom("banner");
setRandom("rev");

});
</script>
