jQuery(document).off('submit', '#rh-job-search-quick-search-form');
        jQuery(document).on('submit', '#rh-job-search-quick-search-form', function(e) {
            e.stopImmediatePropagation();
            e.preventDefault();
            if (jQuery('#edit-rh-job-search-qs-country').val() == "-1") {
                alert(Drupal.t("Please select a Country"));
            } else if (jQuery('#job-search-specialization select').val() == "-1") {
                alert(Drupal.t("Please select a Specialization"));
            } else {
                var url = jQuery('#job-search-specialization select').val();
                url += '?keywords=' + jQuery('#edit-rh-job-search-qs-keywords').val();
                url += '&location=' + jQuery('#edit-rh-job-search-qs-location').val();
                window.location.href = url;
            }
            return false;
        });

});

