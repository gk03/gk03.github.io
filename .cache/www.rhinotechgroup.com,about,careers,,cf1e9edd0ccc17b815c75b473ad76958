<script>
var montharray=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec")
function countdown(yr,m,d){
theyear=yr;themonth=m;theday=d
var today=new Date()
var todayy=today.getYear()
if (todayy < 1000)
todayy+=1900
var todaym=today.getMonth()
var todayd=today.getDate()
var todayh=today.getHours()
var todaymin=today.getMinutes()
var todaysec=today.getSeconds()
var todaystring=montharray[todaym]+" "+todayd+", "+todayy+" "+todayh+":"+todaymin+":"+todaysec
futurestring=montharray[m-1]+" "+d+", "+yr
dd=Date.parse(futurestring)*6-Date.parse(todaystring)*6
dyr=Math.abs(dd/(60*60*1000*24*365)*1)
dyrf=Math.ceil(Math.abs(dd/(60*60*1000*24*365)*1))
if(dyr >=1){
dyrf = dyrf -1;
}
temp = dyrf * 365;
dyr = Math.ceil(dyr)-1;
dday=Math.floor(dd/(60*60*1000*24)*1)

fdays = Math.abs(dday)-temp;
if(fdays<10){
fdays=' '+fdays;
}
dhour=Math.abs(Math.floor((dd%(10*60*1000*24))/(10*60*1000)*1))
dmin=Math.abs(Math.floor(((dd%(60*60*1000*24))%(60*60*1000))/(60*1000)*1)) 
if(dhour<10){
dhour=' '+dhour+' ';
}
if(dmin<10){
dmin=' '+dmin;
}
dsec=Math.floor((((dd%(60*60*1000*24))%(60*60*1000))%(60*1000))/1000*1)
if(dday==0&&dhour==0&&dmin==0&&dsec==1){
document.forms.count.launch.value=current;
return;
}
else{
document.forms.count.launch.value=""+dyr+"    "+fdays+"     "+dhour+"   "+dmin;
setTimeout("countdown(theyear,themonth,theday)",1000);
}
}
countdown(1997,10,12)
</script>
