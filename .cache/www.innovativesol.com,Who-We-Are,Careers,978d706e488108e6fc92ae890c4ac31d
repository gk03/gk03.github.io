<script>

    $(document).ready(function () {

        // Removes Excess "Height" Made from Padding on DNNEmptyPanes
        $(".row").has(".DNNEmptyPane").css("padding", "0");

    });

</script>

</div>

<script>

    $(document).ready(function () {

        setUpContentPods();

        $(window).resize(function () {

            setUpContentPods();

        });

    });


    function setUpContentPods() {

        if (window.innerWidth > 1200) {

            var maxHeightThreeCol = 0;
            var maxHeightFourCol = 0;

            $('#threeColumn').find('section > div').each(function () {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    $(this).parent('section').addClass('hasBorder');

                    //get max height
                    if ($(this).parent('section').outerHeight() > maxHeightThreeCol) {

                        maxHeightThreeCol = $(this).parent('section').outerHeight()

                    }

                }

            });


            $('#threeColumn').find('section > div').each(function () {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    //set max height
                    $(this).parent('section').css('height', maxHeightThreeCol);

                }

            });


            $('#fourColumn').find('section > div').each(function () {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    $(this).parent('section').addClass('hasBorder');


                    //get max height
                    if ($(this).parent('section').outerHeight() > maxHeightFourCol) {

                        maxHeightFourCol = $(this).parent('section').outerHeight()

                    }

                }

            });

            $('#fourColumn').find('section > div').each(function () {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    //set max height
                    $(this).parent('section').css('height', maxHeightFourCol);

                }

            });

        } else {

            var isFourColumnLastDiv = $('#fourColumn section > div').length;
            var isThreeColumnLastDiv = $('#threeColumn section > div').length;

            $('#threeColumn section, #fourColumn section').css('height', 'auto');

            $('#threeColumn').find('section > div').each(function (index, element) {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    $(this).parent('section').addClass('hasBorder');

                    //remove border on last div
                    if ((index + 1) == isThreeColumnLastDiv) {

                        $(element).parent('section').css('border-bottom', '0');

                    }

                }

            });

            $('#fourColumn').find('section > div').each(function (index, element) {

                var isEmptyPane = $(this).hasClass('DNNEmptyPane');

                if (!isEmptyPane) {

                    $(this).parent('section').addClass('hasBorder');

                    //remove border on last div
                    if ((index + 1) == isFourColumnLastDiv) {

                        $(element).parent('section').css('border-bottom', '0');

                    }

                }

            });

        }

    }

</script>
