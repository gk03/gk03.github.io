<script language="javascript">
	function validateSendEmail(f) {
		document.getElementById('questionCommentFormSent').style.display = 'none';
		if (isblank(f.contactName.value) || f.contactName.value == 'Name *'){
			alert('Please enter your Name');
			f.contactName.focus();
			return false;
		}
		if (isblank(f.contactEmail.value) || f.contactEmail.value == 'Email *'){
			alert('Please enter your Email address');
			f.contactEmail.focus();
			return false;
		}
		if (!checkEmail(f.contactEmail)) {
			alert('Invalid Email address');
			f.contactEmail.focus();
			return false;
		}
		if (isblank(f.contactComment.value) || f.contactComment.value == 'Comments or Questions *'){
			alert('Please enter your Comments or Questions');
			f.contactComment.focus();
			return false;
		}
		var wContactComment = f.contactComment.value.replace(/\|/g, '/');
  		u = new utilityCFC();
  		u.setCallbackHandler(utilResult);
  		u.sendCommentEmail(f.contactEmail.value + '|' + f.contactName.value + '|' + wContactComment);
  		}
		
	function utilResult(r) {
		if (isblank(r)){
			document.questionCommentForm.contactComment.value = '';
			document.getElementById('questionCommentFormSent').style.display = '';
		}
		else {
	     	alert(r);
		}
	}
</script>


<!-- Contact Form -->
<div class="contact">
	<h2>Questions or Comments</h2>
	<form name="questionCommentForm">
		<p>
	   	<span><input type="text" name="contactName" value="Name *" class="inputtext" onfocus="if (this.value == 'Name *') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Name *';}" /></span>
		<span><input type="text" name="contactEmail" value="Email *" class="inputtext" onfocus="if (this.value == 'Email *') {this.value = '';}" onblur="if (this.value == '') {this.value = 'Email *';}" /></span>
		</p>
		<p>
	   	<textarea name="contactComment" class="inputtext">Comments or Questions *</textarea>
		</p>
		<p>
	   	<span class="button button-grey"><input type="button" value="Submit" onclick="validateSendEmail(this.form);" /></span>
	   	<span style="float:right;line-height:36px;padding-right:.5em;">* Required Field</span>
		</p>
		<p>
		<span id="questionCommentFormSent" style="display:none;margin-left:5px;padding-right:.5em;">Your question and/or comment has been sent to Marinalife. We welcome all of your comments and suggestions that will help us serve you better.</span>
		</p>
	</form>
</div>
<!-- End Contact Form -->



                    
                    
