				<script type="text/javascript">						
					delayedAlert();			
					$(document).ready(function(){						
						delayedAlert();	  
					});
					
					function delayedAlert()  
					{  
						timeoutID = window.setTimeout(registerNowPopUp, 15000);
					}  
					
					function registerNowPopUp()
					{ 
						jQuery(document).ready(function(){
							tb_show("Marinalife.com","http://www.marinalife.com/register/register-signup-new.cfm?qf_register=1&thisReferrerLink=1&hcnt=1&TB_iframe=true&width=650&height=auto&modal=true", "");
						});
					}
				      
				    function clearAlert()  
					{  
						window.clearTimeout(timeoutID);  
					}  
			    </script>
		    

