				$('#mktoForm_1324').on('submit', function(event) {
					var _, errors;

					_ = $(this);
					errors = 0;

					_.find('.mktoRequired').each(function() {
						if ($(this).hasClass('mktoInvalid')) {
							errors++;
						}
					});

					if (errors > 0) {
						return false;
					} else {
						window.open(clickedLink.data('href'),'_newtab');
						return true;
					}
				});
			});
