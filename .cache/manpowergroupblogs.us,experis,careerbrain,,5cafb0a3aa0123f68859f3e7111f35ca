	function JobSearchSubmit9(){
		//Package up all input fields, and any other data that needs to be in query string
		//for the office locator, it's just the hidden lob input, and the search location	
		var searchKeywords9 = document.getElementById("searchKeywords9").value;
		var searchJobLocations9 = document.getElementById("searchJobLocations9").value;

	                   if ( searchKeywords9 == "Job Title, Industry or Skills" )  {  searchKeywords9 = "experis";  }
                                      if ( searchJobLocations9 == "Zip Code -or- City, State" )  {  searchJobLocations9 = "53212";  }


		location.href="http://offices.experis.us/job-search/frame.jsp?keywords1=" + searchKeywords9 + "&location1=" + searchJobLocations9 + "&lang=&inst=&start=1";
		return false;					
        }
  </script>


