<script type="text/javascript">
    var buttonID = "573a0000000HEE4";
    loadLiveAgent(buttonID);
    function loadLiveAgent(buttonID) {
        $.getScript('https://c.la2c2.salesforceliveagent.com/content/g/js/36.0/deployment.js', function () {
            window._laq = null;
            if (window._laq == null) {
                window._laq = [];
            }
            window._laq.push(function () {

                if (document.getElementById('onlineChatBar') != null) {
                    liveagent.showWhenOnline(buttonID, document.getElementById('onlineChatBar'));
                }
                if (document.getElementById('offlineChatBar') != null) {
                    liveagent.showWhenOffline(buttonID, document.getElementById('offlineChatBar'));
                }

                if (document.getElementById('onlineChatIcon') != null) {
                    liveagent.showWhenOnline(buttonID, document.getElementById('onlineChatIcon'));
                }
                if (document.getElementById('offlineChatIcon') != null) {
                    liveagent.showWhenOffline(buttonID, document.getElementById('offlineChatIcon'));
                }
            });
            liveagent.init('https://d.la2c2.salesforceliveagent.com/chat', '572a0000000LQJh', '00D300000006yn4');
            if (document.getElementById('chat-component') != null) {
                document.getElementById('chat-component').style.display = "block";
            }
            if (document.getElementById('contact-component-chat') != null) {
                document.getElementById('contact-component-chat').style.display = "block";
            }
        });
    }
</script>

