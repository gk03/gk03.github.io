<div class="chat-window-overlay" ng-controller="chatWindowController"
     ng-class="$storage.chatSession.minimized ? 'minimized' : 'maximized'" ng-show="$storage.chatSession.visible"
     ng-cloak>

    <div class="chat-window-overlay__bar"
         ng-class="$storage.chatSession.activity && $storage.chatSession.minimized ? 'active':''">
            <span ng-click="toggleChat()" class="chat-window-overlay__toggle-icon"
                  ng-class="{'icon-maximize' : $storage.chatSession.minimized && !$storage.chatSession.activity, 'icon-maximize-white' : $storage.chatSession.minimized && $storage.chatSession.activity, 'icon-minimize' : !$storage.chatSession.minimized}"
                  id="chat-window-minimize-maximize"></span>

        <p class="chat-window-overlay__headline"
           ng-class="$storage.chatSession.activity && $storage.chatSession.minimized ? 'active':''">Chat with CA</p>
        <span ng-click="endChat(true)" class="chat-window-overlay__close"
              ng-class="$storage.chatSession.activity && $storage.chatSession.minimized ? 'icon-close-white':'icon-close'"
              id="chat-window-closex"></span>
    </div>

    <div class="chat-window-overlay__area" ng-class="!$storage.chatSession.active && !$storage.chatSession.established ? 'scroll' : ''">

        <!-- initial request form -->
        <form name="chatWindowForm" class="chat-window-overlay__request-form" ng-show="!$storage.chatSession.active && !$storage.chatSession.established" ng-submit="establishChat(request)">

            <div class="chat-window-overlay__welcome">
                <p>Just give us some brief information and we'll connect you to the right CA Expert.</p>
                <p>Our hours of availability are 8AM - 5PM CST.</p>
            </div>

            <p><i>All Fields Required</i></p>

            <p ng-if="engineer">
                <label>First Name
                    <input type="text" ng-model="request.firstName" required
                           class="chat-window-overlay__user-input"></label>
            </p>

            <p ng-if="engineer">
                <label>Last Name
                    <input type="text" ng-model="request.lastName" required
                           class="chat-window-overlay__user-input"></label>
            </p>

            <p ng-if="engineer">
                <label>Email Address</label>
                <input type="text" ng-model="request.email" required
                       class="chat-window-overlay__user-input"></label>
            </p>
            <label>How can we help you?
            <textarea rows="4" ng-model="request.message" required class="chat-window-overlay__user-input"
                      ng-enter="establishChat(request)"></textarea></label>
        </form>

        <!-- establishing chat connection -->
        <div class="chat-window-overlay__establishing"
             ng-if="$storage.chatSession.active && !$storage.chatSession.established && !$storage.chatSession.failed"
             ng-show="$storage.chatSession.active && !$storage.chatSession.established && !$storage.chatSession.failed && !$storage.chatSession.minimized">
            <p><img src="/etc/designs/digital-experience/cacom/images/preloader.gif" alt="connecting"></p>

            <p>We're matching your request.</p>
        </div>

        <!-- chat connection failed -->
        <div class="chat-window-overlay__establishing"
             ng-if="$storage.chatSession.active && $storage.chatSession.failed">
            <p>Unfortunately, we can't connect you to an agent. If you are not automatically redirected please <a ng-href="{{callbackURL}}">click here</a>.</p>
        </div>

        
        <ul class="chat-window-overlay__conversation" ng-if="$storage.chatSession.established"
            scroll-bottom="$storage.chatSession.messages" scroll-bottom-increase="20"
            ng-class="$storage.chatSession.ended ? 'ended' : ''">

            
            <li ng-repeat-start="message in $storage.chatSession.messages"
                ng-if="message.profile && message.agentProfile.name"
                class="chat-window-overlay__conversation-profile">
                <img ng-src="{{message.agentProfile.profileImage}}">
                <p>{{message.agentProfile.name}} will be helping you today.</p>
                <p><a ng-href="{{message.agentProfile.page}}"><strong>View Profile</strong></a></p>
            </li>

            <li class="chat-window-overlay__conversation-rule" ng-if="!$first && message.agentProfile.name != $storage.chatSession.messages[$index - 1].agentProfile.name">
                <hr>
            </li>

            
            <li class="chat-window-overlay__conversation-item"
                ng-class="message.self ? 'user-message' : 'agent-message'"
                ng-if="!message.profile && !message.status && !message.ended">
                <img ng-src="{{message.agentProfile.profileImage}}" ng-if="!message.self" class="chat-window-overlay__conversation-picture">
                <span ng-bind-html="message.text | autoLink:'_blank'"></span>
            </li>

            
            <li ng-if="message.status" class="chat-window-overlay__conversation-status">
                <p ng-if="message.text == 'transfer'"><strong>Transfered to {{message.agentProfile.name}}</strong></p>

                <p ng-if="message.text == 'joined'"><strong>{{message.agentProfile.name}} joined the
                    conversation</strong></p>

                <p ng-if="message.text == 'left'"><strong>{{message.agentProfile.name}} left the conversation</strong>
                </p>
            </li>

            
            <li ng-repeat-end ng-if="message.ended" class="chat-window-overlay__conversation-completed">
                <p><span class="chat-window-overlay__conversation-completed-icon icon-check"></span></p>

                <p><strong>Your chat with {{$storage.chatSession.messages[$index - 1].agentProfile.name}} has
                    ended.</strong><br>
                    Thank you for your interest in CA.</p>
                <hr>
                <div class="chat-window-overlay__survey">
                    <p><strong>How Did We Do?</strong><br>
                        Let us know how we did so that we can maintain a quality experience.</p>

                    <p><a href="http://fluidsurveys.com/s/ca-tech-chat-survey/" target="_blank">Take Our Survey ></a>
                    </p>
                </div>
                <div class="chat-window-overlay__survey">
                    <input-stars max="5" on-star-click="submitRating(rating)" ng-model="rating"></input-stars>
                </div>
            </li>
        </ul>

        <div ng-if="$storage.chatSession.established && !$storage.chatSession.ended"
             ng-show="!$storage.chatSession.minimized" class="chat-window-overlay__user-input-area">
            <p ng-show="$storage.chatSession.agentTyping" class="chat-window-overlay__typing">
                <small>agent is typing</small>
            </p>
            <textarea rows="2" ng-model="request.message" class="chat-window-overlay__user-input"
                      ng-enter="sendChatMessage()" placeholder="Start typing..."></textarea>
        </div>

        <div class="chat-window-overlay__button-area" ng-hide="$storage.chatSession.minimized">
            <!-- start new chat -->
            <p ng-if="!$storage.chatSession.active && !$storage.chatSession.established">
                <a href="#" ng-click="establishChat(request)" class="chat-window-overlay__button" id="chat-window-request-chat" ng-disabled="chat-window-form.$invalid">Request Chat</a>
            </p>
            <!-- cancel establishing chat -->
            <p ng-if="$storage.chatSession.active && !$storage.chatSession.established">
                <a href="#" ng-click="endChat(true)" class="chat-window-overlay__button" id="chat-window-cancel-chat">Cancel
                    Chat</a>
            </p>
            <!-- send message / end chat -->
            <p ng-if="$storage.chatSession.established && !$storage.chatSession.ended">
                <a href="#" ng-click="sendChatMessage()" class="chat-window-overlay__button chat-window-overlay__action-left" id="chat-window-send">Send</a>
                <a href="#" ng-click="endChat()" class="chat-window-overlay__button alt chat-window-overlay__action-right" id="chat-window-close-chat">End Chat</a>
            </p>

            <!-- close chat -->
            <p ng-if="$storage.chatSession.ended">
                <a href="#" ng-click="endChat(true)" class="chat-window-overlay__button" id="chat-window-ended-close-chat">Close Chat</a>
            </p>
        </div>

