			function reloadImg2()
			{
				if (document.getElementById('imgid').src.indexOf('&d') !== -1)
				{
					document.getElementById('imgid').src = document.getElementById('imgid').src.substring(0, document.getElementById('imgid').src.indexOf('&d')) + '&d' + new Date().getTime();
