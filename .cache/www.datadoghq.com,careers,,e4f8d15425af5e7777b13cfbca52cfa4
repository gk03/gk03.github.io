<script>
	jQuery(document).ready(function($){

		$('.button-alt').on('click',function(){
		   $('html, body').animate({
		        scrollTop: $(".above-departments").offset().top -66
		    }, 1000);
		})

		  $('.job-listings').mixItUp({
		  	layout:{
		  		display: 'block'
		  	},
		  	load : {
		  		filter: false,
		  		duration: 100
		  	},
		  	animation: {
	  			enable: false,
	  			effects: 'fade',
	  			easing: 'ease',
	  			duration: 100
	  		},
		  	callbacks: {
		  		onMixFail: function(state){

		  		},
		  		onMixStart: function(state, futureState){

  				},
	  			onMixEnd: function(state) {

	  				var container_count = $('.job-listings').length;
	  				console.log(container_count);
	  				if ($(".job-listings").length == $(".job-listings.fail").length) {
	  				    $('.no-jobs').show();
	  				}else{
	  					$('.no-jobs').hide();
	  				}

	  				var currentFilter = state.activeFilter;

	  				var container_id = $(this).attr('ID');

	  				var visible_container = $('#'+container_id).not('#'+container_id+'.fail')

	  				if(visible_container.length >= 1){

	  					var visible =  $(this).attr('ID');

	  					if(visible){

	  						$('#'+visible).addClass('fadein');

	  					}

	  				}else if(visible_container.length < 1){

	  					var empty_container = $('#'+container_id).attr('ID');

	  				}

	  				totalItems = state.$show.length;

  					if(currentFilter === ".newyork"){

  						$('.above-departments').removeClass('bg-boston');
  						$('.above-departments').removeClass('bg-france');
  						$('.above-departments').removeClass('bg-all-jobs');
  						$('.above-departments').addClass('bg-newyork');
  						$('.above-departments .masthead-blurb').hide();
  						$('.above-departments .brand-light').text('New York');
  					}
  					if(currentFilter === ".boston"){
  						$('.above-departments').removeClass('bg-newyork');
  						$('.above-departments').removeClass('bg-france');
  						$('.above-departments').removeClass('bg-all-jobs');
  						$('.above-departments').addClass('bg-boston');
  						$('.above-departments .masthead-blurb').hide();
  						$('.above-departments .brand-light').text('Boston');
  					}
  					if(currentFilter === ".france"){
  						$('.above-departments').removeClass('bg-newyork');
  						$('.above-departments').removeClass('bg-boston');
  						$('.above-departments').removeClass('bg-all-jobs');
  						$('.above-departments').addClass('bg-france');
  						$('.above-departments .masthead-blurb').hide();
  						$('.above-departments .brand-light').text('Paris');
  					}

	  			}
		  	}

		  });

			$('.locations button').on("click",function(){
				var location = $(this).data("location");
				//$('.job-listings').not('.fail').fadeIn(1500);

				if($('.departments').is(':visible')) {
				    $('.departments').slideDown().hide();
				    $('.jobs').slideDown().removeClass('hidden');
				    $('.view-departments').removeClass('active');
				}
				var uls = $('.job-listings ul');

				$.each(uls, function(){

				    $(this).find('li').css('border-bottom','none');
				    $(this).find('li:visible:last').css('border-bottom','1px solid #979797');

				});
			});
			$('.view-all-jobs').on('click',function(){
				$('.departments').hide();
				//$('.job-listings').not('.fail').fadeIn("slow");
				$('.view-departments').removeClass('active');
				$('.above-departments').removeClass('bg-boston')
				$('.above-departments').removeClass('bg-france')
				$('.above-departments').addClass('bg-all-jobs')
				$('.above-departments').removeClass('bg-newyork');
				$('.above-departments .masthead-blurb').hide();
				$('.above-departments .brand-light').text('Come join us');
			});
			$('.view-departments').on('click',function(){
				$('.departments').fadeIn("slow");
				$('.jobs').hide();
				$('.view-departments').addClass('active');
				$('.job-listings').mixItUp('filter', '');
				$('.view-all-jobs').removeClass('active');
				$('.above-departments').removeClass('bg-newyork');
				$('.above-departments').removeClass('bg-boston');
				$('.above-departments').removeClass('bg-all-jobs');
				$('.above-departments').removeClass('bg-france');
				$('.above-departments .masthead-blurb').show();
				$('.above-departments .brand-light').text('This is where you come in');

			});

	});
</script>
