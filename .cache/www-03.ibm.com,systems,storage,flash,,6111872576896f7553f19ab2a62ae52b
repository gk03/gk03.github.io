<script src="//1.www.s81c.com/common/v18/js/dyntabs.js"></script>
<script>
	/*
	 component/v18_smooth_scroll_to_id_element 
	 */
	var gap = 0;
	var sitenavHeight = (jQuery('#ibm-leadspace-height').width() >= 907 && jQuery('.ibm-sitenav-menu-container').length) ? jQuery(
			'.ibm-sitenav-menu-container').height() + 1
			: 0;
	var mastheadHeight = sitenavHeight;

	function reCalculate() {
		sitenavHeight = (jQuery('.ibm-sitenav-menu-container').css('display')
				.indexOf('none') != 0 ? jQuery('.ibm-sitenav-menu-container')
				.height() + 1 : 0);
		mastheadHeight = sitenavHeight;
		mastheadHeight += jQuery('.ibm-mhtype-minimal').hasClass(
				'hastransition') ? (jQuery('body').hasClass(
				'ibm-masthead-sticky-showing') ? jQuery('#ibm-masthead')
				.height() : 0) : 0;
		var onTopShowing = !jQuery('body').hasClass('ibm-sitenav-menu-sticky');
		var scrolledABit = jQuery('body').hasClass('ibm-sitenav-menu-sticky')
				&& !jQuery('body').hasClass('ibm-masthead-sticky');
		var scrolledAndShowing = jQuery('body').hasClass(
				'ibm-masthead-sticky-showing');
		var scrolledMoreAndNotShowing = jQuery('body').hasClass(
				'ibm-masthead-sticky')
				&& !jQuery('body').hasClass('ibm-masthead-sticky-showing');
		var sitenavHidden = jQuery('.ibm-sitenav-menu-container')
				.css('display').indexOf('none') == 0;

		if (onTopShowing || scrolledABit || scrolledAndShowing) {
			if (sitenavHidden) {
				gap = mastheadHeight - sitenavHeight;
			} else {
				gap = mastheadHeight;
			}
		}

		if (scrolledMoreAndNotShowing) {
			if (sitenavHidden) {
				gap = 0;
			} else {
				gap = sitenavHeight;
			}
		}
	}

	function setSmoothScroll(anchorSetSelector) {
		jQuery(anchorSetSelector).click(function(e) {
			e.preventDefault();
			var to = jQuery.attr(this, 'href').toString();

			jQuery("html, body").stop().animate({
				scrollTop : jQuery(to).offset().top - gap
			}, 300, "linear").promise().done(function() {
				reCalculate();
				jQuery("html, body").stop().animate({
					scrollTop : jQuery(to).offset().top - gap
				}, 50, "swing");
			});
		});
	}
</script>
<script>
jQuery(document).ready(function() {
        //Define selector for anchor links:
        var anchorSetSelector = 'div#s-0 a';
        setSmoothScroll(anchorSetSelector);
    });
</script>

