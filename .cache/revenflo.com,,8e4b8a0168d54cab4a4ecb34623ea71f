				<script>
					var id;
					var sxsw = {

						full_bleed: function(boxWidth, boxHeight, imgWidth, imgHeight) {

							// Calculate new height and width...
							var initW = imgWidth;
							var initH = imgHeight;
							var ratio = initH / initW;

							imgWidth = boxWidth;
							imgHeight = boxWidth * ratio;

							// If the video is not the right height, then make it so...     
							
							if(imgHeight < boxHeight){
								imgHeight = boxHeight;
								imgWidth = imgHeight / ratio;
							}
							
							
							//  Return new size for video
							return {
								width: imgWidth,
								height: imgHeight
							};

						},

					};				
					jQuery( document ).ready(function() {
					
						 function recalculateFills() {
							 //get pixel size of browser window.
							var browserHeight = Math.round(jQuery(window).height());
							var browserWidth = Math.round(jQuery(window).width());
							
							//jquery all items on page with fill tag
							var fills = jQuery('.fill');
							
							
							//for each fill, recalculate size and position and apply using jQuery
							fills.each(function () {
								
							//height of element. not neccessarily video
							var videoHeight = jQuery(this).data( "vidheight");
							console.log('Height: ' + videoHeight);
							var videoWidth = jQuery(this).data( "vidwidth");
							console.log('Width: ' + videoWidth);


							//calculate new size
							var new_size = sxsw.full_bleed(browserWidth, browserHeight, videoWidth, videoHeight);

							//distance from top and left is half of the difference between the browser width and the size of the element
							jQuery(this)
								.width(new_size.width)
								.height(new_size.height)
								.css("margin-left", ((browserWidth - new_size.width)/2))
								.css("margin-top", ((browserHeight - new_size.height)/2));
								
							});
						}
						 
						 
						recalculateFills();
						 
						jQuery(window).resize(function () {
							clearTimeout(id);
							id = setTimeout(recalculateFills, 1);
						});						 
					});
				</script>
