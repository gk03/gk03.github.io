		<script type="text/javascript">
			/*testimonial arrow right click*/
			jQuery(document).ready(function() {
				jQuery('.touch .testimonials-slider .testimonial-set').swipe( {
				  swipe:function(event, direction, distance, duration, fingerCount) {
					if(direction == 'left') {
						testimonialsRight();
					} else if(direction == 'right') {
						testimonialsLeft();
					}
				  },
				  threshold:0,
				  fingers:'all'
				});	
				jQuery('.testimonials-slider .arrow.right').click(testimonialsRight);				
				function testimonialsRight() {
					var outer = jQuery('.testimonials-slider .testimonials-outer');
					var inner = jQuery('.testimonials-slider .testimonials-inner');
					
					var outerWidth = outer.width();
					var innerWidth = inner.width();
					
					var fullWidth = jQuery('.testimonials-slider .testimonials-fullwidth').width();
					var percentage = Math.round(fullWidth / outerWidth);			
					
					var left = inner.position().left;
					if (percentage == 4) {
						var last = jQuery('.testimonials-slider .testimonial-set').last().children().length;
						var testimonials = Math.round(innerWidth / outerWidth) - (4 - last);
					}else {
						var testimonials = Math.round(innerWidth / outerWidth);
					}
					
					var leftNumber = - Math.round(left / outerWidth);
					if (testimonials > (leftNumber + 1)) {
						inner.css('left', ((-leftNumber - 1) * (100 / percentage)) + '%');
						if (testimonials <= (leftNumber + 2)) {
							jQuery('.testimonials-slider .arrow.right').css('opacity', '.2');
						}
						jQuery('.testimonials-slider .arrow.left').css('opacity', 1);
					}
				}
				/*testimonial arrow left click*/
				jQuery('.testimonials-slider .arrow.left').click(testimonialsLeft);
				function testimonialsLeft() {
					var outer = jQuery('.testimonials-slider .testimonials-outer');
					var inner = jQuery('.testimonials-slider .testimonials-inner');
					var outerWidth = outer.width();
					var innerWidth = inner.width();				
					
					var fullWidth = jQuery('.testimonials-slider .testimonials-fullwidth').width();
					var percentage = Math.round(fullWidth / outerWidth);
					
					var left = inner.position().left;
					var testimonials = Math.round(innerWidth / outerWidth);
					var leftNumber = - Math.round(left / outerWidth);
					console.log('Left: ' + leftNumber);
					if (leftNumber > 0) {
						if ((leftNumber) < 2) {
							console.log('Less than 2?');
							jQuery('.testimonials-slider .arrow.left').css('opacity', '.2');
						}					
						inner.css('left', ((-leftNumber + 1) * (100 / percentage)) + '%');

						jQuery('.testimonials-slider .arrow.right').css('opacity', 1);
					}
				}
				
				/*used when the browser window resizes to adjust the testimonial slider*/
				function resetSlider() {
				
					var outer = jQuery('.testimonials-slider .testimonials-outer');
					var inner = jQuery('.testimonials-slider .testimonials-inner');

					var outerWidth = outer.width();
					var innerWidth = inner.width();
					
					var fullWidth = jQuery('.testimonials-slider .testimonials-fullwidth').width();
					var percentage = Math.round(fullWidth / outerWidth);

					var left = inner.position().left;
					if (percentage == 4) {
						var last = jQuery('.testimonials-slider .testimonial-set').last().children().length;
						var testimonials = Math.round(innerWidth / outerWidth) - (4 - last);
					}else {
						var testimonials = Math.round(innerWidth / outerWidth);
					}
					var leftNumber = - Math.round(left / outerWidth);
					if (testimonials > (leftNumber + 1)) {
						inner.css('left', ((-leftNumber) * (100 / percentage)) + '%');
					}
					if (leftNumber == 0) {
						jQuery('.testimonials-slider .arrow.left').css('opacity', '.2');
					} else {
						jQuery('.testimonials-slider .arrow.left').css('opacity', 1);
					}					
				}
				
				jQuery(window).resize(function(){
					if (document.body.clientWidth > 768) {
						resetSlider();
					}
				});
			});
			</script>
	</div>
			<script>
			function moveClientSlides() {
				if ( typeof moveClientSlides.slidcounter == 'undefined' ) {
					moveClientSlides.slidcounter = 0;
				}
				var clientSliderWidth = jQuery('.client-slide:first-child()').width();
				if (clientSliderWidth > 0) {
					var innerContainer = jQuery('.client-slider-inner');
					innerContainer.css('width', clientSliderWidth);
				}
				var innerWidth = innerContainer.width();
				var outerWidth = jQuery('.client-slider .client-slider-outer').width();

				if (innerWidth > outerWidth) {
					jQuery('.client-slider .white-fade').show();
					var slides = jQuery('.client-slider .client-slider-inner .client-slide');
					
					var duration = jQuery('.client-slider .client-slider-inner .client-slide .client-logo').length *800;	
					
					jQuery.each(slides, function(index, value) {
						
						switch ((moveClientSlides.slidcounter + index) % 3) {
							
							case 0:
								jQuery(this).css('opacity', 1);
								jQuery(this).css('z-index', 3);
								jQuery(this).animate({ left: "-100%"}, duration, "linear");
								break;
							case 1: 
								jQuery(this).css('opacity', 0);
								jQuery(this).css('z-index', 1);
								jQuery(this).animate({ left: "100%"}, 0, "linear");
								break;
								
							case 2: 
								jQuery(this).css('opacity', 1);
								jQuery(this).css('z-index', 3);
								jQuery(this).animate({ left: "0%"}, duration,"linear");
								break;
						}
					});
					moveClientSlides.slidcounter = (moveClientSlides.slidcounter + 1) % 3;
				} else {
					jQuery('.client-slider .client-slider-inner .client-slide:nth-child(2)').css('opacity', 0);
					jQuery('.client-slider .white-fade').hide();
				}
			}	
			jQuery( window ).load(function() {	
				var clientSliderWidth = jQuery('.client-slide:first-child()').width();
				if (clientSliderWidth > 0) {
					var innerContainer = jQuery('.client-slider-inner');
					innerContainer.css('width', clientSliderWidth);
				}	
				/*initialize values from jQuery perspective*/
				var duration = jQuery('.client-slider .client-slider-inner .client-slide .client-logo').length *800;	
				jQuery('.client-slider .client-slider-inner .client-slide:first-child').css('left', "0%");
				jQuery('.client-slider .client-slider-inner .client-slide:first-child').css('opacity', 1);
				jQuery('.client-slider .client-slider-inner .client-slide:nth-child(2)').css('z-index', 3);
				jQuery('.client-slider .client-slider-inner .client-slide:nth-child(2)').css('left', "100%");
				jQuery('.client-slider .client-slider-inner .client-slide:nth-child(2)').css('opacity', 1);
				jQuery('.client-slider .client-slider-inner .client-slide:nth-child(3)').css('left', "100%");
				jQuery('.client-slider .client-slider-inner .client-slide:nth-child(3)').css('opacity', 0);
				
				var logos = jQuery('.client-logo > img');
				jQuery.each(logos, function(index, value) {
					var imageHeight = jQuery(this).height();
					if(imageHeight < 70) {
						jQuery(this).css('margin-top', ((70 - imageHeight) / 2) + 'px');
					}
				});				
				//first-slide woo
				moveClientSlides();
				var t = setInterval(function(){moveClientSlides()}, duration);			
			});
		</script>
