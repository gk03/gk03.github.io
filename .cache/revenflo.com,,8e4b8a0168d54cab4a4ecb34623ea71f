		<script type="text/javascript">
			jQuery( document ).ready(function() {
				/*fancy hash-link scroll*/
				jQuery(".scroll").click(function(e){	
					e.preventDefault();
					jQuery('body').animate({scrollTop:jQuery(this.hash).offset().top}, 500);
				});
				/*fix for incorrect character in Amatic font set*/
				jQuery(".panel-title:contains('?'), h1:contains('?'), h2:contains('?'), h3:contains('?')").each(function() {
					var newText = jQuery(this).html().replace("?", "<span class='amaticQmark'>?</span>");
					jQuery(this).html(newText);
				});
				
				/*disable the 3 top level menu items*/
				jQuery('a[href="http://revenflo.com/about/"], a[href="http://revenflo.com/services/"], a[href="http://revenflo.com/markets/"]').css('cursor', 'default');
				jQuery('a[href="http://revenflo.com/about/"], a[href="http://revenflo.com/services/"], a[href="http://revenflo.com/markets/"]').on('click', function(e) {
					e.stopImmediatePropagation();
					e.stopPropagation();
					e.preventDefault();

					return false;
				});
				/*prevents initial load of page when navigating the menu, waits for a second click*/
				jQuery('.touch .menu-icon').on('click', function (e) {
					if (jQuery(this).hasClass('active')) {
						return true;
					}	
					jQuery('.touch .menu-icon').removeClass('active');
					e.stopImmediatePropagation();
					e.stopPropagation();
					e.preventDefault();
					jQuery(this).addClass('active');
					return false;
				});
				/*closes search when content is clicked*/
				jQuery('#top, #content, .footer').on('touchstart', function (e) {
					if (jQuery('.main-search').hasClass('open')) {
						jQuery('.touch .main-search').removeClass('open');
					}
				});
				/*fix for search on mobile devices*/
				jQuery('.touch .main-search').on('click', function (e) {			
					e.stopImmediatePropagation();
					e.stopPropagation();
					e.preventDefault();				
					if (jQuery(this).hasClass('open')) {
						jQuery(this).removeClass('open'); 
						return;
					}
					jQuery(this).addClass('open');
					jQuery('.main-search > .inner-search div input[type=text]').focus();
					return false;
				});	
				if (Modernizr.mq('only screen and (max-width: 768px)')) {
					jQuery('a[rel="prettyPhoto"]').unbind('click');
					//jQuery('a[rel!^="prettyPhoto"]').attr("target", "_blank");
				}				
	
			});	
		</script>
		
