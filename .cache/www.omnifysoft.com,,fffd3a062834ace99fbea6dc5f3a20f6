<script language="JavaScript" type="text/javascript">
function ShowProductGroup(objectid)
{
    HideProductGroups();
    if(objectid > -1)
    {
        document.getElementById("currPage").value = objectid;
        document.getElementById("desc_ProductGroup_" + objectid).style.display = "";
        document.getElementById("cell_ProductGroup_" + objectid).className = "ProductHighlightHover";
    }
}
function HideProductGroups()
{
    for(var i = 0; i < 11; i++)
    {
        document.getElementById("desc_ProductGroup_" + i).style.display = "none";
        document.getElementById("cell_ProductGroup_" + i).className = "ProductHighlight";
    }
}
function MouseOutProductGroup(hItem, pageindex)
{
    var strClassName = "ProductHighlight";
    var defPageObj = document.getElementById("currPage");
    if(defPageObj != null)
    {
        if(defPageObj.value == pageindex)
            strClassName = "ProductHighlightHover";
    }
    hItem.className=strClassName;
}

</script>

