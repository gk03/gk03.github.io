<script language="JavaScript" type="text/javascript">
function RunSearch()
{
    if(document.getElementById('SearchText').value != "")
    {
        window.location.href = "http://www.omnifysoft.com/SiteSearch.aspx?term=" + document.getElementById('SearchText').value;
    }
}
function ToggleSubMenu(menuid, tdclass)
{
    var currDisplay = document.getElementById("menu_row_" + menuid).style.display;
    HideAllSubMenus();
    if(currDisplay == "none")
    {
        document.getElementById("menu_row_" + menuid).style.display = "";
        document.getElementById("menu_cell_" + menuid).className = tdclass;
        document.getElementById("menu_img_" + menuid).src = "images/condense_white.gif";
    }
    else
    {
        document.getElementById("menu_row_" + menuid).style.display = "none";
        document.getElementById("menu_cell_" + menuid).className = "TextWhite";
        document.getElementById("menu_img_" + menuid).src = "images/expand_white.gif";
    }
}
function HideAllSubMenus()
{
    for(var i = 1; i < 7; i++)
    {
        document.getElementById("menu_cell_" + i).className = "TextWhite";
        document.getElementById("menu_row_" + i).style.display = "none";
        document.getElementById("menu_img_" + i).src = "images/expand_white.gif";
    }
}
</script>
