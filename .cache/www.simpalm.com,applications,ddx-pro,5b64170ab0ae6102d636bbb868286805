			var swapElements = function(siblings, subjectIndex, objectIndex) 
			{
				// Get subject jQuery
				var subject = $(siblings.get(subjectIndex));
				// Get object element
				var object = siblings.get(objectIndex);
				// Insert subject after object
				subject.insertAfter(object);
			}
			$(function() 
