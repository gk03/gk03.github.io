    <script type="text/javascript">
        $(document).ready(function () {
            /* Left Nav - only show parent, child & sibling nodes of current page */
            var selectedNode = $(".localmenu li.selected");
            var parentUL = selectedNode.parent('ul');
            var isRoot = parentUL.hasClass('root');
            selectedNode.css("display", "list-item");
            /* Show parent nodes up the tree */
            selectedNode.parents('li').css("display", "list-item");
            /* Show siblings, unless root */
            if (!isRoot) parentUL.children('li').css("display", "list-item");
            /* Show all child menu items */
            selectedNode.find('li').css("display", "list-item");

            /* External menu items should open in new windows */
            $('a.menu-item:not([href^="/"])').attr('target', '_blank');
        });
    </script>
