<script type="text/javascript">

    $(document).ready(function () {

        var loginText = $("#dnn_dnnLogin_enhancedLoginLink").attr("title");
        var returnurl = $("#dnn_dnnLogin_enhancedLoginLink").attr("href");
        //hostname from the config
        var landmarkHostName = $("#dnn_dnnLogin_hdnLandmarkHostName").val();
        var landmarkruHostName = $("#dnn_dnnLogin_hdnRuLandmark").val();
        var landmarklaHostName = $("#dnn_dnnLogin_hdnLaLandmark").val();
        //Reading localizes text from resource files
        var signIntext = $("#dnn_dnnLogin_hdnsigninvalue").val();
        var Landmarktext = $("#dnn_dnnLogin_hdnLandmarkvalue").val();
        var Neftextext = $("#dnn_dnnLogin_hdnneftexvalue").val();
        var hostName = window.location.hostname;
        if (loginText == "Login" || loginText == "Вход" || loginText == "Iniciar") {

            $("#dnn_dnnLogin_enhancedLoginLink").remove();


            var loginHTML = "<div class='mobilehide'><div id='SuperFish'><ul class='mobilemenu mmstyle menuright sf-js-enabled'><li class='root first haschild child-1'>";

            if ((hostName == landmarkHostName) || (hostName == landmarkruHostName) || (hostName == landmarklaHostName)) {
                loginHTML += "<a href='#' class='sf-with-ul'>" + signIntext + "</a><ul style='display: none;'><li class='category last child-2'><a href='" + returnurl + "'>" + Landmarktext + "</a></li>";

            }
            else {
                loginHTML += "<a href='#' class='sf-with-ul'>Login</a><ul style='display: none;'><li class='category last child-2'><a href='" + returnurl + "'>iEnergy</a></li>";
               
            }
            loginHTML += "<li class='category first child-1'><a href='https://geoweb.neftex.com/Account/Login?ReturnUrl=%2f'>" + Neftextext + "</a></li></ul></li></ul></div></div>"
            $("#dnn_dnnLogin_loginGroup").append(loginHTML);
            

            $('.mobilemenu').superfish();


        }
        if (loginText == "Logout") {
            var hostName = window.location.hostname;

            if (hostName == landmarkHostName) {

                $('#dnn_dnnLogin_enhancedLoginLink').text('Sign Out');
            }

        }
        /*Links to the multilingual links on the homepage*/
        if (hostName == landmarkHostName) {

            var languageHTML = '<a   id="dnn_Regional" href="/Regional-Sites">Regional Sites</a>';
            $(".regionalSites").append(languageHTML);
        }
        else if (hostName == landmarklaHostName) {

            var languageHTML = '<a id="dnn_Regional" href="/Regional-Sites">Sitios Regionales</a>';
            $(".regionalSites").append(languageHTML);
        }

        else if (hostName == landmarkruHostName) {

            var languageHTML = '<a  id="dnn_Regional" href="/Regional-Sites">Региональные сайты</a>';
            $(".regionalSites").append(languageHTML);
        }
    });	
				
</script>
