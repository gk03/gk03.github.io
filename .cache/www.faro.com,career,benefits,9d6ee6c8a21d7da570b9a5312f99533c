<script type="text/javascript">
    $(function () {
        $('#AA_mainNavigation .rmLink').hover(
            function () {
                var itemText = $('.rmText', this).text().toUpperCase().replace("İ", "I");

                var index = $(this).attr('class').replace('rmLink rmRootLink menu_item_', '').replace(' pie_first-child', '');

                SetFlyOutVisibility(false);

                $(".AA_modMnFoArrow").hide();

                if ($('.FlyOutOuterWrapper' + index).length > 0) {
                    $(".AA_modMnFoArrow").show();

                    $(".RadMenu_Faro").find(".rmRootGroup").find(".rmItem").each(function (index) {
                        if ($(this).find("a").find("span").text().toUpperCase().replace("İ", "I") == itemText) {
                            $(".AA_modMnFoArrow").animate({ left: ($(this).position().left + ($(this).width() / 2)) + 'px' }, 200);
                        }
                    });

                    var FlyOutSelector = $('.FlyOutOuterWrapper' + index);

                    FlyOutSelector.css("top", "40px");

                    $(" .AA_FlyOut_Middle ul", FlyOutSelector).hide();
                    $(" .AA_FlyOut_MiddleSecond ul", FlyOutSelector).hide();
                    $(" .AA_FlyOut_Right div", FlyOutSelector).each(function () {
                        if ($(this).hasClass("drop-shadow") == false) {
                            $(this).css("top", "-100000px");
                        }
                    });

                    $(" .AA_FlyOut_Middle ul", FlyOutSelector).first().show();
                    $(" .AA_FlyOut_MiddleSecond ul", FlyOutSelector).first().show();
                    $(" .AA_FlyOut_Right div", FlyOutSelector).first().css("top", "20px");
                }
            },
            function () { }
        );
    });

    $(document).ready(function () {
        if ($(".AA_mainNavigation .Current").length == 0) {
            $(".AA_mainNavigation").css("height", "40px");
        }
        $(".AA_breadcrumb a").each(function (sender) {
            $(this).html(ReplaceString($(this).html()));
        });

        try {
            var pageTitle = $('input.hfPageTitle').val();
            if (pageTitle != '')
                $(".AA_breadcrumb .Current").html('<a href="' + document.URL + '">' + pageTitle + '</a>');    
        } catch(e) {

        } 
    });


    function ReplaceString(val) {
        var rs = val;
        rs = rs.replace("&lt;b&gt;", "");
        rs = rs.replace("&lt;/b&gt;", "");
        rs = rs.replace("&lt;strong&gt;", "");
        rs = rs.replace("&lt;/strong&gt;", "");
        rs = rs.replace("&lt;sub&gt;", "<sub>");
        rs = rs.replace("&lt;/sub&gt;", "</sub>");
        rs = rs.replace("&lt;sup&gt;", "<sup>");
        rs = rs.replace("&lt;/sup&gt;", "</sup>");
        rs = rs.replace("&lt;i&gt;", "");
        rs = rs.replace("&lt;/i&gt;", "");
        rs = rs.replace("&lt;u&gt;", "");
        rs = rs.replace("&lt;/u&gt;", "");
        rs = rs.replace("&lt;br&gt;", "");
        rs = rs.replace("&lt;br/&gt;", "");
        rs = rs.replace("&lt;br /&gt;", "");
        return rs;
    }
</script>
