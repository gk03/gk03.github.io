<script type="text/javascript">
    $(document).ready(function () {
        AAG_$ = window.$;
        AAG_$("#FAROLinkOverlay").dialog({
            autoOpen: false,
            draggable: false,
            resizable: true,
            modal: true,
            width: 'auto',
            height: 679,
            dialogClass: 'AA_ContentGallery_Overlay_Wrapper'
        });
        
        $('.AA_Next_btn').unbind('click').click(function (ev) {
            var gallery = $(this).attr('data-gallery');
            var itemsInGallery = $('[rel*="gallery=' + gallery + '"]');
            var indexToShow = 0;
            for (var i = 0; i < itemsInGallery.length; i++) {
                if ($(itemsInGallery[i]).attr('href') == $('.AA_Gallery iframe').attr('src')) {
                    indexToShow = i + 1;
                    break;
                }
            }
            if (indexToShow >= itemsInGallery.length) {
                indexToShow = 0;
            }
            $('.AA_Gallery iframe').attr('src', $(itemsInGallery[indexToShow]).attr('href'));
        });

        $('.AA_Prev_btn').unbind('click').click(function (ev) {
            var gallery = $(this).attr('data-gallery');
            var itemsInGallery = $('[rel*="gallery=' + gallery + '"]');
            var indexToShow = 0;
            for (var i = 0; i < itemsInGallery.length; i++) {
                if ($(itemsInGallery[i]).attr('href') == $('.AA_Gallery iframe').attr('src')) {
                    indexToShow = i - 1;
                    break;
                }
            }
            if (indexToShow < 0) {
                indexToShow = itemsInGallery.length - 1;
            }
            $('.AA_Gallery iframe').attr('src', $(itemsInGallery[indexToShow]).attr('href'));
        });

        AAG_$("a[rel]").click(function (ev) {
            ev.preventDefault();
            AAG_$("#FAROLinkOverlay").dialog('open');

            // grab wrapper element inside content
            var wrap = $(".AA_ContentGallery_Overlay_Inner Iframe");
           
            var overlayWrap = AAG_$(".AA_ContentGallery_Overlay_Wrapper");

            // load the page specified in the trigger
            wrap.attr("src", this);

            // Pass the gallery value to the previous/next buttons
            $('.AA_Prev_btn_Wrapper').hide();
            $('.AA_Next_btn_Wrapper').hide();
            $('.AA_Gallery').css('left', '0');
            if ($(this).attr("rel").toLowerCase().indexOf("gallery") >= 0) {
                var gallerySplit = $(this).attr("rel").toLowerCase().split("gallery=");
                if (gallerySplit[1] != null) {
                    var gallery = gallerySplit[1].split(",");
                    $('.AA_Next_btn').attr('data-gallery', gallery[0]);
                    $('.AA_Prev_btn').attr('data-gallery', gallery[0]);
                    $('.AA_Prev_btn_Wrapper').show();
                    $('.AA_Next_btn_Wrapper').show();
                    $('.AA_Gallery').css('left', '50px');
                }
            }

            // set height and width
            if ($(this).attr("rel").toLowerCase().indexOf("width") >= 0 && $(this).attr("rel").toLowerCase().indexOf("height") >= 0) {
                var widthSplit = $(this).attr("rel").toLowerCase().split("width=");
                var heightSplit = $(this).attr("rel").toLowerCase().split("height=");

                if (widthSplit[1] != null && heightSplit[1] != null) {
                    var width = widthSplit[1].split(",");
                    var height = heightSplit[1].split(",");
                    if (width[0] != null && height[0] != null) {
                        wrap.addClass("resizeModal");
                        wrap.removeClass("contentWrap");
                        overlayWrap.addClass("modal-resize");

                        var spaceForPrevNextBtns = $('.AA_Next_btn').is(":visible") ? 100 : 0;

                        wrap.attr("width", width[0] + "px");
                        AAG_$("#FAROLinkOverlay").css("width", parseInt(width[0]) + 62 + spaceForPrevNextBtns);
                        AAG_$("#FAROLinkOverlay .AA_ContentGallery_Overlay_Inner").css("width", parseInt(width[0]) + spaceForPrevNextBtns);
                        overlayWrap.css("left", ((AAG_$(".ui-widget-overlay").width() - AAG_$("#FAROLinkOverlay").outerWidth()) / 2));

                        wrap.attr("height", height[0] + "px");
                        AAG_$("#FAROLinkOverlay").css("height", parseInt(height[0]) + 44);
                        AAG_$("#FAROLinkOverlay .AA_ContentGallery_Overlay_Inner").css("height", parseInt(height[0]));
                    }
                }
            } 
            else {
                overlayWrap.removeClass("modal-resize");
                wrap.removeClass("resizeModal");
                wrap.addClass("contentWrap");
                wrap.attr("width", "500px");
                AAG_$("#FAROLinkOverlay").css("width", "");
                AAG_$("#FAROLinkOverlay .AA_ContentGallery_Overlay_Inner").css("width", "");
                overlayWrap.css("left", "245px");

                wrap.attr("height","650px");
                AAG_$("#FAROLinkOverlay").css("height", "");
                AAG_$("#FAROLinkOverlay .AA_ContentGallery_Overlay_Inner").css("height", "");
            }

            this.focus();
        });


        AAG_$("#FAROLinkOverlay_Close_Btn").click(function () {
            // grab wrapper element inside content
            var wrap = $(".contentWrap");
            wrap.attr('src', "");
            AAG_$("#FAROLinkOverlay").dialog('close');
        });
    });
</script><div class='sfContentBlock'><p><br /> </p> <p><br /> </p> <p><br /> </p>


</div>

