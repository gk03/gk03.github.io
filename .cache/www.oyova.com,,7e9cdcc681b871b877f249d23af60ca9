	<script>
		var slideshow_height = jQuery('#homepage_slider_container').height();
		var fade_point = (slideshow_height * .9); //set the fading point at 90% of the slideshow's height
		function parallax() {
			var scrolled = jQuery(window).scrollTop();
			
			// add box shadow
			if ((slideshow_height-scrolled) < 95) {
				//jQuery('#branding').stop().animate({height: "65px"}, 200);
				jQuery('#branding').addClass('solidbg');
			} else {
				//jQuery('#branding').stop().animate({height: "95px"}, 200);
				jQuery('#branding').removeClass('solidbg');
			} 
			
			//set the height of the div relative to how much the user has scrolled
			jQuery('#homepage_slider_container').css('height', (slideshow_height-scrolled) + 'px');
			//set the opacity as the user scrolls down
			if ((slideshow_height-scrolled) < fade_point) {
				jQuery('#homepage_slider_container #homepage_slider').css('opacity', (((slideshow_height-scrolled)/fade_point) * 0.8));
			} else {
				jQuery('#homepage_slider_container #homepage_slider').css('opacity', '1');
			}
			
		}
		
		//if(jQuery.browser.mobile){
		//} else {
		if (jQuery(document).width() > 1060) {
			jQuery(window).scroll(function(e) {
				parallax();
			});
		}

		
	</script>



