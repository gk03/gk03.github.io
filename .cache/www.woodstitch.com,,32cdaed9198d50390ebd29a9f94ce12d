<script type="text/javascript">
$(document).ready(function(){
    var slide_num=1;
    setInterval(function(){
      $('.slide'+slide_num).animate({
            left:'548px'
      }, 1000, function() {
           $(this).css('left','-548px');          
      });
      
       $('.slide_image'+slide_num).fadeOut(1000);
        
   if(slide_num == 3){
        slide_num=1; 
    }else{
    
        slide_num++; 
    }
         
     $('.slide'+slide_num).animate({
            left:'0px'
      }, 1000, function() {
      
       $('.slide_image'+slide_num).fadeIn(1000);
     });
                       
        },9000);
});
</script>

