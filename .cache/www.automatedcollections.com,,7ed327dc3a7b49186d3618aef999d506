        // PRELOAD IMAGES USING jQuery
        (function ($) {
            var cache = [];
            // Arguments are image paths relative to the current page.
            $.preLoadImages = function () {
                var args_len = arguments.length;
                for (var i = args_len; i--; ) {
                    var cacheImage = document.createElement('img');
                    cacheImage.src = arguments[i];
                    cache.push(cacheImage);
                }
            }
        })(jQuery)

        jQuery.preLoadImages("/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_1.jpg", "/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_1.jpg");
        jQuery.preLoadImages("/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_3.jpg", "/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_3.jpg");
        jQuery.preLoadImages("/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_4.jpg", "/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_4.jpg");
        jQuery.preLoadImages("/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_5.jpg", "/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_5.jpg");
        jQuery.preLoadImages("/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_6.jpg", "/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_6.jpg");

        function ChangeImage(elementid) {
            $("#home_rotator_image").attr('src', '/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/' + elementid + '.jpg');

            // set the first button background color to off
            $("#home_rotator_bg_image_1").css("background-color", "#012751");

            if (elementid == 'home_rotator_bg_image_1') {
                var content = "The sophisticated and interlocking layers of ACSI\'s multi-tiered security program ensure ACSI protects all client and consumer information from internal or external threats, theft, misuse, and abuse.";
                // set the first button background color to on
                $("#home_rotator_bg_image_1").css("background-color", "#153e72");
            } else if (elementid == 'home_rotator_bg_image_2') {
                var content = "No matter what characteristic or mix of accounts receivable your portfolio contains, ACSI delivers the highest possible returns across a broad spectrum of challenges.";
            } else if (elementid == 'home_rotator_bg_image_3') {
                var content = "Total customer satisfaction is the benchmark against which we conduct ourselves each day, not only as representatives of ACSI, but also as service providers to our clients and their customers.";
            } else if (elementid == 'home_rotator_bg_image_4') {
                var content = "Our most important task is staffing our ranks with the best and the brightest.";
            } else if (elementid == 'home_rotator_bg_image_5') {
                var content = "We build quality into every part of our organization through mandatory compliance initiatives, daily audits, and independent, third-party certifications.";
            } else if (elementid == 'home_rotator_bg_image_6') {
                var content = "ACSI\'s comprehensive strategies — from account placement through payment — target immediate and full resolution of each account while minimizing complaints.";
            }

            //$("#HomeRotatorLinkContainer_Text").val(content);

            $('#HomeRotatorText').html(content);
        }
        
        function RestoreImage() {
            $("#home_rotator_image").attr('src', '/Sitefinity/WebsiteTemplates/AutomatedCollections/App_Themes/AutomatedCollectionsTheme/Images/home_rotator_bg_image_1.jpg');
            var content = "The sophisticated and interlocking layers of ACSI\'s multi-tiered security program ensure ACSI protects all client and consumer information from internal or external threats, theft, misuse, and abuse.";
            //$("#HomeRotatorLinkContainer_Text").val(content);
            $('#HomeRotatorText').html(content);

            // set the first button background color to on	  
            $("#home_rotator_bg_image_1").css("background-color", "#153e72");
        }
	                                 
    </script> 

	
