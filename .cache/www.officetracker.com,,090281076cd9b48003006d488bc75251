<script type="text/javascript">
var graphics=new Array("otimage1.jpg","medicalofficescheduling.html","otimage2.jpg","webhosted.html","otimage3.jpg","facilityscheduling.html","otimage4.jpg","smallbusinessscheduling.html")
var current=0
var prev=0
function graphicLink()
{
window.location.href="html/"+graphics[prev+1]
}
function setBanner()
{
currentbanner=setInterval("rotateGraphics()", 5000)
}
function rotateGraphics()
{
document.getElementById('rotategraphic').src="html/"+graphics[current]
prev=current
if (current+2 < graphics.length)
{
current=current+2
}
else
{
current=0
}
}
function switchGraphic(x)
{
clearInterval(currentbanner)
current=(x-1)*2
rotateGraphics()
setBanner()
}
  </script>
  
