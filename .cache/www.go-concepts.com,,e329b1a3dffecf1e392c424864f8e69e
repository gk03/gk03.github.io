<script type="text/javascript">

    $(document).ready(function () {

        var myForm = $('#contact-form');
        var submitBtn = $('#contact-submit');

        submitBtn.toggle(false);

        myForm.validate({
            submitHandler: function (form) {
                //ajax form submit
                submitBtn.toggle(false);

                var postData = myForm.serializeArray();
                var formURL = myForm.attr("action");

                $.ajax(
                    {
                        url: formURL,
                        type: "POST",
                        data: postData,
                        success: function (data, textStatus, jqXHR) {
                            if (data.successUrl) {
                                window.location.href = data.successUrl;
                            } else {
                                alert('Sorry, there has been an error.  The web administrator has been notified.  Please try again, or call us directly.');
                                submitBtn.toggle(true);
                            }
                        },
                        error: function (jqXHR, textStatus, errorThrown) {
                            alert('Error encountered: ' + errorThrown);
                            submitBtn.toggle(true);
                        }
                    });
            }
        });

    });

    var recaptchaWidget;

    var captchaVerified = function(response) {
        $('#contact-submit').toggle(true);
    };

    var captchaExpired = function() {
        $('#contact-submit').toggle(false);
    };

    var onloadCallback = function() {
        recaptchaWidget = grecaptcha.render('g-recaptcha', {
            'sitekey': '6LdxAAQTAAAAAKpDQ32zGinS55RY6RE5Hr9Peus3',
            'theme': 'light',
            'callback': captchaVerified,
            'expired-callback': captchaExpired
        });
    };
</script>
    


