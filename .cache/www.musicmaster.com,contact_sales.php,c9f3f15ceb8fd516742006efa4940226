<script type="text/javascript">
$(document).ready(function() {
	var contactSalesForm = new inputForm({
		formElement: $('.inputForm'),
		formCheck: function() {
			if (isEmpty("#name")) {
				var formError = true;
			}

			if (isEmpty("#email") || isNotFormatted("#email",/.+@.+\..+/)) {
				var formError = true;
			}

			if (formError) {
				return false;
			} else {
				return true;
			}
		}
	});
});
</script>
