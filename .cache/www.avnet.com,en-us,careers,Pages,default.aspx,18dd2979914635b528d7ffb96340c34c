<script>
  var detectSite = "www.avnet.com";
  var s_account = "";

  detectSite = detectSite.toLowerCase();
  switch (true) {
    case /^(auth-test|test|auth).avnet\.com$/.test(detectSite):
      s_account = "avndev";
      break;
    case /^((?!test|temp|dev|auth).)*avnet\.com$/.test(detectSite):
      s_account = "avnInc";
      break;
    default:
      s_account = "avnavndemocentral";
  }

</script>
<script src="/Style Library/Avnet/Common/scripts/AppMeasurement.js"></script>
<script type="text/javascript">

  function oscStrip(a) {
    a = a.split("|").join("");
    a = a.split("&").join("");
    a = a.split("'").join("");
    a = a.split("#").join("");
    a = a.split("$").join("");
    a = a.split("%").join("");
    a = a.split("^").join("");
    a = a.split("*").join("");
    a = a.split(":").join("");
    a = a.split("!").join("");
    a = a.split("<").join("");
    a = a.split(">").join("");
    a = a.split("~").join("");
    a = a.split(";").join("");
    return a;
  }

  function getPageName() {
    var pageName = document.location.href;
    pageName = pageName.replace(/^http:\/\/www\.|^http:\/\//, '');
    var ayame = pageName.split('/');
    pageName = pageName.replace(/$\//, '');
    if (ayame.length <= 2 && (ayame[1] == 'index.asp' || ayame[1] == '')) {
      return pageName;
    } else {
      return pageName;
    }
  }

  function getSection(currentUrl) {
    var siteCollectionUrl = '/';

    switch (true) {
    case /\/en-us\/who-we-are\//.test(currentUrl):
      return "who-we-are";
      break;
    case /\en-us\/what-we-do\//.test(currentUrl):
      return "what-we-do";
      break;
    case /\en-us\/who-we-help\//.test(currentUrl):
      return "who-we-help";
      break;
    case /\en-us\/contact-us\//.test(currentUrl):
      return "contact-us";
      break;
    case /\en-us\/legal\//.test(currentUrl):
      return "contact-us";
      break;
    default:
      return "welcome";
      break;
    }

    return "";
  }


  if (!(typeof s === "undefined")) {
    //get region for site
    var strCurrentUrl = location.pathname;

    s.pageName = getPageName();
    s.pageType = "";
    s.channel = getSection(strCurrentUrl);
    s.prop1 = "Avnet";
    s.prop2 = "US";
    s.prop3 = "INC";

    //www.avnet.se, www.avnet.de, www.avnet.fr
    if (window.location.hostname == 'www.avnet.se') {
      s.prop3 = "INC SE";
    };
    if (window.location.hostname == 'www.avnet.de') {
      s.prop3 = "INC DE";
    };
    if (window.location.hostname == 'www.avnet.fr') {
      s.prop3 = "INC FR";
    };

    s.prop4 = "";
    s.prop5 = "";
    s.prop6 = "";
    s.prop7 = "";
    s.prop9 = "";

    /* Conversion Variables */
    s.campaign = "";
    s.state = "";
    s.zip = "";
    s.events = "";
    s.products = "";
    s.purchaseID = "";
    s.eVar1 = "";
    s.eVar2 = "";
    s.eVar3 = "";
    s.eVar4 = "";
    s.eVar5 = "";

    /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
    var s_code = s.t(); if (s_code) document.write(s_code);

  }
</script>


