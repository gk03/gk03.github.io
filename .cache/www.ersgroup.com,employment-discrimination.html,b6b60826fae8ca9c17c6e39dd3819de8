                            <!-- /m_editable -->
                            <!-- m_editable region-id="editable-static-tag-U22226-BP_infinity" template="employment-discrimination.html" data-type="html" data-ice-options="disableImageResize,link" -->
                            <div class="expert-links light-on-dark clearfix colelem" id="u22226-7" data-muse-uid="U22226" data-muse-type="txt_frame">
                                <!-- content -->
                                <p>More Information on Federal Government Projects <a class="nonblock" href="federal-government-projects.html" data-href="page:U21999">here</a>.</p>
