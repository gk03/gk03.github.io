                            <a class="nonblock nontext Button clearfix colelem" id="buttonu13767" href="employment-discrimination-case-studies.html" data-href="page:U12798" data-muse-uid="U13767">
                                <!-- container box -->
                                <div class="clearfix grpelem" id="u13770">
                                    <!-- column -->
                                    <!-- m_editable region-id="editable-static-tag-U13769-BP_infinity" template="employment-discrimination.html" data-type="html" data-ice-options="disableImageResize,link" -->
                                    <div class="clearfix colelem" id="u13769-4" data-muse-uid="U13769" data-muse-type="txt_frame">
                                        <!-- content -->
                                        <p>Case Studies</p>
                                    </div>
                                    <!-- /m_editable -->
                                    <!-- m_editable region-id="editable-static-tag-U13768-BP_infinity" template="employment-discrimination.html" data-type="html" data-ice-options="disableImageResize,link" -->
