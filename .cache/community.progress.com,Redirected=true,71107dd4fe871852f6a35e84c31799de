<script type="text/javascript">stLight.options({publisher: "ur-78f8140b-dee9-a242-b108-58acc1a075c1", doNotHash: false, doNotCopy: false, hashAddressBar: false});
jQuery(function ($) {
    var flyout = $('.social-flyout');
    flyout.on('click', function (e) {
        e.stopPropagation();
        $(this).addClass("active");
    });
    flyout.find('.close').on('click', function (e) {
        e.stopPropagation();
        flyout.removeClass('active');
    });
});
        
</script>
