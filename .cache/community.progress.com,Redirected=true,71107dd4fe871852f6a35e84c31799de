<script src="/cfs-file.ashx/__key/communityserver-components-sitefiles/new_2D00_ui-js/sort.js" type="text/javascript"></script>

<script type="text/javascript">
function joinGroup(groupId, userId,context) {
    
    
   jQuery.telligent.evolution.post({
       url: jQuery.telligent.evolution.site.getBaseUrl() + 'api.ashx/v2/groups/' + groupId + '/members/users.json',
       data: { UserId: userId, GroupId: groupId },
       success: function (response) {
           window.location.reload();
       },
       defaultErrorMessage: function(){alert("Unble to join Group. Please contact Administrator.");}
   });
};

 jQuery( document ).ready(function() {
     
     if(sessionStorage.getItem("lasthomepagetab")!=null){         
         jQuery('#filterProducts').val('All');  
         
     }
    else if (jQuery('#filterProducts').val() == 'subscribed') {
         var products = jQuery('#products');
         var data = products.clone();
 		var filteredData = data.find('div.subscribed');		
 	//	var sortedData = filteredData.sorted();
 		products.quicksand(filteredData, {
 		  duration: 800,
 		  easing: 'easeInOutQuad'
 	   });
 	}
 });

jQuery( "#filterProducts" ).live( "change", function( e ) {jQuery('.last-activity .dial').donutchart("animate");jQuery('.active-members .dial').donutchart("animate");} ); 
jQuery('.last-activity .dial').donutchart("animate");
jQuery('.active-members .dial').donutchart("animate");
 
</script></div>
