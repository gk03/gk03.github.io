<script type="text/javascript">
function industryJoinGroup(groupId, userId,context) {
    
    
   jQuery.telligent.evolution.post({
       url: jQuery.telligent.evolution.site.getBaseUrl() + 'api.ashx/v2/groups/' + groupId + '/members/users.json',
       data: { UserId: userId, GroupId: groupId },
       success: function (response) {
           window.location.reload();
       },
       defaultErrorMessage: function(){alert("Unble to join Group. Please contact Administrator.");}
   });
};
</script></div>
