<script type="text/javascript">
   if(jQuery.browser.msie && window.XDomainRequest) {  
    var xdr = new XDomainRequest();
    xdr.open("get", "https://cdn.telerik-web-assets.com/progress-footer/latest/external.html");
    xdr.onload = function () {
    var data = (xdr.responseText);
    jQuery("#footer").html(data);
    };
    xdr.send();
   
   }else{
     jQuery.ajax({
        url: "https://cdn.telerik-web-assets.com/progress-footer/latest/external.html",
        success: function(data) {
            jQuery("#footer").html(data);
        }
    });
    }
</script></div>
