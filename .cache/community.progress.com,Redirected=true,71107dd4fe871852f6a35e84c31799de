<script>
    var tab_class='';
jQuery(document).ready(function(){
    $( "div.activity-story-stream" ).addClass( "ctab-content current" );
    $( "div.community-groups" ).addClass( "ctab-content" );
    $( "div.industry-groups" ).addClass( "ctab-content" );
    $( "div.codeshare-activity-story-stream" ).addClass( "ctab-content" );
    jQuery('ul.ctabs li').click(function(){
		tab_class = $(this).attr('data-tab');

		jQuery('ul.ctabs li').removeClass('current');
		jQuery('.ctab-content').removeClass('current');

		jQuery(this).addClass('current');
		jQuery("."+tab_class).addClass('current');
        sessionStorage.setItem("lasthomepagetab", tab_class );
	})
    
    if(sessionStorage.getItem("lasthomepagetab")!=null){
           jQuery('ul.ctabs li').removeClass('current');
           jQuery('.ctab-content').removeClass('current');
           jQuery('ul.ctabs li[data-tab="'+sessionStorage.getItem("lasthomepagetab")+'"]').addClass('current');
           jQuery("."+sessionStorage.getItem("lasthomepagetab")).addClass('current');
     }
})
</script></div>
