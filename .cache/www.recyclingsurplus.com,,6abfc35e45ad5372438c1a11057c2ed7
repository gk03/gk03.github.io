				responsive: true,
				minWidth: 480,
				autoHideButton: true,
				showArrow: true			});   
			resize(); 
		});   
		var wn = 850,
		hn = 400;
		var resize = function (){ 
			var w = $jq('#wrapper152').width(),
				h = $jq('#wrapper152').height();
			if(w <= wn){ 
				$jq('.oneByOne1', $slider).css({'width':w, 'height':h});
				$jq('.oneByOne_item', $slider).css({'width':w, 'height':h}); 
			}else{
				$jq('.oneByOne1', $slider).css({'width':wn, 'height':hn});
				$jq('.oneByOne_item', $slider).css({'width':wn, 'height':hn}); 
			} 
		} 
		$jq(window).bind("resize", function () {
			resize();
		});  
	});
