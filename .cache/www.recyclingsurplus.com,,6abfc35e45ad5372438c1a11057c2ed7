	var $jq = jQuery.noConflict();
	$jq(document).ready(function() {
		var w = $jq('#wrapper152').width();
		$jq('#zt-onebyone152 .loading').css({'width':w});
	});
	$jq(document).ready(function() {  
		var $slider = $jq('#zt-onebyone152');
