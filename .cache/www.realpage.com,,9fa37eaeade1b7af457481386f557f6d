<script>
(function($) {
    $(function() {
        $('.rp-testimonial-toggle a').toggle(function() {
                $(this).text('Show Less');
                $(this).attr('class', 'rp-testimonial-less');
                $(this).parent().prev().slideToggle().parent().addClass('rp-testimonial-open');
            },
            function() {
                $(this).text('Show More');
                $(this).attr('class', 'rp-testimonial-more');
                $(this).parent().prev().slideToggle().parent().removeClass('rp-testimonial-open');
            });
    });
})(jQuery);
</script>

