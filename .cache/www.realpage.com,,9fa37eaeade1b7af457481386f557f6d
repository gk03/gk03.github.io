jQuery(".open-webinar").click(function() {
    jQuery('#formiframe').attr('src',this.href);
    jQuery("#demo-form").foundation('reveal', 'open');
    return false;
});
jQuery(".doclick").click(function() {
    window.location = this.href;
});
jQuery('#demo-form').foundation('reveal', {
    closed: function () {
        jQuery('#formiframe').attr('src','');
    }
});
</script>
