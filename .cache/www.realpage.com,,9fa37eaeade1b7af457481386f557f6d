<script>
jQuery(document).ready(function(){
    if (Querystring('lboxsubmit') == "yes" && Querystring('cstm') != "webinar" && Querystring('cstm') != "survey"  && Querystring('dlink') != "1" && Querystring('demolink') != "1"){
        jQuery('#form-thank-you').foundation('reveal', 'open');
    }

    if (Querystring('lboxsubmit') == "yes" && Querystring('cstm') == "survey"){
        jQuery('#survey-thank-you').foundation('reveal', 'open');
    }
})
</script>

