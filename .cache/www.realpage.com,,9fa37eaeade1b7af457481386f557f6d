<script type="text/javascript">
// Contact us / Get Started form / Homepage Sales
jQuery(".lboxcontact").click(function() {
    //jQuery('#rmi-form').foundation('reveal', 'open');

    var product = this.rel;
    var proarray = product.split('|');
    setTimeout( function(){loadText(proarray[0],'RMITextHolder');}, 100);
    loadMarketoForm(210,false, proarray[1]);
	//mktoValues(proarray[1]);
});

// Homepage hero contact sales
jQuery(".lboxcontacthphero").click(function() {

    var product = this.rel;
    var proarray = product.split('|');
	loadMarketoForm(163,false, proarray[1]);
    //mktoValues(proarray[1]);
});

//White paper form
jQuery(".lboxwhitepaper").click(function() {

    var product = this.rel;
    var proarray = product.split('|');
    setTimeout( function(){loadText(proarray[0],'WhitePaperTextHolderNewForm');}, 100);
    loadMarketoForm(212,false, proarray[1]);
	//mktoValues(proarray[1]);
});

// appPartner
jQuery(".lboxapppartner").click(function() {
	jQuery('.AppTextHolder').show();
	jQuery('.appThankYou').hide();
    var product = this.rel;
    var proarray = product.split('|');
    setTimeout( function(){loadText(proarray[0],'AppTextHolder');}, 100);
	mktoValues(proarray[1]);
});

// Product Sales from top nav
function setProductSales() {
	jQuery(".product-sales-lb").click(function() {

		var product = jQuery('.sub-menu .product-sales-lb a[rel]').attr('rel');
		var proarray = product.split('-');
		setTimeout( function(){loadText(proarray[0],'RMITextHolder');}, 100);
		loadMarketoForm(210,false, proarray[1]);
        jQuery("#rmi-form").foundation('reveal', 'open');
		//mktoValues(proarray[1]);
		return false;
	});
}

// Add values to forms
function mktoValues(product){
	MktoForms2.whenReady( function(form) {
		form.getFormElem().show();
		form.vals({"FirstName":jQuery.cookie("localmktofirst")});
		form.vals({"LastName":jQuery.cookie("localmktolast")});
		form.vals({"Email":jQuery.cookie("localmktoemail")});
		form.vals({"Company":jQuery.cookie("localmktocompany")});
		form.vals({"Phone":jQuery.cookie("localmktophone")});
		setTimeout(function(){form.vals({"Standardized_Title__c":jQuery.cookie("localmktotitle")})},200);
		form.vals({"State":jQuery.cookie("localmktostate")});
		form.vals({"EmailContactGroup__c_lead":jQuery.cookie("localmktodept")});
		form.vals({"Web_Form_Name__c":product});
		form.vals({"Engagement_Solution_of_Interest__c":""});
		form.vals({"Original_PPC_Conversion__c":'false'});
		form.vals({"Original_PPC_Date__c":'2016-09-21'});
		form.vals({"Original_PPC_Keyword__c":''});
		form.vals({"Original_PPC_Network__c":''});
		form.vals({"Original_PPC_URL__c":'http://www.realpage.com/'});
	});
}
</script>

