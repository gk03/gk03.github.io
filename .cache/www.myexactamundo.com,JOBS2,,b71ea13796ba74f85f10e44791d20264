<!-- JQUERY -->
<script>
    $(document).ready(function () {
         $("#chkShowOpenJob").attr("checked", true);  $("#hidShowOpenJobs").val("on");  
        if ( $("#hidShowOpenJobs").val() == 'on'){
            $("#chkShowOpenJob").attr("checked", true);
        }
        //header sorting click
        $('.header').on('mouseover', function (event) {
            $(this).css("cursor", "pointer");
        });
        $('.header').click(function (e) {
            if ($('#chkShowOpenJob').is(':checked')){
                $('#hidShowOpenJobs').val('on');
            } 
            $('#hidJobsSorting').val( $(this).text() );
            $('#formSearch').submit();
        });
        var jobID = -1;
        var jobApplyString = "";
        //JOB CLICK
        $('ul.jobs').on('click', 'li.title', function (e) {
            function toggle(el) {
                el.toggleClass('active', 250);
                el.slideUp();
            }
            var me = $(this)
            if (jobID == -1) {
                toggle(me.next().slideDown(250).siblings('li.fields'));
                jobID = me.next().val();
            } else {
                if (jobID == me.next().val()) {
                    me.next().slideUp(250).siblings('li.fields').slideUp(250);
                    jobID = -1;
                } else {
                    me.next().slideDown(250).siblings('li.fields').slideUp(250);
                    jobID = me.next().val();
                }
            }
        });
        $('#chkShowOpenJob').change(function(){
            if (this.checked){
                $('#hidShowOpenJobs').val('on');
            }  
            $('#formSearch').submit();
        });
        $('li.fields').addClass("hide");
        //apply click
        $('.cmdJobsApply').click(function (e) {
            $('#modalJobContactContent').empty();
            $('#modalJobContactContent').load('contents/modal/modalContact.html');
            var strJob = $(this).val();
            if (strJob.length > 0) {
                var myArray = $(this).val().split(';;');
                if ((myArray[1] != "undefined")) {
                    $('#JobDescriptionModal').html('<h5><b>' + myArray[3] + ':&nbsp;&nbsp;' + myArray[1] + ' - ' + myArray[2] + '</h5>');
                    jobApplyString = $('#JobDescriptionModal').html();
                }
            } // end if
            $('#cmdJobApplySubmit').show();
            $('#cmdModCancel').html("Cancel");
        });
        $("#cmdSearchJob").click(function (e) {
            if ($('#chkShowOpenJob').is(':checked')){
                $('#hidShowOpenJobs').val('on');
            }  
            $("#formSearch").submit();
        });
        var JQUERY4U = {};
        JQUERY4U.UTIL =
        {
            setupFormValidation: function () {
                $('#frmJobApplying').validate({
                    rules: {
                        txtFirstName: "required",
                        txtLastName: "required",
                        txtEmail: {
                            required: true,
                            email: true
                        },
                        txtContactNumber: "required"
                    },
                    messages: {
                        txtFirstName: "Please enter your First Name",
                        txtLastName: "Please enter your Last Name",
                        txtEmail: "Please enter a valid Email Address",
                        txtContactNumber: "Please enter Contact Number"
                    },
                    submitHandler: function (form) {
                        $('#cmdJobApplySubmit').hide();
                        $('#cmdModCancel').html("Close");
                        var serJobApplying = $('#frmJobApplying').serializeArray();
                        serJobApplying.push({ "JobDes": jobApplyString });
                         $.post("include/formActions.php", { "actionName": "requestJob", "jobrequest_data": serJobApplying }, function (data) {
                            if ($('#radSubcontractorNo').is(":checked")){
                                $(location).attr('href',"http://myexactamundo.com/EMPFORMS2");
                            } else {
                                $('#modalJobContactContent').empty();
                                $('#modalJobContactContent').html("<h4>Thank you, your request has been sent.<br/>A subcontractor coordinator will contact you soon.</h4>");
                            }
                        });
                    }
                });
            }
        }
        JQUERY4U.UTIL.setupFormValidation();
        //when apply button click
        $('#cmdJobApplySubmit').click(function (e) {
            if ($('#chkShowOpenJob').is(':checked')){
                $('#hidShowOpenJobs').val('on');
            } 
            $('#frmJobApplying').submit();
            e.preventDefault();
        });
        //show
        $('#layout').show();
        
        
        $('#xlsOutput').click(function(){
     
          $('#tblJobsList').show().tableExport({type:'excel',escape:'false'})
                  $('#tblJobsList').hide();


        });
        
    });



</script>



