        $(document).ready(function () {
            //JobsOpeningPaginate
            $("#JobsOpeningPaginate").pagy({
                totalPages: 5.84 ,
                currentPage: 1,
                outerWindow: 1,
                first: null,
                last: null,
                page: function (page) {
                    if ($('#chkShowOpenJob').is(':checked')){
                        $('#hidShowOpenJobs1').val('on');
                    } 
                    $('#hidJobsSorting1').val( "" );
                    $('#hidSearch').val( $('#txtSearchJob').val() );
                    $('#hidPage').val(page);
                    $('#frmPaginateJobs').submit();
                    //alert(page);
                    //   $("#current-page").text(page);
                }
            });
        });
