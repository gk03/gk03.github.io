         function showImage(imgId, typ) {
             var objImg = document.getElementById(imgId);
             if (objImg) {
                 if (typ == "1")
                     objImg.src = "/Images/send-info-RO.png";
                 else if (typ == "2")
                     objImg.src = "/Images/send-info.png";
             }
         }
