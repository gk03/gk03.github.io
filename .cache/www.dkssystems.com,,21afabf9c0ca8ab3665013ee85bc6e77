<script type="text/javascript">
    function FooterValidate() {
        var fields = document.getElementsByClassName('footervalidate');

        var message = "";

        for (var i = 0; i < fields.length; i++) {
            var field = fields[i];

            var value = field.value;
            value = value.replace(/^\s+|\s+$/g, '');

            var tooltip = field.title;

            if (value.length == 0 || value == tooltip)
                message += tooltip + "\n";
        }

        if (message.length > 0) {
            var alertBox = "The following fields are required:\n\n" + message;
            alert(alertBox);
            return false;
        }

        return true;
    }

    function searchFocusProposal(f) {
        document.getElementById("Footer_Form_captchactr").className = "";
        if (f.value == f.title) {
            f.value = '';
        }
    }

    function searchBlurProposal(f) {

        if (f.value == '') {
            f.value = f.title;
        }


    }
</script>
