        }
      )
      .click(function () {
          var index = $(this).attr("index").valueOf();
          var left = index * 673;
          $(".slide").animate({ "opacity": 0 }, 150, function () {
              $(".slide").animate({ "left": "-" + left + "px" }, 0, function () {
                  $(this).animate({ "opacity": 1 }, 150);
              });
          });
      });
    });

