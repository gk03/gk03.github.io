</div>
<script type="text/javascript">
    $(function () {
        var $form = $("#eyb-modal form");
        var $requestsModal = $("#eyb-modal .modal-dialog.request");
        var $successModal = $("#eyb-modal .modal-dialog.success");
        var $showFormButton = $("#eyb-modal .btn-primary.show-form");
        var $sendButton = $("#eyb-modal .btn-primary.send-form");
        var $okeyButton = $("#eyb-modal .btn-primary.okey");

        $showFormButton.click(function () {
            $form.show();
            $showFormButton.hide();
            $sendButton.show();
        });

        $sendButton.click(function () {
            $sendButton.button("loading");
            $("#eyb-modal .error").hide();
            $.post("/listing-request/", $form.serialize()).done(function () {
                $form.find(".form-control").val("");
                $showFormButton.show();
                $sendButton.hide();
                $showFormButton.show();

                $requestsModal.hide();
                $successModal.show();

            }).fail(function () {
                $("#eyb-modal .error").show();
            }).always(function () {
                $sendButton.button("reset");
            });
        });

        $okeyButton.click(function () {
            $successModal.hide();
            $requestsModal.show();
        });

        $('#eyb-modal').on('show.bs.modal', function (e) {
            var listingPk = $(e.relatedTarget).attr("data-pk");
            $("#eyb-modal #listing_pk").val(listingPk);

            $form.hide();
            $showFormButton.show();
            $sendButton.hide();

            document.getElementById('request-intro').currentTime = 0;
        })
    });
</script>

