            if ($(this).scrollTop() > 100) {
                $('.scroll-top').fadeIn('slow');
            } else {
                $('.scroll-top').fadeOut('slow');
            }

        });

        $('.scroll-top').click(function () {
            $('body,html').animate({
                scrollTop: 0
            }, 800);
