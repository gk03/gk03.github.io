        $(document).ready(function () {
            $("#popup_box").dialog({ autoOpen: false, modal: true, width: 855,
                open: function () {
                    jQuery('.ui-widget-overlay').bind('click', function () {
                        jQuery('#popup_box').dialog('close');
                    })
                }
            });
        });

        function ShowPopUp(url) {
            $('#frmActOnURL').attr('src', url)
            $("#popup_box").dialog("open");
        }

