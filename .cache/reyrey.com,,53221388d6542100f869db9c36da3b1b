							<script type="text/javascript">
							
							
							var uri = 'http://www.reyrey.com/reyreyapps/webservices/newsheadlines.asmx/GetHeadlinesByCountryId?callback=?';
							$(document).ready(function () {
								$.getJSON(uri,
								{
									countryId: 1
								},
								function (response) {
									var obj = response;
									var nextLink = "";
									var curIndex = 0;
									document.getElementById("story").innerHTML = obj[curIndex].Text;
									$("#story").attr("href", obj[curIndex].Url);
									$("#story").fadeTo("slow", 1);
									
									var int = setInterval(function () { changeImage() }, 5000);
									
									function changeImage() {
										if (curIndex == (obj.length - 1)) {
											curIndex = 0;
											$("#story").fadeTo("slow", 0, function ()
											{
												curIndex = 0;
												nextLink = obj[curIndex].Url;
												document.getElementById("story").innerHTML = obj[curIndex].Text;
												$("#story").attr("href", nextLink);
												$("#story").fadeTo("slow", 1);
											});
										}
										else {
											$("#story").fadeTo("slow", 0, function () {
												curIndex++;
												nextLink = obj[curIndex].Url;
												document.getElementById("story").innerHTML = obj[curIndex].Text;
												$("#story").attr("href", nextLink);
												$("#story").fadeTo("slow", 1);
											});
										}
									};
								});
							});
							</script>
						</div>
					</td>
				</tr>
			</table>
		</td>
