            function sendCustomDimensions() {
                var dimensions = {
                    1: 'dimension1',
                    2: 'dimension2',
                    3: 'dimension3',
                    4: 'dimension4',
                    5: 'dimension5',
                    6: 'dimension6',
                    7: 'dimension7',
