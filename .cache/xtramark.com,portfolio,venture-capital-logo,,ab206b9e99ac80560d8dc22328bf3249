$('#subMenu').change(function(){
	var content = $(this).val();
	var checkURL = content.match(/^#([^\/]+)$/i);
	if(checkURL){

		var goPosition = $(content).offset().top - 80;
		$('html,body').animate({ scrollTop: goPosition}, 'slow');

	}else{
		window.location = content;
	}
});

});

</script>


