<script type="text/javascript" charset="utf-8">
  $(window).load(function() {
var speed = 400;
        $('#close-bar').on('click', function(){

            var $$ = $(this),
                panelWidth = $('#hiddenPanel').outerWidth();

            if( $$.is('.myButton') ){$('#hiddenPanel').animate({right:0}, speed);
                $$.removeClass('myButton')
            } else {$('#hiddenPanel').animate({right:-panelWidth}, speed);
                $$.addClass('myButton')
            }

        });

		$('#close-x').on('click', function(){

                panelWidth = $('#hiddenPanel').outerWidth();

				$('#hiddenPanel').animate({right:-panelWidth}, speed),
				$('#close-bar').addClass( "myButton" );

        });

  });
</script>

