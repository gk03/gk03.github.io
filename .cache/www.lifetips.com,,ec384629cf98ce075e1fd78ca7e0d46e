                <script type="text/javascript">
                    audiojs.events.ready(function() {
                        //var as = audiojs.createAll({preload: false});
                        $('.audiostart').click(function() {
                            $('.audiojs').remove();
                            $('.audiostart').show();
                            var h = $(this).attr('href');
                            var c = $(this).parent();
                            $(this).hide();
                            var p = $('<audio/>').attr('src', h);
                            c.append(p);
                            audiojs.create(p.get(0), { autoplay: true });
                            return false;
                        });
                    });
                    
                </script>

