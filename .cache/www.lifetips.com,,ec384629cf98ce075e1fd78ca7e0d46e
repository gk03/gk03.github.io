                                    <script type="text/javascript">
                                        $(function() {

                                            // activate sliders
                                            $('.scrollBlockGlobal').each(function(i, obj) {
                                                var block = $(obj);
                                                var container = $('.scrollContainerGlobal', block);
                                                var panel = $('.scrollPanelGlobal', block);

                                                var offset = parseInt(block.attr('data-offset')) || 0;

                                                var animating = false;
                                                $('.scrollArrowGlobal', block).click(function() {
                                                    var distance = parseInt(container.width());
                                                    if (animating)
                                                        return false;
                                                    var delta = parseInt($(this).attr('data-delta'));
                                                    if (delta > 0) {
                                                        if (panel.position().left < 0) {
                                                            animating = true;
                                                            if (panel.position().left <= (-1 * distance)) {
                                                                panel.animate({ left: '+=' + distance }, 500, function() { animating = false; });
                                                            } else {
                                                                panel.animate({ left: 0 }, 500, function() { animating = false; });
                                                            }
                                                        }
                                                    } else {
                                                        if (panel.position().left > (-1 * (panel.width() - distance))) {
                                                            animating = true;
                                                            panel.animate({ left: '-=' + distance }, 500, function() { animating = false; });
                                                        }
                                                    }
                                                    return false;
                                                });
                                                $('.scrollPagerGlobal A', block)
                                                    .click(function() {
                                                        var distance = parseInt(container.width());
                                                        if (animating)
                                                            return false;
                                                        var dOffset = parseInt($(this).attr('data-panel'));
                                                        //alert(dOffset);
                                                        animating = true;
                                                        calcDisplayWidth();
                                                        if (window.loadDisplayWidth < 480) {
                                                            //panel.animate({left: -1 * distance * dOffset }, 500, function() { animating = false; });
                                                            panel.css('left', (-1 * distance * dOffset) + 'px');
                                                            animating = false;
                                                        } else {
                                                            panel.animate({ left: -1 * distance * dOffset / 4 }, 500, function() { animating = false; });
                                                        }
                                                        //panel.animate({
                                                        return false;
                                                    })
                                                    .hover(function() {
                                                            $(this).css('border-color', '#77b800');
                                                        }, function() {
                                                            $(this).css('border-color', 'transparent');
                                                        }
                                                    );
                                                block
                                                        .hover(function() {
                                                            $(this).find('.scrollArrowGlobal DIV').stop().animate({ 'opacity': 1.0 });
                                                        }, function() {
                                                            $(this).find('.scrollArrowGlobal DIV').stop().animate({ 'opacity': 0.35 });
                                                        })
                                                        .find('.scrollArrowGlobal DIV').css({ 'opacity': 0.35 });

                                                if (offset > 0) {
                                                    var distance = parseInt(container.width());
                                                    panel.css({ 'left': '-=' + (offset * distance) });
                                                }

                                            });
                                        });
                                    </script>

                                </div>
                            </div>
                        </div>
