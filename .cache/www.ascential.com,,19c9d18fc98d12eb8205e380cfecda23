            var keyCode = ('which' in event) ? event.which : event.keyCode;
            isNumericOrWithShift = (keyCode == 48 || keyCode == 57 || keyCode == 56 || keyCode == 188 || keyCode == 190) || (keyCode >= 96 && keyCode <= 105 );
