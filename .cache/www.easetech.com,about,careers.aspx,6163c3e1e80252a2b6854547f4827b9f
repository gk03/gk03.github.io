function SelectLocation(myLocation)
{
    var locationInteractiveweb = document.getElementById("locationInteractiveweb");
    var locationDotnetnuke = document.getElementById("locationDotnetnuke");
    var locationSnowcovered = document.getElementById("locationSnowcovered");
    var hidPurchaseSite = document.getElementById("dnn_ctr1408_ViewPositionsAvailable_Validate1_hidPurchaseSite");
     ChangeStep('2');
    if (myLocation == "locationInteractiveweb")
    {
       locationInteractiveweb.className = "piclink2";
       locationDotnetnuke.className = "piclink";
       locationSnowcovered.className = "piclink";
       hidPurchaseSite.value = "InteractiveWebs";
    }
    else if(myLocation == "locationDotnetnuke")
    {
       locationInteractiveweb.className = "piclink";
       locationDotnetnuke.className = "piclink2";
       locationSnowcovered.className = "piclink";
       hidPurchaseSite.value = "DotnetNuke";
    }
    else if(myLocation == "locationSnowcovered")
    {
       locationInteractiveweb.className = "piclink";
       locationDotnetnuke.className = "piclink";
       locationSnowcovered.className = "piclink2";
       hidPurchaseSite.value = "Snowcovered";
    }
}

function ChangeStep(step)
{   
    if (step == "1")
    {
        document.getElementById('divStep1').style.display = "";
        document.getElementById('divStep2').style.display = "none";
        document.getElementById('divStep3').style.display = "none";
    }
    else if (step == "2")
    {
        document.getElementById('divStep1').style.display = "none";
        document.getElementById('divStep2').style.display = "";
        document.getElementById('divStep3').style.display = "none";
    }
    else if (step == "3")
    {
        document.getElementById('divStep1').style.display = "none";
        document.getElementById('divStep2').style.display = "none";
        document.getElementById('divStep3').style.display = "";
    }
}

function CheckSelectPurchase()
{
    var p  = document.getElementById("dnn_ctr1408_ViewPositionsAvailable_Validate1_hidPurchaseSite");
    if (p != null && p.value == "")
    {
        alert("Please select the location that you purchased this module");
        return false;
    }
    
    var p  = document.getElementById("dnn_ctr1408_ViewPositionsAvailable_Validate1_txtEmail");
    if (p != null && p.value == "")
    {
        alert("Email is required.");
        return false;
    }
    else
    {
         if (p.value.match(/^[\w]{1}[\w\.\-_]*@[\w]{1}[\w\-_\.]*\.[\w]{2,4}$/i))
         {
            return true;
         }
         else
         {
            alert("Invalid email.");
            return false;
         }
    }
    
    return true;
}

//-->
</script>
