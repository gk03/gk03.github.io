<script type="text/javascript">
            $(function() {
				/**
				 * the menu
				 */
				var $menu = $('#ldd_menu');
				
				/**
				 * for each list element,
				 * we show the submenu when hovering and
				 * expand the span element (title) to 510px
				 */
				$menu.children('li').each(function(){
					var $this = $(this);
					var $span = $this.children('span');
					$span.data('width',$span.width());
					
					$this.bind('mouseenter',function(){
						$menu.find('.ldd_submenu').stop(true,true).hide();
						$span.stop().animate({},function(){
							$this.find('.ldd_submenu').slideDown();
						});
					}).bind('mouseleave',function(){
						$this.find('.ldd_submenu').stop(true,true).hide();
						$span.stop().animate({});
					});
				});
				
				/**
				 * the menu
				 */
				var $menu1 = $('#ldd_menu1');
				
				/**
				 * for each list element,
				 * we show the submenu when hovering and
				 * expand the span element (title) to 510px
				 */
				$menu1.children('li').each(function(){
					var $this = $(this);
					var $span = $this.children('span');
					$span.data('width',$span.width());
					
					$this.bind('mouseenter',function(){
						$menu1.find('.ldd_submenu1').stop(true,true).hide();
						$span.stop().animate({},function(){
							$this.find('.ldd_submenu1').slideDown();
						});
					}).bind('mouseleave',function(){
						$this.find('.ldd_submenu1').stop(true,true).hide();
						$span.stop().animate({});
					});
				});
            });
        </script>
