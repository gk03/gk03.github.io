                    function initialize() {
                        var mapOptions = {
                            zoom: 15,
                            center: coordData,
                            scrollwheel: false,
                            styles: styleArray
                        }
                        var map = new google.maps.Map(document.getElementById("map-canvas-217909494"), mapOptions);

                        var markerIcon = {
                            url: "https://www.guard-it.com/wp-content/themes/theme53367/images/gmap_marker.png",
                            size: new google.maps.Size(41, 61),
                            origin: new google.maps.Point(0,0),
                            anchor: new google.maps.Point(21, 61)
                        };

                        marker = new google.maps.Marker({
                            map:map,
                            draggable:false,
                            position: coordData,
                            icon: markerIcon
                        });
                    }
                    google.maps.event.addDomListener(window, "load", initialize);
                }

