<script type="text/javascript">
    function ShowhideMoreicon(divname,numicon) {
        if (window.jQuery('#' + divname + ' a.shareicon').not('.moreicon').length > numicon) {
            window.jQuery('#' + divname + ' a.shareicon:lt(' + numicon + ')').addClass('socialiconvisible');
            window.jQuery('#' + divname + ' a.moreicon').addClass('socialiconvisible');
            window.jQuery('#' + divname + ' a.moreicon').click(function () {
                window.jQuery('#' + divname + ' a.shareicon').not('.moreicon').fadeIn('slow');
                window.jQuery('#' + divname + ' a.shareicon').addClass('socialiconvisible');
                window.jQuery(this).fadeOut('slow');
                window.jQuery(this).removeClass('socialiconvisible');
                return false;
            });
        } else {
            window.jQuery('#' + divname + ' a.shareicon').addClass('socialiconvisible');
            window.jQuery('#' + divname + ' a.moreicon').removeClass('socialiconvisible');
        }
    }

    jQuery(function () {
        //For desktop Screen size
        ShowhideMoreicon('main', 7);
        //For mobile
        ShowhideMoreicon('modules', 3);
    });

</script>


