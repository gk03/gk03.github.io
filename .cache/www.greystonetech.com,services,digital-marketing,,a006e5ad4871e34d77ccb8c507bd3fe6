jQuery("#header").headroom("destroy");
new WOW().init();
jQuery(document).ready(function() {
/*
	jQuery('.learnit').click(function(e){ 
		e.preventDefault();
	  jQuery('.service-web-container').toggle();
	  jQuery('.it-details').toggleClass('animated fadeInUp inlineblock');   
	});
*/
	jQuery('.learnweb').click(function(e){ 
		e.preventDefault();
		
	  jQuery('#learnweb').fadeToggle();
	  var shown = jQuery('.web-details').hasClass('inlineblock');
	  //console.log(shown);
	  if (!shown) {
		  jQuery('.service-it-container').addClass('hideme');
	  jQuery('.web-details').removeClass('hideme').addClass('animated fadeInUp inlineblock').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
		  
		  jQuery('.web-details').removeClass('animated fadeInUp');
	  });
	  } // end if !shown
	  else {
		  jQuery('.web-details').removeClass('animated fadeInUp inlineblock').addClass('hideme');
	  jQuery('.service-it-container').removeClass('hideme').addClass('animated fadeInUp').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
		  jQuery('.service-it-container').removeClass('animated fadeInUp');
	  }); 
	  } // end else
	});	// end learnweb click
	// learnit click
	jQuery('.learnit').click(function(e){ 
		e.preventDefault();
		
	  
	  var shown = jQuery('.it-details').hasClass('inlineblock');
	  jQuery('#learnit').fadeToggle();
	  //console.log(shown);
	  if (!shown) {
		  jQuery('.service-web-container').addClass('hideme');
	  jQuery('.it-details').removeClass('hideme').addClass('animated fadeInUp inlineblock').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
		  
		  jQuery('.it-details').removeClass('animated fadeInUp');
	  });
	  } // end if !shown
	  else {
		  jQuery('.it-details').removeClass('animated fadeInUp inlineblock').addClass('hideme');
	  jQuery('.service-web-container').removeClass('hideme').addClass('animated fadeInUp').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
		  jQuery('.service-web-container').removeClass('animated fadeInUp');
	  }); 
	  } // end else
	});	// end learnweb click	
	
	jQuery(".greenarrow img").click(function() {
		var scrollpos = jQuery(this).attr('scrollto');
	    jQuery('html, body').animate({
	        scrollTop: jQuery(scrollpos).offset().top
	    }, 500);
	});        
}); //doc ready
    </script>
      </body>

