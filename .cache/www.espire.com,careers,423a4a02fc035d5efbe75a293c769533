        <div class="choose-file-input">Choose file</div> &nbsp;<span class="field-validation-valid" data-valmsg-for="fileUpload" data-valmsg-replace="true"></span>
        <input accept=".pdf,.PDF,.doc,.DOC,.rtf,.RTF,.DOCX,.docx" data-val="true" data-val-required="Required *" id="fileUpload" multiple="multiple" name="fileUpload" required="required" style="float: left;" type="file" value="" />       
    </div>
    <div id="selectedFiles"></div>
    <p>Cover Note</p>
<textarea class="form-control custom-textarea" cols="20" data-val="true" data-val-required="View Notes Is Required" id="ViewNotes" name="ViewNotes" rows="2">
</textarea>    <div class="btn-parent">
