<script>
// Defines Variables
var referrer = document.referrer;
var nothing;

// Sets Cookies 
if(referrer!=nothing && readCookie('referrer') == null) setCookie('referrer', referrer, 7, '/','intersystems.com');

// Show referrer in logs
//console.log('document.referrer=' + referrer);

// Defines a cookie
// If the exdays = 0, then the cookie expires @ end of session
// If the exdays <> 0, then the cookie expires that many days later.
function setCookie(cname, cvalue, exdays, path, domain) {
	var d = new Date();
	d.setTime(d.getTime() + (exdays*24*60*60*1000));
	var expires = "expires="+d.toUTCString();
	var str = cname + "=" + cvalue + "; path=" + path + "; domain=" + domain;
	if(exdays!=0) str += "; " + expires;

	document.cookie = str;
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}
</script>

