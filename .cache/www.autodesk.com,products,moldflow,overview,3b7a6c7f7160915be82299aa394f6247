        <script type="text/javascript">
            $(function() {
                var userSetLocation;
                if(typeof adsk !== 'undefined' && adsk.clientContext) {
                    userSetLocation = adsk.clientContext.getStore("usersetlocation").getProperty("location");
                }
                if (typeof userSetLocation === 'undefined' || userSetLocation === "" || userSetLocation === null || userSetLocation == null) {
                    var overlay = $('#pbl-alert-overlay').first();
                    var target = $('.global-menu-toggle-li').first();
                    
                    if(adsk.clientContext.getStore("demandbase")) {
                        var geolocation = adsk.clientContext.getStore("demandbase").getProperty("country_code");
                        var hidePopup = adsk.clientContext.getStore("profiledata").getProperty("hidePopup", false);
                        if(hidePopup !== "true" && geolocation.toLowerCase() !== "us") {
                            adsk.personalization.showPBLAlertOverlay(overlay, target, geolocation);
                        }
                        //Show popup when countrychange event has been fired
                        adsk.clientContext.getStore("demandbase").addListener("countryupdate", function() {
                            var geolocation = adsk.clientContext.getStore("demandbase").getProperty("country_code");
                            adsk.clientContext.getStore("profiledata").setProperty("hidePopup", false);
                            adsk.personalization.showPBLAlertOverlay(overlay, target, geolocation);
                        });
                    }
                
                    $('.close-pbl-overlay').on('click', function(e) {
                        adsk.personalization.closePBLAlertOverlay();
                    });
                }
                
                $('#adsk-global-control').attr('href', 'http://www.autodesk.com/site-selector?country=US&locale=en_US');
                $('#site-search-url').attr('value', 'http://usa.autodesk.com/adsk/servlet/u/gsearch/results?siteID=123112&catID=123155&id=2088334');

            });
        </script>
   
   
   
   
  
     
