<script type="text/javascript">
function loadText(textsection,outputholder){
    jQuery.ajax({
        type: "POST",
        url: 'http://www.realpage.com/careers/wp-content/themes/realpage/marketo/lightboxformtext.php',
        data: 'section=' + textsection,
        success: function (data) {
            var formText = data;
            jQuery('#'+outputholder).html(formText);
        }
    });
}
</script>
