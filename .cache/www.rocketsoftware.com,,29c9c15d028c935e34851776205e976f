  <div class="row">
    
    <section class="col-sm-12">
            <a id="main-content"></a>
                    <h1 class="page-header">i18n Homepage</h1>
                                                          <div class="region region-content">
     <div id="carousel-example-generic" class="carousel rocketon-carousel slide" data-class="classes.CarouselCapes">
     <div class="view view-rocket-i18n-slides-view view-id-rocket_i18n_slides_view view-display-id-block view-dom-id-bc22ccc5a04a2b5948d12fe602a9c875">
        
  
  
