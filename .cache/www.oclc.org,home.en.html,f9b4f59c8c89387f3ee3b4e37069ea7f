                                url: uri,
                                dataType: "json",
                                type: "GET",
                                success: function (data) {
                                    response(data);
                                }
                            }); //ajax closed
                        } //source closed
                    }); //autocomplete closed
                }
            }
        }]);
    });
}(jQuery));

