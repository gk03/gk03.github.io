                $(function(){
                    if(window.location.href.indexOf('thank-you') > -1){
                        $('form').hide();
                        console.log($('#success'));
                        $('#success').show();
                    }else{
                         $('form').show();
                        $('#success').hide();
                    };
                });
                MktoForms2.loadForm("//app-sjo.marketo.com", "952-BOO-904", 1489, function(form) {
                    
                    $('form > .mktoFormRow:nth-of-type(-n+6)').wrapAll('<span class="mkto-half-wrap"/>');
                    $('form > .mktoFormRow').wrapAll('<span class="mkto-half-wrap"/>');
                               
                    $('.mktoForm input, .mktoForm select').wrap('<span class="mkto-wrap"></span>');
                    $('.mktoForm select').addClass('chosen-select');
                    
                    form.onSuccess(function(values, followUpUrl) {
                        form.getFormElem().hide();
                        $('.success').show();
                        setTimeout(function(){
                            window.location.href = '/#thank-you';
                             //setTimeout(function(){
                             window.location.reload()
                             form.getFormElem().hide();
                             $('.success').show();
                                //}, 2000);
                        }, 1000);
                        

                        return false;
                    });
                   
                });
                
