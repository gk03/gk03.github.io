<script type="text/javascript">
function setDlgHeight(dlg) {
    var id = dlg.element[0].id;
	$.ui.dialog.prototype._originalOpen.apply(dlg, arguments);
	var parent = $("#"+id).parent();
	var title_ht = parent.find("div.ui-dialog-titlebar").outerHeight(true);
	var buttons_ht = parent.find("div.ui-dialog-buttonpane").outerHeight(true);
	var need_ht = Math.ceil(title_ht+buttons_ht);
	var win_ht = $(window).height()-10;
	var orig_dlg_height = $("#"+id).dialog('option','height');
	if(orig_dlg_height == 'auto') {
		orig_dlg_height = $("#"+id).outerHeight(true);
	}
	orig_dlg_height = parseInt(orig_dlg_height,10);
	var dlg_ht = parseInt(Math.min(orig_dlg_height,(win_ht-need_ht)),10);
	$("#"+id).css('height',(dlg_ht-20)+'px');
	parent.css('height',(dlg_ht+need_ht)+'px');
}
// This is a fix for the dialog height problem with IE
$.ui.dialog.prototype._originalOpen = $.ui.dialog.prototype.open;
$.ui.dialog.prototype.open = function() {
	setDlgHeight(this);
};
$.ui.dialog.prototype._originalDrag = $.ui.dialog.prototype.drag;
$.ui.dialog.prototype.drag = function() {
	setDlgHeight(this);
};
</script>

<script>
	$(document).ready(function() {
		$("body").append('<div id="resultsDiv" style="float:left;clear:both;width:100;display:none;"><ul id="results"></ul></div>');
	});

	function assert(value, desc) {
		if(value == false) {	// Only show failures
			var html = '<li class="fail">'+desc+'</li>';
			$("#resultsDiv").show().find(">#results").append(html);
		}
	}
</script>
<style>
	#results li.pass { color: green; }
	#results li.fail { color: red; }
