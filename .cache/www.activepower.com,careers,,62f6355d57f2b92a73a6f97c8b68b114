$(document).ready(function() { 
$('#mainNav ul li:last-child ul li').each(function() { 
$(this).find('a').attr('href',$(this).find('a').attr('href')+'careers');
})
$('#menu-item-2040').hover(function() {								
$('#menu-item-2040 .dropdown_3columns > div').css('display','block');	
$(this).addClass('active-menu');				
})
$('#menu-item-2040 .dropdown_3columns > div').hover(function() {								
$('#menu-item-2040 .dropdown_3columns > div').css('display','block');
})
$('#menu-item-2041').hover(function() {
$('#menu-item-2040 .dropdown_3columns > div').css('display','none');
})
$('#menu-item-2039').hover(function() {
$('#menu-item-2040 .dropdown_3columns > div').css('display','none');
})
$('.dropdown_3columns ul li').hover(function() {
$('.dropdown_3columns ul li a').css('color','#FFFFFF');
$(this).find('a').css('color','#CC0033');
})
$('.dropdown_3columns ul li').click(function() {
window.location = $(this).find('a').attr('href');
})
//for an unmatched combination				
$('.top-featured-image-li').hover(function() {
$(this).find('span').toggle('slow');
})
//for industry sectors			
$('.live-tile').hover(function() {
$(this).find('.tile-content').toggle('slow');
});
$('#specification-header').click(function() {
$('#specification-content').toggle('slow');
if($('#specification-title2').html() == "Minimize") {
$('#specification-title2').html('Maximize');						
$('#specification-title').css('background-image','url("/wp-content/themes/gconverter/images/arrow-up.png")');
$('#specification-title2').css('background-image','url("/wp-content/themes/gconverter/images/arrow-down.png")');
} else {
$('#specification-title2').html('Minimize');						
$('#specification-title').css('background-image','url("/wp-content/themes/gconverter/images/arrow-down.png")');
$('#specification-title2').css('background-image','url("/wp-content/themes/gconverter/images/arrow-up.png")');
}			
})
$('#specification-footer').click(function() {
$('#specification-content').toggle('slow');
if($('#specification-title2').html() == "Minimize") {
$('#specification-title2').html('Maximize');
$('#specification-title').css('background-image','url("/wp-content/themes/gconverter/images/arrow-up.png")');
$('#specification-title2').css('background-image','url("/wp-content/themes/gconverter/images/arrow-down.png")');
} else {
$('#specification-title2').html('Minimize');
$('#specification-title').css('background-image','url("/wp-content/themes/gconverter/images/arrow-down.png")');
$('#specification-title2').css('background-image','url("/wp-content/themes/gconverter/images/arrow-up.png")');
}					
})
$('#v1Td').click(function(){
$('#v400-tbl').css('display','block');
$('#v480-tbl').css('display','none');
if($(this).css('background-color') == "#d11f36" ) {
$(this).css('background-color','#d7d7d7');
$(this).css('color','#9d9d9d');
$('#v2Td').css('background-color','#d11f36');
$('#v2Td').css('color','#FFFFFF');
} else {
$(this).css('background-color','#d11f36');
$(this).css('color','#FFFFFF');
$('#v2Td').css('background-color','#d7d7d7');
$('#v2Td').css('color','#9d9d9d');
}
$('#v400-tbl').fadeOut('slow');
$('#v480-tbl').fadeIn('slow');
});
$('#v2Td').click(function(){
$('#v480-tbl').css('display','block');
$('#v400-tbl').css('display','none');
if($(this).css('background-color') == "#d11f36" ) {
$(this).css('background-color','#d7d7d7');
$(this).css('color','#9d9d9d');
