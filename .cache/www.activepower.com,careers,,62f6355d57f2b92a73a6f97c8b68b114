$('#menu-product-and-services li a').click(function() {
var parentid = $(this).parent().attr('id');
var postno = 0;
var i = 0;
$('#menu-product-and-services li').each(function() { 
i = i + 1;
if($(this).attr('id') == parentid) {
postno = i;
}
})
$('.serviceposts').fadeOut('slow');
$('#servicepost-'+postno).fadeIn('slow');	
return false;
});
