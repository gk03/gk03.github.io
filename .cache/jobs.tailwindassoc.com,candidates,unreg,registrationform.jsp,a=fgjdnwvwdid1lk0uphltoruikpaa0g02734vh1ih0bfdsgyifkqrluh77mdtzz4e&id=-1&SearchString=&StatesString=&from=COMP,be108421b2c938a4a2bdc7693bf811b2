function paste(){
	
		uploadMethod = 2;
		checkCookie();
		
}
function paste_continue() {
	var xhrArgs = {
			form: "form3",
			url: "/candidates/unreg/verifyinfopasteform_outside.jsp?a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e",
			load: function(data, ioArgs){
				var res = data.replace(/^\s+|\s+$/g,'');  
				if(res.indexOf("REDIRECT")==0){
					var url="/candidates/unreg/verifyinfo_outside.jsp?a=fgjdnwvwdid1lk0uphltoruikpaa0g02734vh1ih0bfdsgyifkqrluh77mdtzz4e&id=-1";
					window.location=url;
		        	return;
				}else{
					dojoAlert(data);
				}
			}
	};	  
	dojo.xhrPost(xhrArgs);
}
