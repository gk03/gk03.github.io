<script language='javascript'>
	b_now = new Image(150,50);		b_now.src = "images/b_ordernow.png";
	b_nowover = new Image(150,50);	b_nowover.src = "images/b_ordernowover.png";
	b_nowdown = new Image(150,50);	b_nowdown.src = "images/b_ordernowdown.png";
function hiLiteOver(bname) {
	document.images[bname].src = eval(bname + "over.src");
	return true;
}
function hiLiteDown(bname) {
	document.images[bname].src = eval(bname + "down.src");
	return true;
}
function hiLiteOff(bname) {
	document.images[bname].src = eval(bname + ".src");
	return true;
}
</script>
