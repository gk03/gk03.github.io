        <script>
            $(document).ready(function () {
                $("#owl-demo").owlCarousel({
                    autoPlay: 3000,
                    items: 4,
                    itemsDesktop: [1199, 3],
                    itemsDesktopSmall: [979, 3]
                });
                $('.video_icon_mobile img').click(function () {
                    var check = $('#video_popup').attr('autoPlay');
                    if (typeof check === typeof undefined || check === false) {
                        $('#video_popup').css('display', 'block').attr({ 'autoPlay': true });
                        $("#video_popup").each(function () { this.play() });
                    }
                    else {
                        $('#video_popup').css('display', 'none');
                        $('#video_popup').removeAttr('autoPlay');
                        $("#video_popup").each(function () { this.pause() });
                    }

                });


            });
        </script>
