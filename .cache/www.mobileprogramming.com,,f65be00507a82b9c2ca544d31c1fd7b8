                <script type="text/javascript">
                    $('.parallax-window1').parallax({ imageSrc: 'images/top-image.jpg' });
                    $('.parallax-window2').parallax({ imageSrc: 'images/bottom.jpg' });

                    function validateForm1() {
                        var x = document.getElementById('txtName').value;
                        if (x == null || x == "") {
                            alert("Name must be filled out");
                            document.getElementById('txtName').focus();
                            return false;
                        }
                        var x = document.getElementById('txtEmail').value;
                        if (x == null || x == "") {
                            alert("Email must be filled out");
                            document.getElementById('txtEmail').focus();
                            return false;
                        }
                        var atpos = x.indexOf("@");
                        var dotpos = x.lastIndexOf(".");
                        if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
                            alert("Not a valid e-mail address");
                            document.getElementById('txtEmail').focus();
                            return false;
                        }

                        var x = document.getElementById('txtPhone').value;
                        if (x == null || x == "") {
                            alert("Phone must be filled out");
                            document.getElementById('txtPhone').focus();
                            return false;
                        }
                        var x = document.getElementById('txtMsg').value;
                        if (x == null || x == "") {
                            alert("Message must be filled out");
                            document.getElementById('txtMsg').focus();
                            return false;
                        }
                    }

                </script>

