		//'#--- VALIDATE THAT FIELDS ARE NOT BLANK OR WHITESPACE
		function isblank(s)
		{
		for(var i = 0; i < s.length; i++)	{
		var c = s.charAt(i);
		if ((c != ' ') && (c != '\t')) return false;
		}
		return true;
		}
		
		//'#--- VALIDATE THAT REQUIRED FIELDS HAVE VALUES
function CheckValues(registrationForm) {
			if (registrationForm.username.value == "" || registrationForm.username.value == null || isblank(registrationForm.username.value)) {
				alert("Please enter your username.");
				registrationForm.username.focus();
				return false;
			}
			if (registrationForm.password.value == "" || registrationForm.password.value == null || isblank(registrationForm.password.value)) {
				alert("Please enter your password.");
				registrationForm.password.focus();
				return false;
			}

return true;
}
