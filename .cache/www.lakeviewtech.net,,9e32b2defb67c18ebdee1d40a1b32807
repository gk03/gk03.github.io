					<!-- Image Slider -->
					<script type="text/javascript">

						jQuery(document).ready(function($) {
							
							var carousel = jQuery("#img_carousel_6150");
							
							var args = {
									items: 				5,
									slideBy: 			1,						
									autoplay:			true,
									autoplayTimeout:	4000,
									nav: 				true,
									dots: 				false,
									loop: 				false,
									smartSpeed:			500,
									navText:			["<span class='icon  icon-angle-left-circle'></span>","<span class='icon icon-angle-right-circle'></span>"],
									navRewind: 			false,
									margin: 			25,
									responsive:{
										0:{
										  items:1,
										},
										480:{
										  items:1,
										  margin:20,
										},
										768:{
										  items:5
										}
									},
									onInitialized: showCarousel_6150
							};
							
							carousel.owlCarousel(args);

							var initital_width = carousel.css("width");		
								
							/* Refresh it for full width rows */
							$(window).load(function(){
								if(carousel.css("width") != initital_width) {
									carousel.trigger("destroy.owl.carousel").removeClass("owl-carousel owl-loaded");
									carousel.find(".owl-stage-outer").children().unwrap();
									carousel.owlCarousel(args);
								}
							});
							
							/* Show once loaded */
							function showCarousel_6150() {
								carousel.fadeTo(0,1);
								$("#img_carousel_6150 .owl-item .boc_owl_lazy").css("opacity","1");
							}
						});
						
					</script>
