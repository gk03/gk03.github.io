<script src="js/validation.js"></script><script type="text/javascript">

$(document).ready(function(){												

       //Navigation Menu Slider

        $('#navbar').on('click',function(e){

      		e.preventDefault();

      		$('body').toggleClass('nav-expanded');

      	}); 

      });

//header js start

/*$(document).ready(function () {

    $("header").before($("header").clone().addClass("small"));

    $(window).on("scroll", function () {

        $("body").toggleClass("down", ($(window).scrollTop() > 200));

    });

});*/

    function init() {

        window.addEventListener('scroll', function(e){

            var distanceY = window.pageYOffset || document.documentElement.scrollTop,

                shrinkOn = 80,

                header = document.querySelector("header");

            if (distanceY > shrinkOn) {

                classie.add(header,"small");

            } else {

                if (classie.has(header,"small")) {

                    classie.remove(header,"small");

                }

            }

        });

    }

    window.onload = init();



