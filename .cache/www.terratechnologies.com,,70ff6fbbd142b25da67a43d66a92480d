var variableslide=new Array()

//variableslide[x]=["path to image", "OPTIONAL link for image", "OPTIONAL text description (supports HTML tags)"]

variableslide[0]=['Pictures/Button_Files/Vena_Monticello_PR.gif', 'http://www.terratechnologies.com/Press_Releases/Vena%20_Monticello_PR/Vena_Monticello_PR.htm', '', '']
variableslide[1]=['Pictures/Button_Files/Houston_Lake_PR.gif', 'http://www.terratechnologies.com/Press_Releases/Houston_Lake_PR/Houston_Lake_PR.htm', '', '']
variableslide[2]=['Pictures/Button_Files/Delphi_PR.gif', 'http://www.terratechnologies.com/Press_Releases/Delphi_PR/Delphi_PR.htm', '', '']
variableslide[3]=['Pictures/Button_Files/Copperleaf_PR.gif', 'http://www.terratechnologies.com/Press_Releases/Copperleaf_PR/Copperleaf_PR.htm', '', '']	
variableslide[4]=['Pictures/Button_Files/Innovation_Business_Park_PR.gif', 'http://www.terratechnologies.com/Press_Releases/Innovation_Business_Park_PR/Innovation_Business_Park_PR.htm', '', '']


				






//configure the below 3 variables to set the dimension/background color of the slideshow

var slidewidth='515px' //set to width of LARGEST image in your slideshow
var slideheight='21px' //set to height of LARGEST iamge in your slideshow, plus any text description


//configure the below variable to determine the delay between image rotations (in miliseconds)
var slidedelay=5000

////Do not edit pass this line////////////////

var ie=document.all
var dom=document.getElementById

for (i=0;i<variableslide.length;i++){
var cacheimage=new Image()
cacheimage.src=variableslide[i][0]
}

var currentslide=0

function rotateimages(){
if (variableslide[currentslide][1]!="")
contentcontainer='<a href="'+variableslide[currentslide][1]+'">'
contentcontainer+='<img src="'+variableslide[currentslide][0]+'" border="0" vspace="0">'
if (variableslide[currentslide][1]!="")
contentcontainer+='</a>'
if (variableslide[currentslide][2]!="")
contentcontainer+=variableslide[currentslide][2]

if (document.layers){
crossrotateobj.document.write(contentcontainer)
crossrotateobj.document.close()
}
else if (ie||dom)
crossrotateobj.innerHTML=contentcontainer
if (currentslide==variableslide.length-1) currentslide=0
else currentslide++
setTimeout("rotateimages()",slidedelay)
}

if (ie||dom)
document.write('<div id="slidedom" style="width:'+slidewidth+';height:'+slideheight+'"></div>')

function start_slider(){
crossrotateobj=dom? document.getElementById("slidedom") : ie? document.all.slidedom : document.slidensmain.document.slidenssub
if (document.layers)
document.slidensmain.visibility="show"
rotateimages()
}

if (ie||dom)
start_slider()
else if (document.layers)
window.onload=start_slider

                    </script>
