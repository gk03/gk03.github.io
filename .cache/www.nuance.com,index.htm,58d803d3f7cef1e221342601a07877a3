	<script type="text/javascript">	
		var str=String(g_navNode_Path);
		patt1=/forbusiness/g;
		patt2=/forhealthcare/g;
		patt3=/byproduct/g;
		if ((str.match(patt3) != "byproduct" && str.match(patt1) == "forbusiness") || str.match(patt2) == "forhealthcare") { 
			$('.ncom_shoping-cart').hide();
		} else {
			$('.ncom_shoping-cart').show();
		}
		$('#shoppingCartSpinner').html('<img style="height:12px; width:12px" id="cartLoader" src="/assets/images/cartLoading.gif"/>'); 
		
		setTimeout(timeoutCart, 5000);
		function timeoutCart() {
			$('#shoppingCartSpinner').html("");
		}
  		  		 	
  		var cartCount = 0;	
		function receiveCartData(p) {	
			if (p["ns1:GetShoppingCartResponse"]["shoppingCartLineItems"] !== undefined) {	
				var cartItems = p["ns1:GetShoppingCartResponse"]["shoppingCartLineItems"];
				if (cartItems.length === undefined) {
					cartCount = 1;
				} else {
					cartCount = cartCount = cartItems.length;
				}
				if (cartCount == 1) {
					$('#ncom_shoppingCartCount').html(cartCount + " item in cart");		
				} else {
					$('#ncom_shoppingCartCount').html(cartCount + " items in cart");		
				}
				$('#ncom_shoppingCartCount').css("margin-right", "8px");	
				$('#ncom_shoppingCartSpinner').html("");
			} 
		}
		
		$('#ncom_cart-icon').mouseover(function() {
			if (cartCount == 0) {		
				$('#ncom_no-cart-items').show();			
			}
		});
		
		$('#ncom_cart-icon').mouseout(function() {
			if (cartCount == 0) {		
				$('#ncom_no-cart-items').hide();				
			}
		});
		
		$('#ncom_cart-icon').click(function() {
			if (cartCount == 0) {		
				$('#ncom_no-cart-items').show();			
				//pageTracker._trackPageview('/cart/clicked_no_items');
				return true;
			} else {
				//pageTracker._trackPageview('/cart/clicked_with_items');
				return true;
			}
		});
	</script>
	
