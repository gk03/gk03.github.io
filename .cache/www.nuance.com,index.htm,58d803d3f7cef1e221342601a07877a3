<script type="text/javascript">

function n_t_getLastDiv()
{
    var last_formdiv = n_getCookies("lastformdiv");
    var last_formdivdt = n_getCookies("lastformdivdt");
    var last_pagediv = n_getCookies("lastpagediv");
    var last_pagedivdt = n_getCookies("lastpagedivdt");
    var dtEarlier = new Date(); dtEarlier.setDate(dtEarlier.getDate() + 30);
    
    var allDivisionStr = "_Enterprise_Healthcare_Imaging_Dragon_Automotive_Mobile_"; 
    // just dragon now
    var activeDivisionStr = "_Dragon_Enterprise_Mobile_Imaging_Healthcare_"; 
    
    var viewDivision ="Corp"; 
    // make the logic: 
    if ( (!n_isStringBlank(last_formdiv)) && (allDivisionStr.indexOf(last_formdiv) >= 0 )) {
        viewDivision = last_formdiv; 
        if ( (!n_isStringBlank(last_pagediv)) && (dtEarlier > last_formdivdt) && ( allDivisionStr.indexOf(last_pagediv) >= 0) && ( last_pagedivdt >= dtEarlier)) {
            viewDivision = last_pagediv; 
        };
    } else {
        if ((!n_isStringBlank(last_pagediv)) && (allDivisionStr.indexOf(last_pagediv) >=0)) {
            viewDivision = last_pagediv;
        }
    }
    // also take effect from URL parameters
    var divVarOverride = n_getParameterByName("divisionVarOverride");
    if ( n_isASpecificDivision(divVarOverride) || n_isNuanceCorpDivision(divVarOverride)) {
        viewDivision = divVarOverride;
    }
    
    // but not active go back to Corp
    if ( activeDivisionStr.indexOf(viewDivision) < 0 ) { viewDivision = "Corp"; }

  // set division for Engage Us
  division = viewDivision;
    
    return viewDivision;
}
// a global variable for GA to log
var n_homePageDynmacDiv = n_t_getLastDiv();
// figure out which division block to show
function n_t_getVisibleDiv() {
    return n_homePageDynmacDiv;
}
function n_t_getShowDivCLS()
{
    return ".division_" + n_homePageDynmacDiv;
}

// Normalize Carousel Heights - pass in Bootstrap Carousel items.
$.fn.carouselHeights = function() {

    var items = $(this), //grab all slides
        heights = [], //create empty array to store height values
        tallest; //create variable to make note of the tallest slide

    var normalizeHeights = function() {

        items.each(function() { //add heights to array
            heights.push($(this).height()); 
        });

        tallest = Math.max.apply(null, heights); //cache largest value
        
        items.each(function() {           
            $(this).css('min-height',tallest + 'px');
        });

        // Set the carousel's Prev/Next arrow vertical position
        // Top is half of (carousel-inner-height + control-height)
        var ci_height = $("div .carousel-inner").height();
        var control_height = $(".carousel-control").height();
        var top_height = (ci_height + control_height) / 2;
        $("#carousel_cont_left").css('top', '-' + top_height + 'px');
        $("#carousel_cont_right").css('top', '-' + top_height + 'px');
    };

    normalizeHeights();

    $(window).on('resize orientationchange', function () {
        //reset vars
        tallest = 0;
        heights.length = 0;

        items.each(function() {
            $(this).css('min-height','0'); //reset min-height
        }); 
        normalizeHeights(); //run it again 
    });
};

   $(document).ready(function() {

      var divBlockCLS = n_t_getShowDivCLS();
      $(divBlockCLS).css('display', 'block'); 

      // Normalize all slide heights 
      $('#hpCarousel .item').carouselHeights();

      // === carousel auto play === //
      $('#hpCarousel').carousel({ 
          interval: 4500,
          pause: "hover"
      });
      $('#hpCarousel').carousel('cycle');
   
      // Show/Hide prev/next on hover
      $('#hpCarousel').hover(
          function(){  // mouseover
              // Show controls only if viewport width is 1130px or more
              if (window.innerWidth >= 1130) {
                  $('#carousel_cont_left').fadeIn();
                  $('#carousel_cont_right').fadeIn();
              }
          },
          function(){ // mouseout
              $('#carousel_cont_left').fadeOut();
              $('#carousel_cont_right').fadeOut();
          }
      );

      // track clickthrough of banners in GA
      trackBannerLink(); 
      // track clickthrough of spotlight items in GA
      trackSpotlightLink();
      // track clickthrough of footer items in GA
      trackFooterLink();

      // equalize promo tile height
      var maxHeight = 0;
      var theDivVerticalPos = $("div.promoTile_prodinfo:visible:first").offset().top;
      var bNeedAdj = true;
    $("div.promoTile_prodinfo:visible").each(function(){
       if ($(this).height() > maxHeight) { maxHeight = $(this).height(); }
       if ($(this).offset().top != theDivVerticalPos) { bNeedAdj = false; }
       
    });
    if ( bNeedAdj ) {   $("div.promoTile_prodinfo:visible").height(maxHeight); }

  });

  var bannerDivision, bannerDivisionOnClick, spotLinkURL, spotLinkURLOnClick, exploreLink, exploreLinkOnClick, footerLink, footerLinkOnClick;
  function trackBannerLink() {
    // make sure we use the current visible one:
    var bannerSelector = n_t_getShowDivCLS() + " #hpCarousel a";
    $(bannerSelector).each(function(index) {
        bannerDivision = $(this).attr('rel');
        bannerDivisionOnClick = "_gaq.push(['_trackEvent', 'Homepage Banner - " + n_t_getVisibleDiv() + "', 'Clickthrough', '" + bannerDivision + "']);";
        $(this).attr("onClick", bannerDivisionOnClick);
     });
    bannerSelector = n_t_getShowDivCLS() + " .mastheadImage a, " + n_t_getShowDivCLS() + " #logoContainer a";
    $(bannerSelector).each(function(index) {
        bannerDivision = $(this).attr('rel');
        bannerDivisionOnClick = "_gaq.push(['_trackEvent', 'Homepage Banner - " + n_t_getVisibleDiv() + "', 'Clickthrough', '" + bannerDivision + "']);";
        $(this).attr("onClick", bannerDivisionOnClick);
     });
  }

  function trackSpotlightLink() {
  
    var mySelector = n_t_getShowDivCLS() + " .promoTile a";
    $(mySelector).each(function(index) {
        spotLinkURL = $(this).attr('rel');
        spotLinkURLOnClick = "_gaq.push(['_trackEvent', 'Homepage Spotlight - " + n_t_getVisibleDiv() + "', 'Clickthrough', '" + spotLinkURL + "']);";
        $(this).attr("onClick", spotLinkURLOnClick);
     });
    mySelector = n_t_getShowDivCLS() + " .exploreLinks a";
    $(mySelector).each(function(index) {
        exploreLink = $(this).text();
        exploreLinkOnClick = "_gaq.push(['_trackEvent', 'Homepage Explore Menu - " + n_t_getVisibleDiv() + "', 'Clickthrough', '" + exploreLink + "']);";
        $(this).attr("onClick", exploreLinkOnClick);
     });
  }

  function trackFooterLink() {
    $("#footer a").each(function(index) {
        footerLink = $(this).text();
        footerLinkOnClick = "_gaq.push(['_trackEvent', 'Homepage Footer', 'Clickthrough', '" + footerLink + "']);";
        $(this).attr("onClick", footerLinkOnClick);
     });
  }

</script>   
 
<script type="text/javascript">
  $(document).ready(function() {

     $("#hpCarousel").swiperight(function() {
        $("#hpCarousel").carousel('prev');
      });
      $("#hpCarousel").swipeleft(function() {
        $("#hpCarousel").carousel('next');
      });
  });
</script>

