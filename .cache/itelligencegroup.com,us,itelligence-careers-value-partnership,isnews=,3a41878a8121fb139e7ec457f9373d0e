<script type="text/javascript">
	jQuery(document).ready(function () {
		initialize();

		jQuery("a").each(function () {
			jQuery(this).addClass("ga-track");
		});

		// Below is dynamically adding tracking for asset links
		jQuery("a.ga-track").each(function () {
			var href = jQuery(this).attr("href");
			if (href === undefined) {
				return true;
			} else {
				var qUrl = href.substring(href.indexOf("?"))
				var firstLetter = qUrl.charAt(0);
				var title = jQuery('.open-box-panel h1').text();

				if (href.indexOf("youtube") > -1) {
					// This checks for youtube links
					jQuery(this).click(function () {
						ga('send', 'event', title, 'click', href);
					});
				}
				if (firstLetter == '?') {
					// This checks for asset links
					var id_check = /[?&]name=([^&]+)/i;
					var match = id_check.exec(qUrl);
					if (match != null) {
						jQuery(this).click(function () {
							var filename = match[1].substring(match[1].lastIndexOf('/') + 1);
							ga('send', 'event', title, 'click', filename);
						});
					}
				} else if (jQuery(this).hasClass('ext-link')) {
					// This checks for external links
					jQuery(this).click(function () {
						ga('send', 'event', title, 'click', href);
					});
				}
			}

		});

		// General form - formerly known as general-form.php
