<script type="text/javascript">
jQuery(window).load(function() {
	//clear all
	$(".t-nav a").removeClass("active");
	$(".tabs artile").removeClass("current");
	//set default
	$(".tab1").addClass("active");
	$(".tabs article.txt1").addClass("current");
	//actions
	$(".tab1").click(function(){
		$(".t-nav a").removeClass("active");
		$(".tabs article").removeClass("current");
		$(this).addClass("active");
		$(".tabs article.txt1").addClass("current");
	});
	//actions
	$(".tab2").click(function(){
		$(".t-nav a").removeClass("active");
		$(".tabs article").removeClass("current");
		$(this).addClass("active");
		$(".tabs article.txt2").addClass("current");
	});
	//actions
	$(".tab3").click(function(){
		$(".t-nav a").removeClass("active");
		$(".tabs article").removeClass("current");
		$(this).addClass("active");
		$(".tabs article.txt3").addClass("current");
	});
	$(".f-tab2").click(function(){
		$(".f-tab1").removeClass("active");
		$(".fade1-pager").removeClass("active");
		$(this).addClass("active");
		$(".fade1").removeClass("current");
		$(".fade2-pager").addClass("active");
		$(".fade2").addClass("current").delay(1200);
	});
});
</script>
<script type="text/javascript">
jQuery(window).load(function() {
    //caches a jQuery object containing the header element
    var header = $(".topper");
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        if (scroll >= 500) {
            header.removeClass('topper').addClass("fixer");
        } else {
            header.removeClass("fixer").addClass('topper');
        }
    });
});
</script>
