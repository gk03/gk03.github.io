<script type="text/javascript">
jQuery(document).ready(function($) {
//IF WINDOW RESIZED
$(window).on('resize', function(){
      var win = $(this); //this = window
      if (win.width() >= 800) {
	  $('body').removeClass('stop-scroll');
	  $(".dimmer").fadeOut();
	  $("nav.mob").css("display", ""); //reset display
	  $(".nav-close").css("display", "");
	  $(".nav-open").css("display", "");
	  }
});
//OPENER
$(".nav-open").click(function(){
	$(".nav-open").hide();
	$(".nav-close").css("display","block");
	$(".dimmer").fadeIn(300);
	$("nav.mob").slideDown(300);
	$('body').addClass('stop-scroll');
	e.preventDefault();
});
//CLOSER
$(".nav-close").click(function(){
	$(".nav-close").hide();
	$(".nav-open").show();
	$(".dimmer").fadeOut(300);
	$("nav.mob").slideUp(300);
	$('body').removeClass('stop-scroll');
	e.preventDefault();
});
$(".nav-close").click(function(){
	$(".nav-close").hide();
	$(".nav-open").show();
	$(".dimmer").fadeOut(300);
	$("nav.mob").slideUp(300);
	$('body').removeClass('stop-scroll');
	e.preventDefault();
});
$("nav.mob a").click(function(){
	$(".nav-close").hide();
	$(".nav-open").show();
	$(".dimmer").fadeOut(300);
	$("nav.mob").slideUp(300);
	$('body').removeClass('stop-scroll');
	e.preventDefault();
});
//BG CLOSER
$(".dimmer").click(function(){
	$(".nav-close").hide();
	$(".nav-open").show();
	$(".dimmer").fadeOut(300);
	$("nav.mobe").slideUp(300);
	$('body').removeClass('stop-scroll');
	e.preventDefault();
});
});
</script>
