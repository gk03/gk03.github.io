<script type="text/javascript">
jQuery(window).load(function() {
$( ".map-box" ).append( '<a class="close">X</a>' );
$(".map-canvas a").click(function () {
    $('.active-m').removeClass("active-m");
    $(this).addClass("active-m");
    $("div.map-box").removeClass("current").eq($(this).index()).addClass("current");
	$(".dimmer2").fadeIn(300);
	$('body').addClass('stop-scroll');
	e.preventDefault();
});
//BG CLOSER
$(".map-box .close").click(function(){
    $("div.map-box").removeClass("current");
    $(".active-m").removeClass("active-m");
	$("body").removeClass("stop-scroll");
	$(".dimmer2").fadeOut(300);
});
$(".dimmer2").click(function(){
    $("div.map-box").removeClass("current");
    $(".active-m").removeClass("active-m");
	$("body").removeClass("stop-scroll");
	$(this).fadeOut(300);
});
});
</script>
