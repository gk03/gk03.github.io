if(document.title.indexOf("Editor") == -1 && document.title.indexOf("Login") == -1) {
	var data,nhp,ntz,rf,sr;
	document.cookie='__support_check=1';
	nhp='http';
	rf=document.referrer;
	sr=document.location.search;
	if(top.document.location==document.referrer
			|| (document.referrer == '' && top.document.location != ''))
	{
		rf=top.document.referrer;
		sr=top.document.location.search
	}
	ntz=new Date();
	
	if((location.href.substr(0,6)=='https:') || 
		(location.href.substr(0,6)=='HTTPS:')) nhp='https'; 

	data='&an='
		+ escape(navigator.appName)
		+ '&ck='
		+ document.cookie.length
		+ '&rf='
		+ escape(rf)
		+ '&sl='
		+ escape(navigator.systemLanguage)
		+ '&av='
		+ escape(navigator.appVersion)
		+ '&l='
		+ escape(navigator.language)
		+ '&pf='
		+ escape(navigator.platform)
		+ '&pg=' 
		+ escape(document.title);
	if(navigator.appVersion.substring(0,1)>'3') 
	{
		data= data
			+ '&cd='
			+ screen.colorDepth
			+ '&rs='
			+ escape(screen.width
			+ ' x '
			+ screen.height)
			+ '&tz='
			+ ntz.getTimezoneOffset()
			+ '&je='
			+ navigator.javaEnabled();
	}
	document.write('<img border=0 hspace=0 '
		+ 'vspace=0 width=1 height=1 src="'
		+ nhp
		+ '://loc1.hitsprocessor.com/'
		+ 'statistics.asp?v=1&s=22&pl=1&acct=mts_mts'+data+'">');
}
 
