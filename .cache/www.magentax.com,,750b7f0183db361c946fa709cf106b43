<script type="text/javascript">
Tawk_API = Tawk_API || {};
Tawk_API.onStatusChange = function (status){
if(status === 'online')
{
document.getElementById('status').innerHTML = '<a href="javascript:void(Tawk_API.toggle())" class="livechat"><i class="chat-icon"></i>Live Chat Support!</a>';
}
else if(status === 'away')
{
document.getElementById('status').innerHTML = '<a href="javascript:void(Tawk_API.toggle())" class="livechat"><i class="chat-icon"></i>Live Chat Support!</a>';
}
else if(status === 'offline')
{
document.getElementById('status').innerHTML = '<a href="javascript:void(Tawk_API.toggle())" class="livechat"><i class="chat-icon"></i>Free Consultation!</a>';
}
};
</script>
 
