    <script type="text/javascript">

        $(document).ready(function () {

	    $('#theYear').html(new Date().getFullYear());

            fixCountryZip();


            $("#idRegion_country").change(function () {

                fixCountryZip();
            });


        });



        function fixCountryZip() {

            if ($("#idRegion_country").val() == '42') {

                $("#ZipCode").attr('maxlength', '6');
                $("#ZipCode").attr('placeholder', 'Enter postal code');

                $("#pInfo").css('visibility', 'visible');
            }
            else {
                $("#ZipCode").attr('maxlength', '5');
                $("#ZipCode").attr('placeholder', 'Enter Zip');
                $("#pInfo").css('visibility', 'hidden');

            }

        }


        function fire_reg_button_click() {

            var splashData = [];

            //grab values from the form.
            var idGenderMask = $("#idgendermask").val();
            var idEmailAddress = $("#idemailaddress").val();
            var idRegionCountryid = $("#idregioncountryid").val();
            var idRegionZip = $("#regionZipId").val();

            if (idGenderMask != "")
                splashData.push({ Name: "idgendermask", Value: idGenderMask });
            else
                splashData.push({ Name: "idgendermask", Value: "" });

            if (idEmailAddress != "")
                splashData.push({ Name: "idemailaddress", Value: idEmailAddress });
            else
                splashData.push({ Name: "idemailaddress", Value: "" });

            if (idRegionCountryid != "")
                splashData.push({ Name: "idregioncountryid", Value: idRegionCountryid });
            else
                splashData.push({ Name: "idregioncountryid", Value: "" });

            if (idRegionZip != "")
                splashData.push({ Name: "regionZipId", Value: idRegionZip });
            else
                splashData.push({ Name: "regionZipId", Value: "" });

            //save to cookie
            var cookie = ["splashFormValues", '=', JSON.stringify(splashData), '; domain=.', window.location.host.toString(), '; path=/;'].join('');

            var now = new Date();
            var time = now.getTime();
            time += 3600 * 1000;
            now.setTime(time);

            document.cookie = cookie + '; expires=' + now.toUTCString();


            //$.ajax({
            //    type: "POST",
            //    url: "/Registration/CaptureSplashPageFormValues",
            //    //data: stepData,
            //    contentType: 'application/json; charset=utf-8',
            //    data: JSON.stringify(splashData),
            //    dataType: 'json',
            //    beforeSend: function () {

            //    }
            //}).done(function (msg) {
            //    if (msg.Status === 0) {
            //        console.log("we good");
            //    }
            //    if (msg.Status === 1) {
            //        console.log("some thing happened with the call moving forward check logs");

            //    }

            //

            //});


            return false;
        }




    </script>

