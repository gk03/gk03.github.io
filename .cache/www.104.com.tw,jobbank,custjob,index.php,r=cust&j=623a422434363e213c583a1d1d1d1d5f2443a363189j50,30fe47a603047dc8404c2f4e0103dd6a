		{
			jobCnt = jobcatRoot.n[i].n.length;
			JobCatCnt = jobcatRoot.n.length;
			if(JobCatCnt > 1)
			{
				optionName = jobcatRoot.n[i].pureDes+'-職務類別(不拘)';
				optionValue = jobcatRoot.n[i].no+'_0';
				frm_obj.options[optionItem]=new Option(optionName,optionValue);
				if(optionValue == sel_catlist)
					frm_obj.options[optionItem].selected = true;
				optionItem = optionItem +1;
			}	
			if(jobCnt > 0)
			{
				for(var j=0; j<jobCnt; j++)
				{
					if(jobcatCnt > 1)
						optionName = jobcatRoot.n[i].pureDes+'-'+jobcatRoot.n[i].n[j].des;
					else
						optionName = jobcatRoot.n[i].n[j].des;
					optionValue = jobcatRoot.n[i].no+'_'+jobcatRoot.n[i].n[j].no;
					frm_obj.options[optionItem]=new Option(optionName,optionValue);
					if(optionValue == sel_catlist)
						frm_obj.options[optionItem].selected = true;
					optionItem = optionItem +1;
				}
			}
		}
				
		var jobcatlv = 3;
		if (sel_main_jobcat > 0 && IsNumeric(sel_main_jobcat)){
			if (sel_main_jobcat.Right(6) == '000000'){
				jobcatlv = 1;
			}
			else if (sel_main_jobcat.Right(3) == '000'){
				jobcatlv = 2;
			}
		}
