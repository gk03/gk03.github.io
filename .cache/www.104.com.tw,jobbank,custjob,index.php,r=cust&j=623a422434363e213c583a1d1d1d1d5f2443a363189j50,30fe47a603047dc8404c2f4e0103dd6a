		String.prototype.Right = function (n)
	{
	    if (n <= 0)
	       return "";
	    else if (n > String(this).length)
	       return this;
	    else {
	       var iLen = String(this).length;
	       return String(this).substring(iLen, iLen - n);
	    }
	}
