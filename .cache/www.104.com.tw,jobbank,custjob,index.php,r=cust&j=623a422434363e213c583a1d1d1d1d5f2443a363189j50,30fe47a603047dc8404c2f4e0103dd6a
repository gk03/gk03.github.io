		function IsNumeric(sText) 
	{   		var ValidChars = "0123456789"; 
	    var IsNumber=true; 
	    var Char;
	    	  
	   	for (i = 0; i < sText.length && IsNumber == true; i++){  
	    	Char = sText.charAt(i);
	      	if (ValidChars.indexOf(Char) == -1){
	        	IsNumber = false;
	        }
	    }
	   return IsNumber;
	 }
	 
	function sort(field){
		var frm = document.jobform;
				$104get('sel_main_jobcat_name').value = $('#sel_catlist :selected').text();		
		frm.action = _global_basehref + '&page=1#info06';		
		frm.target = '_self';
		var sortlog = frm.sortlog.value;		
		var sortlog_field = sortlog.substr(0,sortlog.indexOf("|"));
		var sortlog_mode = sortlog.substr(sortlog.indexOf("|")+1,sortlog.length);
		frm.sortfield.value = field;
		if (sortlog_field == field || sortlog_field.indexOf(field) >= 0){
			if (sortlog_mode == 'desc')
				frm.sortmode.value = 'asc';
			else
				frm.sortmode.value = 'desc';
		}else{
			frm.sortmode.value = 'desc';
		}	
		frm.submit();
	}

	function change_bgcolor(pos, act){
        if (act == 0){
        	$("#joblist_cont" + pos).addClass("tr_hover");
			$("#joblist_summary" + pos).addClass("tr_hover");
        }
        else{
        	$("#joblist_cont" + pos).removeClass("tr_hover")
        	$("#joblist_summary" + pos).removeClass("tr_hover")
        }
	}
		function to_buffet(x){
		var choice_state = false;
		var jobno = '';
		var custno = '';
		if (x == null)
			var x = 0;
			cookie_string = '';
			(x==1)?objname=document.jobform:objname = document.jobform;
			if (x>0){
				if (x==1){
					if (objname.to_cookie.checked && objname.to_cookie.value !=0){
						choice_state = true;
						jobno = objname.orderno.value;
						custno = objname.custno_item.value;
					}
				}else{
					for (i=0;i<x;i=i+1){
						if (objname.to_cookie[i].checked && objname.to_cookie[i].value !=0){
							choice_state=true;
							if ( jobno == ''){
								jobno = objname.orderno[i].value;
								custno = objname.custno_item[i].value;
							}else{
								jobno = jobno + ','+objname.orderno[i].value;
								custno = custno + "," + objname.custno_item[i].value;
							}
						}
					}
				}
			}
			if (choice_state){
				objname.action = 'http://login.104.com.tw/login.cfm?channel=104buffet';
				objname.target = 'buffet_pda';
				objname.custno.value = custno;
				if (objname.jobno !=null){
					objname.jobno.value = jobno;
				}else{
					objname.action = 'http://login.104.com.tw/login.cfm?channel=104buffet&save_job='+jobno;
				}
				objname.buffet_type.value = '';
				objname.submit();
			}else{
				alert('請勾選工作機會!');
			}
	}	
	
	function setReturnURL(){
		var frm = document.jobform;
		frm.return_url.value = 'http://www.104.com.tw/jobbank/login/mail_to_self.cfm';
		frm.url_destination.value = 'http://www.104.com.tw/jobbank/login/mail_to_self.cfm';
	}
    	function to_custno_buffet(){ 
		var objname = document.jobform;
		objname.buffet_type.value='cust'
		objname.action = 'http://login.104.com.tw/login.cfm?channel=104buffet';
		objname.jobno.value='0';
		objname.target='buffet_pda';
		objname.submit();
		objname.buffet_type.value = '';
	}
