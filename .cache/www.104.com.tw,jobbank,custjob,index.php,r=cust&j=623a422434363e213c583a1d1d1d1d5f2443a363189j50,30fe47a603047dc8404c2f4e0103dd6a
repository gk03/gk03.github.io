	function changeselect(objtarget,objlevel,selrole,selcat){
		var jrcnt = jobcatRoot.n.length;
		if (!jrcnt) return false;
		var roleindex = 0;
		if (jrcnt > 1 && jrcnt == 2){
			roleindex = selrole - 1;
		}else if (jrcnt > 1){
			roleindex = selrole - jrcnt;
		}
		if (roleindex < 0) roleindex=0;
		var objlevel = (objlevel)?objlevel:2;
		var selcat = (selcat)?selcat:0;

		var frm_obj = $104get(objtarget);
		if (roleindex == -1){
			frm_obj.options.length=1;
			return false;
		}
		var obj_root = (objlevel==1)?jobcatRoot.n:jobcatRoot.n[roleindex].n;
		if (frm_obj){
			frm_obj.options.length = 0;
			if (objtarget == 'sel_rolelist'){
				frm_obj.options[0]=new Option('全部(6)','0');
			}else{
				frm_obj.options[0]=new Option('職務類別(不拘)','0');	
			}
			OptionLen =frm_obj.options.length;
		  var j=0;
		  for (var i=0;i<obj_root.length;i++){
		  	j = OptionLen++;
		  	frm_obj.options[j]=new Option(obj_root[i].des,obj_root[i].no);
		  	if (obj_root[i].no == selcat) frm_obj.options[j].selected=true;
		  }
		}
	}	
