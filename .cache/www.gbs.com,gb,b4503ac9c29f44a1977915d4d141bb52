       sub.hide();
        $('#submit').click( function( event ) {
            event.preventDefault();
            sub.trigger( 'click' );
        });
    });
    
    function cookiebarf()
      {  
        
      if ($('#cookiebar').length){	
  var allcookies = document.cookie;
  var  cb = allcookies.search('COOKIEBAR');
    if (cb>-1)  {$('#cookiebar').remove();}    
      }
  }
cookiebarf();
  
	
