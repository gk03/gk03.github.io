<SCRIPT>
	function onViewer()
	{

		if (document.all.cnt1.style.display == 'none')
		{
			document.all.cnt1.style.display = '';
		}
		else{
			document.all.cnt1.style.display = 'none';
		}
	}

//	window.onload = onViewer();

</script>
