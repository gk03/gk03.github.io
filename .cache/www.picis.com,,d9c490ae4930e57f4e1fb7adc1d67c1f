			var $ = jQuery;
			$().UItoTop();
			var search = $('#header .search');
			var button = $('#header .search .wsite-search-button');
			var input = $('#header .search .wsite-search-input');
			$('body').click(function(e){
				if($(e.target).attr("class") != "wsite-search-input"){
					if(search.hasClass('show')){
						search.removeClass('show');
					}
				}
			});
			button.click(function(){
				if(input.val()==""){
					search.toggleClass('show');
					input.focus();
					return false;
				}
			});
			$('.wsite-menu-default a, .wsite-menu a, .baamboo-responsive-nav a').click(function(){ var href = $(this).attr('href'); if(href=='http://javascript:void(0)' || href=='http://javascript:;' || href=='http://#'){ return false; } });
