    <script>

        jQuery(".magento-footer").load("https://www.iwdagency.com/extensions/iwd_blog/integration/footer", function(response, status, xhr) {
            if (status == "error") {
                var msg = "Sorry but there was an error: ";
                console.log(msg + xhr.status + " " + xhr.statusText);
            } else {
                IWD.Signin.init();

                if (typeof IWD.ES.config === 'string' || IWD.ES.config instanceof String) {
                    IWD.ES.config = JSON.parse(IWD.ES.config);
                }
            }
        });
