<script type="text/javascript">
//<![CDATA[
    var contactForm = null;
    if(typeof VarienForm != "undefined"){
        contactForm = new VarienForm('contact-form-footer', false);
    }

    function fsubm() {
        if (contactForm == null || contactForm.validator.validate()) {
            var formData = $ji('#contact-form-footer').serialize();
            $ji.ajax({
                url: 'https://www.iwdagency.com/extensions/contacts/index/post/',
                type: "POST",
                dataType: 'json',
                data: formData,
                success: function (response) {
                    //var response = $ji.parseJSON(data.responseText);
                    if (response.error == true) {
                        if(response.type == 'captcha'){
                            message = "<span class='error'>Please confirm you are not a robot.</span>";
                        } else {
                            message = "<span class='error'>Your message is not sent. Try again later</span>";
                        }
                    } else {
                        message = "<span class='success'>Thanks for your message! We will contact you shortly</span>";
                    }
                    $ji('#message').show().html(message);
                    if(response.type != 'captcha'){
                        $ji('#contact-form-footer button').hide();
                    }

                    $ji('body, html').animate({
                        scrollTop: $ji("#contact-form-footer").offset().top - 160
                    }, 2000);                },
                error: function () {
                    var message = "<span class='error'>Your message is not sent. Try again later</span>";
                    $ji('#message').show().html(message);
                }
            });
        }
    }

    function showcaptcha() {
        document.getElementById("rcode").style.display = "none";
    }

//]]>
</script>
