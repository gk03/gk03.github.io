		var $ = jQuery;
        $(document).ready(function () {

            var containerHeight = jQuery(".navi-container").height();
	    var duration = 200;

            jQuery(".navi-container > .container >  ul.root > li.menu-item").each(function () {
                var titleHeight = jQuery(this).find(".navi-item-title").height();
                jQuery(this).find(".navi-item-title").css("top", (containerHeight - titleHeight) / 2);
                jQuery(this).find(".navi-item-description").css("top", containerHeight);
		
            });

            jQuery(".navi-container  > .container > ul.root > li.menu-item").bind("mouseover", function () {
		//jQuery(this).height();
                var titleHeight = jQuery(this).find(".navi-item-title").height();
		jQuery(this).addClass("height140");
		jQuery(this).css("overflow","visible !important");
                jQuery(this).children(".navi-item-container").find(".navi-item-title").stop().animate({ "top": 0 }, duration );
                jQuery(this).children(".navi-item-container").find(".navi-item-description").stop().show().animate({"top": titleHeight, "opacity" : 1 }, {  duration: duration, complete : function(){
			
		} });
            })

            jQuery(".navi-container  > .container > ul.root > li.menu-item").bind("mouseout", function () {
		//jQuery(this).height(containerHeight);
		jQuery(this).removeClass("height140");
		jQuery(this).css("overflow","visible !important");
                var titleHeight = jQuery(this).find(".navi-item-title").height();
                jQuery(this).children(".navi-item-container").find(".navi-item-title").stop().animate({"top": (containerHeight - titleHeight) / 2 }, duration );
                jQuery(this).children(".navi-item-container").find(".navi-item-description").stop().animate({"top": containerHeight, "opacity" : 0 },{  duration: duration, complete : function(){ jQuery(this).hide("fast");  } });
            })  
		});
    </script>
