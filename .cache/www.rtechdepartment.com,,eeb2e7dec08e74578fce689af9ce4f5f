var titles = new Array("Service Contracts","PC Repair &amp; Support","Computers &amp; Hardware");
var contents = new Array("Our service contracts are the perfect way for businesses to fulfill the need for an IT staff, at a fraction of the cost. Our service contracts enable you to do what you do best by keeping your network and systems running smoothly and efficiently. Pick from one of our several service contracts to accommodate your needs and your budget.<ul><li>Unlimited Hours of Support*</li><li>Remote Assistance</li><li>Free Phone Support</li><li>Quick Response Time (varies per plan)</li><li>Priority Access to our Experienced Staff</li><li>Discounted or free parts (varies per plan)</li></ul><br />","At RTech Department our technicians are highly experienced with computer diagnostics, repair and problem resolution. With over 25 combined years experience resolving the toughest computer problems. In addition, RTech Department provides the following computer services:<ul><li>Troubleshooting</li><li>Spyware / Malware / VirusÂ Removal</li><li>Upgrades (Hardware / Software / Operating System)</li><li>Hardware Installation</li><li>Wireless Networks</li><li>System Optimization</li><li>Printer / Peripheral Installation</li></ul><br />","We can custom build home PCs, business workstations and servers. We provide the following and more.<ul><li>Workstations (Intel / AMD)</li><li>Servers (Intel / AMD)</li><li>Software (OS, Anti-Virus, Office, Monitoring)</li><li>LCD Monitors</li><li>Hard Drives</li><li>CPUs</li><li>Motherboards</li><li>Memory</li><li>Power Supplies</li><li>Cables</li><li>Routers / Switches</li></ul><br />");

var rotKey = 1;
function swapFeature() {
	if (rotKey == 3) {
		rotKey = 1;
	} else {
		rotKey++;
	}
	$("#featured").html(titles[rotKey-1]);
	$("#content-home2").html(contents[rotKey-1]);
}


function rotate() {
	$("#featured").animate({opacity:0},1000,"swing");
	$("#content-home2").animate({opacity:0},1000,"swing",function () {														   
		swapFeature();
		$("#featured").animate({opacity:1},1000,"swing");
		$("#content-home2").animate({opacity:1},1000,"swing",function() {
			window.setTimeout('rotate();',6000);
		});
	});
}

$(document).ready(function(){
	window.setTimeout('rotate();',6000);
});
</script>
