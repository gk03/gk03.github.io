<script>
var screen_timeout = null;

// auto-scrolling after 6 seconds
function slideTimerRestart() {
    clearTimeout(screen_timeout);
    screen_timeout = setTimeout(function () {
        $.fn.fullpage.moveSectionDown();
    }, 6000);
}

function onLeave(index, next, dir) {
    // first slide
    if (index == 1) {
        $('#meetqx').removeClass('active');
    }

    // second slide
    $('#qx200back').toggleClass('active', next == 2);

    // slide 2 and 3 (show static)
    var s = $('#static-section');
    if (next != 2 && next != 3) {
        s.removeClass('active');
    }

    s.toggleClass('slide3', next == 3);
    s.toggleClass('slide2', next == 2);
	
	// fourth slide
    $('#isdn4plus').toggleClass('active', next == 4);
}

function afterLoad(index) {
    // slide 1
    $('#meetqx').toggleClass('active', index == 1);

    // slide 2
    $('#qx200back').toggleClass('active', index == 2);

    // slide 2 & 3
    var s = $('#static-section');
    s.toggleClass('slide3', index == 3);
    s.toggleClass('slide2', index == 2);
	
	// slide 4
	$('#isdn4plus').toggleClass('active', index == 4);
}

$(document).ready(function () {
    $('#fullpage').fullpage({
        // anchors : ['meetqx', 'sleeker', 'customizable'],
        autoScrolling : true,
        css3 : true,
        loopBottom : true,
        navigation : true,
        navigationPosition : 'right',
        navigationTooltips : ['Meet the QX', 'Sleeker', 'Customizable','QXISDN4+'],
        resize : false,
        verticalCentered : false,

        afterRender : function () {
            slideTimerRestart();
            afterLoad(1);
        },

        'afterLoad' : function (anchorLink, index) {
            slideTimerRestart();
            afterLoad(index);
        },

        'onLeave' : function (index, nextIndex, direction) {
            slideTimerRestart();
            onLeave(index, nextIndex, direction);
        }
    });
	
	$('#newsReelCarousel').carousel({
  			interval: 3500
	});
});
</script>
