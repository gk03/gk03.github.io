<script type="text/javascript">

(function ($) {
    $(function () {

		// foresee survey code
    	var hostname = location.hostname;
    	if ( hostname == "www.oclc.org"  ||
    		 hostname == "wwwtest.ent.oclc.org" ||
    		 hostname == "wwwtest2.ent.oclc.org" || 
    		 hostname == "localhost" ) {
			if ( document.cookie.indexOf("OCLC_IP_ID") == -1 ) {
				var urlString = '/apps/oclc/foresee';
		    	var parms = "hostname=" + hostname;
		         $.ajax( {
		             type: "GET",
		             url: urlString + "?" + parms, 
		             dataType: "json",
		             cache: false,
		             success : function displayMsg(data) {
		            	document.cookie = "OCLC_IP_ID="+data.internalIP+"; expires=0; path=/";
		             }
		        });
			}
		
			var head = document.getElementsByTagName('head')[0];
			var script = document.createElement('script');
			script.type = 'text/javascript';
			if ( hostname == "www.oclc.org" ) {
				script.src = "/etc/oclc/js/foresee-prod.js";
			} else {
				script.src = "/etc/oclc/js/foresee-test.js"
			}
			head.appendChild(script);
    	}
    	
    	if ( hostname == "www.oclc.org" ) {	
	    	var head = document.getElementsByTagName('head')[0];
			var script = document.createElement('script');
			script.type = 'text/javascript';
			script.src = "/etc/oclc/js/siteimprove.js";
			head.appendChild(script);
    	}
       
    });
})(jQuery);

