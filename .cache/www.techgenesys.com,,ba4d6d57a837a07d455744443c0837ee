function showDate() {//display current date
	var currDate = new Date();
	var currMonth = currDate.getMonth();
	var currMonthName;
	var currDay = currDate.getDate();
	var currYear = currDate.getFullYear();
	switch (currMonth){
	case 0: currMonthName="January";
	break;
	case 1:	currMonthName="February";
	break;
	case 2:	currMonthName="March";
	break;
	case 3: currMonthName="April";
	break;
	case 4:	currMonthName="May";
	break;
	case 5:	currMonthName="June";
	break;	
	case 6: currMonthName="July";
	break;
	case 7: currMonthName="August";
	break;
	case 8: currMonthName="September";
	break;
	case 9: currMonthName="October";
	break;
	case 10: currMonthName="November";
	break;
	case 11: currMonthName="December";
	}
dateString = currMonthName + " " + currDay + ", " + currYear;
  }	
