<script type="text/javascript">
window.onresize=doSize;
doSize();

function doSize() {
	var ob=document.getElementById('widercss');
	if (getWidth()>1200) {
		if (ob) ob.disabled=false;
		else {
			var css=document.createElement('link');
			css.id='widercss';
			css.rel='stylesheet'
			css.type='text/css';
			css.href='savantechwider.css';
			getFirstTag('head').appendChild(css);
		}
	} else if (ob) ob.disabled=true;
}
function getWidth() {
	if (window.innerWidth) return window.innerWidth;
	if ((typeof document.documentElement!='undefined') && (typeof document.documentElement.clientWidth!='undefined') && (document.documentElement.clientWidth!=0)) return document.documentElement.clientWidth;
	return getFirstTag('body').clientWidth;
}
function getFirstTag(t) {return document.getElementsByTagName(t)[0];}
</script>
