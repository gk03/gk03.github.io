	//On mouse over those thumbnail
	jQuery('.thumb_overlay').hover(function() {
		
		//Display the caption
		jQuery(this).find('div.thumb_caption').stop(false,true).fadeIn(500);
	},
	function() {
		//Reset the image
		jQuery(this).find('img').stop(false,true).animate({'width':jQuery('.thumb_overlay').width(), 'height':jQuery('.thumb_overlay').height(), 'top':'0', 'left':'0'}, {duration:100});	

		//Hide the caption
		jQuery(this).find('div.thumb_caption').stop(false,true).fadeOut(500);
	});

});


