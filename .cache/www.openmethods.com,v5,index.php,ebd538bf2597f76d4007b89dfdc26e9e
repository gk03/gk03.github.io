<script>
    $(document).ready(function() {

        $('#hdrContainer').scrollToFixed();
        $('#sectionBarFtr').scrollToFixed( {
            bottom: 0,
            limit: $('#sectionBarFtr').offset().top
        });

        var summaries = $('.summary');
        summaries.each(function(i) {
            var summary = $(summaries[i]);
            var next = summaries[i + 1];

            summary.scrollToFixed({
                marginTop: $('#hdrContainer').outerHeight(true) + 10,
                limit: function() {
                    var limit = 0;
                    if (next) {
                        limit = $(next).offset().top - $(this).outerHeight(true) - 10;
                    } else {
                        limit = $('#sectionBarFtr').offset().top - $(this).outerHeight(true) - 10;
                    }
                    return limit;
                },
                zIndex: 999
            });
        });
    });
</script>
