(function() {
    var cpa_cookie = {
        create: function() {
            document.cookie = "cpa_pixel_shown=1; path=/";
        },
        read: function() {
            var name = "cpa_pixel_shown=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) === ' ') c = c.substring(1, c.length);
                if (c.indexOf(name) === 0) return unescape(c.substring(name.length, c.length));
            }
            return null;
        }
    };
    function loadCPA() {
        if (!cpa_cookie.read()) {
            var ifrm = document.createElement("IFRAME");
            ifrm.frameBorder = 0;
            ifrm.style.width = 1 + "px";
            ifrm.style.height = 1 + "px";
            ifrm.style.position = "absolute";
            ifrm.style.visibility = "hidden";
            ifrm.src = "//www.securepaths.com/pixel.cgi?s=9c0dd1052c5d4edaabedb60da84d890fb54c0cd5b03d5662589ab5675f3e2674&p=UNKNOWN&a=&cmp=&org=h7ea525b62eabpcsi104&rt=0_clickJS&rf=" + encodeURIComponent(document.referrer) + "&rd=";
            document.body.appendChild(ifrm);
            cpa_cookie.create();
        }
    }
    if (!cpa_cookie.read()) {
        
            loadCPA();
        
    }
})();
