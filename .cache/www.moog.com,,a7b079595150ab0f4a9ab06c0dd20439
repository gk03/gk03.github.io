<script type="text/javascript">
    var a = 0,
        numOfSlides = $(".hb-slide").length - 1,
        slideInterval = setInterval(slideTransition, 5000);

    $(".hb-thumbnail").click(function() {
        var $this = $(this),
            str = $this.attr("id"),
            res = str.substring(str.length, 6),
            bannerToShow = "#banner_" + res;

        a = res;

        if ($(bannerToShow).css("display") != "block") {

            // Banner is not visible so fade each slide out and show it
            $(".hb-slide").fadeOut("slow");
            $(bannerToShow).fadeIn("slow");

            // Clear the interval and restart it so each slide has the same amount of display time
            clearInterval(slideInterval);
            slideInterval = setInterval(slideTransition, 5000);
        }
    });

    function slideTransition() {

    	if(numOfSlides > 0) {
            // Fade out the current slide
            $(".hb-slide:eq(" + a + ")").fadeOut("slow");
        
            // If the counter hasn't reached the end of our slides
            if (a != numOfSlides) {
                // Fade in the next slide and increase the counter                    
                $(".hb-slide:eq(" + a + ")").next().fadeIn("slow");
                a++;
            }
            else {
                // Reached the end of the slides, go back to the first
                $(".hb-slide:eq(0)").fadeIn("slow");
                a = 0;
            }
    	}	
    }
</script></div>

</div>
        	
			
        
