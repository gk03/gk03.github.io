<script lang="javascript">
    $("#filterSQL").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(showsqlfeatures());
    });

    function resetbuttons() {
        $("#nofilter").attr('src', '/images/icons/all-filter-button.png')
        $("#filterSQL").attr('src', '/images/icons/sql-filter-button.png')
        $("#filterWIN").attr('src', '/images/icons/win-filter-button.png')
        $("#filterSP").attr('src', '/images/icons/sharepoint-filter-button.png')
        $("#filterO").attr('src', '/images/icons/oracle-filter-button.png')
    }

    function shownewfeatures() {
        $("div")
            .filter(".feature").slideUp();

        resetbuttons();
        $("#filterNEW").attr('src', '/images/icons/new-filter-button-on.png')

        $("div")
            .filter(".filterNEW").slideDown();

        $("div")
            .filter(".featurelist").slideDown();
    }

    $("#filterNEW").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(shownewfeatures());
    });

    function showsqlfeatures() {
        $("div")
            .filter(".feature").slideUp();

        resetbuttons();
        $("#filterSQL").attr('src', '/images/icons/sql-filter-button-on.png')

        $("div")
            .filter(".filterSQL").slideDown();

        $("div")
            .filter(".featurelist").slideDown();
    }

    $("#filterWIN").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(showwinfeatures());
    });

    function showwinfeatures() {
        $("div")
            .filter(".feature").slideUp();

        resetbuttons();
        $("#filterWIN").attr('src', '/images/icons/win-filter-button-on.png')

        $("div")
            .filter(".filterWIN").slideDown();

        $("div")
            .filter(".featurelist").slideDown();
    }

    $("#filterSP").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(showspfeatures());
    });

    function showspfeatures() {
        $("div")
            .filter(".feature").slideUp();

        resetbuttons();
        $("#filterSP").attr('src', '/images/icons/sharepoint-filter-button-on.png')

        $("div")
            .filter(".filterSP").slideDown();

        $("div")
            .filter(".featurelist").slideDown();
    }

    $("#filterO").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(showoraclefeatures());
    });

    function showoraclefeatures() {
        $("div")
            .filter(".feature").slideUp();

        resetbuttons();
        $("#filterO").attr('src', '/images/icons/oracle-filter-button-on.png')

        $("div")
            .filter(".filterO").slideDown();

        $("div")
            .filter(".featurelist").slideDown();
    }

    $("#nofilter").click(function () {
        $.when($("div")
            .filter(".featurelist").slideUp(500)).then(showallfeatures());
    });

    function showallfeatures() {
        $("div")
            .filter(".feature").slideDown();

        resetbuttons();
        $("#nofilter").attr('src', '/images/icons/all-filter-button-on.png')

        $("div")
            .filter(".featurelist").slideDown(500);
    }
</script>

