					$('#header, #header-v2 #navigation, #header-v3, #header-v4 #navigation, #header-v5 #navigation, #header-v6').waypoint('sticky');
				}			
	    
	    			$('html').css({'background-color' : '#222222'});
			    

});
	
</script>
	
