<script language="javascript" type="text/javascript">
        var sp1 = new Spry.Widget.SlidingPanels("valueImage", { enableAnimation: false });

        // function to move port automatically
        var timer;
        var fadeIn;
        var fadeOut;
        var box = 1;
        var auto = function() {
            /*if(box > 1) {
            fadeOut = new Spry.Effect.Fade('panel'+(box-1), {duration:500, from: 100, to: 0, toggle: false});
            fadeOut.start();
            }
            */
            fadeIn = new Spry.Effect.Fade('panel' + box, { setup: function() { sp1.showPanel('panel' + box); }, duration: 2000, from: 0, to: 100, toggle: false });
            active(box);
            //sp1.showPanel('panel'+box);
            //fadeOut.start()
            fadeIn.start();
            box = box + 1;
            // start a timer to make it loop through the images
            timer = setTimeout('auto(box)', '6000');
            // revert back to image one
            if (box == 5)
                box = 1;
        }
        auto(box);
        var endTimer = function() {
            clearTimeout(timer);
        }
    </script>
