function lazyLoadImage(myImg)
{
    if($(myImg).is(":visible"))
    {
        var currentSrc = $(myImg).attr("src");
        if(currentSrc === null || currentSrc === undefined || currentSrc === '')
        {
            var newSrc = $(myImg).attr("data-original");
            if(newSrc !== null && newSrc !== undefined && newSrc !== '')
            {
                $(myImg).attr("src", newSrc);
                $(myImg).removeAttr('data-original');
            }
        }
    }
}
function applyInforMask()
{
    var maskDiv = $("<div/>",{"id":"inforMask"});
    $("body").append(maskDiv);
    $("#inforMask").show();
}

function removeInforMask()
{
    $("#inforMask").hide();
    $("#inforMask").remove();
}</script> <script>
    function sendWebTrendsClick(element) {
    
        var sectionId = "";
        var text = "";
        var tiValue = "";
        var curURL = window.location.href;
        var tagName = $(element).prop("tagName").toLowerCase();
        sectionId = $(element).parents("section").attr("id");
        
        if(sectionId === null || sectionId === undefined)
             sectionId = $(element).parents("header").attr("id");
         
        if(sectionId === null || sectionId === undefined)
             sectionId = $(element).parents("footer").attr("class");
         
        text = $(element).text();
        tiValue = text;
        
        if (sectionId === "hp_banner_slide_group")
        {         
            if (tagName === "span") {
                tiValue = "toggle next";     
                if (text === "Prev") {
                    tiValue = "toggle prev";
                }
            }
        }
        else if(sectionId === "global-header"){
   
            if(element.attr("title") === "Infor"){
                tiValue = "Infor Logo"
            }
        }
               
       // alert(" nvValue = " + sectionId + " tiValue = " + tiValue + " url = " + curURL);
       try
       {
        dcsMultiTrack('DCS.dcsui', curURL, 'WT.ti', tiValue, 'WT.nv', sectionId);
    } catch(exception)
    {
        // exception
    }
    }

    $(".click-track").click(function () {
        sendWebTrendsClick($(this));
    });
    
</script> <script>
