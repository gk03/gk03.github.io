<script type="text/javascript">
function lCss(url, media) {
    var d = document;
    var l = d.createElement('link');
    l.rel = 'stylesheet';
    l.type = 'text/css';
    l.href = url;
    l.media = media;
    d.getElementsByTagName('head')[0].appendChild(l);
}

function deferredCSS() {
    lCss('http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css', 'all');
    lCss('http://www.icsfl.com/wp-content/themes/ics/assets/css/normalize.css', 'all');
    lCss('http://www.icsfl.com/wp-content/themes/ics/assets/css/animations.min.css', 'all');
	 lCss('http://fonts.googleapis.com/css?family=Droid+Sans:400,700', 'all');
	  lCss('http://fonts.googleapis.com/css?family=Roboto:400italic,300,700,400', 'all');
	  lCss('http://fonts.googleapis.com/css?family=Oswald:400,300,700', 'all');
}

if (window.addEventListener) {
    window.addEventListener('DOMContentLoaded', deferredCSS, false);
} else {
    window.onload = deferredCSS;
}
