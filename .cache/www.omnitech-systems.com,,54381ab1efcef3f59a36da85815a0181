<script>
var PageHeader = new function() {

    this.setup = function() {

        this.timeout = 8000;

        this.idx = 0;
        this.num = $('ot_slider').getChildren('div').length;

        $$('.bullet').each(function(item, index) {
            $(item).addEvent('click', this.bulletClick.bind(this));
        }.bind(this));


        this.fx = new Fx.Scroll($('ot_container'), {
            duration: 'short'
        });
        this.fx.set(0, 0);

        this.tid = window.setTimeout(this.next.bind(this), this.timeout);
    }

    this.next = function() {

        var index = 0;
        if(this.idx + 1 < this.num)
            index = this.idx + 1;

        this.changePage(index);
    }

    this.bulletClick = function(event) {

        if(undefined != this.tid) {
            window.clearTimeout(this.tid);
            delete this.tid;
        }

        var index = parseInt($(event.target).get('data-page'));

        this.changePage(index);
        return false;
    }

    this.changePage = function(index) {

        $('header_page_' + this.idx).removeClass('active');
        $('bullet_' + this.idx).removeClass('active');    

        this.idx = index;

        $('header_page_' + this.idx).addClass('active');
        $('bullet_' + this.idx).addClass('active');

        this.fx.start(this.idx * 650, 0);

        this.tid = window.setTimeout(this.next.bind(this), this.timeout);
    }

    window.addEvent('load', function() {
        PageHeader.setup();
    });

};
</script>
