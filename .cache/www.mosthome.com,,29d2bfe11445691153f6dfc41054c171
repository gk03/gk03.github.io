$(function(){
  $('#slideshow').cycle({ 
        fx:     'blindY', 
        speed:   1400, 
        timeout: 9000, 
        pager:  '#nav',
        pagerEvent: 'mouseover', 
        pauseOnPagerHover: true,
        before:  onBefore, 
        after:   onAfter 
    }); 

  function onBefore() { 
    $('#textWrapper').fadeOut("fast"); 
  } 

  function onAfter() { 
    $('#textWrapper').fadeIn("slow");
    $('#textWrapper h3').html(this.title);
    $('#textWrapper p').html(this.alt);
    $('#textWrapper a').attr("href", this.name);
  }
});
</script>

