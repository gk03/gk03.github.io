<script>
jQuery(function() {
	jQuery('.fixed').makeFixed({
		
		
		onFixed: function (el)
		{
			jQuery(el).css("top", (jQuery("#wpadminbar").length > 0)? "-50px" : "-80px");
			jQuery(el).css("height", "60px");
			jQuery(el).css("padding", "10px");
			jQuery(el).css("left", "0px");
			jQuery(el).css("width", jQuery("html,body").width() + "px");
			jQuery(el).css("border-bottom", "3px solid #108B9A");
			jQuery(el).css("z-index", "9999");
			//jQuery("#menu, #menuDIV").css("position", "relative");
			jQuery("#menu, #menuDIV").css("z-index", "9999");
			jQuery("#menuDIV").css("width", "960px");
			jQuery("#menuDIV").css("margin", "0px auto");
			jQuery("#menuDIV #contentlogo").css("margin-top", "-5px");
			jQuery("#menuDIV #contentlogo").css("height", "50px");
			jQuery("#menu .sub-menu").css("top", "31px");
			//jQuery(el).css("background-image", "url(http://riverpoint.com/wp-content/themes/riverpoint/images/dropdown_menu_background.png?t=33)");
			jQuery(el).css("background", "#FFFFFF");
			//jQuery("#menu ul").css("margin", "0px auto");
			jQuery("#menu ul").css("margin-top", "7px");
			//jQuery("#menuDIV").css("border", "1px solid red");			
					
		},
		onUnFixed: function (el)
		{
			jQuery(el).css("border-bottom", "none");
			jQuery("#menuDIV #contentlogo").css("height", "71px");
			jQuery("#menu .sub-menu").css("top", "auto");
			jQuery("#menuDIV #contentlogo").css("margin-top", "-45px")
			jQuery(el).css("left", "-25px");
			jQuery("#menu ul").css("margin-top", "0px");
			//jQuery("#menu .sub-menu").css("margin-top", "22px");
			jQuery(el).css("height", "0px");
			jQuery(el).css("top", "auto");
			//jQuery(el).css("position", "relative");
			//jQuery(el).css("width", "100%");
			jQuery(el).css("background-image", "none");
			jQuery("#menuDIV").css("width", "950px");
			jQuery("#menuDIV").css("margin", "0px auto");
			jQuery(el).css("padding", "0px");
			//jQuery("#menuDIV").css("border", "1px solid red");
		}
	});
});
</script>
