				var jQuery = jQuery.noConflict();
var cw = "300px";

jQuery(window).load(function(){

    jQuery("#wp_bullhorn_saved_check_link").bind("click", showHideSimilarJobsByEmail);

    jQuery("#ViewAllJobsButton").click(function() {
    	getJobs(1);
    	return false;
    });
    
    jQuery("#wp_bullhorn_search_title_small").click(function() {
    	if(jQuery("#searchsection").is(":visible")) {
    		//rollupSearch(1);
    		jQuery(".icon-arrowcaret-down").css("display", "none");
    		jQuery(".icon-arrowcaret-up").css("display", "inline-block");
    	}
    	else {
    		//rollupSearch(0);
    		jQuery(".icon-arrowcaret-down").css("display", "inline-block");
    		jQuery(".icon-arrowcaret-up").css("display", "none");
    	}
    	
    	return false;
    });
    
    jQuery("#SearchNow").click(function() {
    	getJobs(0);
    });

		jQuery("#MessagePopup .close").click(hidePopup);
    
    jQuery("#SaveAndSearch, #EmailMeJobsButtonSpan").click(function() {
    	showLoading();
    	var jobname = jQuery("#job_name").val();
    	//var jobemail = jQuery("#job_email").val();
    	var jobemail = jQuery("#EnterEmail").val();
    	
    	/*
    	if(jQuery.trim(jobname) == "") {
    		showPopup("Please provide a name for your Job Alert.");
    		return;
    	}
    	*/
    	
    	if(jQuery.trim(jobemail) == "" || !isEmail(jobemail)) {
    		showPopup("Please provide a valid email address for sending Job Alerts.");
    		return;
    	}

    	if(jQuery.trim(jQuery("#JobListings").html()) == "") {
    		showPopup("Please perform a search before trying to create a Job Alert.");
    		return;
    	}
    	
			postParams = { saveemailjobalerts: 1, job_email: jobemail, job_name: jobname, job_term: jQuery("#job_term").val(), job_title: jQuery("#job_title").val(), job_cat: jQuery("#job_cat").val(), job_location: jQuery("#job_location").val(), job_no: jQuery("#job_no").val()};
			
		  ajaxHolder = jQuery.ajax({
		      url: document.location.href,
		      data: postParams,
		      dataType: "html",
		      type: "POST",
		      success: function (data) {
		      	if(data == "success") {
		      		showPopup("Job Alert has been saved and will send email alerts to: " + jobemail);
		      	}
		      	else {
		      		showPopup("There was a problem saving your job alert, please refresh your web page and try again.");
		      	}
		      	hideLoading();
		      },
		      error: function (JSONobj, textStatus, errorMSG) {
		          showPopup("Error: " + textStatus + " - " + JSONobj.responseText + " - " + errorMSG);
		          hideLoading();
		          return false;
		      }
		  });
    });
    
    jQuery("#SendButton").click(function() {
    	postParams = { sendmessage: 1, ContactEmail: jQuery("#ContactEmail").val(), ContactSubject: jQuery("#ContactSubject").val(), ContactMessage: jQuery("#ContactMessage").val()};
		  ajaxHolder = jQuery.ajax({
		      url: document.location.href,
		      data: postParams,
		      dataType: "html",
		      type: "POST",
		      success: function (data) {
		      	if(data == "success") {
		      		showPopup("Message sent.");
		      	}
		      	else {
		      		showPopup("There was a problem sending your message, please refresh your web page and try again.");
		      	}
		      	hideLoading();
		      },
		      error: function (JSONobj, textStatus, errorMSG) {
		          showPopup("Error: " + textStatus + " - " + JSONobj.responseText + " - " + errorMSG);
		          hideLoading();
		          return false;
		      }
		  });
    	
    });

	  jQuery("#OtherJobSearchResultsLeft").click(function() {
