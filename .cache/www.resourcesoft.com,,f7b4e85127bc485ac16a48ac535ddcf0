                        $(window).load(function() {
                            var ticker = function()
                            {
                                setTimeout(function(){
                                    $('#ticker li:first').animate( {marginTop: '-2em'}, 1000, function()
                                    {
                                        $(this).detach().appendTo('ul#ticker').removeAttr('style');
                                    });
                                    ticker();
                                }, 8000);
                            };
                            ticker();
                        });
