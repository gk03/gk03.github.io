                                function submitsearchForm(event) {
                                    event.preventDefault();
                                    var keywd = document.forms["searchSubmit"].elements["keyword"].value;  
                                    window.location = "/search/public/?keyword=" + keywd;  
                                    return false;
                                }

                                function searchKeyword(){
                                    if(document.forms["searchSubmit"].getAttribute("class").indexOf("is-active")!=-1){
                                        if(document.forms["searchSubmit"].elements["keyword"].value!=""){
                                            window.location = "/search/public/?keyword=" + document.forms["searchSubmit"].elements["keyword"].value;
                                            return false;
                                        }
                                    }
                                }
