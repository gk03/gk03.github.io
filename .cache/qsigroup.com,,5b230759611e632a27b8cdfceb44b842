	    <script>
		    var container = document.getElementById('slider_container');
		    var loading = document.getElementById('loading');
		    var slides = document.getElementById('slides');

		    var w = parseInt(container.style.width);
		    var h = parseInt(container.style.height);

		    var v_w  = document.getElementsByTagName('html')[0].offsetWidth;
			if(v_w < 1170) v_w = 1170;
		    var r = v_w / w ;

		    container.style.width = v_w + 'px';
		    container.style.height = (h*r) + 'px';

		    loading.style.width = v_w + 'px';
		    loading.style.height = (h*r) + 'px';

		    slides.style.width = v_w + 'px';
		    slides.style.height = (h*r) + 'px';


