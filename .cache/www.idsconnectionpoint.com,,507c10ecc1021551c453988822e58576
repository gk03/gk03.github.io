    <script type="text/javascript">
    $(function() {
      var endDate = "October 12, 2016 20:00:00";

      $('.countdown').countdown({
        date: endDate,
        render: function(data) {
          $(this.el).html('<div class="timeBlock"><span class="timeData days">' + this.leadingZeros(data.days, 3) + '<\/span><span class="timeText">Days<\/span><\/div><div class="timeBlock"><span class="timeData hours">' + this.leadingZeros(data.hours,2) + ' <\/span><span class="timeText">Hours<\/span><\/div><div class="timeBlock"><span class="timeData minutes">' + this.leadingZeros(data.min, 2) + '<\/span><span class="timeText">Minutes<\/span><\/div><div class="timeBlock"><span class="timeData seconds">' + this.leadingZeros(data.sec, 2) + ' <\/span><span class="timeText">Seconds<\/span><\/div>');
       }
      });
    });
    </script>

