            jQuery(function($){
                var maxHeight = -1;
                $('.single_home_repeater .home_five_repeater_content p').each(function() {
                    maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
                });     
                $('.single_home_repeater .home_five_repeater_content p').each(function() {
                    $(this).height(maxHeight);
                });

		var maxHeight = -1;
                $('.single_home_repeater .home_five_repeater_header h3').each(function() {
                    maxHeight = maxHeight > $(this).height() ? maxHeight : $(this).height();
                });     
