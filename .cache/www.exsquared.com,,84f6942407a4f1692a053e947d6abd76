    <script>
    $(document).ready(function() { 
      // Set up featured slideshow
      var sOptions = {
        autoPlay: true,
        nextButton: false,
        prevButton: false,
        pagination: true,
        preloader: true,
        animateStartingFrameIn: true,
        fallback: {
          theme: "slide",
          speed: 500
        }
      }
      var sequence = $("#sequence").sequence(sOptions).data("sequence");
      
      $('.sequence-pagination li div').click(function(){
        sequence.goTo($(this).attr('data-frame'));
      });
	});
