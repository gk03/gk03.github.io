    <script>
    $(document).ready(function() { 
      // Set up featured slideshow
      $('.slide-img').css('opacity','0');
      var length = $('.slideshow-slides > li').length;
      for (var i = 0; i < length; i++) {
        $('.pagers ul').append('<li><div>' + i + '</div></li>');
      }

      $('.slideshow-wrapper').flexslider({
        animation:  "slide",
        start: function(slider){
          $('.slide-img[rel='+slider.currentSlide+']').animate({opacity: 1});
        },
        before: function(slider) {
          $('.slide-img[rel='+slider.currentSlide+']').animate({opacity: 0});
          $('.slide-img[rel='+slider.animatingTo+']').animate({opacity: 1});
        },
        useCSS:  false,
        manualControls: ".pagers div"
      });    
    
      $('.pagers').detach().insertAfter('.flexslider .flex-viewport');  
      
      // set up testimonials slider
      var length2 = $('.home-testimonials > li').length;
      for (var i = 0; i < length2; i++) {
        $('.testimony-pagers ul').append('<li><div>' + i + '</div></li>');
      }
      
      $('.homepage-testimonial-slider').flexslider({
        animation:  "slide",
        directionNav: false,
        useCSS:  false,
        manualControls: ".homepage-testimonial-slider .testimony-pagers div",
        slideshow: false
      });
      
      $('.homepage-testimonial-slider .testimony-pagers').detach().insertAfter('.homepage-testimonial-slider .flex-viewport');

    });  
    </script>

