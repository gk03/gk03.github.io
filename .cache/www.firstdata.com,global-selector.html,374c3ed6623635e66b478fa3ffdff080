<script>
        $(window).load(function() {
            try {
                if($("li[data-iso='"+window.geoLocation.iso_code+"']").length > 0 && window.location.hash !== "#change") {
                    redirectPath = document.URL.substr(0, document.URL.lastIndexOf('/')) + "/en_" + window.geoLocation.iso_code.toLowerCase() + "/home.html";
                    window.location.href = redirectPath;
                }
            } catch (err) {};
            $("body").show();
        });
    </script>
