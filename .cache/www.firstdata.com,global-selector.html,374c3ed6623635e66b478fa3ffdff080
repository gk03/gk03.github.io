    <script src="//js.maxmind.com/js/apis/geoip2/v2.1/geoip2.js" charset="ISO-8859-1" type="text/javascript"></script>
    <script>
        window.geoLocation = {}
        //Language redirect
        function getCookie(name) {
            var dc = document.cookie;
            var prefix = name + "=";
            var begin = dc.indexOf("; " + prefix);
            if (begin == -1) {
                begin = dc.indexOf(prefix);
                if (begin != 0) return null;
            } else {
                begin += 2;
            }
            var end = document.cookie.indexOf(";", begin);
            if (end == -1) {
                end = dc.length;
            }
            return unescape(dc.substring(begin + prefix.length, end));
        }
        function geoipRedirect(location) {
            try {
                window.geoLocation.iso_code = location.country.iso_code;
            } catch (err) {}
        }
        if (typeof CQ === 'undefined' || CQ.WCM.isPreviewMode(true)) {
            geoip2.country(geoipRedirect);
            var redirectPath = "";
            var source = document.referrer;
            var cookieVal = getCookie("Lang");
            var re = new RegExp("^[a-zA-Z]{2}_[a-zA-Z]{2}$");
            if (re.test(cookieVal)) {
                redirectPath = document.URL.substr(0, document.URL.lastIndexOf('/')) + "/" + cookieVal + "/home.html";
                window.location.href = redirectPath;
            }
        }
    </script>

