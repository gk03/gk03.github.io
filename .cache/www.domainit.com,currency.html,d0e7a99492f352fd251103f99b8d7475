<script language="javascript">
function truncateString(myString, truncateLength) {
	if(! myString && ! truncateLength) { return; }
        var newstring = myString.substring(0,truncateLength) + '...' + myString.substring((myString.length), myString.length);
        document.write((newstring.length > myString.length) ? myString : newstring);
}
function truncateString2(myString, truncateLength) {
	if(! myString && ! truncateLength) { return; }
        var newstring = new String();
        if (myString.length > truncateLength + 4) {
                newstring = myString.substring(0,truncateLength) + '..' + myString.substring((myString.length-4), myString.length);
        } else {
		document.write(myString);
	}
        document.write((newstring.length > myString.length) ? myString : newstring);
}

</script>

