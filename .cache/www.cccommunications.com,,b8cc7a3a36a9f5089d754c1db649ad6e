<script type="text/javascript">
    jQuery().ready(function ($) {

        function HoverOvere824f01e38() {

            $(this).addClass('rmhover').find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOute824f01e38() {
            $(this).removeClass('rmhover').find(".subMenu:first").animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        function HoverOversube824f01e38() {
            $(this).find(".subMenu:first").stop().animate({ height: 'show' }, 450);
        }

        function HoverOutsube824f01e38() {
            $(this).find(".subMenu:first").stop().animate({ height: 'hide' }, 450, function () {
                $(this).hide();
            });
        }

        var confige824f01e38 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOvere824f01e38,
            timeout: 1000,
            out: HoverOute824f01e38
        };
        var configsube824f01e38 = {
            sensitivity: 1,
            interval: 100,
            over: HoverOversube824f01e38,
            timeout: 1000,
            out: HoverOutsube824f01e38
        };
        $("#gomenue824f01e38 li.dir").hoverIntent(confige824f01e38);
        $("#gomenue824f01e38 .subMenu li.dir").hoverIntent(configsube824f01e38);
        $("#gomenue824f01e38").find("li.current").each(function (i) {
            $(this).find(".subMenu:first").show();
        });
    });
</script>
 
 


