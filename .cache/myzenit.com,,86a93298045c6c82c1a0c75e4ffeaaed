<script>
jQuery(function() {
    var overlayBG = ZI_IMG_URL+'light-black.png';
    var position_top = '37%';
    var homeServiceListingBlock = function(position_top) {
        jQuery('.horizontal-block-wrap .portfolio-item').hoverIntent(function() {
          var $self = jQuery(this);
          var $self_link  = $self.find('.portfolio-mask .portfolio-view a');
          
          $self.find('.portfolio-mask').css({ "background" : 'url('+overlayBG+')', "background-repeat": "repeat" });
          $self_link.animate({
              'top'   : position_top,
          }, 'speed');
        },function() {
          var $self = jQuery(this);
          var $self_link  = $self.find('.portfolio-mask .portfolio-view a');
          $self.find('.portfolio-mask').css({ "background" : "none" });
          $self_link.animate({
              'top'   : '-'+position_top,
          }, 'speed');
        });

    }

    jQuery( window ).resize(function() {
        var zi_windowsize = jQuery(window).width();
        if(zi_windowsize >= 767 && zi_windowsize < 980) {
           position_top = '47%';
           console.log('TESET');
        } else {
           position_top = '37%';
        }
        homeServiceListingBlock(position_top);
    });
     var zi_windowsize = jQuery(window).width();
    if(zi_windowsize >= 767 && zi_windowsize < 980) {
       position_top = '47%';
    } else {
       position_top = '37%';
    }
    homeServiceListingBlock(position_top);
    
 
    
 });
</script>

