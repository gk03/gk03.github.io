<script type="text/javascript">

function checkEmail(str) {
	var at="@"
	var dot="."
	var lat=str.indexOf(at)
	var lstr=str.length
	var ldot=str.indexOf(dot)
	
	if (str.indexOf(at)==-1){
	   return false
	}
	if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
	   return false
	}
	if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
	   return false
	}
	if (str.indexOf(at,(lat+1))!=-1){
	   return false
	}
	if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
	   return false
	}
	if (str.indexOf(dot,(lat+2))==-1){
	   return false
	}
	if (str.indexOf(" ")!=-1){
	   return false
	}
	return true					
}

function validateData() {
 if (document.getElementById('ETEmail').value == 'Email Address' || document.getElementById('ETEmail').value == '') {
  alert('Please enter a valid email address');
  return false;
 }
 if (!checkEmail(document.getElementById('ETEmail').value)) {
  alert('Please enter a valid email address');
  return false;
 }
 return true;
}

function openPrettyWindow() {

	var URL = 'http://www.didax.com/exacttarget/signup.cfm?iframe=true&width=500&height=500'

// Save the email address first
	saveEmail();

// Then open the form in PrettyPhoto
	jQuery.prettyPhoto.open(URL);
}

function closePrettyPhoto() {
 
    jQuery.prettyPhoto.close();

}

var xmlHttp;

function saveEmail() {

	xmlHttp=GetXmlHttpObject()

	if (xmlHttp==null) {
//		alert ("Your browser does not support AJAX!");
		return;
	}

	var url="http://www.didax.com/exacttarget/SaveEmail.cfm";
	url=url+"?ETEmail=" + document.getElementById('ETEmail').value;

	xmlHttp.onreadystatechange=stateChangedSaveEmail;
	xmlHttp.open("GET",url,true);
	xmlHttp.send(null);
}

function stateChangedSaveEmail() {

	if (xmlHttp.readyState==4) {
		    // Do nothing
  			
			// Remove comments from next two lines to popup response from SaveEmail page
			// var strResponse = xmlHttp.responseText;
  			// alert(strResponse);

	}
}

function GetXmlHttpObject() {
var xmlHttp=null;

	try {
		// Firefox, Opera 8.0+, Safari
		xmlHttp=new XMLHttpRequest();
	}
	catch (e) {
		// Internet Explorer
		try {
			xmlHttp=new ActiveXObject("Msxml2.XMLHTTP");
		}
		catch (e) {
			xmlHttp=new ActiveXObject("Microsoft.XMLHTTP");
		}
	}
	return xmlHttp;
}

</script>


<script type="text/javascript" language="javascript">

var funcTimer;  // Variable to clear the windows timer
 
function startPopupTimer(secs) {
 	
	// Start the window timer
	funcTimer = window.setInterval('openPopup()', secs*1000);

}
 
function openPopup() {
 
	// Clear the window timer first
	window.clearInterval(funcTimer);
 
	// Call function to open the pretty window here
	openPrtyWindow();
 
}

function openPrtyWindow() {
 
	var URL = 'http://www.didax.com/exacttarget/signup2.cfm?iframe=true&width=500&height=300'

	// Then open the form in PrettyPhoto
	jQuery.prettyPhoto.open(URL);
}

</script>



