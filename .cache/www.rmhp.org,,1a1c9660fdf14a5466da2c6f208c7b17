    <script type="text/javascript">
        

        var SLIDER = new HeroSlider();

        /* STARTING SLIDE */
        var CURRENT_SLIDE = 3;

        /* SIZE DEFINITIONS */
        var RMHP_HERO_LARGE_WIDTH = 1200;

        /* ANIMATION TIMING */
        var RMHP_IS_ANIMATING = true;
        var RMHP_CYCLE_TIMEOUT = -1;
        var RMHP_PAUSE_TIMEOUT = -1;
        var RMHP_CYCLE_DELAY = 6 * 1000;
        var RMHP_SLIDER_SPEED = 2 * 1000;
        var RMHP_PAUSE_DELAY = 10 * 1000;


        $(document).ready(function () {
            /**
            * Adds all of the assets for the HeroSlider for each instance 1-4
            */
            SLIDER.addSlide(new HeroSlide(
                /* thumb class (tUrl) */
                "hero-text-4",
                /* thumb rel (tRel) */
                "hero-4",
                /* giant hero image (hUrl) */
                "/images/homepage---rotating-hero-banners/img-home-hero-4CF0BD72D0D48.jpg?Status=Temp&sfvrsn=10",
                /* side text/banner image (bUrl) */
                " ",
                /* learn more URL that executes on click (lUrl) */
                "#"
            ));
            SLIDER.addSlide(new HeroSlide(
                "hero-text-3",
                "hero-3",
                "/images/homepage---rotating-hero-banners/heroimage-mountainclimber.jpg?Status=Temp&sfvrsn=6",
                " ",
                "#"
            ));
            SLIDER.addSlide(new HeroSlide(
                "hero-text-2",
                "hero-2",
                "/images/default-source/homepage---rotating-hero-banners/online_premium2.jpg?Status=Temp&sfvrsn=2",
                " ",
                "#"
            ));
            SLIDER.addSlide(new HeroSlide(
                "hero-text-1",
                "hero-1",
                "/images/default-source/homepage---rotating-hero-banners/ask-dr.jpg?Status=Temp&sfvrsn=4",
                " ",
                "#"
            ));
            
            /* CLICK HANDLERS TO ACTIVATE ANIMATION */
            $(".home-hero-slider-ul li a").click(RMHP_sliderDot_onClick);
            $('.home-hero-holder').click(RMHP_slide_onClick);
            $('#home-arrow-left').click(RMHP_nextSlide_onClick);
            $('#home-arrow-right').click(RMHP_prevSlide_onClick);

            setTimeout(RMHP_cycle, RMHP_CYCLE_DELAY);


        });

        function RMHP_slide_onClick(evt) {
            var slide = SLIDER.getItemAt(CURRENT_SLIDE);
            document.location = slide.learnMoreUrl;
        }

        function RMHP_sliderDot_onClick(evt) {
            var ele = evt.currentTarget;
            var slideTarget = $(ele).attr('rel');
            if (slideTarget) {
                var slideNumber = parseInt(slideTarget.substring(5), 10);
                CURRENT_SLIDE = SLIDER.length() - slideNumber;
                RMHP_changeCurrentSlide();
                RMHP_pauseTimer();
            }
        }

        function RMHP_nextSlide_onClick(evt) {
            CURRENT_SLIDE++;
            if (CURRENT_SLIDE >= SLIDER.length()) {
                CURRENT_SLIDE = 0;
            }
            RMHP_changeCurrentSlide();
            RMHP_pauseTimer();
        }

        function RMHP_prevSlide_onClick(evt) {
            CURRENT_SLIDE--;
            if (CURRENT_SLIDE < 0) {
                CURRENT_SLIDE = SLIDER.length() - 1;
            }
            RMHP_changeCurrentSlide();
            RMHP_pauseTimer();
        }

        function RMHP_pauseTimer() {
            clearTimeout(RMHP_CYCLE_TIMEOUT);
            clearTimeout(RMHP_PAUSE_TIMEOUT);
            RMHP_IS_ANIMATING = false;
            RMHP_PAUSE_TIMEOUT = setTimeout(RMHP_playTimer, RMHP_PAUSE_DELAY);
        }

        function RMHP_playTimer() {
            RMHP_IS_ANIMATING = true;
            RMHP_cycle();
        }

        /**
         * cycle the slides and create a new timeout
         * to loop.
         */
        function RMHP_cycle() {
            if (RMHP_IS_ANIMATING) {
                RMHP_nextSlide();
                RMHP_CYCLE_TIMEOUT = setTimeout(RMHP_cycle, RMHP_CYCLE_DELAY);
            }
        }


        function RMHP_changeCurrentSlide() {
            var slide = SLIDER.getItemAt(CURRENT_SLIDE);

            // MOVE HERO IMAGE
            //................................
            RMHP_moveHeroLarge();

            // TEXT AREA CHANGE
            //..................................
            $('.hero-text-active').stop();
            $('.home-hero-text-left').css("opacity", 1);
            $('.hero-text-active').delay(RMHP_SLIDER_SPEED / 4).fadeOut((RMHP_SLIDER_SPEED / 2), function () {
                $('.home-hero-text-right').removeClass('hero-text-active');
                $('.home-hero-text-left').removeClass('hero-text-active');
                $('.hero-text-' + CURRENT_SLIDE).fadeIn((RMHP_SLIDER_SPEED / 2));
                $('.hero-text-' + CURRENT_SLIDE).addClass("hero-text-active");
            });

            // SMALL ROUND BUTTONS CHANGE
            //..............................
            var currentDot = SLIDER.length() - CURRENT_SLIDE;
            if (currentDot > SLIDER.length()) {
                currentDot = currentDot % SLIDER.length();
            }
            $('.home-hero-slider a').removeClass('slider-active');
            $('.home-hero-slider a[rel="hero-' + (currentDot) + '"]').addClass('slider-active');
        }

        /**
         * Change the slides in the thumbnails and
         * in the hero image.
         */
        function RMHP_nextSlide() {
            CURRENT_SLIDE--;
            if (CURRENT_SLIDE < 0) {
                CURRENT_SLIDE = SLIDER.length() - 1;
            }

            RMHP_changeCurrentSlide();
        }


        function RMHP_moveHeroLarge() {
            var imageSource = SLIDER.getItemAt(CURRENT_SLIDE).heroUrl;

            $('.home-hero-mask.hero-next').html('<a href="javascript:void(0);" name="home-hero-' + (CURRENT_SLIDE + 1) + '"><img src="' + imageSource + '"/></a>');

            $('.home-hero-mover').animate({
                left: ['-' + RMHP_HERO_LARGE_WIDTH, 'swing']
            }, RMHP_SLIDER_SPEED, function () {
                $('.home-hero-mask.hero-active').html('<a href="javascript:void(0);" name="home-hero-' + (CURRENT_SLIDE + 1) + '"><img src="' + imageSource + '"/></a>');
                $(this).css('left', '0px');

            });
        }

        /**
        * Creates an array of slides that make up one instance of the HeroSlider
        */
        function HeroSlider() {

            this.slides = new Array();
            this.addSlide = function (slide) {
                this.slides.push(slide);
            };

            this.getItemAt = function (idx) {
                var realIndex = idx % this.slides.length;
                return this.slides[realIndex];
            };

            this.length = function () {
                return this.slides.length;
            };

            /**
             * Reverse the internal array.
             */
            this.reverse = function () {
                var newArray = new Array();
                for (var i = this.slides.length - 1; i >= 0; i--) {
                    newArray.push(this.slides[i]);
                }
                this.slides = newArray;
            }

            this.getIndexByRel = function (rel) {
                for (var i = 0; i < this.slides.length; i++) {
                    if (this.slides[i].thumbRel == rel) {
                        return i;
                    }
                }
            }

            this.getSlideByRel = function (rel) {
                return this.getItemAt(this.getIndexRel(rel));
            }

        }

        function HeroSlide(tUrl, tRel, hUrl, bUrl, lUrl) {

            /**
            * The image URL for the small 3-up thumbnails across the bottom.
            */
            this.thumbUrl = tUrl;

            /**
            * The rel property in the anchor for this thumb.
            */
            this.thumbRel = tRel;

            /**
            * The image URL for the large hero image that encompasses the top area.
            */
            this.heroUrl = hUrl;

            /**
            * The image URL for the floating text banner box on the right.
            */
            this.bannerUrl = bUrl;

            /**
            * The href URL for the "learn more" link.
            */
            this.learnMoreUrl = lUrl;


        }





    </script>
    
