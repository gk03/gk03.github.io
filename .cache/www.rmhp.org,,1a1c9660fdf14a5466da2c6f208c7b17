        $(document).ready(function () {
            if ($.cookie('RMHP_Health_Countdown') == null) {
                CountDownTimer('02/16/2015 12:01 AM', 'countdown');
            }
            $.cookie('RMHP_Health_Countdown', true, { expires: 1825, path: '/' });
        });
    });
</script>
    
