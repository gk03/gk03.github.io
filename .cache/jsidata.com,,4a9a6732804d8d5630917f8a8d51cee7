<script type="text/javascript">
		$(function(){
			var pathname = window.location.href;
			 var homeUrl = $('a#isHome').attr('href');
			 if(pathname == homeUrl)
			 {
					$('a#isHome').attr('class','current');
			 }
			/*$('.label').click(function(){
				$(this).hide();
			});
			$('.label').blur(function(){
				$(this).show();
			});*/
			
			$('input.field1').click(function(){
				if($(this).val() == ""){
					$(this).parent('span').parent('p').parent('div.field').children('.label').hide();
				}
				
			});
			$('input.field1').blur(function(){
				if($(this).val() == ""){
					$(this).parent('span').parent('p').parent('div.field').children('.label').show();
				}
			});
			$('#submit').val("");
			$('#slides').slides({
				preload: true,
				preloadImage: 'http://jsidata.com/wp-content/themes/JSI-Data/images/loading.gif',
				play: 5000,
				pause: 2500,
				hoverPause: true,
				generatePagination: true
			});
		});
	</script>
