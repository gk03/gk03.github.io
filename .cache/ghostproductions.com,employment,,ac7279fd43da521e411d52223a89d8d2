    <script>
        jQuery('.contact-new-footer .more-details-link').click(function(){
            jQuery( ".contact-new-footer .more-details" ).addClass( " show" );
            jQuery( ".contact-new-footer .more-details-link" ).hide( "" );
        });
    </script>

    <script type="text/javascript">
        // on keypress - hide label
        jQuery('.contact-new-footer textarea').on('keypress',function(){
            label = jQuery(this).closest('.form-item').find('label');
            label.hide();
        });
        // on blur - show label if field is empty
        jQuery('.contact-new-footer textarea').on('blur',function(){
            label = jQuery(this).closest('.form-item').find('label');
            if ( jQuery(this).val() == '' ) {
                label.show();
            }
        });
    </script>

