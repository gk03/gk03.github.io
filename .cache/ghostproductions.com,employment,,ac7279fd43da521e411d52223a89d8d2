          	    // on keypress - hide label
  	            jQuery('.contact-new-footer input').on('keypress',function(){
  	                label = jQuery(this).closest('.form-item').find('label');
  	                label.hide();
  	            });
  	            // on blur - show label if field is empty
  	            jQuery('.contact-new-footer input').on('blur',function(){
  	                label = jQuery(this).closest('.form-item').find('label');
  	                if ( jQuery(this).val() == '' ) {
  	                    label.show();
  	                }
  	            });
