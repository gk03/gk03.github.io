function handleSearchSuggest(){if(searchReq.readyState==4){var ss=document.getElementById('search_suggest')
ss.innerHTML='';var str=searchReq.responseText.split("\n");var suggest='<table style="border-collapse:collapse; border:1px solid #aaaaaa; width:215px;">';for(i=0;i<str.length-1;i++){if(i==0){suggest+='<tr class="dropmenu_over"><td style="font-family:arial,univers;font-size:12px;border-bottom: 1px solid #aaaaaa;" colspan="2"><b>Suggestions</b></td></tr>';}
var srchterm=str[i].split("\t");suggest+='<tr onMouseOver="suggestOver(this);" ';suggest+='onMouseOut="suggestOut(this);" ';suggest+='onClick="setSearch(\''+escape(srchterm[0])+'\');" ';suggest+='class="dropmenu"><td>'+srchterm[0]+'</td></tr>';}
suggest+='</table>';ss.innerHTML+=suggest;}}
function suggestOver(div_value){div_value.className='dropmenu_over';}
function suggestOut(div_value){div_value.className='dropmenu';}
