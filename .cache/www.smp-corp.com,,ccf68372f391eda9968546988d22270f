<!-- END Responsive Carousel MODULE -->
<script type="text/javascript">
  jQuery(document).ready(function(){
    var $itemWidthNew = 180;
    var options = {};
    function adjustSlide(){
      var $moduleWidth = jQuery('#jmnewspro-124').width();
      var $itemWidth = 180;
      var $items = $moduleWidth/$itemWidth;
      $items = parseInt($items.toString());
      if($items > 20){
        $items = 20;
      }
      var $marginTotal = 20*($items-1);
      $itemWidthNew = ($moduleWidth - $marginTotal)/$items;
      if($itemWidthNew > $moduleWidth) $itemWidthNew = $moduleWidth;
      jQuery('#jmnewspro-124 .slide-item').width($itemWidthNew);
      jQuery('#jmnewspro-124 .slide-item img').width($itemWidthNew);
      options.slideSelector = 'div.slide-item';
      options.slideWidth = $itemWidthNew;
      options.minSlides =	2;
      options.maxSlides = 20;
      options.moveSlides = 10;
      options.slideMargin = 20;
      options.speed = 1500;
      options.infiniteLoop = true;
      options.autoHover = true;
      options.auto = true;
      options.pause = 4000;
      options.adaptiveHeight = true;
      options.controls = false;
    }
    adjustSlide();
    var jmnewspro124=jQuery('#jmnewspro-124 .slider').bxSlider(options);
   
    
  });
</script>
</div>
