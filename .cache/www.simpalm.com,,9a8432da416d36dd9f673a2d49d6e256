	function testimonial_bullets() 
	{ 
		var flex_ol_present = jQuery('ol').hasClass('flex-control-paging');
		var flex_ul_present = jQuery('ul').hasClass('flex-direction-nav');			
		
		if(flex_ol_present && flex_ul_present)
		{
			jQuery("#home_testimonial_slider .flex-direction-nav").wrapAll( "<div class='slider_nav_section'></div>" );
			var testimoinal_nav = jQuery('#home_testimonial_slider ol.flex-control-paging');
			var testimoinal_dir_nav = jQuery('#home_testimonial_slider .flex-direction-nav li:first-child');
			//jQuery('test').insertAfter(testimoinal_dir_nav);
			jQuery(testimoinal_dir_nav).after(testimoinal_nav);
			return false;		
		}
		setTimeout(testimonial_bullets, 1500);
	}
	
	function testimonial_bullets_new() 
	{ 
		var testimonial_div_present = jQuery('#home_new_testimonial_section div').hasClass('nextend-widget');
		var testimonial_prenav_present = jQuery('#home_new_testimonial_section div').hasClass('nextend-arrow-previous');
		var testimonial_bullet_present = jQuery('#home_new_testimonial_section div').hasClass('nextend-widget-bullet');
		if(testimonial_div_present && testimonial_prenav_present && testimonial_bullet_present)
		{
			jQuery( "#home_new_testimonial_section .nextend-widget" ).wrapAll( "<div class='slider_nav'></div>" );
			
			var testimoinal_pre_nav = jQuery('#home_new_testimonial_section .nextend-widget.nextend-arrow-previous');
			var testimoinal_bullet = jQuery('#home_new_testimonial_section .nextend-widget.nextend-widget-bullet');
			
			jQuery(testimoinal_pre_nav).after(testimoinal_bullet); 
			return false;		
		}
		setTimeout(testimonial_bullets_new, 1500);     
	}	
