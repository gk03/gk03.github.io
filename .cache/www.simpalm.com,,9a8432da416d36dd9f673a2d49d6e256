						$('#commentform').children('p').addClass('comm_section');
	
			var swapElements = function(siblings, subjectIndex, objectIndex) 
			{
				// Get subject jQuery
				var subject = $(siblings.get(subjectIndex));
				// Get object element
				var object = siblings.get(objectIndex);
				// Insert subject after object
				subject.insertAfter(object);
			}
			$(function() 
			{
				swapElements($('#commentform .comm_section'), 4, 5);
			});
			$(".fancybox").fancybox();
			var custom_HTML5_container = $('div').hasClass('custom_HTML5_container');
			if(custom_HTML5_container)
			{
				$('body').addClass('body_HTML5_container');
			}
			var custom_hybrid_container = $('div').hasClass('custom_hybrid_container');
			if(custom_hybrid_container)
			{
				$('body').addClass('body_hybrid_container');
			}
			
			

			$(".entry-content .one_page_menu_bar ul li a").on("click",function()
			{			
				var current_tab = $(this).attr('href');
				$('.link_div').removeClass('scroll_onepage_section');
				$(current_tab).addClass('scroll_onepage_section');

				var lastScrollTop = 0;
				$(window).scroll(function(event)
				{	
	   				var st = $(this).scrollTop();
	   				if (st > lastScrollTop){
						$('.link_div').removeClass('scroll_onepage_section');
						$(current_tab).addClass('scroll_onepage_section');
	   				} else {
						$('.link_div').removeClass('scroll_onepage_section');
	   				}
	   			lastScrollTop = st;
				});
	
				if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') && location.hostname == this.hostname)
				{
					var target = $(this.hash);
					target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
					if (target.length) {
						$('html, body').animate({
						scrollTop: target.offset().top
						}, 1000);
						return false;
					}				
				}
				window.location.hash=current_tab;
			});	
		 	 
			$('.service_testimonial_slider').bxSlider(
