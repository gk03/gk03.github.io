<script type='text/javascript'>
var selector = {
	self: document.getElementById("featured_arrow"),
	title: document.getElementById("featured_title"),
	timer: null,
	photo: "photoDiv0",
	next: "",
	position: 0,
	positions: [126,84,42,0],
	photos:["","","",""],
	px:126,
	opacity: 100,
	setTitle: function(){
		var src = this.photos[this.position];
		if(src == ""){
			src = document.getElementById("img"+this.position).src;
			src = src.substring(src.lastIndexOf("/")+1, src.lastIndexOf("."));
			
			src = src.replace(/&/g,"&amp;");
			src = src.replace(/_/g,"&nbsp;");
			this.photos[this.position] = src;
		}
		
		this.title.innerHTML = src;
		
	},
	_setPosition: function(delta){
		//var px = parseInt(this.self.style.right);// = px+"px";
		var end = this.positions[this.position];
		//clearTimeout(this.timer);
		if(Math.abs(this.px - end) > 1){

			this.px = (this.px + ((end-this.px)/4));
			this.self.style.right = this.px +"px";
			//this.timer = setTimeout("selector._setPosition()", 10);
		}
		else{
			this.px = end;
			this.self.style.right = end + "px";
		}
	},
	__setPosition: function(pos){
		this.position = pos;//this.positions[pos] - start;
		clearTimeout(this.timer);
		this.timer = setTimeout("selector._setPosition()", 500);
	},
	setPosition: function(pos){
		document.getElementById("featured"+this.position).className = "";
		this.position = pos;//this.positions[pos] - start;
		document.getElementById("featured"+this.position).className = "selected";
		//setPhoto(pos);
		this._setPosition(30);
	},
	fadeProcedure:function (newid) {
		this.next = newid;
		this.setOpacity(0,newid);
		this.fadeOut();
		//this.fadeTo(0,5,'selector.introProcedure()');
		//var speeder = 5;
		/*for (i = this.opacity; i >= -speeder; i-=speeder)
			{
			setTimeout("selector.fadeTo(" + 0 + ", "+speeder+",'selector.introProcedure()')", (i));
			
			}*/
		
	}, //end fadeProcedure()
	introProcedure: function() {
		document.getElementById(this.photo).style.display='none';
		document.getElementById(this.next).style.display='block';
		this.photo = this.next;
		this.setTitle();
		//this.fadeTo(100,10);
	}, //end introProcedure()
	fadeToX: function(endfade, speeder, action) {
		clearTimeout(this.timer);
		//console.log("opacity: "+this.opacity+ " endfade: "+endfade);
	  	if (this.opacity == endfade){
			this.setOpacity(this.opacity, this.photo);
			if(action){
				this.timer = setTimeout(action, speeder);
			}
		}
		else{
			
			if (this.opacity > endfade){
				this.opacity = Math.max(endfade,this.opacity-(speeder*3));
			}
			else{
				this.opacity = Math.min(endfade,this.opacity+(speeder*2));
				//this._setPosition(Math.abs(endfade-this.opacity)/speeder);
			}
			this.setOpacity(this.opacity, this.photo);
		  	this.timer = setTimeout("selector.fadeTo(" +endfade+ ","+speeder+",'"+action+"')", speeder);
			this._setPosition();
		}
  	}, //end fadeOut()
	fadeIn: function() {
		clearTimeout(this.timer);
		//console.log("opacity: "+this.opacity+ " endfade: "+endfade);
	  	if (this.opacity >= 100){
			this.opacity = 100;
			this.setOpacity(this.opacity, this.photo);
		}
		else{
			this.opacity = this.opacity+(10);
			this.setOpacity(this.opacity, this.photo);
		  	this.timer = setTimeout("selector.fadeIn()", 5);
		}
		this._setPosition();
  	},
	fadeOut: function() {
		clearTimeout(this.timer);
		//console.log("opacity: "+this.opacity+ " endfade: "+endfade);
	  	if (this.opacity <= 0){
			this.opacity = 0;
			this.setOpacity(this.opacity, this.photo);
			this.introProcedure();
			this.timer = setTimeout("selector.fadeIn()", 5);
		}
		else{
			this.opacity = this.opacity-(15);
			this.setOpacity(this.opacity, this.photo);
		  	this.timer = setTimeout("selector.fadeOut()", 5);
		}
		this._setPosition();
  	},
	setOpacity: function (_opacity, id) {
		//console.log("opacity: "+_opacity);
		//this.opacity = opacity;
		var el = document.getElementById(id).style;
		el.opacity = (_opacity / 100);
		el.MozOpacity = (_opacity / 100);
		el.KhtmlOpacity = (_opacity / 100);
		el.filter = "alpha(opacity=" + _opacity + ")";
		
	}
};


var index = 0;
var photos = new Array(4);
photos[0]='images/ban1_1.png';
photos[1]='images/ban1_2.png';
photos[2]='images/ban1_3.png';
photos[3]='images/ban1_4.png';
var photoindex = photos.length;

//we now have an array called photos, index as 0, and photoindex as the highest index of photos.
//setOpacity(opacity, id)
function setPhoto(new_index)
{
	new_index = new_index % photoindex;
	//selector.position = (new_index);
	//'photoDiv'+(index),
	selector.fadeProcedure('photoDiv'+(new_index));
	index = new_index;
	selector.setPosition(index);
	clearTimeout(phototimer);
	phototimer=setTimeout('refreshPhotos()', 10000);
}

function refreshPhotos()
{
	//alert("world");
	setPhoto(index+1);
	//refreshNews();
}

var phototimer=setTimeout('refreshPhotos()', 10000);
</script>



