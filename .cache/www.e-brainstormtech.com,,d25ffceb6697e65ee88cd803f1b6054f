<script type="text/javascript"><!--
function newImage(arg) {

    if (document.images) {

        rslt = new Image();

        rslt.src = arg;

        return rslt;

    }

}



function changeImages() {

    if (document.images && (preloadFlag == true)) {

        for (var i=0; i<changeImages.arguments.length; i+=2) {

            document[changeImages.arguments[i]].src = changeImages.arguments[i+1];

        }

    }

}



var preloadFlag = false;

function preloadImages() {

    if (document.images) {

        websitedesign_01_WebsiteDesign_over = newImage("images/websitedesign_01-WebsiteDes.gif");

        content_01_ContentManagement_over = newImage("images/content_01-ContentManagemen.gif");

        erpcrm_01_ERPCRM_over = newImage("images/erpcrm_01-ERPCRM_over.gif");

        ecommerce_01_e_Commerce_over = newImage("images/ecommerce_01-e-Commerce_ove.gif");

        webdemo_01_WebDemo_over = newImage("images/webdemo_01-WebDemo_over.gif");

        preloadFlag = true;

    }

}
// --></script>
