<div class="span12 widget-span widget-type-raw_html " style="" data-widget-type="raw_html" data-x="0" data-w="12">
<div class="cell-wrapper layout-widget-wrapper">
<span id="hs_cos_wrapper_module_1458068306045340416" class="hs_cos_wrapper hs_cos_wrapper_widget hs_cos_wrapper_type_raw_html" style="" data-hs-cos-general-type="widget" data-hs-cos-type="raw_html"><script>
$(document).ready(function() {
$('.testimonials div').hide();
$('.testimonials div').eq(Math.floor((Math.random() * $('.testimonials div').length))).show();
setInterval(function() {
var $items = $('.testimonials div');
var $currentItem = $items.filter(':visible');
var currentIndex = $currentItem.index();
var nextIndex = currentIndex;
while (nextIndex == currentIndex) {
nextIndex = Math.floor((Math.random() * $items.length));
}
$currentItem.fadeOut(1000, function() {
$items.eq(nextIndex).fadeIn(1000);
});
}, 10000);
});
</script></span>
</div><!--end layout-widget-wrapper -->
