<script>
    $(document).ready(function() {
        setHeight();
    });
    
    $(window).bind("load",function() {
        setHeight();
    });
    
    $(window).resize(function() {
       setHeight(); 
    });
    function setHeight() {
        $('.three-box-outer').each(function() {
            $('.three-box-outer .box').css("height","auto");
            var maxHeight=Math.max.apply(null,$(this).find(".box").map(function(){return $(this).outerHeight();}).get());
            $(this).find('.box').css("height",maxHeight);
        });
    } 
</script>











