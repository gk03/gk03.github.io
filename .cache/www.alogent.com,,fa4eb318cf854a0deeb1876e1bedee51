                                <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js"></script> 
                                <script type="text/javascript">
                                    $(function () {
                                        $('#subForm').submit(function (e) {
                                            e.preventDefault();
                                            $.getJSON(
                                            this.action + "?callback=?",
                                            $(this).serialize(),
                                            function (data) {
                                                if (data.Status === 400) {
                                                    $('#error-response').text(data.Message).fadeIn().delay(10000).fadeOut;
                                                } else { // 200
                                                    $('#success-response').text(data.Message).fadeIn().delay(10000).fadeOut;
                                                }
                                            });
                                        });
                                    });
                                </script>
                                <!-- End Signup Form -->
