<script type="text/javascript">
    $(window).resize(function () {
        if ($(window).width() < 569) {
            var num1 = $("#cnt_ny").html().replace(/\./g, '-');
            $("#cnt_ny").html(num1);
            var num2 = $("#cnt_tnt").html().replace(/\./g, '-');
            $("#cnt_tnt").html(num2);
            var num3 = $("#cnt_tf").html().replace(/\./g, '-');
            $("#cnt_tf").html(num3);
            var num4 = $("#cnt_ind").html().replace(/\./g, '-');
            $("#cnt_ind").html(num4);
        }
        else if ($(window).width() > 568) {
            var num1 = $("#cnt_ny").html().replace(/\-/g, '.');
            $("#cnt_ny").html(num1);
            var num2 = $("#cnt_tnt").html().replace(/\-/g, '.');
            $("#cnt_tnt").html(num2);
            var num3 = $("#cnt_tf").html().replace(/\-/g, '.');
            $("#cnt_tf").html(num3);
            var num4 = $("#cnt_ind").html().replace(/\-/g, '.');
            $("#cnt_ind").html(num4);
        }
    });
</script>


