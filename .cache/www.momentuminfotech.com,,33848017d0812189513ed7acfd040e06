        function top_page() {
            if ($(window).width() < 569) {
                $('html, body').animate({ scrollTop: 0 }, 'fast');
            }
        }
        $(document).ready(function () {
            if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                $(window).scroll(function () {
                    if ($(this).scrollTop() > 0) {
                        $('.head-top.col-xs-12').slideUp('fast');
                        $('.logo-bar .search-btn').css('top', '13px');
                    }
                    else {
                        $('.logo-bar .search-btn').css('top', '59px');
                        $('.head-top.col-xs-12').slideDown('fast');
                    }
                });
            }
        });
    </script>

