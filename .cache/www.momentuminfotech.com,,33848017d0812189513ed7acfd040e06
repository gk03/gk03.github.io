<script>
    $(document).ready(function () {
        var pageid = $('#hdnPageId').val();
        $.ajax({
            url: "/Testimonials/GetTestimonialByPageId",
            data: { PageId: pageid },
            type: "Post",
            success: function (data) {
                if (data.List.length > 0) {
                    var str = "";
                    var i = 0;
                    for (i = 0; i < data.List.length; i++) {
                        var testimonialtext = data.List[i].TestimonialText;
                        if (i == 0) {
                            str = str + "<div class='item active'><div class='testimonials col-lg-12 col-md-12 col-sm-10 col-xs-10'><span class='quote-2'><img src='/Content/Images/quote_2.png' alt='testimonial' /></span><div class='testimonial-content'>"
                                + testimonialtext + "<small><span class='Testimonial_by'>"
                                + data.List[i].FirstName + ' ' + data.List[i].LastName + "<span class='Orange'> @ </span>"
                            + data.List[i].CompanyName + "</span></small></div></div><div class='clearfix'></div></div></div>";

                        }
                        else {
                            str = str + "<div class='item'><div class='testimonials col-lg-12 col-md-12 col-sm-10 col-xs-10'><span class='quote-2'><img src='/Content/Images/quote_2.png' alt='testimonial' /></span><div class='testimonial-content'>"
                                + testimonialtext + "<small><span class='Testimonial_by'>"
                                + data.List[i].FirstName + ' ' + data.List[i].LastName + "<span class='Orange'> @ </span>"
                            + data.List[i].CompanyName + "</span></small></div></div><div class='clearfix'></div></div></div>";
                        }
                    }
                    if (!$('.inner-page').html() == "") {
                        $('.testimonial-arrows').css('display', 'none');
                    }
                    $('#dvpagetestimonial').css('display', 'block');
                    $('#dvtestimonialdata').html(str);
                }
                else {
                    $('.testimonial').html('');
                }
            }
        });
    });

</script>
