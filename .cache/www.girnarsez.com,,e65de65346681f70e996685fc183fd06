        <script type="text/javascript">
                function ValidateInquiry() {

                    var name = $('#name').val();
                    var email = $('#email').val();
                    var mobile = $('#mobile').val();
                    var comment = $('#comment').val();
                    var len = mobile.toString().length;
                    var enquiry1 = $('#selectbox1').val();
                    var error = 0;
                    var phoneno = /^\d{10}$/;  
                  // console.log(email);console.log(enquiry1);
                    var error = 0;
                    if (name == null || name == '' || !isValidName(name)) {
                        $('#name_container span').html('Please provide valid Name!');
                        $('#name_container span').removeClass('hidden_container');
                        error++;
                    } else {
                        $('#name_container span').addClass('hidden_container');
                        $('#name_container span').html('');

                    }

                    if (email == null || email == '' || !isValidEmailAddress(email)) {
                        $('#email_container span').html('Please provide Valid Email Address!');
                        $('#email_container span').removeClass('hidden_container');
                        error++;
                    } else {
                        $('#email_container span').addClass('hidden_container');
                        $('#email_container span').html('');
                    }
                    if (mobile == null || mobile == '' || !(mobile.match(phoneno))) {
                       $('#mobile_container span').html('Please provide valid mobile no !');
                        $('#mobile_container span').removeClass('hidden_container');           
                        error++;

                    } else {           
                              $('#mobile_container span').addClass('hidden_container');
                              $('#mobile_container span').html('');                        

                    } 
                    if (enquiry1 == null || enquiry1=='') 
                    {
                        $('#option_container span').html('Please Select Option!');
                        $('#option_container span').removeClass('hidden_container');
                        error++;
                     } else {             
                        $('#option_container span').addClass('hidden_container');
                        $('#option_container span').html('');    
                         //console.log(comment);
                    }  
                    if (!$.trim(comment)) 
                    {
                        $('#comment_container span').html('Please provide comment!');
                        $('#comment_container span').removeClass('hidden_container');
                        error++;
                        console.log(comment);
                    } else {             
                        $('#comment_container span').addClass('hidden_container');
                        $('#comment_container span').html('');    
                         //console.log(comment);
                    }   
                    console.log(error);
                    if (parseInt(error) <= 0) {
                        $.post("/mail_inquirynew", $('#enquiry-form-new').serialize() ,function (data) {
                            if(data == '1'){
                                $('#success-message').html('Enquiry Send Successfully');                   
                                $('#success-message').show();
                                document.getElementById("enquiry-form-new").reset();
                                window.location = "http://www.girnarsez.com/thankyou";
                            }
                        });
                    } else {
                        $('#success-message').hide();
                        $('#success-message').html('');
                    }
                }

                function ValidateInquiry1() {

                               var name = $('#name1').val();
                               var email = $('#email1').val();
                               var mobile = $('#mobile1').val();
                               var comment = $('#comment1').val();
                               var len = mobile.toString().length;
                               var enquiry1 = $('#selectbox2').val();
                               var error = 0;
                               var phoneno = /^\d{10}$/;  
                             // console.log(email);console.log(enquiry1);
                               var error = 0;
                               if (name == null || name == '' || !isValidName(name)) {
                                   $('#name_container1 span').html('Please provide valid Name!');
                                   $('#name_container1 span').removeClass('hidden_container');
                                   error++;
                               } else {
                                   $('#name_container1 span').addClass('hidden_container');
                                   $('#name_container1 span').html('');

                               }

                               if (email == null || email == '' || !isValidEmailAddress(email)) {
                                   $('#email_container1 span').html('Please provide Valid Email Address!');
                                   $('#email_container1 span').removeClass('hidden_container');
                                   error++;
                               } else {
                                   $('#email_container1 span').addClass('hidden_container');
                                   $('#email_container1 span').html('');
                               }
                               if (mobile == null || mobile == '' || !(mobile.match(phoneno))) {
                                  $('#mobile_container1 span').html('Please provide valid mobile no !');
                                   $('#mobile_container1 span').removeClass('hidden_container');           
                                   error++;

                               } else {           
                                         $('#mobile_container1 span').addClass('hidden_container');
                                         $('#mobile_container1 span').html('');                        

                               } 
                               if (enquiry1 == null || enquiry1=='') 
                               {
                                   $('#option_container1 span').html('Please Select Option!');
                                   $('#option_container1 span').removeClass('hidden_container');
                                   error++;
                                } else {             
                                   $('#option_container1 span').addClass('hidden_container');
                                   $('#option_container1 span').html('');    
                                    //console.log(comment);
                               }  
                               if (!$.trim(comment)) 
                               {
                                   $('#comment_container1 span').html('Please provide comment!');
                                   $('#comment_container1 span').removeClass('hidden_container');
                                   error++;
                                   console.log(comment);
                               } else {             
                                   $('#comment_container1 span').addClass('hidden_container');
                                   $('#comment_container1 span').html('');    
                                    //console.log(comment);
                               }   
                               console.log(error);
                               if (parseInt(error) <= 0) {
                                   $.post("/mail_inquirynew1", $('#enquiry-form-neww').serialize() ,function (data) {
                                       if(data == '1'){
                                           $('#success-message').html('Enquiry Send Successfully');                   
                                           $('#success-message').show();
                                           document.getElementById("enquiry-form-neww").reset();
                                           window.location = "http://www.girnarsez.com/thankyou";
                                       }
                                   });
                               } else {
                                   $('#success-message').hide();
                                   $('#success-message').html('');
                               }
                           }
                function isValidEmailAddress(emailAddress) {
                    var pattern = new RegExp(/^(("[\w-\s]+")|([\w-]+(?:\.[\w-]+)*)|("[\w-\s]+")([\w-]+(?:\.[\w-]+)*))(@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$)|(@\[?((25[0-5]\.|2[0-4][0-9]\.|1[0-9]{2}\.|[0-9]{1,2}\.))((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\.){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[0-9]{1,2})\]?$)/i);
                    return pattern.test(emailAddress);
                }
                function isValidName(name){        
                       return /^[A-Za-z ]+$/.test(name);
                         }

            </script>
