		}else if (url.indexOf('/fr-ch/') > 0) {
			$("div.copyright").each(function() { 
				$(this).text($(this).text().replace("All rights reserved","Tous droits reserves")); 
			});
		}


    // http://objectmix.com/sharepoint/362792-spswc-searchboxex-control-does-not-work-initial-site-firefox.html
</script>
