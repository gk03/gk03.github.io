  <script>jQuery( document ).ready(function( $ ){
    var cnt=0;
    $("#SlideToggle").click(function () {
			cnt++;
			if(cnt%2 == 0) {
				$('.SlideToggleBox').stop().slideToggle("slow");
				$(".SlideToggleDown").stop().animate({"margin-top":"0px"}, "slow");
			}
			else {
				$('.SlideToggleBox').stop().slideToggle("slow");
				$(".SlideToggleDown").stop().animate({"margin-top":"328px"}, "slow");
			}
		});
	var	bannerform=jQuery(".home-page-form").height();
	jQuery(".home-page-form").height(bannerform);
	jQuery('body').delegate(".phonevalidation_d","keypress",function(a) {
      var b = a.charCode ? a.charCode : a.keyCode;
      if (8 != b) {
          if (43 == b ) return !0;
          if (48 > b || b > 57) {
            return !1
          }else{
          }
      }
 });
   $(".js-contact-submit").click(function () {
     //$("form").hide();
