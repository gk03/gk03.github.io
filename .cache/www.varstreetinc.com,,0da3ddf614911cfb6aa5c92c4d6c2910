var spa_js_path = 'http://www.varstreetinc.com/wp-content/plugins/awesome-ui-2/shortcodes/js/';

jQuery( document ).ready(function( $ ) {
	var d1 = $.Deferred();
	$LAB.queueScript("https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.7.0/underscore-min.js","https://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.1.2/backbone-min.js");
	$LAB.queueWait();
	$LAB.queueScript(spa_js_path + "aw2_spa.js");
	$LAB.runQueue().wait(function(){
		d1.resolve();
	})
	d1.done(function(){
		$(".default_spinner").hide();
		$("#splashscreen").hide();
		$('.spa_main').fadeTo( 1000,1 )

		spa_libs.get('core').load_spa();
	})

});

