  <script type='text/spa'  spa_activity='match_height:run' lib='match_height' byrow='true' selector='.foot-menu-col'></script>
  
  <script type='text/spa'  spa_activity='core:run_script'>
      
    
      jQuery( ".foot-menu-col .modtitle" ).click(function() {
         
        
            var screenwitdth=jQuery(window).width();
            if(screenwitdth < 768){
              
              

if (jQuery(this).parent().hasClass('active'))
{
      jQuery(".foot-menu-col.active .fmenu-wrapper").stop(true, true).slideUp();
      jQuery(".foot-menu-col.active .fa").removeClass("fa-minus");
      jQuery(".foot-menu-col.active .fa").addClass("fa-plus mplus");
      jQuery(".foot-menu-col").removeClass("active");
    return false;
}


              
              jQuery(".foot-menu-col").removeClass("active");
              jQuery(".foot-menu-col .fmenu-wrapper").stop(true, true).slideUp();
               jQuery(".foot-menu-col .fa").removeClass("fa-minus");
               jQuery(".foot-menu-col .fa").addClass("fa-plus mplus");
              jQuery(this).parent().addClass("active");
              
              jQuery(".foot-menu-col.active .fa").removeClass("fa-plus mplus");
              jQuery(".foot-menu-col.active .fa").addClass("fa-minus");
              jQuery(".foot-menu-col.active .fmenu-wrapper").stop(true, true).slideDown();
             
                   /*
                   jQuery(".foot-menu-col .modtitle").parent().next( "div" ).stop(true, true).slideUp();
                   jQuery(".foot-menu-col .modtitle").children().toggleClass("fa-minus fa-plus mplus");
                   
                  ee=jQuery(this).children();
                  // Current anchor has a hash
                   if(jQuery(ee).hasClass( "mplus" ))
                    {
                      jQuery(ee).parent().next( "div" ).stop(true, true).slideUp(); 
                    }else{
                      jQuery(ee).parent().next( "div" ).stop(true, true).slideDown(); 
                    }
                    
                    jQuery(ee).toggleClass("fa-minus fa-plus mplus");
                    
                  */
                  
                  
            }
      });
      
    
    
    function checkslide()
      {
        
          var screenwitdth=jQuery(window).width();
          if(screenwitdth < 768){
            
            jQuery( ".TJitnYPo .fmenu-wrapper" ).each(function() {
                  if(!jQuery(this).parents().hasClass( "active" ))
                  {
                    jQuery(this).stop(true, true).slideUp(); 
                  }
            });
            
            /*
            jQuery(".TJitnYPo .active .fmenu-wrapper").stop(true, true).slideDown();
            */
            
          }else{
            jQuery(".TJitnYPo .fmenu-wrapper").stop(true, true).slideDown();
          }  
      }
      
      checkslide();
      jQuery(window).resize(function() {
          checkslide();
      });
      
      
  </script> 
  
