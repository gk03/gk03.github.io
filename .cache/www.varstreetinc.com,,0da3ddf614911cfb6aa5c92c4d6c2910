 <script type='text/spa'  spa_activity='core:run_script'>
  //spa_validate
	spa_libs.get('core').new_lib({
  	lib:'spa_validate',
	  js:'spa_validate/spa_validate.js'
	})
 
	spa_libs.get('spa_validate').validate=function($form,obj){
		var d1 = $.Deferred();
		var promise=$form.spa_validate();
 
		promise.done(function(status){
			d1.resolve(status);
		});
		
		return d1.promise(status);
	}
	</script>

  
  <script type='text/spa'  spa_activity='core:run_script'>
 // simplePagination
	spa_libs.get('core').new_lib({
  	lib:'simple_pagination',
  	js:'simplePagination/jquery.simplePagination.js',
  	load_if_class:'js-simple-pagination'
	})
	
	spa_libs.get('simple_pagination').run=function(obj){
		var model=obj.model;

		var total_records = parseInt(model.get("total_records"));
		var per_page_count = parseInt(model.get("per_page_count"));
	
		var total_pages = Math.ceil(total_records/per_page_count);
		var current_page= parseInt(model.get("current_page"));
		
		var ajax_url = model.get("url");
		
		var options = {
            currentPage: current_page,
            pages: total_pages,
            hrefTextPrefix: ajax_url,
            nextText:'Next <i class="fa fa-caret-right"></i>',
            prevText:'<i class="fa fa-caret-left"></i> Prev',
            onPageClick: function(pageNumber, event){
              },    
        }

		$(model.get("selector")).pagination(options);
	};
  
  
  </script>    


  
