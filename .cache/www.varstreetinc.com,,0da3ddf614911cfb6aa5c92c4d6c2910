  <script type='text/spa'  spa_activity='core:run_script'>
	  spa_libs.get('core').new_lib({
  	  lib:'animatenumber',
  	  js:'animate-number/jquery.animateNumber.min.js',
	  });
	  
	  spa_libs.get('animatenumber').run=function(obj) {
      var model=obj.model;
      var animation_selector = model.get('animation_selector');
      $(animation_selector ).each(function() {
          var comma_separator_number_step = $.animateNumber.numberStepFactories.separator(',')
          $(this).animateNumber({
              number:$(this).attr('data-number'),
              numberStep: comma_separator_number_step
          });
       });
	  }  
	  
	  spa_libs.get('animatenumber').runOnShow=function(obj) {
      var model=obj.model;
      var trigger_selector = model.get('trigger_selector');
      
      $(trigger_selector).appear({interval:1000});
      
      if($(trigger_selector).is(':appeared')){
        spa_libs.get('animatenumber').run(obj);
      }
      
      $(trigger_selector).on('appear', function(event, $all_appeared_elements) {
        spa_libs.get('animatenumber').run(obj);
      }); 
   }
  </script>    

  
  <script type='text/spa'  spa_activity='core:run_script'>
      spa_libs.get('core').new_lib({
      lib:'awesome-form',
      load_if_class:"js-awesome-form",
      js:'awesome-form/awesome-form.js',
      set_defaults: function(){
        
        $( ".js-awesome-form" ).each(function(){
            awesome_setup_form($(this));
        })
        $( ".js-awesome-form" ).off('submit');
        $( ".js-awesome-form" ).on('submit', function(event) {
          event.preventDefault();
          awesome_form_submit($(event.target));
        });
      
      }      
      });
      
  </script>    

