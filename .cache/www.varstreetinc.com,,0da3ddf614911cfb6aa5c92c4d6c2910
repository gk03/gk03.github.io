       <script>jQuery( document ).ready(function( $ ){
    var widthwindow=$(document).width();
		var widthbox=$(".box").width()/2;
		var xside=widthwindow/2-widthbox;
		var heightwindow=$(window).height();
		var heightbox=$(".box").height()/2;
