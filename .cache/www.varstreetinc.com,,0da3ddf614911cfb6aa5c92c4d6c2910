<script>jQuery( document ).ready(function( $ ){
  jQuery("#menu-main-menu .menu-item-has-children").prepend("<span class='pluss fa fa-angle-down'></span>");         
  $( "#menu-main-menu li.dropdown" ).hover(
  function() {
      var wid=jQuery(window).width();
                if(992 >= wid)
                {
                  <!--mobile-->
                }else{
                <!--desktop-->
                    $( this ).find('ul').first().stop(true, true).slideDown("fast");
                }
  }, function() {
     var wid=jQuery(window).width();
                if(992 >= wid)
                {
                  <!--mobile-->
                }else{
                       <!--desktop-->
                     $( this ).find('ul').stop(true, true).slideUp("fast");
                }
  }
);
 $( "#menu-main-menu li" ).hover(
  function() {
     $(this).parents('li').last().addClass("acparent");   
  }, function() {
      $(this).parents('li').last().removeClass("acparent"); 
  }
);
    jQuery('#menu-main-menu .pluss').on('click', function(){
      if(jQuery(this).hasClass( "mpluss" ))
      {
        jQuery(this).next().next( "ul" ).stop(true, true).slideUp(); 
      }else{
     // var ddd=$( this ).parent();
      //jQuery( ddd ).find('ul').stop(true, true).slideDown("fast");
        jQuery(this).next().next( "ul" ).stop(true, true).slideDown(); 
      }
        jQuery(this).toggleClass("fa-angle-down fa-angle-up mpluss");
    });
  jQuery( window ).resize(function() {
      var wid=jQuery(window).width();
      if(992 >= wid)
      {
          var getheaderheight= jQuery("header").height();
          var docheight=jQuery(window).height();
          var fixednavHeight=docheight-getheaderheight;
          jQuery("div.navbar-collapse").addClass("wpoet");
          var menuheight= jQuery("#menu-main-menu").height();
          if(menuheight > fixednavHeight){
                jQuery("#menu-main-menu").height(menuheight);
                jQuery('#menu-main-menu').css('max-height', fixednavHeight); 
                //set max height
          } else{
                  jQuery('#menu-main-menu').css('max-height', fixednavHeight); 
          }
      }else{
          <!--- remove mobile fix ----->
         jQuery('#menu-main-menu').height('auto');
         jQuery('#menu-main-menu').css('max-height', ''); 
      }
  });
    $(window).trigger('resize');
      $('#nav').affix({offset: {top: $("header").outerHeight(true)} });
     jQuery('.mobilem').on('click', function(){
        jQuery("#primary-navigation-block div.navbar-collapse").slideToggle();
        jQuery('.mobilem').toggleClass("active");
     });  
})</script>    


     
<script type='text/spa'  spa_activity='core:run_script'>
  
  spa_libs.get('core').new_lib({
          lib:'customfunctions',
  })
        
          spa_libs.get('customfunctions').slideinbox=function(obj){
            var model=obj.model;
            
            jQuery("#sb-search .subbt").addClass("active");
            jQuery('.sb-search-input').animate({width: '100%'}, 500, function() {  });
             
          }
  
           spa_libs.get('customfunctions').slideoutbox=function(obj){
            var model=obj.model;
            
            jQuery('.sb-search-input').animate({width: '0px'}, 500, function() {
              jQuery("#sb-search .subbt").removeClass("active"); 
            });
             
          }
          
          spa_libs.get('customfunctions').topsearch=function(obj){
            var model=obj.model;
            
              if(jQuery( "#s" ).val()!="")
              {
                jQuery( "#shortserachbox" ).submit();
              }
             
          }
          
          spa_libs.get('customfunctions').sociallinksdropdown=function(obj){
            var model=obj.model;
            
              jQuery(".social-links").slideToggle();
             
          }
          
          spa_libs.get('customfunctions').mobilemenutoggle=function(obj){
            var model=obj.model;
            
              jQuery('.header-container .navbar-collapse').slideToggle().addClass('in');
      
               var windowheight =  jQuery( window ).height();
               var menuheight =jQuery( ".header-container .navbar-collapse.in" ).height();
                
                if(menuheight > windowheight){
                   jQuery('.header-container .navbar-collapse.in').height(windowheight);
                   jQuery('.header-container .navbar-collapse.in').css("overflow", "scroll");
                }
             
          }
          
          spa_libs.get('customfunctions').desktopmenuhover=function(obj){
            var model=obj.model;
            t = obj.$bound_element;
            jQuery(t).find(' > .dropdown-menu').stop(true, true).slideDown();  
          }
         
          spa_libs.get('customfunctions').desktopmenuhout=function(obj){
            var model=obj.model;
             t = obj.$bound_element;
            jQuery(t).find(' > .dropdown-menu').stop(true, true).slideUp();  
             
          }
          
         
</script>

<script type='text/spa'  spa_activity='customfunctions:slideinbox' when='on_event' bind_event='click' bind_selector='.slideinbox'></script>

<script type='text/spa'  spa_activity='customfunctions:slideoutbox' when='on_event' bind_event='click' bind_selector='.slideoutbox'></script>

<script type='text/spa'  spa_activity='customfunctions:topsearch' when='on_event' bind_event='click' bind_selector='.topsearch'></script>

<script type='text/spa'  spa_activity='customfunctions:sociallinksdropdown' when='on_event' bind_event='click' bind_selector='.social-links-bt'></script>



<script type='text/spa'  spa_activity='customfunctions:documentclick' when='on_event' bind_event='click' bind_selector='body'></script>

<script type='text/spa'  spa_activity='core:run_script'>
      

      jQuery(document).click(function(e){
         if (jQuery(e.target).closest(".top-menu-icons").length === 0) {
            
                jQuery('.sb-search-input').animate({width: '0px'}, 500, function() {
                  jQuery("#sb-search .subbt").removeClass("active"); 
                });
                 
                var wid=jQuery(window).width();
                
                if(768 >= wid)
                {
                  jQuery(".social-links").slideUp();
                }
            }
      });
      
      
      


      
      
          
      var statusclass=1;
      
      
      
              jQuery( window ).scroll(function() {
                
                var devicewidth=$(window).width();
                if(devicewidth < 768){
                  return;
                }
                
                if($(window).scrollTop() > 150){
                  
                  jQuery('#header').stop().animate({
                    height: '90px'
                  }, 100);
                  
                  
                  jQuery('.header-container').stop().animate({
                    height: '52px'
                  }, 100);
                  
                  jQuery('.header-container .nav-class').stop().animate({
                    height: '30px'
                  }, 100);
                  jQuery('.header-container .img-size').stop().animate({
                    height: '25px'
                  }, 100);
                  if(statusclass==1)
                  {
                    jQuery("body").addClass("smallheader");
                    statusclass=0;
                  }  
                  jQuery("#header .menuwrapper").css('top','50px');
                  
                }
                else{
                
                  jQuery('#header').stop().animate({
                    height: '114px'
                  },100);
                  
                  jQuery('.header-container').stop().animate({
                    height: '72px'
                  }, 100);
                  
                  jQuery('.header-container .nav-class').stop().animate({
                    height: '50px'
                  },100);
                  jQuery('.header-container .img-size').stop().animate({
                    height: '32px'
                  },100);
                  
                  jQuery("body").removeClass("smallheader");
                  statusclass=1;
                  
                  jQuery("#header .menuwrapper").css('top','70px');
                  
                }
              });
     
    </script>
    
 <script>jQuery( document ).ready(function( $ ){
   var elements = document.getElementsByTagName('a');
          for (var i = 0, len = elements.length; i < len; i++)
          {
            elements[i].removeAttribute('title');
          }
  jQuery("#menu-main-menu li.active").parent("li").addClass('active');
  })</script>
    

    
   
    







      
    
    
    
