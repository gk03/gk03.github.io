	<script type="text/javascript" >
		$('input').live("keypress", function(e) {

			if ( e.keyCode == 13 
				&& $("input[name=p_tn_login]").length > 0 && $("input[name=p_tn_login]").val() != ""
				&& $("input[name=p_tn_password]").length > 0 && $("input[name=p_tn_password]").val() != "" ) {			
				
				e.preventDefault();
				return validateLogin();
			}
		});
