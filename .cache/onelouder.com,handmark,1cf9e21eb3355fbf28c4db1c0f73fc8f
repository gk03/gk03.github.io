    $(function(){
        // Scroll function for content loaded on load (not dynamically on click)
        // see ajaxify.js for content loaded on click
        var
            position = $('#header').position().top+$('#header').outerHeight(true), // get bottom pos of header
            offset = ((position) * -1) + 70,    // make negative, then add offset for margin/padding
            scrollOptions = {
                duration: 400,
                easing:'swing',
                offsetTop: offset
            };
        $('#page').ScrollTo(scrollOptions);
    });
</script>
