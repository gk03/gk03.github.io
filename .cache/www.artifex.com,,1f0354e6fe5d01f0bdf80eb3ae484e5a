//<![CDATA[
$(function() {
    $('a.fadehover img').fadeOut(1000).fadeIn(1000);
    $('a.fadehover').hover(function() {
        var fade = $('img', this);
        if (fade.is(':animated')) {
            fade.stop().fadeTo(500, 0);
        } else {
            fade.fadeOut(500);
        }
    }, function () {
        var fade = $('img', this);
        if (fade.is(':animated')) {
            fade.stop().fadeTo(500, 1);
        } else {
            fade.fadeIn(500);
        }
    });
});
//]]>
</script>

