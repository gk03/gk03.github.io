	function validateLogin() {
		if (!document.clientLogin.vchusername.value || !document.clientLogin.vchpassword.value) {
			alert('Please enter your User Name and Password');
			return false;
		}
		return true;
	}

	function setFocus(ele) {
		ele.focus();
		return true;
	}
