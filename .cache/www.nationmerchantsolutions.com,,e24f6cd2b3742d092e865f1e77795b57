<!--Google Map Script-->
    <script type="text/javascript" src="//maps.googleapis.com/maps/api/js?key=AIzaSyB1tbIAqN0XqcgTR1-FxYoVTVq6Is6lD98&sensor=false">
    </script>
    <script type="text/javascript">

        var locations = [
             ['Main office:', 40.945623, -74.017227, '<div class="box-style1">Nationwide Merchant Solutions<br> 195 Main St.<br> New Milford, NJ07646</div>' ], ['Los Angeles Office:', 34.019081, -118.455862, '<div class="box-style1">Nationwide Merchant Solutions<br> 2801 Ocean park Blvd.  #190 <br> Santa monika Ca 90405</div>' ], ['Ohio Branch Office:', 39.274984, -82.823504, '<div class="box-style1">Nationwide Merchant Solutions<br> 66 Margaret Rd <br> Londonderry, OH 45647</div>' ], ['Southern New Jersey Office:', 40.04868, -74.078452, '<div class="box-style1">Nationwide Merchant Solutions<br> 81 Sandy Island Drive <br> Brick, New Jersey 08723</div>' ]        ];
        function initialize() {

            var myOptions = {
                center: new google.maps.LatLng(41.252363, -95.997988),
                zoom: 5,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                disableDefaultUI: true,
                zoomControl: false,
                scrollwheel: false,
                disableDoubleClickZoom: true,
                draggable: false

            };
            var map = new google.maps.Map(document.getElementById("google-map1"),
                myOptions);

            setMarkers(map,locations)

        }



        function setMarkers(map,locations){

            var marker, i

            for (i = 0; i < locations.length; i++)
            {

                var title = locations[i][0]
                var lat = locations[i][1]
                var long = locations[i][2]
                var add =  locations[i][3]

                latlngset = new google.maps.LatLng(lat, long);

                var marker = new google.maps.Marker({
                    map: map, title: title , position: latlngset
                    //, icon: 'http://' /*Hide Marker icon by passing invalid url*/
                });
              //  map.setCenter(marker.getPosition())


                var content = "<h3>" + title +  '</h3>' + "<p>" + add + "</p>";

                var infowindow = new google.maps.InfoWindow()

                google.maps.event.addListener(marker,'click', (function(marker,content,infowindow){
                    return function() {
                        infowindow.setContent(content);
                        infowindow.open(map,marker);
                    };
                })(marker,content,infowindow));
                /*** Show marker by default**
                infowindow.setContent(content);
                infowindow.open(map,marker);
                /****/

            }
        }


        jQuery(document).ready(function(){
            initialize();
        });
    </script>
