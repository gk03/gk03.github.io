	<script>
		jQuery(document).ready(function(){
		
			jQuery("#link_opening").click(function(){	
				jQuery("#link_uploadcv").removeClass('act');
				jQuery(this).addClass('act');
				jQuery(".ctab_toggle").hide();
				jQuery("#curopening").show();				
			});
			
			jQuery("#link_uploadcv").click(function(){				
				jQuery("#link_opening").removeClass('act');
				jQuery(this).addClass('act');
				jQuery(".ctab_toggle").hide();
				jQuery("#upload-cv").show();				
				
			});
			
			jQuery(".job-apply a").click(function(){
				jQuery("#link_opening").removeClass('act');
				jQuery("#link_uploadcv").addClass('act');
				jQuery(".ctab_toggle").hide();
				jQuery("#upload-cv").show();				
			});
			
			jQuery("#openapplyform").click(function(){
				jQuery("#link_uploadcv").removeClass('act');
				jQuery("#link_opening").addClass('act');								
				jQuery(".ctab_toggle").hide();
				jQuery("#curopening").show();	
							
			});
						
			
			
		});
	</script>
<script type="text/javascript">
	if(window.location.hash) 
	{
      var hash = window.location.hash.substring(1); //Puts hash in variable, and removes the # character
      
	  // Upload cv
	   if(hash == "wpcf7-f1384-p39-o1")
	   {
			//alert("YES");
		    jQuery("#link_opening").removeClass('act');
			jQuery(this).addClass('act');
			jQuery("#link_uploadcv").addClass('act');
			jQuery(".ctab_toggle").hide();
			jQuery("#upload-cv").show();
			
			var stateMesaeg= $(".wpcf7-mail-sent-ok").css('visibility');
			 
				
				
				if(stateMesaeg == "visible")
				{
					
					 
					// Remove the # from the hash, as different browsers may or may not include it
					window.location.hash = '';
					
					setTimeout(function() {
						
						
						$(".wpcf7-mail-sent-ok").fadeOut('fast');
						
					}, 5000);
					

					
				}
		   
	   }
	} 
</script>
