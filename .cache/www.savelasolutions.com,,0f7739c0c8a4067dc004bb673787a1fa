    jQuery(function($) {
        $('.responsive-menu-btn').on('click', function() {
            $('.responsive-nav-wrap').slideToggle();
        });

        $('.footer-wrap').backstretch('http://www.pangeatwo.com/wp-content/themes/pangeatwo/style/images/bham-skyline.jpg');
        
        if( $(window).width() <= 820 ) {
            var searchLabel = $('.search-label').text();
            $('#searchBarTextBox').attr('placeholder', searchLabel );
            $('.search-label').hide();
        }
    });
</script>
