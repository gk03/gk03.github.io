<script>
    jQuery(document).ready(function($) {
        setTimeout(function() {
            if( jQuery('.rotator-wrap').height() == 0) {
                jQuery('.rotator-wrap').height(804);
            }
        }, 500);
    });

    jQuery('.rotators-wrap').height(804);
    
        var imagesArr = [
                        'http://www.pangeatwo.com/wp-content/uploads/2015/12/rotator-1.jpg',
                        'http://www.pangeatwo.com/wp-content/uploads/2015/12/rotator-2.jpg',
                        'http://www.pangeatwo.com/wp-content/uploads/2015/12/rotator-3.jpg',
                    ];
        
        var rotator = jQuery('.rotator-wrap').backstretch(imagesArr, { duration: 8000, fade: 1000 });

        rotator.on("backstretch.before", function (e, instance, index) {
            jQuery('.rotators-wrap').cycle('next');
        });

</script>
