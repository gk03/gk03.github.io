function onHover() {
    $("#clientImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_clients_select.jpg');
}
 
function offHover() {
    $("#clientImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_clients_nm.jpg');
}

function onHover2() {
  $("#candImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_candidates_select.jpg');
}
function offHover2() {
  $("#candImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_candidates_nm.jpg');
}

function onHover3() {
  $("#empImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_employees_select.jpg');
}
function offHover3() {
  $("#empImg").attr('src', 'http://tplusinc.com/wordpress/wp-content/themes/techplus/foundation/img/stock_employees_nm.jpg');
}

